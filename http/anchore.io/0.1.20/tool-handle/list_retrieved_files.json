{
  "name": "list_retrieved_files",
  "description": "Return a list of analyzer artifacts of the specified type",
  "parameters": {
    "type": "object",
    "properties": {
      "imageDigest": {
        "type": "string"
      }
    },
    "required": [
      "imageDigest"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//images/{imageDigest}/artifacts/retrieved_files"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Simple array of RetrievedFileMetadata objects",
        "**Key properties:**",
        {
          "$ul": [
            "**b64_content**",
            "**path**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
