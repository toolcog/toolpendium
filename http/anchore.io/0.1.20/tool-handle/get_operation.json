{
  "name": "get_operation",
  "description": "Get detail on a single import",
  "parameters": {
    "type": "object",
    "properties": {
      "operation_id": {
        "type": "string"
      }
    },
    "required": [
      "operation_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//imports/images/{operation_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "An import record, creating a unique identifier for referencing the operation as well as its state",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**expires_at**",
            "**status**",
            "**uuid**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
