{
  "name": "import_image_manifest",
  "description": "Import a docker or OCI distribution manifest to associate with the image",
  "parameters": {
    "type": "object",
    "properties": {
      "operation_id": {
        "type": "string"
      }
    },
    "required": [
      "operation_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//imports/images/{operation_id}/manifest"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**digest**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
