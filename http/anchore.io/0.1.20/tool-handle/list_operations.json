{
  "name": "list_operations",
  "description": "Lists in-progress imports",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//imports/images"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "An import record, creating a unique identifier for referencing the operation as well as its state",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**expires_at**",
            "**status**",
            "**uuid**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
