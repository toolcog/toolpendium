{
  "name": "list_import_packages",
  "description": "List uploaded package manifests",
  "parameters": {
    "type": "object",
    "properties": {
      "operation_id": {
        "type": "string"
      }
    },
    "required": [
      "operation_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//imports/images/{operation_id}/packages"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "String digest of an uploaded content",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
