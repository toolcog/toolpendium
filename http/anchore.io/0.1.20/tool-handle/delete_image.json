{
  "name": "delete_image",
  "description": "Delete an image analysis",
  "parameters": {
    "type": "object",
    "properties": {
      "imageDigest": {
        "type": "string"
      },
      "force": {
        "type": "boolean"
      },
      "x-anchore-account": {
        "type": "string",
        "description": "An account name to change the resource scope of the request to that account, if permissions allow (admin only)"
      }
    },
    "required": [
      "imageDigest"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "//images/{imageDigest}{?force}"
    },
    "headers": {
      "x-anchore-account": {
        "$": "x-anchore-account"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Image deletion response containing status and details",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**digest**",
            "**status**: Current status of the image deletion"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
