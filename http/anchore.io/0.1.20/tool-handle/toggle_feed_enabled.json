{
  "name": "toggle_feed_enabled",
  "description": "Disable the feed so that it does not sync on subsequent sync operations",
  "parameters": {
    "type": "object",
    "properties": {
      "feed": {
        "type": "string"
      },
      "enabled": {
        "type": "boolean"
      }
    },
    "required": [
      "feed",
      "enabled"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "//system/feeds/{feed}{?enabled}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Metadata on a single feed based on what the engine finds from querying the endpoints",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**: Date the metadata record was created in engine (first seen on source)",
            "**groups**",
            "**last_full_sync**",
            "**name**: name of the feed",
            "**updated_at**: Date the metadata was last updated"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
