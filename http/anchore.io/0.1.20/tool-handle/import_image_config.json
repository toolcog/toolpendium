{
  "name": "import_image_config",
  "description": "Import a docker or OCI image config to associate with the image",
  "parameters": {
    "type": "object",
    "properties": {
      "operation_id": {
        "type": "string"
      },
      "body": {
        "type": "object"
      }
    },
    "required": [
      "operation_id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//imports/images/{operation_id}/image_config"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**digest**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
