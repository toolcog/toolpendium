{
  "name": "getSiteAssetsOfInterest",
  "description": "Get a list of BLE beacons that matches Asset or AssetFilter",
  "parameters": {
    "type": "object",
    "properties": {
      "duration": {
        "default": "1d",
        "example": "10m",
        "type": "string",
        "description": "For historical stats and/or logs where time range is needed, you can specify the time range in a few different ways:\n  * ?start=1430000000&end=1430864000\tspecify the start / end\n  * ?end=1430864000&duration=1d\tspecify end time and duration\n  * ?duration=1d\tspecify duration, end will be now() in seconds"
      },
      "start": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "end": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer"
      },
      "limit": {
        "default": 100,
        "minimum": 0,
        "type": "integer"
      },
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "site_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/stats/filtered_assets{?duration,start,end,page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**_checkpoint_prep**",
            "**_checkpoint_preparer**",
            "**_checkpoint_scan**",
            "**_id**",
            "**_timestamp**",
            "**_ttl**",
            "**ap_mac**",
            "**beam**",
            "**by**",
            "**curr_site**",
            "**device_name**",
            "**id**",
            "**last_seen**",
            "**mac**",
            "**manufacture**",
            "**map_id**",
            "**name**",
            "**rssi**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
