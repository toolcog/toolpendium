{
  "name": "getSiteLicenseUsage",
  "description": "This shows license usage (i.e. needed) based on the features enabled for the site.",
  "parameters": {
    "type": "object",
    "properties": {
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "site_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/licenses/usages"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**org_entitled**: license entitlement for the entire org",
            "**svna_enabled**: license entitlement for the Switch device whether SVNA enabled",
            "**trial_enabled**",
            "**usages**: subscriptions and their quantities",
            "**vna_enabled**: license entitlement for the AP device whether VNA enabled",
            "**wvna_enabled**: license entitlement for the Gateway device whether WVNA enabled"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
