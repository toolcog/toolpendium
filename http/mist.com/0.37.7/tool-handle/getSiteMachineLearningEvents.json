{
  "name": "getSiteMachineLearningEvents",
  "description": "Get Machine Learning Events",
  "parameters": {
    "type": "object",
    "properties": {
      "device_id": {
        "type": "string",
        "description": "device_id (as filter, optional)"
      },
      "map_ip": {
        "type": "string",
        "description": "map_id (as filter, optional)"
      },
      "client_type": {
        "type": "string",
        "description": "client_type (as filter, optional)"
      },
      "duration": {
        "type": "string",
        "description": "instead of start, you can use 1d, 30m, 5h, where the start will be calculated"
      },
      "start": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "end": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "interval": {
        "type": "string"
      },
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "site_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
