{
  "name": "listApChannels",
  "description": "Get List of List of Available channels per country code",
  "parameters": {
    "type": "object",
    "properties": {
      "country_code": {
        "example": "US",
        "pattern": "^[a-zA-Z]{2}$",
        "type": "string",
        "description": "country code, in two-character"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/const/ap_channels{?country_code}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**band24_channels**: The property key is the channel width",
            "**band24_enabled**",
            "**band5_channels**: The property key is the channel width",
            "**band5_enabled**",
            "**code**",
            "**dfs_ok**",
            "**key**",
            "**name**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
