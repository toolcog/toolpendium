{
  "name": "getSiteDeviceRadioChannels",
  "description": "Get a list of allowed channels (per channel width)",
  "parameters": {
    "type": "object",
    "properties": {
      "country_code": {
        "type": "string",
        "description": "country code for the site (for AP config generation), in [two-character](http://en.wikipedia.org/wiki/ISO_3166-1_alpha-2)"
      },
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "site_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/devices/ap_channels{?country_code}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**band24_40mhz_allowed**",
            "**band24_channels**",
            "**band24_enabled**",
            "**band5_channels**",
            "**band5_enabled**",
            "**certified**",
            "**code**",
            "**dfs_ok**",
            "**key**",
            "**name**",
            "**uses**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
