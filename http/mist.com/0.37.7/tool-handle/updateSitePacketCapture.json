{
  "name": "updateSitePacketCapture",
  "description": "Update or add notes to a completed packet capture",
  "parameters": {
    "type": "object",
    "properties": {
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "pcap_id": {
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "properties": {
          "notes": {
            "example": "wired pcap test",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "site_id",
      "pcap_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/pcaps/{pcap_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
