{
  "name": "getSiteSleImpactedChassis",
  "description": "For Wired and WAN SLEs. Get list of impacted interfaces optionally filtered by classifier and failure type",
  "parameters": {
    "type": "object",
    "properties": {
      "start": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "end": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "duration": {
        "default": "1d",
        "example": "10m",
        "type": "string",
        "description": "For historical stats and/or logs where time range is needed, you can specify the time range in a few different ways:\n  * ?start=1430000000&end=1430864000\tspecify the start / end\n  * ?end=1430864000&duration=1d\tspecify end time and duration\n  * ?duration=1d\tspecify duration, end will be now() in seconds"
      },
      "classifier": {
        "type": "string"
      },
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "scope": {
        "enum": [
          "site",
          "switch",
          "gateway"
        ],
        "type": "string"
      },
      "scope_id": {
        "format": "uuid",
        "type": "string"
      },
      "metric": {
        "type": "string",
        "description": "values from /api/v1/sites/{site_id}/sle/{scope}/{scope_id}/metrics"
      }
    },
    "required": [
      "site_id",
      "scope",
      "scope_id",
      "metric"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/sle/{scope}/{scope_id}/metric/{metric}/impacted-chassis{?start,end,duration,classifier}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**chassis**",
            "**classifier**",
            "**end**",
            "**failure**",
            "**limit**",
            "**metric**",
            "**page**",
            "**start**",
            "**total_count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
