{
  "name": "listMxEdgeModels",
  "description": "Get List of available Mx Edge models",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/const/mxedge_models"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**custom_ports**",
            "**display**",
            "**model**",
            [
              "**ports**",
              {
                "$ul": [
                  [
                    "**0**",
                    {
                      "$ul": [
                        "**display**",
                        "**speed**"
                      ]
                    }
                  ],
                  [
                    "**1**",
                    {
                      "$ul": [
                        "**display**",
                        "**speed**"
                      ]
                    }
                  ],
                  [
                    "**2**",
                    {
                      "$ul": [
                        "**display**",
                        "**speed**"
                      ]
                    }
                  ],
                  [
                    "**3**",
                    {
                      "$ul": [
                        "**display**",
                        "**speed**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
