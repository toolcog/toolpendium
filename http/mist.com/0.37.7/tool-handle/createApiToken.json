{
  "name": "createApiToken",
  "description": "Create API Token\nNote that the key is only available during creation time.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "name": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.mist.com/api/v1/self/apitokens"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "API Token",
        "**Key properties:**",
        {
          "$ul": [
            "**created_by**: for Org token only. email of the token creator / null if creator is deleted",
            "**created_time**",
            "**id**",
            "**key**",
            "**last_used**",
            "**name**: name of the token",
            "**org_id**",
            "**privileges**: list of privileges the token has on the orgs/sites"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
