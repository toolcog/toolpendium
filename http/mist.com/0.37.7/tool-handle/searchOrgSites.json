{
  "name": "searchOrgSites",
  "description": "Search Sites",
  "parameters": {
    "type": "object",
    "properties": {
      "analytic_enabled": {
        "type": "boolean",
        "description": "if Advanced Analytic feature is enabled"
      },
      "app_waking": {
        "type": "boolean",
        "description": "if App Waking feature is enabled"
      },
      "asset_enabled": {
        "type": "boolean",
        "description": "if Asset Tracking is enabled"
      },
      "auto_upgrade_enabled": {
        "type": "boolean",
        "description": "if Auto Upgrade feature is enabled"
      },
      "auto_upgrade_version": {
        "type": "string",
        "description": "if Auto Upgrade feature is enabled"
      },
      "country_code": {
        "type": "string",
        "description": "site country code"
      },
      "honeypot_enabled": {
        "type": "boolean",
        "description": "if Honeypot detection is enabled"
      },
      "id": {
        "type": "string",
        "description": "site id"
      },
      "locate_unconnected": {
        "type": "boolean",
        "description": "if unconnected client are located"
      },
      "mesh_enabled": {
        "type": "boolean",
        "description": "if Mesh feature is enabled"
      },
      "name": {
        "type": "string",
        "description": "site name"
      },
      "rogue_enabled": {
        "type": "boolean",
        "description": "if Rogue detection is enabled"
      },
      "remote_syslog_enabled": {
        "type": "boolean",
        "description": "if Remote Syslog is enabled"
      },
      "rtsa_enabled": {
        "type": "boolean",
        "description": "if managed mobility feature is enabled"
      },
      "vna_enabled": {
        "type": "boolean",
        "description": "if Virtual Network Assistant is enabled"
      },
      "wifi_enabled": {
        "type": "boolean",
        "description": "if WIFI feature is enabled"
      },
      "limit": {
        "default": 100,
        "minimum": 0,
        "type": "integer"
      },
      "start": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "end": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "duration": {
        "default": "1d",
        "example": "10m",
        "type": "string",
        "description": "For historical stats and/or logs where time range is needed, you can specify the time range in a few different ways:\n  * ?start=1430000000&end=1430864000\tspecify the start / end\n  * ?end=1430864000&duration=1d\tspecify end time and duration\n  * ?duration=1d\tspecify duration, end will be now() in seconds"
      },
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "org_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/orgs/{org_id}/sites/search{?analytic_enabled,app_waking,asset_enabled,auto_upgrade_enabled,auto_upgrade_version,country_code,honeypot_enabled,id,locate_unconnected,mesh_enabled,name,rogue_enabled,remote_syslog_enabled,rtsa_enabled,vna_enabled,wifi_enabled,limit,start,end,duration}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**end**",
            "**limit**",
            "**next**",
            "**results**",
            "**start**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
