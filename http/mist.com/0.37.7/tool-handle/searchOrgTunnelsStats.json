{
  "name": "searchOrgTunnelsStats",
  "description": "Search Org Tunnels Stats",
  "parameters": {
    "type": "object",
    "properties": {
      "mxcluster_id": {
        "type": "string",
        "description": "if `type`==`wxtunnel`"
      },
      "site_id": {
        "type": "string"
      },
      "wxtunnel_id": {
        "type": "string",
        "description": "if `type`==`wxtunnel`"
      },
      "ap": {
        "type": "string",
        "description": "if `type`==`wxtunnel`"
      },
      "mac": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "node": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "peer_ip": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "peer_host": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "ip": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "tunnel_name": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "protocol": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "auth_algo": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "encrypt_algo": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "ike_version": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "up": {
        "type": "string",
        "description": "if `type`==`wan`"
      },
      "type": {
        "default": "wxtunnel",
        "enum": [
          "wxtunnel",
          "wan"
        ],
        "type": "string"
      },
      "limit": {
        "default": 100,
        "minimum": 0,
        "type": "integer"
      },
      "start": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "end": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "duration": {
        "default": "1d",
        "example": "10m",
        "type": "string",
        "description": "For historical stats and/or logs where time range is needed, you can specify the time range in a few different ways:\n  * ?start=1430000000&end=1430864000\tspecify the start / end\n  * ?end=1430864000&duration=1d\tspecify end time and duration\n  * ?duration=1d\tspecify duration, end will be now() in seconds"
      },
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "org_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/orgs/{org_id}/stats/tunnels/search{?mxcluster_id,site_id,wxtunnel_id,ap,mac,node,peer_ip,peer_host,ip,tunnel_name,protocol,auth_algo,encrypt_algo,ike_version,up,type,limit,start,end,duration}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**end**",
            "**limit**",
            "**next**",
            "**results**",
            "**start**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
