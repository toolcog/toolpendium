{
  "name": "getOrgMxEdgeUpgradeInfo",
  "description": "Get Mist Edge Upgrade Information",
  "parameters": {
    "type": "object",
    "properties": {
      "channel": {
        "default": "stable",
        "enum": [
          "stable",
          "beta",
          "alpha"
        ],
        "type": "string",
        "description": "upgrade channel to follow, stable (default) / beta / alpha"
      },
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "org_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/orgs/{org_id}/mxedges/version{?channel}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**default**",
            "**package**",
            "**version**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
