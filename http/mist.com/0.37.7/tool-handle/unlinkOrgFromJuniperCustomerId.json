{
  "name": "unlinkOrgFromJuniperCustomerId",
  "description": "Unlink Juniper Customer ID",
  "parameters": {
    "type": "object",
    "properties": {
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "properties": {
          "account_name": {
            "example": "ABC Corp",
            "type": "string"
          },
          "linked_by": {
            "description": "required only if there are duplicate account_names",
            "example": "John Smith (john@abccorp.com)",
            "type": "string"
          }
        },
        "required": [
          "account_name"
        ],
        "type": "object",
        "x-examples": {
          "example-1": {
            "account_name": "ABC Corp"
          }
        }
      }
    },
    "required": [
      "org_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.mist.com/api/v1/orgs/{org_id}/setting/juniper/linked_account"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
