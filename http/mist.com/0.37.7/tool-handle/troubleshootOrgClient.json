{
  "name": "troubleshootOrgClient",
  "description": "Troubleshoot sites, devices, clients, and wired clients. See search APIs for device information:\n- [search Device](/#tag/Orgs-Devices/operation/searchOrgDevices)\n- [search Wireless Client](/#tag/Orgs-Devices/operation/searchOrgClientsWireless)\n- [search Wired Client](/#tag/Orgs-Devices/operation/searchOrgClientsWired)\n\n**NOTE**: requires Marvis subscription license",
  "parameters": {
    "type": "object",
    "properties": {
      "mac": {
        "type": "string",
        "description": "**required** when troubleshooting device"
      },
      "site_id": {
        "format": "uuid",
        "type": "string",
        "description": "**required** when troubleshooting site"
      },
      "start": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "end": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "type": {
        "enum": [
          "wireless",
          "wired",
          "wan"
        ],
        "type": "string",
        "description": "when troubleshooting site, type of network to troubleshoot"
      },
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "org_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/orgs/{org_id}/troubleshoot{?mac,site_id,start,end,type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**end**",
            "**results**",
            "**start**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
