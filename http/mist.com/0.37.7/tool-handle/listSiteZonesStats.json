{
  "name": "listSiteZonesStats",
  "description": "Get List of Site Zones Stats",
  "parameters": {
    "type": "object",
    "properties": {
      "map_id": {
        "type": "string"
      },
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "site_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/stats/zones{?map_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Zone statistics",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**assets_waits**: ble asset wait time right now",
              {
                "$ul": [
                  "**avg**: average wait time in seconds",
                  "**max**: longest wait time in seconds",
                  "**min**: shortest wait time in seconds",
                  "**p95**: 95th percentile of all the wait time(s)"
                ]
              }
            ],
            [
              "**clients_waits**: client wait time right now",
              {
                "$ul": [
                  "**avg**: average wait time in seconds",
                  "**max**: longest wait time in seconds",
                  "**min**: shortest wait time in seconds",
                  "**p95**: 95th percentile of all the wait time(s)"
                ]
              }
            ],
            "**created_time**",
            "**id**: id of the zone",
            "**map_id**: map_id of the zone",
            "**modified_time**",
            "**name**: name of the zone",
            "**num_assets**: number of assets",
            "**num_clients**: number of wifi clients (unconnected + connected)",
            "**num_sdkclients**: number of sdk clients",
            "**occupancy_limit**",
            "**org_id**",
            [
              "**sdkclients_waits**: sdkclient wait time right now",
              {
                "$ul": [
                  "**avg**: average wait time in seconds",
                  "**max**: longest wait time in seconds",
                  "**min**: shortest wait time in seconds",
                  "**p95**: 95th percentile of all the wait time(s)"
                ]
              }
            ],
            "**site_id**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
