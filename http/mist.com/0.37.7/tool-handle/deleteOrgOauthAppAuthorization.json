{
  "name": "deleteOrgOauthAppAuthorization",
  "description": "Delete Org Level OAuth Application Authorization With Mist Portal",
  "parameters": {
    "type": "object",
    "properties": {
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "app_name": {
        "type": "string",
        "description": "OAuth application name (Example : zoom, teams, etc..)"
      },
      "body": {
        "properties": {
          "account_id": {
            "description": "Linked app(zoom/teams) account id",
            "type": "string"
          }
        },
        "required": [
          "account_id"
        ],
        "type": "object",
        "x-examples": {
          "Example 1": {
            "account_id": "iojzXIJWEuiD73ZvydOfg"
          }
        }
      }
    },
    "required": [
      "org_id",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.mist.com/api/v1/orgs/{org_id}/setting/{app_name}/link_accounts"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
