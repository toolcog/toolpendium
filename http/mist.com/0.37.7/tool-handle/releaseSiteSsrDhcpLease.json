{
  "name": "releaseSiteSsrDhcpLease",
  "description": "Releases an active DHCP lease.",
  "parameters": {
    "type": "object",
    "properties": {
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "device_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "properties": {
          "node": {
            "description": "only for HA",
            "enum": [
              "node0",
              "node1"
            ],
            "type": "string"
          },
          "port": {
            "description": "The nework interface on which to release the current DHCP release",
            "example": "wan-interface",
            "minLength": 1,
            "type": "string"
          }
        },
        "required": [
          "port"
        ],
        "type": "object",
        "x-examples": {
          "example-1": {
            "network_interface": "wan-interface"
          }
        }
      }
    },
    "required": [
      "site_id",
      "device_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/devices/{device_id}/release_dhcp"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
