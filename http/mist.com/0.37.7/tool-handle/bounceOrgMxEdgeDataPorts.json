{
  "name": "bounceOrgMxEdgeDataPorts",
  "description": "Bounce TunTerm Data Ports",
  "parameters": {
    "type": "object",
    "properties": {
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "mxedge_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "properties": {
          "hold_time": {
            "description": "in milli seconds, hold time between multiple port bounces",
            "type": "integer"
          },
          "ports": {
            "description": "list of ports to bounce",
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "ports"
        ],
        "type": "object"
      }
    },
