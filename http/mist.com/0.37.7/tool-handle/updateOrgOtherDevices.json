          "device_mac": {
            "type": "string"
          },
          "macs": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "site_id": {
            "description": "Either a site_id OR a device_mac must be provided",
            "type": "string"
          }
        },
        "type": "object",
