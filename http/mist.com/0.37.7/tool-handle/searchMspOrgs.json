{
  "name": "searchMspOrgs",
  "description": "Search Org in MSP",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "default": "",
        "type": "string"
      },
      "org_id": {
        "format": "uuid",
        "type": "string",
        "description": "org id"
      },
      "sub_insufficient": {
        "type": "boolean",
        "description": "if this org has sufficient subscription"
      },
      "trial_enabled": {
        "type": "boolean",
        "description": "if this org is under trial period"
      },
      "usage_types": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "a list of types that enabled by usage"
      },
      "limit": {
        "default": 100,
        "minimum": 0,
        "type": "integer"
      },
      "msp_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "msp_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/msps/{msp_id}/orgs/search{?name,org_id,sub_insufficient,trial_enabled,usage_types,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**end**",
            "**limit**",
            "**next**",
            "**results**",
            "**start**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
