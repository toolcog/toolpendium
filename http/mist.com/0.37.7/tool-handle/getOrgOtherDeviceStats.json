  "name": "getOrgOtherDeviceStats",
  "description": "Get Otherdevice Stats",
  "parameters": {
    "type": "object",
    "properties": {
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "device_mac": {
        "example": "0000000000ab",
        "pattern": "^[0-9a-fA-F]{12}$",
        "type": "string"
      }
    },
    "required": [
      "org_id",
      "device_mac"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
