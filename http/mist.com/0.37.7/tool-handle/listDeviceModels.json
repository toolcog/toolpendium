  "name": "listDeviceModels",
  "description": "Get list of AP device models for the Mist Site",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/const/device_models"
    }
  },
  "responses": {
