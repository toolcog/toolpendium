  "name": "startSiteApAutoOrientation",
  "description": "This API is used to trigger a map for Auto orientation",
  "parameters": {
    "type": "object",
    "properties": {
      "map_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "map_id",
      "site_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/maps/{map_id}/auto_orient"
    }
  },
