{
  "name": "searchSiteClientsWired",
  "description": "Search Wired Clients",
  "parameters": {
    "type": "object",
    "properties": {
      "device_mac": {
        "type": "string",
        "description": "device mac"
      },
      "mac": {
        "type": "string",
        "description": "client mac"
      },
      "port_id": {
        "type": "string",
        "description": "port id"
      },
      "vlan": {
        "type": "string",
        "description": "vlan"
      },
      "ip address": {
        "example": "192.168.1.1",
        "format": "ipv4",
        "type": "string"
      },
      "manufacture": {
        "type": "string",
        "description": "manufacture"
      },
      "text": {
        "type": "string",
        "description": "single entry of hostname/mac"
      },
      "limit": {
        "default": 100,
        "minimum": 0,
        "type": "integer"
      },
      "start": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "end": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "duration": {
        "default": "1d",
        "example": "10m",
        "type": "string",
        "description": "For historical stats and/or logs where time range is needed, you can specify the time range in a few different ways:\n  * ?start=1430000000&end=1430864000\tspecify the start / end\n  * ?end=1430864000&duration=1d\tspecify end time and duration\n  * ?duration=1d\tspecify duration, end will be now() in seconds"
      },
      "site_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "site_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/sites/{site_id}/wired_clients/search{?device_mac,mac,port_id,vlan,ip address,manufacture,text,limit,start,end,duration}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**end**",
            "**limit**",
            "**next**",
            "**results**",
            "**start**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
