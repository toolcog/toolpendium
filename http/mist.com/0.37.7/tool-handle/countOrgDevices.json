{
  "name": "countOrgDevices",
  "description": "Count Org Devices",
  "parameters": {
    "type": "object",
    "properties": {
      "distinct": {
        "default": "model",
        "enum": [
          "hostname",
          "site_id",
          "model",
          "mac",
          "version",
          "ip",
          "mxtunnel_status",
          "mxedge_id",
          "lldp_system_name",
          "lldp_system_desc",
          "lldp_port_id",
          "lldp_mgmt_addr"
        ],
        "type": "string"
      },
      "hostname": {
        "type": "string",
        "description": "partial / full hostname"
      },
      "site_id": {
        "type": "string",
        "description": "site id"
      },
      "model": {
        "type": "string",
        "description": "device model"
      },
      "mac": {
        "type": "string",
        "description": "AP mac"
      },
      "version": {
        "type": "string",
        "description": "version"
      },
      "ip address": {
        "example": "192.168.1.1",
        "format": "ipv4",
        "type": "string"
      },
      "mxtunnel_status": {
        "enum": [
          "up",
          "down"
        ],
        "type": "string",
        "description": "MxTunnel status, up / down"
      },
      "mxedge_id": {
        "type": "string",
        "description": "Mist Edge id, if AP is connecting to a Mist Edge"
      },
      "lldp_system_name": {
        "type": "string",
        "description": "LLDP system name"
      },
      "lldp_system_desc": {
        "type": "string",
        "description": "LLDP system description"
      },
      "lldp_port_id": {
        "type": "string",
        "description": "LLDP port id"
      },
      "lldp_mgmt_addr": {
        "type": "string",
        "description": "LLDP management ip address"
      },
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer"
      },
      "limit": {
        "default": 100,
        "minimum": 0,
        "type": "integer"
      },
      "start": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "end": {
        "default": 0,
        "minimum": 0,
        "type": "integer"
      },
      "duration": {
        "default": "1d",
        "example": "10m",
        "type": "string",
        "description": "For historical stats and/or logs where time range is needed, you can specify the time range in a few different ways:\n  * ?start=1430000000&end=1430864000\tspecify the start / end\n  * ?end=1430864000&duration=1d\tspecify end time and duration\n  * ?duration=1d\tspecify duration, end will be now() in seconds"
      },
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "distinct",
      "org_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mist.com/api/v1/orgs/{org_id}/devices/count{?distinct,hostname,site_id,model,mac,version,ip address,mxtunnel_status,mxedge_id,lldp_system_name,lldp_system_desc,lldp_port_id,lldp_mgmt_addr,page,limit,start,end,duration}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**distinct**",
            "**end**",
            "**limit**",
            "**results**",
            "**start**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
