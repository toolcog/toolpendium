{
  "name": "updateOrgOtherDevice",
  "description": "If the Site / Device cannot be identified, a manual association can be made",
  "parameters": {
    "type": "object",
    "properties": {
      "org_id": {
        "example": "000000ab-00ab-00ab-00ab-0000000000ab",
        "format": "uuid",
        "type": "string"
      },
      "device_mac": {
        "example": "0000000000ab",
        "pattern": "^[0-9a-fA-F]{12}$",
        "type": "string"
      },
      "body": {
        "properties": {
          "device_mac": {
            "type": "string"
          },
          "site_id": {
            "type": "string"
          }
        },
        "type": "object",
