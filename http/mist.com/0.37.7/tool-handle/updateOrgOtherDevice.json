      "org_id",
      "device_mac"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.mist.com/api/v1/orgs/{org_id}/otherdevices/{device_mac}"
    },
    "body": {
      "$": "body",
