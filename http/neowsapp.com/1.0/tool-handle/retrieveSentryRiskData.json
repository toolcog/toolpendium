{
  "name": "retrieveSentryRiskData",
  "description": "Retrieves Near Earth Objects listed in the NASA sentry data set",
  "parameters": {
    "type": "object",
    "properties": {
      "is_active": {
        "default": true,
        "type": "boolean",
        "description": "show current list of Sentry objects, or show removed Sentry objects"
      },
      "page": {
        "default": 0,
        "format": "int32",
        "type": "integer",
        "description": "page"
      },
      "size": {
        "default": 50,
        "format": "int32",
        "type": "integer",
        "description": "size"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://www.neowsapp.com//rest/v1/neo/sentry{?is_active,page,size}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**links**",
            [
              "**page**",
              {
                "$ul": [
                  "**number**",
                  "**size**",
                  "**total_elements**",
                  "**total_pages**"
                ]
              }
            ],
            "**sentry_objects**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
