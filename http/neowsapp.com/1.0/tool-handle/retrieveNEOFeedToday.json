{
  "name": "retrieveNEOFeedToday",
  "description": "Get a list of Near Earth Objects for today",
  "parameters": {
    "type": "object",
    "properties": {
      "detailed": {
        "type": "boolean",
        "description": "detailed"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://www.neowsapp.com//rest/v1/feed/today{?detailed}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**element_count**: Number of NEOs in list",
            "**links**: Hypermedia links to collection",
            "**near_earth_objects**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
