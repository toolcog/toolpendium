{
  "name": "retrieveCurrentNeoStatistics",
  "description": "retrieveCurrentNeoStatistics",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://www.neowsapp.com//rest/v1/stats"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**close_approach_count**: Number of Close Approach listings across all NEOs listed in NeoWs",
            "**last_updated**: TimeStamp the last time the DB was updated",
            [
              "**nasa_jpl_url**",
              {
                "$ul": [
                  "**authority**",
                  "**content**",
                  "**defaultPort**",
                  "**file**",
                  "**host**",
                  "**path**",
                  "**port**",
                  "**protocol**",
                  "**query**",
                  "**ref**",
                  "**userInfo**"
                ]
              }
            ],
            "**near_earth_object_count**: Number of NearEarthObjects listed in NeoWs",
            "**source**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
