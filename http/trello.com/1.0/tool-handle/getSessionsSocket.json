{
  "name": "getSessionsSocket",
  "description": "This is the route for WebSocket requests.  See the socket API reference for a description of WebSocket usage.",
  "parameters": {
    "type": "object",
    "properties": {
      "key": {
        "type": "string",
        "description": "<a href=\"https://trello.com/1/appKey/generate\"  target=\"_blank\">Generate your application key</a>"
      },
      "token": {
        "type": "string",
        "description": "<a href=\"https://trello.com/docs/gettingstarted/index.html#getting-a-token-from-a-user\"  target=\"_blank\">Getting a token from a user</a>"
      }
    },
    "required": [
      "key",
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://trello.com/1/sessions/socket{?key,token}"
    }
  },
  "responses": {}
}
