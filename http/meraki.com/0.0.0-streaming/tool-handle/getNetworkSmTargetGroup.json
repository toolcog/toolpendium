{
  "name": "getNetworkSmTargetGroup",
  "description": "Return a target group",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "targetGroupId": {
        "type": "string"
      },
      "withDetails": {
        "type": "boolean",
        "description": "Boolean indicating if the the ids of the devices or users scoped by the target group should be included in the response"
      }
    },
    "required": [
      "networkId",
      "targetGroupId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v0/networks/{networkId}/sm/targetGroups/{targetGroupId}{?withDetails}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
