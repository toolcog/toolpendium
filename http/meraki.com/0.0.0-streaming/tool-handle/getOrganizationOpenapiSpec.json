{
  "name": "getOrganizationOpenapiSpec",
  "description": "Return the OpenAPI 2.0 Specification of the organization's API documentation in JSON",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v0/organizations/{organizationId}/openapiSpec"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
