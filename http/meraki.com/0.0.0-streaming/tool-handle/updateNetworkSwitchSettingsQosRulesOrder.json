{
  "name": "updateNetworkSwitchSettingsQosRulesOrder",
  "description": "Update the order in which the rules should be processed by the switch",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "body": {
        "example": {
          "ruleIds": [
            "1284392014819",
            "2983092129865"
          ]
        },
        "properties": {
          "ruleIds": {
            "description": "A list of quality of service rule IDs arranged in order in which they should be processed by the switch.",
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "ruleIds"
        ],
        "type": "object"
      }
    },
    "required": [
      "networkId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v0/networks/{networkId}/switch/settings/qosRules/order"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
