{
  "name": "claimNetworkDevices",
  "description": "Claim devices into a network. (Note: for recently claimed devices, it may take a few minutes for API requests against that device to succeed)",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "body": {
        "example": {
          "serials": [
            "Q234-ABCD-0001",
            "Q234-ABCD-0002",
            "Q234-ABCD-0003"
          ]
        },
        "properties": {
          "serial": {
            "description": "[DEPRECATED] The serial of a device to claim",
            "type": "string"
          },
          "serials": {
            "description": "A list of serials of devices to claim",
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v0/networks/{networkId}/devices/claim"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
