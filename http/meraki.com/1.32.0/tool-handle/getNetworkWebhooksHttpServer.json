{
  "name": "getNetworkWebhooksHttpServer",
  "description": "Return an HTTP server for a network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "httpServerId": {
        "type": "string"
      }
    },
    "required": [
      "networkId",
      "httpServerId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/webhooks/httpServers/{httpServerId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**: A Base64 encoded ID.",
            "**name**: A name for easy reference to the HTTP server",
            "**networkId**: A Meraki network ID.",
            [
              "**payloadTemplate**: The payload template to use when posting data to the HTTP server.",
              {
                "$ul": [
                  "**name**: The name of the payload template.",
                  "**payloadTemplateId**: The ID of the payload template."
                ]
              }
            ],
            "**url**: The URL of the HTTP server."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
