{
  "name": "updateOrganizationEarlyAccessFeaturesOptIn",
  "description": "Update an early access feature opt-in for an organization",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "optInId": {
        "type": "string"
      },
      "body": {
        "example": {
          "limitScopeToNetworks": [
            "N_12345"
          ]
        },
        "properties": {
          "limitScopeToNetworks": {
            "description": "A list of network IDs to apply the opt-in to",
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "organizationId",
      "optInId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/earlyAccess/features/optIns/{optInId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
