{
  "name": "getOrganizationCameraOnboardingStatuses",
  "description": "Fetch onboarding status of cameras",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "serials": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "A list of serial numbers. The returned cameras will be filtered to only include these serials."
      },
      "networkIds": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "A list of network IDs. The returned cameras will be filtered to only include these networks."
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/camera/onboarding/statuses{?serials,networkIds}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
