{
  "name": "createDeviceApplianceVmxAuthenticationToken",
  "description": "Generate a new vMX authentication token",
  "parameters": {
    "type": "object",
    "properties": {
      "serial": {
        "type": "string"
      }
    },
    "required": [
      "serial"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/devices/{serial}/appliance/vmx/authenticationToken"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**expiresAt**: The expiration time for the token, in ISO 8601 format",
            "**token**: The newly generated authentication token for the vMX instance"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
