{
  "name": "getNetworkAppliancePrefixesDelegatedStatic",
  "description": "Return a static delegated prefix from a network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "staticDelegatedPrefixId": {
        "type": "string"
      }
    },
    "required": [
      "networkId",
      "staticDelegatedPrefixId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/appliance/prefixes/delegated/statics/{staticDelegatedPrefixId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**createdAt**: Prefix creation time.",
            "**description**: Identifying description for the prefix.",
            [
              "**origin**: WAN1/WAN2/Independent prefix.",
              {
                "$ul": [
                  "**interfaces**: Uplink provided or independent",
                  "**type**: Origin type"
                ]
              }
            ],
            "**prefix**: IPv6 prefix/prefix length.",
            "**staticDelegatedPrefixId**: Static delegated prefix id.",
            "**updatedAt**: Prefix Updated time."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
