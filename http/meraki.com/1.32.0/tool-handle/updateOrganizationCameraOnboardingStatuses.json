{
  "name": "updateOrganizationCameraOnboardingStatuses",
  "description": "Notify that credential handoff to camera has completed",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "body": {
        "example": {},
        "properties": {
          "serial": {
            "description": "Serial of camera",
            "type": "string"
          },
          "wirelessCredentialsSent": {
            "description": "Note whether credentials were sent successfully",
            "type": "boolean"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/camera/onboarding/statuses"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
