{
  "name": "deferNetworkFirmwareUpgradesStagedEvents",
  "description": "Postpone by 1 week all pending staged upgrade stages for a network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/firmwareUpgrades/staged/events/defer"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**products**: The network devices to be updated",
              {
                "$ul": [
                  [
                    "**switch**: The Switch network to be updated",
                    {
                      "$ul": [
                        [
                          "**nextUpgrade**: Details of the next firmware upgrade",
                          {
                            "$ul": [
                              [
                                "**toVersion**: Details of the version the device will upgrade to",
                                {
                                  "$ul": [
                                    "**id**: Id of the Version being upgraded to",
                                    "**shortName**: Firmware version short name"
                                  ]
                                }
                              ]
                            ]
                          }
                        ]
                      ]
                    }
                  ]
                ]
              }
            ],
            "**reasons**: Reasons for the rollback",
            "**stages**: The ordered stages in the network"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
