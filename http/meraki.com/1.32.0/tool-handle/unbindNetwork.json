{
  "name": "unbindNetwork",
  "description": "Unbind a network from a template.",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "body": {
        "example": {
          "retainConfigs": true
        },
        "properties": {
          "retainConfigs": {
            "description": "Optional boolean to retain all the current configs given by the template.",
            "type": "boolean"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/unbind"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**enrollmentString**: Enrollment string for the network",
            "**id**: Network ID",
            "**isBoundToConfigTemplate**: If the network is bound to a config template",
            "**name**: Network name",
            "**notes**: Notes for the network",
            "**organizationId**: Organization ID",
            "**productTypes**: List of the product types that the network supports",
            "**tags**: Network tags",
            "**timeZone**: Timezone of the network",
            "**url**: URL to the network Dashboard UI"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
