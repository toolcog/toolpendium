{
  "name": "updateOrganizationBrandingPoliciesPriorities",
  "description": "Update the priority ordering of an organization's branding policies.",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "body": {
        "example": {
          "brandingPolicyIds": [
            "123",
            "456",
            "789"
          ]
        },
        "properties": {
          "brandingPolicyIds": {
            "description": "      An ordered list of branding policy IDs that determines the priority order of how to apply the policies\n",
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/brandingPolicies/priorities"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**brandingPolicyIds**:       An ordered list of branding policy IDs that determines the priority order of how to apply the policies"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
