{
  "name": "removeNetworkSwitchStack",
  "description": "Remove a switch from a stack",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "switchStackId": {
        "type": "string"
      },
      "body": {
        "example": {
          "serial": "QBZY-XWVU-TSRQ"
        },
        "properties": {
          "serial": {
            "description": "The serial of the switch to be removed",
            "type": "string"
          }
        },
        "required": [
          "serial"
        ],
        "type": "object"
      }
    },
    "required": [
      "networkId",
      "switchStackId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/switch/stacks/{switchStackId}/remove"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
