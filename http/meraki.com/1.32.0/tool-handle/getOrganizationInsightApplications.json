{
  "name": "getOrganizationInsightApplications",
  "description": "List all Insight tracked applications",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/insight/applications"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**applicationId**: Application identifier",
            "**name**: Application name",
            [
              "**thresholds**: Thresholds defined by a user or Meraki models for each application",
              {
                "$ul": [
                  "**byNetwork**: Threshold for each network",
                  "**type**: Threshold type (static or smart)"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
