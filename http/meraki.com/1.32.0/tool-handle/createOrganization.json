{
  "name": "createOrganization",
  "description": "Create a new organization",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "example": {
          "management": {
            "details": [
              {
                "name": "MSP ID",
                "value": "123456"
              }
            ]
          },
          "name": "My organization"
        },
        "properties": {
          "management": {
            "description": "Information about the organization's management system",
            "properties": {
              "details": {
                "description": "Details related to organization management, possibly empty",
                "items": {
                  "properties": {
                    "name": {
                      "description": "Name of management data",
                      "type": "string"
                    },
                    "value": {
                      "description": "Value of management data",
                      "type": "string"
                    }
                  },
                  "type": "object"
                },
                "type": "array"
              }
            },
            "type": "object"
          },
          "name": {
            "description": "The name of the organization",
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**api**: API related settings",
              {
                "$ul": [
                  "**enabled**: Enable API access"
                ]
              }
            ],
            [
              "**cloud**: Data for this organization",
              {
                "$ul": [
                  [
                    "**region**: Region info",
                    {
                      "$ul": [
                        "**name**: Name of region"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**id**: Organization ID",
            [
              "**licensing**: Licensing related settings",
              {
                "$ul": [
                  "**model**: Organization licensing model. Can be 'co-term', 'per-device', or 'subscription'."
                ]
              }
            ],
            [
              "**management**: Information about the organization's management system",
              {
                "$ul": [
                  "**details**: Details related to organization management, possibly empty"
                ]
              }
            ],
            "**name**: Organization name",
            "**url**: Organization URL"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
