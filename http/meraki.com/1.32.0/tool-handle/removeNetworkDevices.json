{
  "name": "removeNetworkDevices",
  "description": "Remove a single device",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "body": {
        "example": {
          "serial": "Q234-ABCD-5678"
        },
        "properties": {
          "serial": {
            "description": "The serial of a device",
            "type": "string"
          }
        },
        "required": [
          "serial"
        ],
        "type": "object"
      }
    },
    "required": [
      "networkId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/devices/remove"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
