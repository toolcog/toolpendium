{
  "name": "createOrganizationEarlyAccessFeaturesOptIn",
  "description": "Create a new early access feature opt-in for an organization",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "body": {
        "example": {
          "limitScopeToNetworks": [
            "N_12345"
          ],
          "shortName": "has_magnetic_beta"
        },
        "properties": {
          "limitScopeToNetworks": {
            "description": "A list of network IDs to apply the opt-in to",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "shortName": {
            "description": "Short name of the early access feature",
            "type": "string"
          }
        },
        "required": [
          "shortName"
        ],
        "type": "object"
      }
    },
    "required": [
      "organizationId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/earlyAccess/features/optIns"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
