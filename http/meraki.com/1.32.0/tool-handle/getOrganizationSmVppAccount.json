{
  "name": "getOrganizationSmVppAccount",
  "description": "Get a hash containing the unparsed token of the VPP account with the given ID",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "vppAccountId": {
        "type": "string"
      }
    },
    "required": [
      "organizationId",
      "vppAccountId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/sm/vppAccounts/{vppAccountId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**: The id of the VPP Account",
            "**vppServiceToken**: The VPP Account's Service Token"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
