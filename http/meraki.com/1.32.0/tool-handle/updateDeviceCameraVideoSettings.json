{
  "name": "updateDeviceCameraVideoSettings",
  "description": "Update video settings for the given camera",
  "parameters": {
    "type": "object",
    "properties": {
      "serial": {
        "type": "string"
      },
      "body": {
        "example": {
          "externalRtspEnabled": true
        },
        "properties": {
          "externalRtspEnabled": {
            "description": "Boolean indicating if external rtsp stream is exposed",
            "type": "boolean"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "serial"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/devices/{serial}/camera/video/settings"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
