{
  "name": "getNetworkSwitchMtu",
  "description": "Return the MTU configuration",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/switch/mtu"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**defaultMtuSize**: MTU size for the entire network. Default value is 9578.",
            "**overrides**: Override MTU size for individual switches or switch profiles."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
