{
  "name": "cloneOrganization",
  "description": "Create a new organization by cloning the addressed organization",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "body": {
        "example": {
          "name": "My organization"
        },
        "properties": {
          "name": {
            "description": "The name of the new organization",
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      }
    },
    "required": [
      "organizationId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/clone"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**api**: API related settings",
              {
                "$ul": [
                  "**enabled**: Enable API access"
                ]
              }
            ],
            [
              "**cloud**: Data for this organization",
              {
                "$ul": [
                  [
                    "**region**: Region info",
                    {
                      "$ul": [
                        "**name**: Name of region"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**id**: Organization ID",
            [
              "**licensing**: Licensing related settings",
              {
                "$ul": [
                  "**model**: Organization licensing model. Can be 'co-term', 'per-device', or 'subscription'."
                ]
              }
            ],
            [
              "**management**: Information about the organization's management system",
              {
                "$ul": [
                  "**details**: Details related to organization management, possibly empty"
                ]
              }
            ],
            "**name**: Organization name",
            "**url**: Organization URL"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
