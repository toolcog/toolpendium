{
  "name": "getAdministeredIdentitiesMe",
  "description": "Returns the identity of the current user.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/administered/identities/me"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**authentication**: Authentication info",
              {
                "$ul": [
                  [
                    "**api**: API authentication",
                    {
                      "$ul": [
                        [
                          "**key**: API key",
                          {
                            "$ul": [
                              "**created**: If API key is created for this user"
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  "**mode**: Authentication mode",
                  [
                    "**saml**: SAML authentication",
                    {
                      "$ul": [
                        "**enabled**: If SAML authentication is enabled for this user"
                      ]
                    }
                  ],
                  [
                    "**twoFactor**: TwoFactor authentication",
                    {
                      "$ul": [
                        "**enabled**: If twoFactor authentication is enabled for this user"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**email**: User email",
            "**lastUsedDashboardAt**: Last seen active on Dashboard UI",
            "**name**: Username"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
