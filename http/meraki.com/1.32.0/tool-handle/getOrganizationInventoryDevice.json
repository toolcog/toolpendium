{
  "name": "getOrganizationInventoryDevice",
  "description": "Return a single device from the inventory of an organization",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "serial": {
        "type": "string"
      }
    },
    "required": [
      "organizationId",
      "serial"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/inventory/devices/{serial}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**claimedAt**: Claimed time of the device",
            "**licenseExpirationDate**: License expiration date of the device",
            "**mac**: MAC address of the device",
            "**model**: Model type of the device",
            "**name**: Name of the device",
            "**networkId**: Network Id of the device",
            "**orderNumber**: Order number of the device",
            "**productType**: Product type of the device",
            "**serial**: Serial number of the device",
            "**tags**: Device tags"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
