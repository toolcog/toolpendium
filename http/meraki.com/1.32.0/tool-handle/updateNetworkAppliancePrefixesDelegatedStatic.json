{
  "name": "updateNetworkAppliancePrefixesDelegatedStatic",
  "description": "Update a static delegated prefix from a network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "staticDelegatedPrefixId": {
        "type": "string"
      },
      "body": {
        "example": {
          "description": "Prefix on WAN 1 of Long Island Office network",
          "origin": {
            "interfaces": [
              "wan1"
            ],
            "type": "internet"
          },
          "prefix": "2001:db8:3c4d:15::/64"
        },
        "properties": {
          "description": {
            "description": "A name or description for the prefix",
            "type": "string"
          },
          "origin": {
            "description": "The origin of the prefix",
            "properties": {
              "interfaces": {
                "description": "Interfaces associated with the prefix",
                "items": {
                  "type": "string"
                },
                "type": "array"
              },
              "type": {
                "description": "Type of the origin",
                "enum": [
                  "independent",
                  "internet"
                ],
                "type": "string"
              }
            },
            "type": "object"
          },
          "prefix": {
            "description": "A static IPv6 prefix",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "networkId",
      "staticDelegatedPrefixId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/appliance/prefixes/delegated/statics/{staticDelegatedPrefixId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
