{
  "name": "getNetworkWebhooksPayloadTemplates",
  "description": "List the webhook payload templates for a network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/webhooks/payloadTemplates"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**body**: The body of the payload template, in liquid template",
            "**headers**: The payload template headers, will be rendered as a key-value pair in the webhook.",
            "**name**: The name of the payload template",
            "**payloadTemplateId**: Webhook payload template Id",
            "**type**: The type of the payload template"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
