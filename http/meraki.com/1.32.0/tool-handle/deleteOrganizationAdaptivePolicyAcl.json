{
  "name": "deleteOrganizationAdaptivePolicyAcl",
  "description": "Deletes the specified adaptive policy ACL. Note this adaptive policy ACL will also be removed from policies using it.",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "aclId": {
        "type": "string"
      }
    },
    "required": [
      "organizationId",
      "aclId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/adaptivePolicy/acls/{aclId}"
    }
  },
  "responses": {}
}
