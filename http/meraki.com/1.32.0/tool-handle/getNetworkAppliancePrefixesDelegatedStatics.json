{
  "name": "getNetworkAppliancePrefixesDelegatedStatics",
  "description": "List static delegated prefixes for a network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/appliance/prefixes/delegated/statics"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**createdAt**: Prefix creation time.",
            "**description**: Identifying description for the prefix.",
            [
              "**origin**: WAN1/WAN2/Independent prefix.",
              {
                "$ul": [
                  "**interfaces**: Uplink provided or independent",
                  "**type**: Origin type"
                ]
              }
            ],
            "**prefix**: IPv6 prefix/prefix length.",
            "**staticDelegatedPrefixId**: Static delegated prefix id.",
            "**updatedAt**: Prefix Updated time."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
