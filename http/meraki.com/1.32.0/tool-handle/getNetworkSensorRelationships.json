{
  "name": "getNetworkSensorRelationships",
  "description": "List the sensor roles for devices in a given network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/sensor/relationships"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**device**: A sensor or gateway device in the network",
              {
                "$ul": [
                  "**name**: The name of the device",
                  "**productType**: The product type of the device",
                  "**serial**: The serial of the device"
                ]
              }
            ],
            [
              "**relationships**: An object describing the relationships defined between the device and other devices",
              {
                "$ul": [
                  [
                    "**livestream**: A role defined between an MT sensor and an MV camera that adds the camera's livestream to the sensor's details page. Snapshots from the camera will also appear in alert notifications that the sensor triggers.",
                    {
                      "$ul": [
                        "**relatedDevices**: An array of the related devices for the role"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
