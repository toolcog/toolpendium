{
  "name": "getNetworkApplianceSsid",
  "description": "Return a single MX SSID",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "number": {
        "type": "string"
      }
    },
    "required": [
      "networkId",
      "number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/appliance/ssids/{number}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**authMode**: The association control method for the SSID.",
            "**defaultVlanId**: The VLAN ID of the VLAN associated to this SSID.",
            "**enabled**: Whether or not the SSID is enabled.",
            "**encryptionMode**: The psk encryption mode for the SSID.",
            "**name**: The name of the SSID.",
            "**number**: The number of the SSID.",
            "**radiusServers**: The RADIUS 802.1x servers to be used for authentication.",
            "**visible**: Boolean indicating whether the MX should advertise or hide this SSID.",
            "**wpaEncryptionMode**: WPA encryption mode for the SSID."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
