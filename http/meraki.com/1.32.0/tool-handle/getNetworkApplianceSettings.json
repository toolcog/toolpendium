{
  "name": "getNetworkApplianceSettings",
  "description": "Return the appliance settings for a network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/appliance/settings"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**clientTrackingMethod**: Client tracking method of a network",
            "**deploymentMode**: Deployment mode of a network",
            [
              "**dynamicDns**: Dynamic DNS settings for a network",
              {
                "$ul": [
                  "**enabled**: Dynamic DNS enabled",
                  "**prefix**: Dynamic DNS url prefix. DDNS must be enabled to update",
                  "**url**: Dynamic DNS url. DDNS must be enabled to update"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
