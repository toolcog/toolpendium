{
  "name": "getOrganizationInventoryOnboardingCloudMonitoringImports",
  "description": "Check the status of a committed Import operation",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "importIds": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "import ids from an imports"
      }
    },
    "required": [
      "organizationId",
      "importIds"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/inventory/onboarding/cloudMonitoring/imports{?importIds}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**device**: Represents the details of an imported device.",
              {
                "$ul": [
                  "**created**: Whether or not the device was successfully created in dashboard.",
                  "**status**: Represents the current state of importing the device.",
                  "**url**: The url to the device details page within dashboard."
                ]
              }
            ],
            "**importId**: Database ID for the new entity entry."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
