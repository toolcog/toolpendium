{
  "name": "updateNetworkCellularGatewaySubnetPool",
  "description": "Update the subnet pool and mask configuration for MGs in the network.",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "body": {
        "example": {
          "cidr": "192.168.0.0/16",
          "deploymentMode": "routed",
          "mask": 24
        },
        "properties": {
          "cidr": {
            "description": "CIDR of the pool of subnets. Each MG in this network will automatically pick a subnet from this pool.",
            "type": "string"
          },
          "mask": {
            "description": "Mask used for the subnet of all MGs in  this network.",
            "type": "integer"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/cellularGateway/subnetPool"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
