{
  "name": "vmxNetworkDevicesClaim",
  "description": "Claim a vMX into a network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "body": {
        "example": {
          "size": "small"
        },
        "properties": {
          "size": {
            "description": "The size of the vMX you claim. It can be one of: small, medium, large, 100",
            "enum": [
              "100",
              "large",
              "medium",
              "small"
            ],
            "type": "string"
          }
        },
        "required": [
          "size"
        ],
        "type": "object"
      }
    },
    "required": [
      "networkId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/devices/claim/vmx"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
