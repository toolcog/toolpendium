{
  "name": "getOrganizationFirmwareUpgrades",
  "description": "Get firmware upgrade information for an organization",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "status": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "The status of an upgrade "
      },
      "productType": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "The product type in a given upgrade ID"
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/firmware/upgrades{?status,productType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**completedAt**: Timestamp when upgrade completed. Null if status pending.",
            [
              "**fromVersion**: ID of the upgrade's starting version",
              {
                "$ul": [
                  "**id**: Firmware version ID",
                  "**releaseDate**: Release date of the firmware version",
                  "**releaseType**: Release type of the firmware version",
                  "**shortName**: Firmware version short name"
                ]
              }
            ],
            [
              "**network**: Network of the upgrade",
              {
                "$ul": [
                  "**id**: ID of network",
                  "**name**: The network"
                ]
              }
            ],
            "**productType**: product upgraded [wireless, appliance, switch, systemsManager, camera, cellularGateway, sensor]",
            "**status**: Status of upgrade event: [Cancelled, Completed]",
            "**time**: Scheduled start time",
            [
              "**toVersion**: ID of the upgrade's target version",
              {
                "$ul": [
                  "**id**: Firmware version ID",
                  "**releaseDate**: Release date of the firmware version",
                  "**releaseType**: Release type of the firmware version",
                  "**shortName**: Firmware version short name"
                ]
              }
            ],
            "**upgradeBatchId**: The upgrade batch",
            "**upgradeId**: The upgrade"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
