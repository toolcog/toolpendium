{
  "name": "getOrganizationSummaryTopDevicesModelsByUsage",
  "description": "Return metrics for organization's top 10 device models sorted by data usage over given time range. Default unit is megabytes.",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "t0": {
        "type": "string",
        "description": "The beginning of the timespan for the data."
      },
      "t1": {
        "type": "string",
        "description": "The end of the timespan for the data. t1 can be a maximum of 31 days after t0."
      },
      "timespan": {
        "format": "float",
        "type": "number",
        "description": "The timespan for which the information will be fetched. If specifying timespan, do not specify parameters t0 and t1. The value must be in seconds and be less than or equal to 31 days. The default is 1 day."
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/summary/top/devices/models/byUsage{?t0,t1,timespan}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**: Total number of devices per model",
            "**model**: The device model",
            [
              "**usage**: Usage info in megabytes",
              {
                "$ul": [
                  "**average**: Average usage in megabytes",
                  "**total**: Total usage in megabytes"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
