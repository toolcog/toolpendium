{
  "name": "getOrganizationWebhooksAlertTypes",
  "description": "Return a list of alert types to be used with managing webhook alerts",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "productType": {
        "enum": [
          "appliance",
          "camera",
          "cellularGateway",
          "health",
          "platform",
          "sensor",
          "sm",
          "switch",
          "wireless"
        ],
        "type": "string",
        "description": "Filter sample alerts to a specific product type"
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/webhooks/alertTypes{?productType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
