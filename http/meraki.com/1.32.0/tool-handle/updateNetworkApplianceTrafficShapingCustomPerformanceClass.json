{
  "name": "updateNetworkApplianceTrafficShapingCustomPerformanceClass",
  "description": "Update a custom performance class for an MX network",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "customPerformanceClassId": {
        "type": "string"
      },
      "body": {
        "example": {
          "maxJitter": 100,
          "maxLatency": 100,
          "maxLossPercentage": 5,
          "name": "myCustomPerformanceClass"
        },
        "properties": {
          "maxJitter": {
            "description": "Maximum jitter in milliseconds",
            "type": "integer"
          },
          "maxLatency": {
            "description": "Maximum latency in milliseconds",
            "type": "integer"
          },
          "maxLossPercentage": {
            "description": "Maximum percentage of packet loss",
            "type": "integer"
          },
          "name": {
            "description": "Name of the custom performance class",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "networkId",
      "customPerformanceClassId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/appliance/trafficShaping/customPerformanceClasses/{customPerformanceClassId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
