{
  "name": "updateNetworkWirelessSsidEapOverride",
  "description": "Update the EAP overridden parameters for an SSID.",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "number": {
        "type": "string"
      },
      "body": {
        "example": {
          "eapolKey": {
            "retries": 5,
            "timeoutInMs": 5000
          },
          "identity": {
            "retries": 5,
            "timeout": 5
          },
          "maxRetries": 5,
          "timeout": 5
        },
        "properties": {
          "eapolKey": {
            "description": "EAPOL Key settings.",
            "properties": {
              "retries": {
                "description": "Maximum number of EAPOL key retries.",
                "type": "integer"
              },
              "timeoutInMs": {
                "description": "EAPOL Key timeout in milliseconds.",
                "type": "integer"
              }
            },
            "type": "object"
          },
          "identity": {
            "description": "EAP settings for identity requests.",
            "properties": {
              "retries": {
                "description": "Maximum number of EAP retries.",
                "type": "integer"
              },
              "timeout": {
                "description": "EAP timeout in seconds.",
                "type": "integer"
              }
            },
            "type": "object"
          },
          "maxRetries": {
            "description": "Maximum number of general EAP retries.",
            "type": "integer"
          },
          "timeout": {
            "description": "General EAP timeout in seconds.",
            "type": "integer"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "networkId",
      "number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/wireless/ssids/{number}/eapOverride"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**eapolKey**: EAPOL Key settings.",
              {
                "$ul": [
                  "**retries**: Maximum number of EAPOL key retries.",
                  "**timeoutInMs**: EAPOL Key timeout in milliseconds."
                ]
              }
            ],
            [
              "**identity**: EAP settings for identity requests.",
              {
                "$ul": [
                  "**retries**: Maximum number of EAP retries.",
                  "**timeout**: EAP timeout in seconds."
                ]
              }
            ],
            "**maxRetries**: Maximum number of general EAP retries.",
            "**timeout**: General EAP timeout in seconds."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
