    "properties": {
      "networkId": {
        "type": "string"
      },
      "switchStackId": {
        "type": "string"
      },
      "staticRouteId": {
        "type": "string"
      }
    },
    "required": [
      "networkId",
      "switchStackId",
      "staticRouteId"
    ]
  },
  "handler": "http",
  "request": {
