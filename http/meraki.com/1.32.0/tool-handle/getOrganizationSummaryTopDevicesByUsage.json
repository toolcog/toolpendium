{
  "name": "getOrganizationSummaryTopDevicesByUsage",
  "description": "Return metrics for organization's top 10 devices sorted by data usage over given time range. Default unit is megabytes.",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "t0": {
        "type": "string",
        "description": "The beginning of the timespan for the data."
      },
      "t1": {
        "type": "string",
        "description": "The end of the timespan for the data. t1 can be a maximum of 31 days after t0."
      },
      "timespan": {
        "format": "float",
        "type": "number",
        "description": "The timespan for which the information will be fetched. If specifying timespan, do not specify parameters t0 and t1. The value must be in seconds and be less than or equal to 31 days. The default is 1 day."
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/summary/top/devices/byUsage{?t0,t1,timespan}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**clients**: Clients",
              {
                "$ul": [
                  [
                    "**counts**: Counts of clients",
                    {
                      "$ul": [
                        "**total**: Total counts of clients"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**mac**: Mac address of the device",
            "**model**: Model of the device",
            "**name**: Name of the device",
            [
              "**network**: Network info",
              {
                "$ul": [
                  "**id**: Network id",
                  "**name**: Network name"
                ]
              }
            ],
            "**productType**: Product type of the device",
            "**serial**: Serial number of the device",
            [
              "**usage**: Data usage of the device",
              {
                "$ul": [
                  "**percentage**: Data usage of the device by percentage",
                  "**total**: Total data usage of the device"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
