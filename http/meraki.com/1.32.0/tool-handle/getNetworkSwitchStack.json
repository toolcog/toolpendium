{
  "name": "getNetworkSwitchStack",
  "description": "Show a switch stack",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "switchStackId": {
        "type": "string"
      }
    },
    "required": [
      "networkId",
      "switchStackId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/switch/stacks/{switchStackId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**: Switch stacks id",
            "**name**: Switch stacks name",
            "**serials**: Serials of the switches in the switch stack"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
