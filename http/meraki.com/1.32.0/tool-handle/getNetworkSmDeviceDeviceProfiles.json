{
  "name": "getNetworkSmDeviceDeviceProfiles",
  "description": "Get the installed profiles associated with a device",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "deviceId": {
        "type": "string"
      }
    },
    "required": [
      "networkId",
      "deviceId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/sm/devices/{deviceId}/deviceProfiles"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**deviceId**: The Meraki managed device Id.",
            "**id**: The numerical Meraki Id of the profile.",
            "**isEncrypted**: A boolean indicating if the profile is encrypted.",
            "**isManaged**: Whether or not the profile is managed by Meraki.",
            "**name**: The name of the profile.",
            "**profileData**: A string containing a JSON object with the profile data.",
            "**profileIdentifier**: The identifier of the profile.",
            "**version**: The verison of the profile."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
