{
  "name": "createNetworkSwitchRoutingMulticastRendezvousPoint",
  "description": "Create a multicast rendezvous point",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "body": {
        "example": {
          "interfaceIp": "192.168.1.2",
          "multicastGroup": "192.168.128.0/24"
        },
        "properties": {
          "interfaceIp": {
            "description": "TheÂ IP address of the interface where the RP needs to be created.",
            "type": "string"
          },
          "multicastGroup": {
            "description": "'Any', or the IP address of a multicast group",
            "type": "string"
          }
        },
        "required": [
          "interfaceIp",
          "multicastGroup"
        ],
        "type": "object"
      }
    },
    "required": [
      "networkId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/switch/routing/multicast/rendezvousPoints"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
