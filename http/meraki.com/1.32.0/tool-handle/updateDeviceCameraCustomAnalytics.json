{
  "name": "updateDeviceCameraCustomAnalytics",
  "description": "Update custom analytics settings for a camera",
  "parameters": {
    "type": "object",
    "properties": {
      "serial": {
        "type": "string"
      },
      "body": {
        "example": {
          "artifactId": "1",
          "enabled": true,
          "parameters": [
            {
              "name": "detection_threshold",
              "value": "0.5"
            }
          ]
        },
        "properties": {
          "artifactId": {
            "description": "The ID of the custom analytics artifact",
            "type": "string"
          },
          "enabled": {
            "description": "Enable custom analytics",
            "type": "boolean"
          },
          "parameters": {
            "description": "Parameters for the custom analytics workload",
            "items": {
              "properties": {
                "name": {
                  "description": "Name of the parameter",
                  "type": "string"
                },
                "value": {
                  "description": "Value of the parameter",
                  "type": "string"
                }
              },
              "required": [
                "name",
                "value"
              ],
              "type": "object"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "serial"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/devices/{serial}/camera/customAnalytics"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
