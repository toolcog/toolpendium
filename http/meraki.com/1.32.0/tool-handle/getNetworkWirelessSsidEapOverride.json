{
  "name": "getNetworkWirelessSsidEapOverride",
  "description": "Return the EAP overridden parameters for an SSID",
  "parameters": {
    "type": "object",
    "properties": {
      "networkId": {
        "type": "string"
      },
      "number": {
        "type": "string"
      }
    },
    "required": [
      "networkId",
      "number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/networks/{networkId}/wireless/ssids/{number}/eapOverride"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**eapolKey**: EAPOL Key settings.",
              {
                "$ul": [
                  "**retries**: Maximum number of EAPOL key retries.",
                  "**timeoutInMs**: EAPOL Key timeout in milliseconds."
                ]
              }
            ],
            [
              "**identity**: EAP settings for identity requests.",
              {
                "$ul": [
                  "**retries**: Maximum number of EAP retries.",
                  "**timeout**: EAP timeout in seconds."
                ]
              }
            ],
            "**maxRetries**: Maximum number of general EAP retries.",
            "**timeout**: General EAP timeout in seconds."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
