{
  "name": "releaseFromOrganizationInventory",
  "description": "Release a list of claimed devices from an organization.",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string"
      },
      "body": {
        "example": {
          "serials": [
            "Q234-ABCD-5678"
          ]
        },
        "properties": {
          "serials": {
            "description": "Serials of the devices that should be released",
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.meraki.com/api/v1/organizations/{organizationId}/inventory/release"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
