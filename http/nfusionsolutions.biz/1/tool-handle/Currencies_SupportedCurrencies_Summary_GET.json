{
  "name": "Currencies_SupportedCurrencies_Summary_GET",
  "description": "Only the currency pairs in the direction noted can be used with the Summary endpoint.\r\nFor example: USD/CAD is not the same as CAD/USD",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "enum": [
          "json",
          "xml"
        ],
        "type": "string",
        "description": "to override content negotiation specify a value of json or xml"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nfusionsolutions.biz/api/v1/Currencies/summary/supported{?format}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
