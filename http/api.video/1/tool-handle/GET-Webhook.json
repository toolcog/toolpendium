{
  "name": "GET-Webhook",
  "description": "This call provides the same JSON information provided on Webjhook creation.",
  "parameters": {
    "type": "object",
    "properties": {
      "webhookId": {
        "type": "string",
        "description": "The unique webhook you wish to retreive details on."
      }
    },
    "required": [
      "webhookId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ws.api.video/webhooks/{webhookId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Webhook"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**createdAt**: When an webhook was created, presented in ISO-8601 format.",
            "**events**: A list of events that will trigger the webhook.",
            "**url**: URL of the webhook",
            "**webhookId**: Unique identifier of the webhook"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
