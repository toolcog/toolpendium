{
  "name": "DELETE-video",
  "description": "If you do not need a video any longer, you can send a request to delete it. All you need is the videoId. Tutorials using [video deletion](https://api.video/blog/endpoints/video-delete).",
  "parameters": {
    "type": "object",
    "properties": {
      "videoId": {
        "type": "string",
        "description": "The video ID for the video you want to delete."
      }
    },
    "required": [
      "videoId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://ws.api.video/videos/{videoId}"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "NotFound"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**name**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
