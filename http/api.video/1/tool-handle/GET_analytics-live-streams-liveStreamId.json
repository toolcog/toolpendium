{
  "name": "GET_analytics-live-streams-liveStreamId",
  "description": "List live stream player sessions",
  "parameters": {
    "type": "object",
    "properties": {
      "liveStreamId": {
        "type": "string",
        "description": "The unique identifier for the live stream you want to retrieve analytics for."
      },
      "period": {
        "format": "period",
        "type": "string",
        "description": "Period must have one of the following formats: \n- For a day : \"2018-01-01\",\n- For a week: \"2018-W01\", \n- For a month: \"2018-01\"\n- For a year: \"2018\"\nFor a range period: \n-  Date range: \"2018-01-01/2018-01-15\"\n"
      },
      "currentPage": {
        "default": 1,
        "type": "integer",
        "description": "Choose the number of search results to return per page. Minimum value: 1"
      },
      "pageSize": {
        "default": 25,
        "type": "integer",
        "description": "Results per page. Allowed values 1-100, default is 25."
      }
    },
    "required": [
      "liveStreamId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ws.api.video/analytics/live-streams/{liveStreamId}{?period,currentPage,pageSize}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "LiveStreamSessions"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**pagination**",
              {
                "$ul": [
                  "**currentPage**: The current page index.",
                  "**currentPageItems**: The number of items on the current page.",
                  "**itemsTotal**: Total number of items that exist.",
                  "**links**",
                  "**pageSize**: Maximum number of item per page.",
                  "**pagesTotal**: Number of items listed in the current page."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "NotFound"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**name**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
