{
  "name": "getServices",
  "description": "List all serviceNames in the directory for a particular providerName\n",
  "parameters": {
    "type": "object",
    "properties": {
      "provider": {
        "example": "apis.guru",
        "maxLength": 255,
        "minLength": 1,
        "type": "string"
      }
    },
    "required": [
      "provider"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.apis.guru/v2/{provider}/services.json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
