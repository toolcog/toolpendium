{
  "name": "getCallLogs",
  "description": "Retrieve call logs for your account",
  "parameters": {
    "type": "object",
    "properties": {
      "account_id": {
        "type": "string",
        "description": "The Vonage Business Cloud account ID"
      },
      "start:gte": {
        "type": "string",
        "description": "Filter records by start date (greater equal or equal to)"
      },
      "start:lte": {
        "type": "string",
        "description": "Filter records by start date (less equal or equal to)"
      },
      "end:gte": {
        "type": "string",
        "description": "Filter records by end date (greater equal or equal to)"
      },
      "end:lte": {
        "type": "string",
        "description": "Filter records by end date (less equal or equal to)"
      },
      "page_size": {
        "default": 10,
        "type": "number",
        "description": "Number of records per page"
      },
      "page": {
        "default": 0,
        "type": "number",
        "description": "Current page number"
      },
      "to": {
        "type": "string",
        "description": "Filter by called number"
      },
      "from": {
        "type": "string",
        "description": "Filter by source number"
      },
      "source_user": {
        "type": "string",
        "description": "Filter by source user"
      },
      "destination_user": {
        "type": "string",
        "description": "Filter by destination user"
      },
      "direction": {
        "enum": [
          "Inbound",
          "Outbound"
        ],
        "type": "string",
        "description": "Filter by call direction."
      }
    },
    "required": [
      "account_id",
      "start:gte",
      "start:lte",
      "page_size",
      "page"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vonage.com/t/vbc.prod/reports/accounts/{account_id}/call-logs{?start:gte,start:lte,end:gte,end:lte,page_size,page,to,from,source_user,destination_user,direction}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_embedded**",
              {
                "$ul": [
                  "**call_logs**"
                ]
              }
            ],
            [
              "**_links**",
              {
                "$ul": [
                  [
                    "**first**: URL to the first page of records",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**next**: URL to the next page of records",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**prev**: URL to the previous page of records",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**self**: URL to the current page of records",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**page**: Current page number",
            "**page_size**: Number of records per page",
            "**total_items**: Total number of records",
            "**total_page**: Total number of pages"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**instance**: Error Track ID",
            "**invalid_parameters**: Invalid parameters and their reason for failing",
            "**status**: Error status code",
            "**title**: Error title"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
