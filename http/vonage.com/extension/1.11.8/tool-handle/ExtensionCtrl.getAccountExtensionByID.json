{
  "name": "ExtensionCtrl_getAccountExtensionByID",
  "description": "Get extension data by account ID and extension number",
  "parameters": {
    "type": "object",
    "properties": {
      "account_id": {
        "type": "string",
        "description": "The Vonage Business Cloud account ID"
      },
      "extension_number": {
        "type": "number",
        "description": "The extension number"
      }
    },
    "required": [
      "account_id",
      "extension_number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vonage.com/t/vbc.prod/provisioning/api/accounts/{account_id}/extensions/{extension_number}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_embedded**",
              {
                "$ul": [
                  [
                    "**data**",
                    {
                      "$ul": [
                        "**block_caller_id**: Block Caller ID status of the extension",
                        "**caller_id**: Caller ID of the extension",
                        "**dids**: Collection of phone numbers assigned to the extension",
                        "**dnd_enabled**: Do Not Disturb status of the extension",
                        "**extension_handsets**: Collection of handsets assigned to the extension",
                        "**extension_number**: Extension number",
                        "**location_id**: Unique identifier of the assigned location",
                        [
                          "**user**",
                          {
                            "$ul": [
                              "**email**: Email address of the user",
                              "**first_name**: First name of the user",
                              "**last_name**: Last name of the user",
                              "**login_name**: Login name of the user"
                            ]
                          }
                        ],
                        "**vtt_enabled**: Voicemail transcription status of the location"
                      ]
                    }
                  ]
                ]
              }
            ],
            [
              "**_links**",
              {
                "$ul": [
                  [
                    "**first**: URL to the first page of records",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**next**: URL to the next page of records",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**prev**: URL to the previous page of records",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**self**: URL to the current page of records",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**page**: Current page number",
            "**page_size**: Number of records per page",
            "**total_items**: Total number of records",
            "**total_pages**: Total number of pages"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**msg**: Error message",
            "**status**: Http Response Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
