{
  "name": "getRoles",
  "description": "Get a call",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Unique identifier of the call"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vonage.com/t/vbc.prod/vis/v1/self/calls/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accountId**: Unique identifier of the account",
            "**answerTime**: Time to answer the call",
            "**callerId**: Remote caller ID",
            "**direction**: Direction of the call",
            "**duration**: Duration of the call in milliseconds",
            "**endTime**: End time of the call",
            "**externalId**: External identifier of the call",
            "**id**: Unique identifier of the call",
            "**phoneNumber**: Unique identifier of the account",
            "**startTime**: Start time of the call",
            "**state**: Status of the call",
            "**type**: Record type",
            "**uciId**: Unique identifier of communications provider",
            "**userId**: Unique identifier of the user"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errorCode**",
            "**errorMessage**",
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errorCode**",
            "**errorMessage**",
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
