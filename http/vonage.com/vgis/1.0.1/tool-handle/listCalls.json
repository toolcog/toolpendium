{
  "name": "listCalls",
  "description": "Lists currently active calls",
  "parameters": {
    "type": "object",
    "properties": {
      "fromDate": {
        "type": "integer",
        "description": "Return calls that occurred after this point in time"
      },
      "toDate": {
        "type": "integer",
        "description": "Return calls that occurred before this point in time"
      },
      "direction": {
        "enum": [
          "INBOUND",
          "OUTBOUND"
        ],
        "example": "INBOUND,OUTBOUND",
        "type": "string",
        "description": "Filter by call direction. For multiple criteria, seperate values by a comma."
      },
      "states": {
        "default": "ACTIVE",
        "enum": [
          "INITIALIZING",
          "RINGING",
          "ACTIVE",
          "HELD",
          "REMOTE_HELD"
        ],
        "example": "ACTIVE,RINGING",
        "type": "string",
        "description": "Filter calls by state. For multiple criteria, seperate values by a comma."
      },
      "offset": {
        "format": "int64",
        "type": "integer",
        "description": "Page number of calls to return"
      },
      "size": {
        "default": 20,
        "type": "integer",
        "description": "Return this amount of calls in the response"
      },
      "order": {
        "default": "ASC",
        "enum": [
          "DESC",
          "ASC"
        ],
        "type": "string",
        "description": "Sort in either ascending or descending order"
      },
      "sort": {
        "type": "string",
        "description": "Sort calls by property"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vonage.com/t/vbc.prod/vis/v1/self/calls{?fromDate,toDate,direction,states,offset,size,order,sort}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accountId**: Unique identifier of the account",
            "**answerTime**: Time to answer the call",
            "**callerId**: Remote caller ID",
            "**direction**: Direction of the call",
            "**duration**: Duration of the call in milliseconds",
            "**endTime**: End time of the call",
            "**externalId**: External identifier of the call",
            "**id**: Unique identifier of the call",
            "**phoneNumber**: Unique identifier of the account",
            "**startTime**: Start time of the call",
            "**state**: Status of the call",
            "**type**: Record type",
            "**uciId**: Unique identifier of communications provider",
            "**userId**: Unique identifier of the user"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errorCode**",
            "**errorMessage**",
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errorCode**",
            "**errorMessage**",
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
