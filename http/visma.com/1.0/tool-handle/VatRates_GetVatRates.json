{
  "name": "VatRates_GetVatRates",
  "description": "Get all organization vat rates",
  "parameters": {
    "type": "object",
    "properties": {
      "countryGuid": {
        "nullable": true,
        "type": "string",
        "description": "If not given, return all vat rates in organizations country. If given return only for that country."
      },
      "active": {
        "nullable": true,
        "type": "boolean",
        "description": "If not given, return all vat rates, if given as true return only active ones, if given as false returns only inactive ones."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/vatrates{?countryGuid,active}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**countryGuid**",
            [
              "**createdBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**createdDateTime**",
            "**guid**",
            "**isActive** (default: true)",
            "**isDefault**",
            [
              "**lastUpdatedBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**",
            "**percentage**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
