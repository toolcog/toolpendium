{
  "name": "ProjectProducts_GetProjectProducts",
  "description": "This is the same as organization's list of products, unless the project has some specific products and UseProductsFromSetting in project model is set to false.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectGuid": {
        "type": "string",
        "description": "GUID of the project."
      },
      "includeProductsFromRegistry": {
        "default": false,
        "type": "boolean",
        "description": "Optional: Includes products available from product registry"
      },
      "pageToken": {
        "nullable": true,
        "type": "string",
        "description": "Optional: page token to fetch the next page."
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: How many rows to fetch, Default all."
      },
      "active": {
        "nullable": true,
        "type": "boolean",
        "description": "Fetch only active"
      }
    },
    "required": [
      "projectGuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/projects/{projectGuid}/projectproducts{?includeProductsFromRegistry,pageToken,rowCount,active}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**guid**",
            [
              "**product**",
              {
                "$ul": [
                  "**guid**",
                  "**isActive**",
                  "**name**",
                  "**productCategory**",
                  "**productCode**",
                  "**productType**: "
                ]
              }
            ],
            [
              "**project**",
              {
                "$ul": [
                  "**guid**",
                  "**name**",
                  "**number**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
