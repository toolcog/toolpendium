{
  "name": "Keywords_GetProjectKeywords",
  "description": "Get all the keywords for project.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectGuid": {
        "type": "string",
        "description": "ID of the project for which keywords are requested."
      },
      "active": {
        "nullable": true,
        "type": "boolean",
        "description": "If not given, return all Keywords, if given as true return only active Keywords, if given as false returns only inactive Keywords."
      },
      "sortings": {
        "items": {
          "$ref": "#/$defs/KeyValuePairOfStringAndSortDirection"
        },
        "nullable": true,
        "type": "array",
        "description": "Optional: A list of Key-Value pairs, containing names of fields and directions by which the results should be sorted. Any sortable field name (submodel fields not supported) in the model can be used, while value can be \"Desc\" or \"Asc\". Example: \"?sortings[0].key=Keyword&sortings[0].value=Desc\"."
      }
    },
    "required": [
      "projectGuid"
    ],
    "$defs": {
      "KeyValuePairOfStringAndSortDirection": {
        "properties": {
          "key": {
            "type": "string"
          },
          "value": {
            "$ref": "#/$defs/SortDirection"
          }
        },
        "type": "object"
      },
      "SortDirection": {
        "description": "",
        "enum": [
          "Asc",
          "Desc",
          "AscNullsFirst",
          "AscNullsLast",
          "DescNullsFirst",
          "DescNullsLast"
        ],
        "type": "string",
        "x-enumNames": [
          "Asc",
          "Desc",
          "AscNullsFirst",
          "AscNullsLast",
          "DescNullsFirst",
          "DescNullsLast"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/projects/{projectGuid}/keywords{?active,sortings}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**category**: ",
            [
              "**createdBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**createdDateTime**",
            "**guid**",
            "**isActive** (default: true)",
            "**keyword**",
            [
              "**lastUpdatedBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
