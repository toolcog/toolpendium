{
  "name": "TravelExpenseTypes_GetSearchedTravelExpenseTypes",
  "description": "Search active travel expense types of project by part of the name or code.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectGuid": {
        "type": "string",
        "description": "Id of the project."
      },
      "textToSearch": {
        "default": "",
        "nullable": true,
        "type": "string",
        "description": "Searched string: part of name or code."
      },
      "firstRow": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: first row to fetch. Default 0 = first row."
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: How many rows to fetch, Default all."
      },
      "userGuid": {
        "nullable": true,
        "type": "string",
        "description": "Optional: Id of the user to fetch travels for."
      },
      "expenseClass": {
        "$ref": "#/components/schemas/ExpensesClass",
        "description": "Optional: Expense class of the travel. Mileage/DailyAllowance/OtherTravelExpense."
      }
    },
    "required": [
      "projectGuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/projects/{projectGuid}/travelexpensetypes{?textToSearch,firstRow,rowCount,userGuid,expenseClass}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            [
              "**costAccount**",
              {
                "$ul": [
                  "**guid**",
                  "**name**",
                  "**number**"
                ]
              }
            ],
            [
              "**createdBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**createdDateTime**",
            "**expenseClass**: ",
            "**guid**",
            "**includeTime**",
            "**isActive** (default: true)",
            "**isDailyAllowance**",
            "**isMileage**",
            [
              "**lastUpdatedBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**",
            "**measurementUnit**",
            "**name**",
            [
              "**productCategory**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            "**purchaseVatRate**",
            [
              "**salesAccount**",
              {
                "$ul": [
                  "**guid**",
                  "**name**",
                  "**number**"
                ]
              }
            ],
            [
              "**unitCost**",
              {
                "$ul": [
                  "**amount**",
                  "**currencyCode**",
                  "**multiCurrencyInfo**"
                ]
              }
            ],
            "**vatRate**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
