                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**",
            [
              "**overtime**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**phase**",
              {
                "$ul": [
                  "**guid**",
                  "**isCompleted**",
