{
  "name": "UserCustomValues_GetUserCustomValues",
  "description": "Get the user custom values.",
  "parameters": {
    "type": "object",
    "properties": {
      "userGuid": {
        "type": "string",
        "description": "ID of the user."
      },
      "pageToken": {
        "nullable": true,
        "type": "string",
        "description": "Optional: page token to fetch the next page."
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: How many rows to fetch, Default 20, maximum 100."
      },
      "isActive": {
        "nullable": true,
        "type": "boolean",
        "description": "Optional: Get only values of active or inactive user custom properties."
      },
      "targets": {
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array",
        "description": "Optional: List of target for which to get the values."
      },
      "changedSince": {
        "format": "date-time",
        "nullable": true,
        "type": "string",
        "description": "Optional: Get user custom values that have been added or changed after this date time (greater or equal)."
      }
    },
    "required": [
      "userGuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/users/{userGuid}/customvalues{?pageToken,rowCount,isActive,targets,changedSince}"
    }
  },
