{
  "name": "Proposals_GetProposals",
  "description": "Get all the proposals",
  "parameters": {
    "type": "object",
    "properties": {
      "pageToken": {
        "nullable": true,
        "type": "string",
        "description": "Optional: page token to fetch the next page."
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: How many rows to fetch, Default 20, maximum 100."
      },
      "changedSince": {
        "format": "date-time",
        "nullable": true,
        "type": "string",
        "description": "Optional: Get proposals that have been added or changed after this date time (greater or equal)."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/proposals{?pageToken,rowCount,changedSince}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**billingAddress**",
              {
                "$ul": [
                  "**addressline**",
                  "**city**",
                  "**country**",
                  "**guid**",
                  "**postalCode**"
                ]
              }
            ],
            [
              "**createdBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**createdDateTime**",
            [
              "**culture**",
              {
                "$ul": [
                  "**code**",
                  "**englishName**",
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**customer**",
              {
                "$ul": [
                  "**guid**",
                  "**name**",
                  "**number**"
                ]
              }
            ],
            [
              "**customerContactPerson**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**freeText1**",
              {
                "$ul": [
                  "**allowTags**",
                  "**plainText**",
                  "**tagContext**",
                  "**text**",
                  "**value**"
                ]
              }
            ],
            [
              "**freeText2**",
              {
                "$ul": [
                  "**allowTags**",
                  "**plainText**",
                  "**tagContext**",
                  "**text**",
                  "**value**"
                ]
              }
            ],
            "**guid**",
            [
              "**language**",
              {
                "$ul": [
                  "**code**",
                  "**englishName**",
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**lastUpdatedBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**",
            "**name**",
            "**number**",
            [
              "**pricelist**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**project**",
              {
                "$ul": [
                  "**guid**",
                  "**name**",
                  "**number**"
                ]
              }
            ],
            "**proposalDate**",
            [
              "**proposalStatus**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**totalExcludingVat**",
              {
                "$ul": [
                  "**amount**",
                  "**currencyCode**",
                  "**multiCurrencyInfo**"
                ]
              }
            ],
            [
              "**totalIncludingVat**",
              {
                "$ul": [
                  "**amount**",
                  "**currencyCode**",
                  "**multiCurrencyInfo**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
