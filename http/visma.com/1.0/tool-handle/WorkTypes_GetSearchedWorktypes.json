{
  "name": "WorkTypes_GetSearchedWorktypes",
  "description": "Search active work types by part of the name or code.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectGuid": {
        "type": "string",
        "description": "Id of the case to which proposal is connected."
      },
      "firstRow": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: first row to fetch. Default 0 = first row."
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: How many rows to fetch, Default 20, maximum 100."
      },
      "textToSearch": {
        "default": "",
        "nullable": true,
        "type": "string",
        "description": "Searched string: part of name or code."
      }
    },
    "required": [
      "projectGuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/projects/{projectGuid}/worktypesforproject{?firstRow,rowCount,textToSearch}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**hourPrice**",
              {
                "$ul": [
                  "**amount**",
                  "**currencyCode**",
                  "**multiCurrencyInfo**"
                ]
              }
            ],
            [
              "**worktype**",
              {
                "$ul": [
                  "**code**",
                  [
                    "**createdBy**",
                    {
                      "$ul": [
                        "**code**",
                        "**firstName**",
                        "**guid**",
                        "**lastName**",
                        "**name**"
                      ]
                    }
                  ],
                  "**createdDateTime**",
                  "**guid**",
                  [
                    "**hourCost**",
                    {
                      "$ul": [
                        "**amount**",
                        "**currencyCode**",
                        "**multiCurrencyInfo**"
                      ]
                    }
                  ],
                  "**isActive** (default: true)",
                  "**isDefault**",
                  "**isProductive**",
                  [
                    "**lastUpdatedBy**",
                    {
                      "$ul": [
                        "**code**",
                        "**firstName**",
                        "**guid**",
                        "**lastName**",
                        "**name**"
                      ]
                    }
                  ],
                  "**lastUpdatedDateTime**",
                  "**name**",
                  [
                    "**salesAccount**",
                    {
                      "$ul": [
                        "**guid**",
                        "**name**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
