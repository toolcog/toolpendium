{
  "name": "PhaseMembers_GetPhaseMembers",
  "description": "Use root phase to get project members.",
  "parameters": {
    "type": "object",
    "properties": {
      "phaseGuid": {
        "type": "string",
        "description": "GUID of the phase."
      },
      "pageToken": {
        "nullable": true,
        "type": "string",
        "description": "Optional: Page token to fetch the next page."
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: How many rows to fetch."
      },
      "isActive": {
        "nullable": true,
        "type": "boolean",
        "description": "Optional: Is the member active on the phase. Filters only root phase members. Default nothing = all."
      },
      "isUserActive": {
        "nullable": true,
        "type": "boolean",
        "description": "Optional: Is the user active. Default nothing = all."
      }
    },
    "required": [
      "phaseGuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/phases/{phaseGuid}/phasemembers{?pageToken,rowCount,isActive,isUserActive}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**createdBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**createdDateTime**",
            "**currentWorkContractTitle**",
            "**guid**",
            "**isActive** (default: true)",
            [
              "**lastUpdatedBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**",
            [
              "**phase**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**user**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
