{
  "name": "Users_GetUsers",
  "description": "Get users",
  "parameters": {
    "type": "object",
    "properties": {
      "pageToken": {
        "nullable": true,
        "type": "string"
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: How many rows to fetch."
      },
      "isActive": {
        "nullable": true,
        "type": "boolean",
        "description": "If not given, return all users, if given as true return only active users, if given as false returns only inactive users"
      },
      "businessUnitGuids": {
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array",
        "description": "Optional: ID of the business unit of the user. If not provided, returns for all business units. Default all."
      },
      "keywordGuids": {
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array",
        "description": "Optional: ID of the keyword of the user. If not provided, returns for all keywords. Default all."
      },
      "changedSince": {
        "format": "date-time",
        "nullable": true,
        "type": "string",
        "description": "Optional: Get users that have been added or changed after this date time (greater or equal)."
      },
      "supervisorUserGuids": {
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array",
        "description": "Optional: ID of the supervisor to get subordinates for."
      },
      "code": {
        "default": "",
        "nullable": true,
        "type": "string",
        "description": "Optional: Code of the user."
      },
      "email": {
        "default": "",
        "nullable": true,
        "type": "string",
        "description": "Optional: Email address of the user."
      },
      "purpose": {
        "$ref": "#/components/schemas/GetUsersPurpose",
        "description": "Optional: Filter users by purpose."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/users{?pageToken,rowCount,isActive,businessUnitGuids,keywordGuids,changedSince,supervisorUserGuids,code,email,purpose}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**address**",
            "**bankAccountNumber**",
            "**birthDate**",
            [
              "**businessUnit**",
              {
                "$ul": [
                  "**code**",
                  "**companyBusinessUnitGuid**",
                  "**companyCountryGuid**",
                  "**companyCurrencyGuid**",
                  "**companyGuid**",
                  "**companyName**",
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            "**city**",
            "**code**",
            [
              "**country**",
              {
                "$ul": [
                  "**code3**",
                  "**englishName**",
                  "**guid**",
                  "**hasRegions**: ",
                  "**name**"
                ]
              }
            ],
            [
              "**countryRegion**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**createdBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**createdDateTime**",
            [
              "**culture**",
              {
                "$ul": [
                  "**code**",
                  "**englishName**",
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**defaultActivityType**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            "**email**",
            "**firstName**",
            "**guid**",
            "**isActive** (default: true)",
            "**keywords**",
            [
              "**language**",
              {
                "$ul": [
                  "**code**",
                  "**englishName**",
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            "**lastLogin**",
            "**lastName**",
            [
              "**lastUpdatedBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**",
            "**notes**",
            [
              "**permissionProfile**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            "**phone**",
            "**postalCode**",
            "**salutation**: ",
            "**satisfaction**: ",
            "**socialSecurityNumber**",
            [
              "**superiorUser**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            [
              "**timezone**",
              {
                "$ul": [
                  "**guid**",
                  "**ianaName**",
                  "**name**",
                  "**systemName**"
                ]
              }
            ],
            "**title**",
            "**userType**: ",
            [
              "**workContract**",
              {
                "$ul": [
                  "**dailyHours**",
                  "**endDate**",
                  "**flextimeLimitPerDay**",
                  "**guid**",
                  [
                    "**hourCost**",
                    {
                      "$ul": [
                        "**amount**",
                        "**currencyCode**",
                        "**multiCurrencyInfo**"
                      ]
                    }
                  ],
                  "**isFlextimeActive** (default: true)",
                  "**isOvertimeAllowed**",
                  "**startDate**",
                  "**title**",
                  "**workWeek**"
                ]
              }
            ],
            [
              "**workType**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.title"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
