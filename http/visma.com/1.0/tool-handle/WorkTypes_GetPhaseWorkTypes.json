{
  "name": "WorkTypes_GetPhaseWorkTypes",
  "description": "Only the active work types are included in the list, whether they come from organization settings or project specific work types.",
  "parameters": {
    "type": "object",
    "properties": {
      "phaseGuid": {
        "type": "string",
        "description": "Id of the phase."
      },
      "pageToken": {
        "nullable": true,
        "type": "string",
        "description": "Optional: page token to fetch the next page."
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: number of rows to fetch"
      },
      "userGuid": {
        "nullable": true,
        "type": "string",
        "description": "Id of the user for whom the work types are retrieved. Default is current user."
      }
    },
    "required": [
      "phaseGuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/phases/{phaseGuid}/worktypes{?pageToken,rowCount,userGuid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            [
              "**createdBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**createdDateTime**",
            "**guid**",
            [
              "**hourCost**",
              {
                "$ul": [
                  "**amount**",
                  "**currencyCode**",
                  "**multiCurrencyInfo**"
                ]
              }
            ],
            "**isActive** (default: true)",
            "**isDefault**",
            "**isProductive**",
            [
              "**lastUpdatedBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**",
            "**name**",
            [
              "**salesAccount**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
