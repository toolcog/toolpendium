{
  "name": "Contacts_GetContacts",
  "description": "Get all the contact persons.",
  "parameters": {
    "type": "object",
    "properties": {
      "active": {
        "nullable": true,
        "type": "boolean",
        "description": "If not given, return all Contact persons, if given as true return only active Contact persons, if given as false returns only inactive Contact persons."
      },
      "firstRow": {
        "default": 0,
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: first row to fetch. Default 0 = first row."
      },
      "rowCount": {
        "format": "int32",
        "nullable": true,
        "type": "integer",
        "description": "Optional: How many rows to fetch, Default 20, maximum 100."
      },
      "textToSearch": {
        "default": "",
        "nullable": true,
        "type": "string",
        "description": "Optional: Text to search from contact person's name or communication method (i.e. phone number or email address)."
      },
      "searchCriterias": {
        "items": {
          "$ref": "#/$defs/KeyValuePairOfStringAndObject"
        },
        "nullable": true,
        "type": "array",
        "description": "Optional: Search criterias."
      },
      "sortings": {
        "items": {
          "$ref": "#/$defs/KeyValuePairOfStringAndSortDirection"
        },
        "nullable": true,
        "type": "array",
        "description": "Optional: A list of Key-Value pairs, containing names of fields and directions by which the results should be sorted. Any sortable field name (submodel fields not supported) in the model can be used, while value can be \"Desc\" or \"Asc\". Example: \"?sortings[0].key=FirstName&sortings[0].value=Desc &sortings[1].key=LastName&sortings[1].value=Asc\"."
      },
      "changedSince": {
        "format": "date-time",
        "nullable": true,
        "type": "string",
        "description": "Optional: Get contact persons that have been added or changed after this date time (greater or equal)."
      }
    },
    "$defs": {
      "KeyValuePairOfStringAndObject": {
        "properties": {
          "key": {
            "type": "string"
          },
          "value": {
            "nullable": true,
            "type": "string"
          }
        },
        "type": "object"
      },
      "KeyValuePairOfStringAndSortDirection": {
        "properties": {
          "key": {
            "type": "string"
          },
          "value": {
            "$ref": "#/$defs/SortDirection"
          }
        },
        "type": "object"
      },
      "SortDirection": {
        "description": "",
        "enum": [
          "Asc",
          "Desc",
          "AscNullsFirst",
          "AscNullsLast",
          "DescNullsFirst",
          "DescNullsLast"
        ],
        "type": "string",
        "x-enumNames": [
          "Asc",
          "Desc",
          "AscNullsFirst",
          "AscNullsLast",
          "DescNullsFirst",
          "DescNullsLast"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.severa.visma.com/rest-api/v1/contactpersons{?active,firstRow,rowCount,textToSearch,searchCriterias,sortings,changedSince}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**addressGuid**",
            [
              "**createdBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**createdDateTime**",
            [
              "**customer**",
              {
                "$ul": [
                  "**guid**",
                  "**name**",
                  "**number**"
                ]
              }
            ],
            "**dateOfBirth**",
            "**description**",
            "**emails**",
            "**firstName**",
            "**guid**",
            "**isActive** (default: true)",
            "**isDeleted**",
            "**isEmailAllowed** (default: false)",
            "**jobTitle**",
            [
              "**language**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            "**lastName**",
            [
              "**lastUpdatedBy**",
              {
                "$ul": [
                  "**code**",
                  "**firstName**",
                  "**guid**",
                  "**lastName**",
                  "**name**"
                ]
              }
            ],
            "**lastUpdatedDateTime**",
            "**name**",
            "**phoneNumbers**",
            [
              "**role**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ],
            "**salutation**: ",
            "**satisfactionLevel**: ",
            [
              "**timeZone**",
              {
                "$ul": [
                  "**guid**",
                  "**name**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**details**",
                  "**httpStatusCode**",
                  "**stack**",
                  "**type**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
