{
  "name": "whois",
  "description": "WHOIS query for a domain",
  "parameters": {
    "type": "object",
    "properties": {
      "domain": {
        "type": "string",
        "description": "Domain"
      },
      "format": {
        "enum": [
          "raw",
          "formatted",
          "json"
        ],
        "type": "string"
      }
    },
    "required": [
      "domain"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://apispot.io/api/v2/domains/{domain}/whois{?format}"
    }
  },
  "responses": {}
}
