{
  "name": "get_almaws_v1_task-lists_printouts__printout_id_",
  "description": "This Web service returns a Printout given a Printout ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "printout_id": {
        "type": "string",
        "description": "The Printout ID"
      }
    },
    "required": [
      "printout_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api-eu.hosted.exlibrisgroup.com/almaws/v1/task-lists/printouts/{printout_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Printout Object.",
        "**Key properties:**",
        {
          "$ul": [
            "**date**: The time that the letter has been send to the Printout Queue.",
            "**id**: The printout id.",
            "**letter**: Letter Html body.",
            "**link**",
            [
              "**printer**: The printer.",
              {
                "$ul": [
                  "**desc**",
                  "**value**"
                ]
              }
            ],
            "**printout**: The printout name.",
            "**size**: The size of the letter.",
            "**source**: The user who printed the letter.",
            [
              "**status**: Status of the printout (Pending/Printed/Canceled).",
              {
                "$ul": [
                  "**desc**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
