{
  "name": "get_almaws_v1_task-lists_printouts",
  "description": "This API returns a list of Printouts.",
  "parameters": {
    "type": "object",
    "properties": {
      "letter": {
        "default": "ALL",
        "type": "string",
        "description": "Printout Name. Optional. "
      },
      "status": {
        "default": "ALL",
        "type": "string",
        "description": "Printout status. Optional. Valid values are: Printed, Pending, Canceled."
      },
      "printer_id": {
        "default": "ALL",
        "type": "string",
        "description": "Printout Printer"
      },
      "printout_id": {
        "default": "ALL",
        "type": "string",
        "description": "A list of Printout IDs (for example: 123,456,778) from 1 to the limit of 100 Optional. Use of this option overrides all of the filtering parameters"
      },
      "limit": {
        "default": "10",
        "type": "integer",
        "description": "Limits the number of results. Optional. Valid values are 0-100. Default value: 10."
      },
      "offset": {
        "default": "0",
        "type": "integer",
        "description": "Offset of the results returned. Optional. Default value: 0, which means that the first results will be returned."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api-eu.hosted.exlibrisgroup.com/almaws/v1/task-lists/printouts{?letter,status,printer_id,printout_id,limit,offset}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A list of printouts.",
        "**Key properties:**",
        {
          "$ul": [
            "**printout**: printout object.",
            "**total_record_count**: The total number of printouts."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
