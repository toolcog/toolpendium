{
  "name": "generateClient",
  "description": "Accepts a `GeneratorInput` options map for spec location and generation options",
  "parameters": {
    "type": "object",
    "properties": {
      "language": {
        "type": "string",
        "description": "The target language for the client library"
      },
      "body": {
        "$ref": "#/components/schemas/GeneratorInput",
        "description": "Configuration for building the client library"
      }
    },
    "required": [
      "language",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//api.openapi-generator.tech/api/gen/clients/{language}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ResponseCode"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: File download code",
            "**link**: URL for fetching the generated client"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
