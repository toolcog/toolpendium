{
  "name": "postRelation",
  "description": "Get `relation` objects.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "example": "{\"subject\":[\"gene1\", \"gene2\"]}\n",
        "type": "string",
        "description": "An Ensembl gene identifier."
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//platform-api.opentargets.io/v3/platform/private/relation"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
