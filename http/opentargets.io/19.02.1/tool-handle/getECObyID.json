{
  "name": "getECObyID",
  "description": "Get `ECO` objects.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "ECO_ID": {
        "type": "string",
        "description": "An [evidence code ontology](http://www.ebi.ac.uk/ols/v2/browse.do?ontName=ECO) ID."
      }
    },
    "required": [
      "ECO_ID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//platform-api.opentargets.io/v3/platform/private/eco/{ECO_ID}"
    }
  },
  "responses": {}
}
