{
  "name": "getQuickSearch",
  "description": "Get `search-result` objects. Enables search bar functionality.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "q": {
        "type": "string",
        "description": "A full text query."
      },
      "size": {
        "type": "string",
        "description": "Maximum amount of results to return. Defaults to 5."
      }
    },
    "required": [
      "q"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//platform-api.opentargets.io/v3/platform/private/quicksearch{?q,size}"
    }
  },
  "responses": {}
}
