{
  "name": "register",
  "description": "Use this endpoint to make the initial call with your email and password (same as on brainbi.net) and get your bearer token in response.",
  "parameters": {
    "type": "object",
    "properties": {
      "first_name": {
        "example": "Felix",
        "type": "string",
        "description": "required"
      },
      "last_name": {
        "example": "Weber",
        "type": "string",
        "description": "required"
      },
      "company_name": {
        "example": "Fiverr",
        "type": "string",
        "description": "required"
      },
      "shop_url": {
        "example": "https;//www.fiverr.de",
        "type": "string",
        "description": "required"
      },
      "email": {
        "example": "fiverr2@fiverr.de",
        "type": "string",
        "description": "required"
      },
      "store_name": {
        "example": "Fiverr Store",
        "type": "string",
        "description": "required"
      },
      "store_url": {
        "example": "https;//www.fiverr.de",
        "type": "string",
        "description": "required"
      },
      "password": {
        "example": "12345678",
        "type": "string",
        "description": "required"
      },
      "fromuser": {
        "example": "1",
        "type": "string",
        "description": "required (always 1)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": ",/api/register{?first_name,last_name,company_name,shop_url,email,store_name,store_url,password,fromuser}"
    }
  },
  "responses": {}
}
