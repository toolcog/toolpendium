{
  "name": "registerAndCreateStoreConnectionForWooCommerce",
  "description": "Use this endpoint to make the initial call with your email and password (same as on brainbi.net) and get your bearer token in response.",
  "parameters": {
    "type": "object",
    "properties": {
      "first_name": {
        "example": "Felix",
        "type": "string",
        "description": "required"
      },
      "last_name": {
        "example": "Weber",
        "type": "string",
        "description": "required"
      },
      "company_name": {
        "example": "Fiverr",
        "type": "string",
        "description": "required"
      },
      "shop_url": {
        "example": "https;//www.fiverr.de",
        "type": "string",
        "description": "required"
      },
      "email": {
        "example": "fiver3r23@fiverr.de",
        "type": "string",
        "description": "required"
      },
      "store_name": {
        "example": "Fiverr Store",
        "type": "string",
        "description": "required"
      },
      "store_url": {
        "example": "https;//www.fiverr.de",
        "type": "string",
        "description": "required"
      },
      "password": {
        "example": "12345678",
        "type": "string",
        "description": "required"
      },
      "fromuser": {
        "example": "1",
        "type": "string",
        "description": "required (always 1)"
      },
      "api_url": {
        "example": "https://woocommerce-351439-1090797.cloudwaysapps.com/",
        "type": "string",
        "description": "required"
      },
      "consumer_key": {
        "example": "ck_59b23d313ae372feaf211652c318fbe4501abda2",
        "type": "string",
        "description": "required"
      },
      "consumer_secret": {
        "example": "cs_acc202d648bfa6b69efe553bb25086230ba116a7",
        "type": "string",
        "description": "required"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": ",/api/register_woocommerce{?first_name,last_name,company_name,shop_url,email,store_name,store_url,password,fromuser,api_url,consumer_key,consumer_secret}"
    }
  },
  "responses": {}
}
