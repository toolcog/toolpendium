{
  "name": "getEvents",
  "description": "By passing in the company, site or deal Id a set of user interaction event records is returned. For pagination of a large result set use queryexecutionid and nexttoken instead.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "company": {
        "type": "string",
        "description": "pass an optional company Id"
      },
      "site": {
        "type": "string",
        "description": "pass an optional site Id"
      },
      "deal": {
        "type": "string",
        "description": "pass an optional deal Id"
      },
      "type": {
        "enum": [
          "all",
          "clicks",
          "contacts"
        ],
        "type": "string",
        "description": "type of records to return"
      },
      "nexttoken": {
        "type": "string",
        "description": "next token to start returning records from"
      },
      "queryexecutionid": {
        "type": "string",
        "description": "id of execution to get more records based on next token"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://analytics.redeal.io/api/1.0.0/events{?company,site,deal,type,nexttoken,queryexecutionid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**agent**",
            "**browsertype**",
            "**channel**",
            "**click**",
            "**clicks**",
            "**code**",
            "**company**",
            "**contact**",
            "**cost**",
            "**costcurrency**",
            "**currency**",
            "**deal**",
            "**deal name**",
            "**delay**",
            "**denyreason**",
            "**device**",
            "**devicetype**",
            "**email**",
            "**emailaddrsha256**",
            "**flow**",
            "**flowstep**",
            "**flowtype**",
            "**impression**",
            "**ip**",
            "**latitude**",
            "**longitude**",
            "**optin**",
            "**os**",
            "**resolution**",
            "**revenue**",
            "**role**",
            "**session**",
            "**site**",
            "**site name**",
            "**timestamp**",
            "**total**",
            "**user**",
            "**userlanguage**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
