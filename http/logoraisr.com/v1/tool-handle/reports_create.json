{
  "name": "reports_create",
  "description": "This POST-Method creates a new report.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/ReportRequest"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "ReportRequest": {
        "properties": {
          "file_id": {
            "description": "Id of the file on which the process is to be applied.",
            "format": "uuid",
            "title": "File id",
            "type": "string"
          },
          "process": {
            "description": "Process which should applied. Processes are \"color-classification\".",
            "minLength": 1,
            "title": "Process",
            "type": "string"
          }
        },
        "required": [
          "process",
          "file_id"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.logoraisr.com/rest-v1/reports/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**: The Date when the project was created.",
            "**file_id**: Id of the file on which the process was applied.",
            "**processing_algorithm**: Process which was applied. Processes are \"color-classification\".",
            "**report_number**: Number of the report which was created.",
            [
              "**result**",
              {
                "$ul": [
                  "**colors**",
                  "**height**: Height of image.",
                  "**number_of_pixel_in_image**: Number of pixel in image.",
                  "**width**: Width of image."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
