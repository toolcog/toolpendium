{
  "name": "previews_read",
  "description": "This GET-Method returns the URL where the preview image of uploaded file can downloaded from.",
  "parameters": {
    "type": "object",
    "properties": {
      "file_id": {
        "type": "string",
        "description": "Id of the file for which the preview_img_url is generated."
      }
    },
    "required": [
      "file_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.logoraisr.com/rest-v1/previews/{file_id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**preview_img_url**: Url from which the preview file can be downloaded."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
