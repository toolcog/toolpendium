{
  "name": "uploads_create",
  "description": "This POST-Method uploads a new file on the server.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "file": {
            "description": "File which should be uploaded. Supported file types are: JPEG and PNG",
            "format": "binary",
            "type": "string"
          }
        },
        "required": [
          "file"
        ],
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.logoraisr.com/rest-v1/uploads/"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**file_id**: Returns the file_id. To apply processes to the uploaded file, this file_id must be referenced in the API-calls."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
