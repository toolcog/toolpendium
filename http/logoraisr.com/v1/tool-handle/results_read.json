{
  "name": "results_read",
  "description": "This GET-Method returns the URL where the result can downloaded from.",
  "parameters": {
    "type": "object",
    "properties": {
      "result_file_id": {
        "type": "string",
        "description": "Id of the result_file for which the result_file_url is generated."
      }
    },
    "required": [
      "result_file_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.logoraisr.com/rest-v1/results/{result_file_id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**result_file_url**: Url from which the result file can be downloaded."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
