{
  "name": "activity_list-repos-starred-by-user",
  "description": "Lists repositories a user has starred.\n\nYou can also find out _when_ stars were created by passing the following custom [media type](https://docs.github.com/enterprise-server@2.22/rest/overview/media-types/) via the `Accept` header:",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string"
      },
      "sort": {
        "default": "created",
        "enum": [
          "created",
          "updated"
        ],
        "type": "string",
        "description": "One of `created` (when the repository was starred) or `updated` (when it was last pushed to)."
      },
      "direction": {
        "default": "desc",
        "enum": [
          "asc",
          "desc"
        ],
        "type": "string",
        "description": "One of `asc` (ascending) or `desc` (descending)."
      },
      "per_page": {
        "default": 30,
        "type": "integer",
        "description": "Results per page (max 100)"
      },
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number of the results to fetch."
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/users/{username}/starred{?sort,direction,per_page,page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
