{
  "name": "enterprise-admin_list-pre-receive-hooks",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "per_page": {
        "default": 30,
        "type": "integer",
        "description": "Results per page (max 100)"
      },
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number of the results to fetch."
      },
      "direction": {
        "default": "desc",
        "enum": [
          "asc",
          "desc"
        ],
        "type": "string",
        "description": "One of `asc` (ascending) or `desc` (descending)."
      },
      "sort": {
        "default": "created",
        "enum": [
          "created",
          "updated",
          "name"
        ],
        "type": "string",
        "description": "One of `created` (when the repository was starred) or `updated` (when it was last pushed to) or `name`."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/admin/pre-receive-hooks{?per_page,page,direction,sort}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**allow_downstream_configuration**",
            "**enforcement**",
            [
              "**environment**",
              {
                "$ul": [
                  "**created_at**",
                  "**default_environment**",
                  [
                    "**download**",
                    {
                      "$ul": [
                        "**downloaded_at**",
                        "**message**",
                        "**state**",
                        "**url**"
                      ]
                    }
                  ],
                  "**hooks_count**",
                  "**html_url**",
                  "**id**",
                  "**image_url**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**id**",
            "**name**",
            "**script**",
            [
              "**script_repository**",
              {
                "$ul": [
                  "**full_name**",
                  "**html_url**",
                  "**id**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
