    "type": "object",
    "properties": {
      "owner": {
        "type": "string"
      },
      "repo": {
        "type": "string"
      },
      "run_id": {
        "type": "integer",
        "description": "The id of the workflow run."
      },
      "exclude_pull_requests": {
        "default": false,
        "type": "boolean",
        "description": "If `true` pull requests are omitted from the response (empty array)."
      }
    },
    "required": [
      "owner",
      "repo",
      "run_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/repos/{owner}/{repo}/actions/runs/{run_id}{?exclude_pull_requests}"
    }
