{
  "name": "codes-of-conduct_get-all-codes-of-conduct",
  "description": "",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://github.com//codes_of_conduct"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Code Of Conduct list"
        },
        "Code Of Conduct",
        "**Key properties:**",
        {
          "$ul": [
            "**body**",
            "**html_url**",
            "**key**",
            "**name**",
            "**url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
