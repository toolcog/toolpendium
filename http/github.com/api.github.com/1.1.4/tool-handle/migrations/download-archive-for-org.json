{
  "name": "migrations_download-archive-for-org",
  "description": "Fetches the URL to a migration archive.",
  "parameters": {
    "type": "object",
    "properties": {
      "org": {
        "type": "string",
        "description": "The organization name. The name is not case sensitive."
      },
      "migration_id": {
        "type": "integer",
        "description": "The unique identifier of the migration."
      }
    },
    "required": [
      "org",
      "migration_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.github.com/orgs/{org}/migrations/{migration_id}/archive"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Basic Error"
        },
        "Basic Error",
        "**Key properties:**",
        {
          "$ul": [
            "**documentation_url**",
            "**message**",
            "**status**",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
