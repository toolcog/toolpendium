{
  "name": "meta_get-octocat",
  "description": "Get the octocat as ASCII art",
  "parameters": {
    "type": "object",
    "properties": {
      "s": {
        "type": "string",
        "description": "The words to show in Octocat's speech bubble"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.github.com/octocat{?s}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
