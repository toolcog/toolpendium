{
  "name": "actions_list-labels-for-self-hosted-runner-for-org",
  "description": "Lists all labels for a self-hosted runner configured in an organization.\n\nYou must authenticate using an access token with the `admin:org` scope to use this endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "org": {
        "type": "string",
        "description": "The organization name. The name is not case sensitive."
      },
      "runner_id": {
        "type": "integer",
        "description": "Unique identifier of the self-hosted runner."
      }
    },
    "required": [
      "org",
      "runner_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.github.com/orgs/{org}/actions/runners/{runner_id}/labels"
    }
  },
