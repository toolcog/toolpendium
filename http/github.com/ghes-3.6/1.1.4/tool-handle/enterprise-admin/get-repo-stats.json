{
  "name": "enterprise-admin_get-repo-stats",
  "description": "Get repository statistics",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://github.com//enterprise/stats/repos"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Repository Enterprise Stats"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**fork_repos**",
            "**org_repos**",
            "**root_repos**",
            "**total_pushes**",
            "**total_repos**",
            "**total_wikis**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
