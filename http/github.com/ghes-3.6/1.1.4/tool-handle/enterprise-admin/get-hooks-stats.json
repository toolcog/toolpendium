{
  "name": "enterprise-admin_get-hooks-stats",
  "description": "Get hooks statistics",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://github.com//enterprise/stats/hooks"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Hooks Enterprise Stats"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active_hooks**",
            "**inactive_hooks**",
            "**total_hooks**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
