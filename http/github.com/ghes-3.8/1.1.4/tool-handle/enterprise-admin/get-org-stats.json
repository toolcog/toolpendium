{
  "name": "enterprise-admin_get-org-stats",
  "description": "",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://github.com//enterprise/stats/orgs"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Enterprise Organization Stats"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**disabled_orgs**",
            "**total_orgs**",
            "**total_team_members**",
            "**total_teams**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
