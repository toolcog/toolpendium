{
  "name": "teams_list-child",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "team_id": {
        "type": "integer"
      },
      "per_page": {
        "default": 30,
        "type": "integer",
        "description": "Results per page (max 100)"
      },
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number of the results to fetch."
      }
    },
    "required": [
      "team_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/teams/{team_id}/teams{?per_page,page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**description**",
            "**id**",
            "**members_url**",
            "**name**",
            "**node_id**",
            [
              "**parent**",
              {
                "$ul": [
                  "**description**",
                  "**html_url**",
                  "**id**",
                  "**members_url**",
                  "**name**",
                  "**node_id**",
                  "**permission**",
                  "**privacy**",
                  "**repositories_url**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**permission**",
            "**privacy**",
            "**repositories_url**",
            "**slug**",
            "**url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
