{
  "name": "teams_list-projects",
  "description": "Lists the organization projects for a team. If you are an [authenticated](https://docs.github.com/enterprise-server@2.20/rest/overview/resources-in-the-rest-api#authentication) site administrator for your Enterprise instance, you will be able to list all projects for the team.",
  "parameters": {
    "type": "object",
    "properties": {
      "accept": {
        "default": "application/vnd.github.inertia-preview+json",
        "type": "string",
        "description": "This API is under preview and subject to change."
      },
      "team_id": {
        "type": "integer"
      },
      "per_page": {
        "default": 30,
        "type": "integer",
        "description": "Results per page (max 100)"
      },
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number of the results to fetch."
      }
    },
    "required": [
      "accept",
      "team_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/teams/{team_id}/projects{?per_page,page}"
    },
    "headers": {
      "accept": {
        "$": "accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Team Project list"
        },
        "A team's access to a project.",
        "**Key properties:**",
        {
          "$ul": [
            "**body**",
            "**columns_url**",
            "**created_at**",
            [
              "**creator**: Simple User",
              {
                "$ul": [
                  "**avatar_url**",
                  "**email**",
                  "**events_url**",
                  "**followers_url**",
                  "**following_url**",
                  "**gists_url**",
                  "**gravatar_id**",
                  "**html_url**",
                  "**id**",
                  "**login**",
                  "**name**",
                  "**node_id**",
                  "**organizations_url**",
                  "**received_events_url**",
                  "**repos_url**",
                  "**site_admin**",
                  "**starred_at**",
                  "**starred_url**",
                  "**subscriptions_url**",
                  "**type**",
                  "**url**"
                ]
              }
            ],
            "**html_url**",
            "**id**",
            "**name**",
            "**node_id**",
            "**number**",
            "**organization_permission**: The organization permission for this project. Only present when owner is an organization.",
            "**owner_url**",
            [
              "**permissions**",
              {
                "$ul": [
                  "**admin**",
                  "**read**",
                  "**write**"
                ]
              }
            ],
            "**private**: Whether the project is private or not. Only present when owner is an organization.",
            "**state**",
            "**updated_at**",
            "**url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
