{
  "name": "gitignore_get-all-templates",
  "description": "List all templates available to pass as an option when [creating a repository](https://docs.github.com/enterprise-server@3.7/rest/reference/repos#create-a-repository-for-the-authenticated-user).",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://github.com//gitignore/templates"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
