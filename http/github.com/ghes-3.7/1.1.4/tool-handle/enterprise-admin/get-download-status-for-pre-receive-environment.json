{
  "name": "enterprise-admin_get-download-status-for-pre-receive-environment",
  "description": "In addition to seeing the download status at the \"[Get a pre-receive environment](#get-a-pre-receive-environment)\" endpoint, there is also this separate endpoint for just the download status.",
  "parameters": {
    "type": "object",
    "properties": {
      "pre_receive_environment_id": {
        "type": "integer",
        "description": "The unique identifier of the pre-receive environment."
      }
    },
    "required": [
      "pre_receive_environment_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://github.com//admin/pre-receive-environments/{pre_receive_environment_id}/downloads/latest"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**downloaded_at**",
            "**message**",
            "**state**",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
