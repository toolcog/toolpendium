            "**organizations_url**",
            "**received_events_url**",
            "**repos_url**",
            "**site_admin**",
            "**starred_at**",
            "**starred_url**",
            "**subscriptions_url**",
            "**type**",
            "**url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
