{
  "name": "users_list-gpg-keys-for-user",
  "description": "Lists the GPG keys for a user. This information is accessible by anyone.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "The handle for the GitHub user account."
      },
      "per_page": {
        "default": 30,
        "type": "integer",
        "description": "The number of results per page (max 100)."
      },
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number of the results to fetch."
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://github.com//users/{username}/gpg_keys{?per_page,page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "GPG Key list"
        },
        "A unique encryption key",
        "**Key properties:**",
        {
          "$ul": [
            "**can_certify**",
            "**can_encrypt_comms**",
            "**can_encrypt_storage**",
            "**can_sign**",
            "**created_at**",
            "**emails**",
            "**expires_at**",
            "**id**",
            "**key_id**",
            "**name**",
            "**primary_key_id**",
            "**public_key**",
            "**raw_key**",
            "**revoked**",
            "**subkeys**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
