{
  "name": "teams_delete-discussion-comment",
  "description": "Deletes a comment on a team discussion. OAuth access tokens require the `write:discussion` [scope](https://docs.github.com/enterprise-server@2.18/apps/building-oauth-apps/understanding-scopes-for-oauth-apps/).",
  "parameters": {
    "type": "object",
    "properties": {
      "accept": {
        "default": "application/vnd.github.echo-preview+json",
        "type": "string",
        "description": "This API is under preview and subject to change."
      },
      "team_id": {
        "type": "integer"
      },
      "discussion_number": {
        "type": "integer"
      },
      "comment_number": {
        "type": "integer"
      }
    },
    "required": [
      "accept",
      "team_id",
      "discussion_number",
      "comment_number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/teams/{team_id}/discussions/{discussion_number}/comments/{comment_number}"
    },
    "headers": {
      "accept": {
        "$": "accept"
      }
    }
  },
  "responses": {}
}
