{
  "name": "enterprise-admin_update-pre-receive-environment",
  "description": "You cannot modify the default environment. If you attempt to modify the default environment, you will receive a `422 Unprocessable Entity` response.",
  "parameters": {
    "type": "object",
    "properties": {
      "pre_receive_environment_id": {
        "type": "integer",
        "description": "The unique identifier of the pre-receive environment."
      },
      "body": {
        "properties": {
          "image_url": {
            "description": "URL from which to download a tarball of this environment.",
            "type": "string"
          },
          "name": {
            "description": "This pre-receive environment's new name.",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "pre_receive_environment_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://github.com//admin/pre-receive-environments/{pre_receive_environment_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**default_environment**",
            [
              "**download**",
              {
                "$ul": [
                  "**downloaded_at**",
                  "**message**",
                  "**state**",
                  "**url**"
                ]
              }
            ],
            "**hooks_count**",
            "**html_url**",
            "**id**",
            "**image_url**",
            "**name**",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
