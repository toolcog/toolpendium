{
  "name": "enterprise-admin_update-pre-receive-hook-enforcement-for-repo",
  "description": "For pre-receive hooks which are allowed to be configured at the repo level, you can set `enforcement`",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "The account owner of the repository. The name is not case sensitive."
      },
      "repo": {
        "type": "string",
        "description": "The name of the repository. The name is not case sensitive."
      },
      "pre_receive_hook_id": {
        "type": "integer",
        "description": "The unique identifier of the pre-receive hook."
      },
      "body": {
        "properties": {
          "enforcement": {
            "description": "The state of enforcement for the hook on this repository.",
            "enum": [
              "enabled",
              "disabled",
              "testing"
            ],
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "owner",
      "repo",
      "pre_receive_hook_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://github.com//repos/{owner}/{repo}/pre-receive-hooks/{pre_receive_hook_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**configuration_url**",
            "**enforcement**",
            "**id**",
            "**name**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
