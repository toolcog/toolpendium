{
  "name": "teams_list-members",
  "description": "If you pass the `hellcat-preview` media type, team members will include the members of child teams.\n\nTo list members in a team, the team must be visible to the authenticated user.",
  "parameters": {
    "type": "object",
    "properties": {
      "team_id": {
        "type": "integer"
      },
      "role": {
        "default": "all",
        "enum": [
          "member",
          "maintainer",
          "all"
        ],
        "type": "string",
        "description": "Filters members returned by their role in the team. Can be one of:  \n\\* `member` - normal members of the team.  \n\\* `maintainer` - team maintainers.  \n\\* `all` - all members of the team."
      },
      "per_page": {
        "default": 30,
        "type": "integer",
        "description": "Results per page (max 100)"
      },
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number of the results to fetch."
      }
    },
    "required": [
      "team_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/teams/{team_id}/members{?role,per_page,page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Simple User list"
        },
        "Simple User",
        "**Key properties:**",
        {
          "$ul": [
            "**avatar_url**",
            "**email**",
            "**events_url**",
            "**followers_url**",
            "**following_url**",
            "**gists_url**",
            "**gravatar_id**",
            "**html_url**",
            "**id**",
            "**login**",
            "**name**",
            "**node_id**",
            "**organizations_url**",
            "**received_events_url**",
            "**repos_url**",
            "**site_admin**",
            "**starred_at**",
            "**starred_url**",
            "**subscriptions_url**",
            "**type**",
            "**url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
