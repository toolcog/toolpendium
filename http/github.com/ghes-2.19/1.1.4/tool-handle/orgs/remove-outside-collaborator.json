{
  "name": "orgs_remove-outside-collaborator",
  "description": "Removing a user from this list will remove them from all the organization's repositories.",
  "parameters": {
    "type": "object",
    "properties": {
      "org": {
        "type": "string"
      },
      "username": {
        "type": "string"
      }
    },
    "required": [
      "org",
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/orgs/{org}/outside_collaborators/{username}"
    }
  },
  "responses": {
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**documentation_url**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
