{
  "name": "enterprise-admin_list-global-webhooks",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "per_page": {
        "default": 30,
        "type": "integer",
        "description": "The number of results per page (max 100)."
      },
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number of the results to fetch."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/admin/hooks{?per_page,page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**",
            [
              "**config**",
              {
                "$ul": [
                  "**content_type**",
                  "**insecure_ssl**",
                  "**secret**",
                  "**url**"
                ]
              }
            ],
            "**created_at**",
            "**events**",
            "**id**",
            "**name**",
            "**ping_url**",
            "**type**",
            "**updated_at**",
            "**url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
