{
  "name": "pulls_check-if-merged",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "The account owner of the repository. The name is not case sensitive."
      },
      "repo": {
        "type": "string",
        "description": "The name of the repository. The name is not case sensitive."
      },
      "pull_number": {
        "type": "integer",
        "description": "The number that identifies the pull request."
      }
    },
    "required": [
      "owner",
      "repo",
      "pull_number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://HOSTNAME/api/v3/repos/{owner}/{repo}/pulls/{pull_number}/merge"
    }
  },
  "responses": {}
}
