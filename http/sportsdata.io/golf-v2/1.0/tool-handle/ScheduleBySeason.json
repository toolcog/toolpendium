{
  "name": "ScheduleBySeason",
  "description": "Schedule by Season",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "season": {
        "type": "string",
        "description": "Year of the season.\n<br>Examples: <code>2016</code>, <code>2017</code>."
      }
    },
    "required": [
      "format",
      "season"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/golf/v2/{format}/Tournaments/{season}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Canceled**",
            "**City**",
            "**Country**",
            "**Covered**",
            "**EndDate**",
            "**Format**",
            "**IsInProgress**",
            "**IsOver**",
            "**Location**",
            "**Name**",
            "**OddsCoverage**",
            "**Par**",
            "**Purse**",
            "**Rounds**",
            "**SportRadarTournamentID**",
            "**StartDate**",
            "**StartDateTime**",
            "**State**",
            "**TimeZone**",
            "**TournamentID**",
            "**Venue**",
            "**Yards**",
            "**ZipCode**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
