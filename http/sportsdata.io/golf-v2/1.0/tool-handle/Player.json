{
  "name": "Player",
  "description": "Player",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "playerid": {
        "type": "string",
        "description": "Unique FantasyData Player ID.\nExample:<code>40000019</code>."
      }
    },
    "required": [
      "format",
      "playerid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/golf/v2/{format}/Player/{playerid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**BirthCity**",
            "**BirthDate**",
            "**BirthState**",
            "**College**",
            "**Country**",
            "**DraftKingsName**",
            "**DraftKingsPlayerID**",
            "**FanDuelName**",
            "**FanDuelPlayerID**",
            "**FantasyAlarmPlayerID**",
            "**FantasyDraftName**",
            "**FantasyDraftPlayerID**",
            "**FirstName**",
            "**LastName**",
            "**PgaDebut**",
            "**PgaTourPlayerID**",
            "**PhotoUrl**",
            "**PlayerID**",
            "**RotoWirePlayerID**",
            "**RotoworldPlayerID**",
            "**SportRadarPlayerID**",
            "**Swings**",
            "**Weight**",
            "**YahooPlayerID**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
