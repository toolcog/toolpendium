{
  "name": "TeamGameLogsBySeason",
  "description": "Game by game log of total team statistics.",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>xml</code> or <code>json</code>."
      },
      "season": {
        "type": "string",
        "description": "Season to get games from. Example <code>2019POST</code>, <code>2020</code>        "
      },
      "teamid": {
        "type": "string",
        "description": "Unique ID of team.  Example <code> 1 </code>"
      },
      "numberofgames": {
        "type": "string",
        "description": "How many games to return. Example <code>all</code>, <code>10</code>, <code>25</code>"
      }
    },
    "required": [
      "format",
      "season",
      "teamid",
      "numberofgames"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/cfb/scores/{format}/TeamGameStatsBySeason/{season}/{teamid}/{numberofgames}"
    }
  },
