{
  "name": "GamesByWeekBasic",
  "description": "Games by Week (Basic)",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>xml</code> or <code>json</code>."
      },
      "season": {
        "type": "string",
        "description": "\n          Year of the season.\n          <br>Examples: <code>2015</code>, <code>2016</code>, etc.\n        "
      },
      "week": {
        "type": "string",
        "description": "\n          The week of the game(s).\n          <br>Examples: <code>1</code>, <code>2</code>, <code>3</code>, etc.\n        "
      }
    },
    "required": [
      "format",
      "season",
      "week"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/cfb/scores/{format}/ScoresBasic/{season}/{week}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
