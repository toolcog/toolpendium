{
  "name": "DriverRaceProjectionsEntryList",
  "description": "Driver Race Projections (Entry List)",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "xml",
        "enum": [
          "xml",
          "json"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "raceid": {
        "type": "string",
        "description": "Unique FantasyData Race ID.\nExample:<code>1</code>, <code>2</code>, etc."
      }
    },
    "required": [
      "format",
      "raceid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/nascar/v2/{format}/DriverRaceProjections/{raceid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Bonus**",
            "**Created**",
            "**CurrentPosition**",
            "**DateTime**",
            "**Day**",
            "**DraftKingsSalary**",
            "**DriverID**",
            "**FantasyPoints**",
            "**FantasyPointsDraftKings**",
            "**FastestLaps**",
            "**FinalPosition**",
            "**Laps**",
            "**LapsLed**",
            "**Manufacturer**",
            "**Name**",
            "**Number**",
            "**NumberDisplay**",
            "**Penalty**",
            "**Points**",
            "**PoleFinalPosition**",
            "**Poles**",
            "**PositionDifferential**",
            "**QualifyingSpeed**",
            "**RaceID**",
            "**Season**",
            "**StartPosition**",
            "**StatID**",
            "**Updated**",
            "**Wins**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
