{
  "name": "RaceResults",
  "description": "Race Results",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "xml",
        "enum": [
          "xml",
          "json"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "raceid": {
        "type": "string",
        "description": "Unique FantasyData Race ID.\nExample:<code>1</code>, <code>2</code>, etc."
      }
    },
    "required": [
      "format",
      "raceid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/nascar/v2/{format}/raceresult/{raceid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DriverRaces**",
            [
              "**Race**",
              {
                "$ul": [
                  "**ActualLaps**",
                  "**Broadcast**",
                  "**Canceled**",
                  "**Created**",
                  "**DateTime**",
                  "**Day**",
                  "**IsInProgress**",
                  "**IsOver**",
                  "**Name**",
                  "**PoleWinnerID**",
                  "**RaceID**",
                  "**RescheduledDateTime**",
                  "**RescheduledDay**",
                  "**ScheduledLaps**",
                  "**Season**",
                  "**SeriesID**",
                  "**SeriesName**",
                  "**Track**",
                  "**Updated**",
                  "**WinnerID**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
