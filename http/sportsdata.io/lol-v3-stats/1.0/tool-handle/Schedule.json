{
  "name": "Schedule",
  "description": "Schedule",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "xml",
        "enum": [
          "xml",
          "json"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "roundid": {
        "type": "string",
        "description": "Unique FantasyData Round ID. RoundIDs can be found in the Competitions and Competition Details endpoints. \nExample: <code>100000278</code>, etc"
      }
    },
    "required": [
      "format",
      "roundid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/lol/stats/{format}/Schedule/{roundid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**BestOf**",
            "**DateTime**",
            "**Day**",
            "**DrawMoneyLine**",
            "**GameId**",
            "**Group**",
            "**IsClosed**",
            "**PointSpread**",
            "**RoundId**",
            "**Season**",
            "**SeasonType**",
            "**Status**",
            "**TeamAId**",
            "**TeamAKey**",
            "**TeamAMoneyLine**",
            "**TeamAName**",
            "**TeamAPointSpreadPayout**",
            "**TeamAScore**",
            "**TeamBId**",
            "**TeamBKey**",
            "**TeamBMoneyLine**",
            "**TeamBName**",
            "**TeamBPointSpreadPayout**",
            "**TeamBScore**",
            "**Updated**",
            "**UpdatedUtc**",
            "**VenueId**",
            "**VenueType**",
            "**Week**",
            "**Winner**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
