{
  "name": "PlayersByTeam",
  "description": "Players by Team",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "team": {
        "type": "string",
        "description": "The abbreviation of the requested team.\n <br>Examples: <code>SF</code>, <code>NYY</code>.\n "
      }
    },
    "required": [
      "format",
      "team"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/nhl/scores/{format}/Players/{team}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**BirthCity**",
            "**BirthDate**",
            "**BirthState**",
            "**Catches**",
            "**DepthChartOrder**",
            "**DepthChartPosition**",
            "**DraftKingsName**",
            "**DraftKingsPlayerID**",
            "**FanDuelName**",
            "**FanDuelPlayerID**",
            "**FantasyAlarmPlayerID**",
            "**FantasyDraftName**",
            "**FantasyDraftPlayerID**",
            "**FirstName**",
            "**GlobalTeamID**",
            "**Height**",
            "**InjuryBodyPart**",
            "**InjuryNotes**",
            "**InjuryStartDate**",
            "**InjuryStatus**",
            "**Jersey**",
            "**LastName**",
            "**PhotoUrl**",
            "**PlayerID**",
            "**Position**",
            "**RotoWirePlayerID**",
            "**RotoworldPlayerID**",
            "**Shoots**",
            "**SportRadarPlayerID**",
            "**SportsDirectPlayerID**",
            "**StatsPlayerID**",
            "**Status**",
            "**Team**",
            "**TeamID**",
            "**UsaTodayHeadshotNoBackgroundUpdated**",
            "**UsaTodayHeadshotNoBackgroundUrl**",
            "**UsaTodayHeadshotUpdated**",
            "**UsaTodayHeadshotUrl**",
            "**UsaTodayPlayerID**",
            "**Weight**",
            "**XmlTeamPlayerID**",
            "**YahooName**",
            "**YahooPlayerID**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
