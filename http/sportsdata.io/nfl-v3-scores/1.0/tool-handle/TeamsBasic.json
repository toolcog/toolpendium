{
  "name": "TeamsBasic",
  "description": "Gets all teams, including pro bowl teams.",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      }
    },
    "required": [
      "format"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/nfl/scores/{format}/TeamsBasic"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ByeWeek**",
            "**City**",
            "**Conference**",
            "**DefensiveCoordinator**",
            "**DefensiveScheme**",
            "**Division**",
            "**FullName**",
            "**GlobalTeamID**",
            "**HeadCoach**",
            "**Key**",
            "**Name**",
            "**OffensiveCoordinator**",
            "**OffensiveScheme**",
            "**PlayerID**",
            "**PrimaryColor**",
            "**QuaternaryColor**",
            "**SecondaryColor**",
            "**SpecialTeamsCoach**",
            "**StadiumID**",
            "**TeamID**",
            "**TertiaryColor**",
            "**WikipediaLogoURL**",
            "**WikipediaWordMarkURL**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
