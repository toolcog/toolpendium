{
  "name": "PlayersByTeamBasic",
  "description": "Players by Team (Basic)",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "team": {
        "type": "string",
        "description": "Abbreviation of the team. Example: <code>WAS</code>."
      }
    },
    "required": [
      "format",
      "team"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/nfl/scores/{format}/PlayersBasic/{team}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
