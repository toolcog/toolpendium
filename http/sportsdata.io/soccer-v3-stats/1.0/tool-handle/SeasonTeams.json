{
  "name": "SeasonTeams",
  "description": "Season Teams",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "xml",
        "enum": [
          "xml",
          "json"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "seasonid": {
        "type": "string",
        "description": "Unique FantasyData Season ID. SeasonIDs can be found in the Competition Hierarchy (League Hierarchy). \nExamples: <code>1</code>, <code>2</code>, <code>3</code>, etc"
      }
    },
    "required": [
      "format",
      "seasonid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/soccer/stats/{format}/SeasonTeams/{seasonid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Active**",
            "**Gender**",
            "**SeasonId**",
            "**SeasonTeamId**",
            [
              "**Team**",
              {
                "$ul": [
                  "**Active**",
                  "**Address**",
                  "**AreaId**",
                  "**AreaName**",
                  "**City**",
                  "**ClubColor1**",
                  "**ClubColor2**",
                  "**ClubColor3**",
                  "**Email**",
                  "**Fax**",
                  "**Founded**",
                  "**FullName**",
                  "**Gender**",
                  "**GlobalTeamId**",
                  "**Key**",
                  "**Name**",
                  "**Nickname1**",
                  "**Nickname2**",
                  "**Nickname3**",
                  "**Phone**",
                  "**TeamId**",
                  "**Type**",
                  "**VenueId**",
                  "**VenueName**",
                  "**Website**",
                  "**WikipediaLogoUrl**",
                  "**WikipediaWordMarkUrl**",
                  "**Zip**"
                ]
              }
            ],
            "**TeamId**",
            "**TeamName**",
            "**Type**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
