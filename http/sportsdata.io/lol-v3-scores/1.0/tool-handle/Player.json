{
  "name": "Player",
  "description": "Player",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "xml",
        "enum": [
          "xml",
          "json"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "playerid": {
        "type": "string",
        "description": "Unique FantasyData Player ID.\nExample:<code>100000576</code>."
      }
    },
    "required": [
      "format",
      "playerid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/lol/scores/{format}/Player/{playerid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**BirthCity**",
            "**BirthCountry**",
            "**BirthDate**",
            "**CommonName**",
            "**FirstName**",
            "**Gender**",
            "**LastName**",
            "**MatchName**",
            "**Nationality**",
            "**PlayerId**",
            "**Position**",
            "**Updated**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
