{
  "name": "BoxScoresByDateDelta",
  "description": "Box Scores by Date Delta",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "date": {
        "type": "string",
        "description": "The date of the game(s).\n<br>Examples: <code>2018-FEB-27</code>, <code>2017-DEC-01</code>."
      },
      "minutes": {
        "type": "string",
        "description": "Only returns player statistics that have changed in the last X minutes.  You specify how many minutes in time to go back.  Valid entries are:\n<code>1</code> or <code>2</code>."
      }
    },
    "required": [
      "format",
      "date",
      "minutes"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/cbb/stats/{format}/BoxScoresDelta/{date}/{minutes}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**Game**",
              {
                "$ul": [
                  "**Attendance**",
                  "**AwayPointSpreadPayout**",
                  "**AwayRotationNumber**",
                  "**AwayTeam**",
                  "**AwayTeamID**",
                  "**AwayTeamMoneyLine**",
                  "**AwayTeamPreviousGameID**",
                  "**AwayTeamPreviousGlobalGameID**",
                  "**AwayTeamScore**",
                  "**AwayTeamSeed**",
                  "**BottomTeamPreviousGameId**",
                  "**Bracket**",
                  "**Channel**",
                  "**DateTime**",
                  "**DateTimeUTC**",
                  "**Day**",
                  "**GameEndDateTime**",
                  "**GameID**",
                  "**GlobalAwayTeamID**",
                  "**GlobalGameID**",
                  "**GlobalHomeTeamID**",
                  "**HomePointSpreadPayout**",
                  "**HomeRotationNumber**",
                  "**HomeTeam**",
                  "**HomeTeamID**",
                  "**HomeTeamMoneyLine**",
                  "**HomeTeamPreviousGameID**",
                  "**HomeTeamPreviousGlobalGameID**",
                  "**HomeTeamScore**",
                  "**HomeTeamSeed**",
                  "**IsClosed**",
                  "**NeutralVenue**",
                  "**OverPayout**",
                  "**OverUnder**",
                  "**Period**",
                  "**Periods**",
                  "**PointSpread**",
                  "**Round**",
                  "**Season**",
                  "**SeasonType**",
                  [
                    "**Stadium**",
                    {
                      "$ul": [
                        "**Active**",
                        "**Address**",
                        "**Capacity**",
                        "**City**",
                        "**Country**",
                        "**GeoLat**",
                        "**GeoLong**",
                        "**Name**",
                        "**StadiumID**",
                        "**State**",
                        "**Zip**"
                      ]
                    }
                  ],
                  "**Status**",
                  "**TimeRemainingMinutes**",
                  "**TimeRemainingSeconds**",
                  "**TopTeamPreviousGameId**",
                  "**TournamentDisplayOrder**",
                  "**TournamentDisplayOrderForHomeTeam**",
                  "**TournamentID**",
                  "**UnderPayout**",
                  "**Updated**"
                ]
              }
            ],
            "**Periods**",
            "**PlayerGames**",
            "**TeamGames**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
