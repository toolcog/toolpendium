      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Games**",
            "**LeftBottomBracketConference**",
            "**LeftTopBracketConference**",
            "**Location**",
            "**Name**",
            "**RightBottomBracketConference**",
            "**RightTopBracketConference**",
            "**Season**",
            "**TournamentID**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
