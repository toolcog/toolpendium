{
  "name": "TournamentHierarchy",
  "description": "Tournament Hierarchy",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "default": "XML",
        "enum": [
          "XML",
          "JSON"
        ],
        "type": "string",
        "description": "Desired response format. Valid entries are <code>XML</code> or <code>JSON</code>."
      },
      "season": {
        "type": "string",
        "description": "Year of the season (with optional season type).<br>Examples: <code>2018</code>, <code>2018POST</code>, <code>2019</code>."
      }
    },
    "required": [
      "format",
      "season"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://azure-api.sportsdata.io/v3/cbb/stats/{format}/Tournament/{season}"
    }
  },
  "responses": {
