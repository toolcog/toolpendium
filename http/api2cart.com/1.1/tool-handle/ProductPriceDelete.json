{
  "name": "ProductPriceDelete",
  "description": "Delete some prices of the product",
  "parameters": {
    "type": "object",
    "properties": {
      "product_id": {
        "type": "string",
        "description": "Defines the product where the price has to be deleted"
      },
      "group_prices": {
        "type": "string",
        "description": "Defines product's group prices"
      }
    },
    "required": [
      "product_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/product.price.delete.json{?product_id,group_prices}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**deleted**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
