{
  "name": "CartMetaDataList",
  "description": "Get entity meta data",
  "parameters": {
    "type": "object",
    "properties": {
      "entity_id": {
        "type": "string",
        "description": "Entity Id"
      },
      "entity": {
        "default": "product",
        "type": "string",
        "description": "Entity"
      },
      "store_id": {
        "type": "string",
        "description": "Store Id"
      },
      "lang_id": {
        "type": "string",
        "description": "Language id"
      },
      "key": {
        "type": "string",
        "description": "Key"
      },
      "count": {
        "default": 10,
        "type": "integer",
        "description": "This parameter sets the entity amount that has to be retrieved. Max allowed count=250"
      },
      "page_cursor": {
        "type": "string",
        "description": "Used to retrieve entities via cursor-based pagination (it can't be used with any other filtering parameter)"
      },
      "params": {
        "default": "key,value",
        "type": "string",
        "description": "Set this parameter in order to choose which entity fields you want to retrieve"
      },
      "response_fields": {
        "type": "string",
        "description": "Set this parameter in order to choose which entity fields you want to retrieve"
      },
      "exclude": {
        "type": "string",
        "description": "Set this parameter in order to choose which entity fields you want to ignore. Works only if parameter `params` equal force_all"
      }
    },
    "required": [
      "entity_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/cart.meta_data.list.json{?entity_id,entity,store_id,lang_id,key,count,page_cursor,params,response_fields,exclude}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**additional_fields**",
            "**custom_fields**",
            [
              "**pagination**",
              {
                "$ul": [
                  "**additional_fields**",
                  "**custom_fields**",
                  "**next**",
                  "**previous**"
                ]
              }
            ],
            [
              "**result**",
              {
                "$ul": [
                  "**additional_fields**",
                  "**custom_fields**",
                  "**items**",
                  "**total_count**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
