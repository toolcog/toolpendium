{
  "name": "CartInfo",
  "description": "Get cart information",
  "parameters": {
    "type": "object",
    "properties": {
      "params": {
        "default": "store_name,store_url,db_prefix",
        "type": "string",
        "description": "Set this parameter in order to choose which entity fields you want to retrieve"
      },
      "response_fields": {
        "type": "string",
        "description": "Set this parameter in order to choose which entity fields you want to retrieve"
      },
      "exclude": {
        "type": "string",
        "description": "Set this parameter in order to choose which entity fields you want to ignore. Works only if parameter `params` equal force_all"
      },
      "store_id": {
        "type": "string",
        "description": "Store Id"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/cart.info.json{?params,response_fields,exclude,store_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**additional_fields**",
                  "**custom_fields**",
                  "**db_prefix**",
                  "**name**",
                  "**shipping_zones**",
                  "**stores_info**",
                  "**url**",
                  "**version**",
                  "**warehouses**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
