    "url": {
      "$uri": "https://api.api2cart.com/v1.1/product.count.json{?category_id,created_from,created_to,modified_from,modified_to,avail_view,avail_sale,store_id,lang_id,product_ids,report_request_id,disable_report_cache,brand_name,product_attributes,status,type}"
