{
  "name": "WebhookCount",
  "description": "Count registered webhooks on the store.",
  "parameters": {
    "type": "object",
    "properties": {
      "entity": {
        "type": "string",
        "description": "The entity you want to filter webhooks by (e.g. order or product)"
      },
      "action": {
        "type": "string",
        "description": "The action you want to filter webhooks by (e.g. order or product)"
      },
      "active": {
        "type": "boolean",
        "description": "The webhook status you want to filter webhooks by"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/webhook.count.json{?entity,action,active}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**webhook_count**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
