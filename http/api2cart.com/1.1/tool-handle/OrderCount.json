{
  "name": "OrderCount",
  "description": "Count orders in store",
  "parameters": {
    "type": "object",
    "properties": {
      "customer_id": {
        "type": "string",
        "description": "Counts orders quantity specified by customer id"
      },
      "customer_email": {
        "type": "string",
        "description": "Counts orders quantity specified by customer email"
      },
      "order_status": {
        "type": "string",
        "description": "Counts orders quantity specified by order status"
      },
      "order_status_ids": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Retrieves orders specified by order statuses"
      },
      "created_to": {
        "type": "string",
        "description": "Retrieve entities to their creation date"
      },
      "created_from": {
        "type": "string",
        "description": "Retrieve entities from their creation date"
      },
      "modified_to": {
        "type": "string",
        "description": "Retrieve entities to their modification date"
      },
      "modified_from": {
        "type": "string",
        "description": "Retrieve entities from their modification date"
      },
      "store_id": {
        "type": "string",
        "description": "Counts orders quantity specified by store id"
      },
      "ids": {
        "type": "string",
        "description": "Counts orders specified by ids"
      },
      "order_ids": {
        "type": "string",
        "description": "Counts orders specified by order ids"
      },
      "ebay_order_status": {
        "type": "string",
        "description": "Counts orders quantity specified by order status"
      },
      "financial_status": {
        "type": "string",
        "description": "Counts orders quantity specified by financial status"
      },
      "fulfillment_status": {
        "type": "string",
        "description": "Create order with fulfillment status"
      },
      "shipping_method": {
        "type": "string",
        "description": "Retrieve entities according to shipping method"
      },
      "delivery_method": {
        "type": "string",
        "description": "Retrieves order with delivery method"
      },
      "ship_node_type": {
        "type": "string",
        "description": "Retrieves order with ship node type"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/order.count.json{?customer_id,customer_email,order_status,order_status_ids,created_to,created_from,modified_to,modified_from,store_id,ids,order_ids,ebay_order_status,financial_status,fulfillment_status,shipping_method,delivery_method,ship_node_type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**orders_count**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
