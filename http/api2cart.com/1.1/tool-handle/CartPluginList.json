{
  "name": "CartPluginList",
  "description": "Get list of installed plugins",
  "parameters": {
    "type": "object",
    "properties": {
      "store_key": {
        "type": "string",
        "description": "Set this parameter if bridge is already uploaded to store"
      },
      "store_id": {
        "type": "string",
        "description": "Store Id"
      },
      "start": {
        "default": 0,
        "type": "integer",
        "description": "This parameter sets the number from which you want to get entities"
      },
      "count": {
        "default": 10,
        "type": "integer",
        "description": "This parameter sets the entity amount that has to be retrieved. Max allowed count=250"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/cart.plugin.list.json{?store_key,store_id,start,count}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**all_plugins**",
                  "**plugins**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
