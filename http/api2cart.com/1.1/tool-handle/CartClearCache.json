{
  "name": "CartClearCache",
  "description": "Clear cache on store.",
  "parameters": {
    "type": "object",
    "properties": {
      "cache_type": {
        "type": "string",
        "description": "Defines which cache should be cleared."
      }
    },
    "required": [
      "cache_type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/cart.clear_cache.json{?cache_type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**cache_cleared**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
