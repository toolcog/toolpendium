{
  "name": "CategoryImageAdd",
  "description": "Add image to category",
  "parameters": {
    "type": "object",
    "properties": {
      "category_id": {
        "type": "string",
        "description": "Defines category id where the image should be added"
      },
      "image_name": {
        "type": "string",
        "description": "Defines image's name"
      },
      "url": {
        "type": "string",
        "description": "Defines URL of the image that has to be added"
      },
      "label": {
        "type": "string",
        "description": "Defines alternative text that has to be attached to the picture"
      },
      "mime": {
        "type": "string",
        "description": "Mime type of image http://en.wikipedia.org/wiki/Internet_media_type."
      },
      "type": {
        "enum": [
          "base",
          "thumbnail"
        ],
        "type": "string",
        "description": "Defines image's types that are specified by comma-separated list"
      },
      "position": {
        "default": 0,
        "type": "integer",
        "description": "Defines imageâ€™s position in the list"
      },
      "store_id": {
        "type": "string",
        "description": "Store Id"
      }
    },
    "required": [
      "category_id",
      "image_name",
      "url",
      "type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/category.image.add.json{?category_id,image_name,url,label,mime,type,position,store_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**image_path**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
