{
  "name": "BridgeDownload",
  "description": "Download bridge for store",
  "parameters": {
    "type": "object",
    "properties": {
      "whitelabel": {
        "default": false,
        "type": "boolean",
        "description": "Identifies if there is a necessity to download whitelabel bridge."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/bridge.download.file{?whitelabel}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
