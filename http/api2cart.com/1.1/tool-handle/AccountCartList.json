{
  "name": "AccountCartList",
  "description": "Get list of carts.",
  "parameters": {
    "type": "object",
    "properties": {
      "params": {
        "default": "force_all",
        "type": "string",
        "description": "Set this parameter in order to choose which entity fields you want to retrieve"
      },
      "exclude": {
        "type": "string",
        "description": "Set this parameter in order to choose which entity fields you want to ignore. Works only if parameter `params` equal force_all"
      },
      "request_from_date": {
        "type": "string",
        "description": "Retrieve entities from their creation date"
      },
      "request_to_date": {
        "type": "string",
        "description": "Retrieve entities to their creation date"
      },
      "store_url": {
        "type": "string",
        "description": "A web address of a store"
      },
      "store_key": {
        "type": "string",
        "description": "Find store by store key"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.api2cart.com/v1.1/account.cart.list.json{?params,exclude,request_from_date,request_to_date,store_url,store_key}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**carts**",
                  "**carts_count**"
                ]
              }
            ],
            "**return_code**",
            "**return_message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
