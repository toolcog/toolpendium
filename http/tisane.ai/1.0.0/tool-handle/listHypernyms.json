{
  "name": "listHypernyms",
  "description": "Lists all hypernyms related to a family. A hypernym is a parent concent. E.g. \"vehicle\" is a hypernym of \"truck\".",
  "parameters": {
    "type": "object",
    "properties": {
      "family": {
        "example": "{family_id}",
        "type": "string",
        "description": "(Required) a numeric identifier of the family"
      },
      "maxLevel": {
        "example": "{max_number_of_levels}",
        "type": "string",
        "description": "(Required) maximum distance from the family"
      },
      "Ocp-Apim-Subscription-Key": {
        "example": "{{apiKey}}",
        "type": "string",
        "description": "{{apiKeyDescription}}"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.tisane.ai/hypernyms{?family,maxLevel}"
    },
    "headers": {
      "Ocp-Apim-Subscription-Key": {
        "$": "Ocp-Apim-Subscription-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
