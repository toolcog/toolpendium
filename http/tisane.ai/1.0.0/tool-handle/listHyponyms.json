{
  "name": "listHyponyms",
  "description": "Lists all hyponyms related to a family. A hyponym is a child concent. E.g. \"truck\" is a hypernym of \"vehicle\".",
  "parameters": {
    "type": "object",
    "properties": {
      "family": {
        "example": "{family_id}",
        "type": "string",
        "description": "(Required) a numeric identifier of the family"
      },
      "maxLevel": {
        "example": "{max_number_of_levels}",
        "type": "string",
        "description": "(Required) maximum distance from the family"
      },
      "Ocp-Apim-Subscription-Key": {
        "example": "{{apiKey}}",
        "type": "string",
        "description": "{{apiKeyDescription}}"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.tisane.ai/hyponyms{?family,maxLevel}"
    },
    "headers": {
      "Ocp-Apim-Subscription-Key": {
        "$": "Ocp-Apim-Subscription-Key"
      }
    }
  },
  "responses": {}
}
