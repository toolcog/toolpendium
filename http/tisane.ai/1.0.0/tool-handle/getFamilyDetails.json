{
  "name": "getFamilyDetails",
  "description": "Fetches and outputs metadata for a family from the Tisane language models.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "{family_id}",
        "type": "string",
        "description": "(Required) a numeric identifier of the family"
      },
      "Ocp-Apim-Subscription-Key": {
        "example": "{{apiKey}}",
        "type": "string",
        "description": "{{apiKeyDescription}}"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.tisane.ai/lm/family{?id}"
    },
    "headers": {
      "Ocp-Apim-Subscription-Key": {
        "$": "Ocp-Apim-Subscription-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**definition**",
            "**description**",
            "**hypernyms**",
            "**permId**",
            "**requestId**",
            "**wikidata**",
            "**wordnet30**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
