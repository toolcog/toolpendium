{
  "name": "doctors_read",
  "description": "Retrieve an existing dcotor",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/doctors/{id}{?doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**cell_phone**: ",
            "**country**: Two-letter conutry code. Default is `US`",
            "**email**: ",
            "**first_name**: ",
            "**group_npi_number**: ",
            "**home_phone**: ",
            "**id**: ",
            "**is_account_suspended**: Indicates the doctor's account is suspended or not",
            "**job_title**: ",
            "**last_name**: ",
            "**npi_number**: If both this field and `group_npi_number` are set, prefer this field",
            "**office_phone**: ",
            "**practice_group**: The ID of the practice group this user belongs to. This can be used to identify users in the same practice.",
            "**practice_group_name**: ",
            "**profile_picture**: ",
            "**specialty**: ",
            "**suffix**: ",
            "**timezone**: ",
            "**website**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
