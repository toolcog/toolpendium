{
  "name": "patient_interventions_read",
  "description": "Retrieve an existing patient intervention for CQM",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "patient": {
        "description": "",
        "title": "patient",
        "type": "integer"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/patient_interventions/{id}{?patient,doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Code from different code system",
            "**code_system**: Can be `SNOMEDCT`, `HCPCS`, `CPT`, `ICD10CM`",
            "**created_at**: ",
            "**doctor**: ",
            "**effective_time**: ",
            "**id**: ",
            "**name**: Description of intervention",
            "**patient**: ",
            "**value_code**: Code from different code system represent intervention value",
            "**value_code_system**: Can be `SNOMEDCT`, `ICD10CM`, `LOINC`",
            "**value_name**: Description of value"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
