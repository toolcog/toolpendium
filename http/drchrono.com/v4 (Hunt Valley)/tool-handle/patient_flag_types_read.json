{
  "name": "patient_flag_types_read",
  "description": "Retrieve an existing patient flag type",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/patient_flag_types/{id}{?doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**archived**: Indicates the flag type is soft-deleted and should not be used",
            "**color**: ",
            "**created_at**: ",
            "**doctor**: Doctor who owns the flag type",
            "**id**: ",
            "**name**: Name of the patient flag type",
            "**priority**: Priority of the flag type",
            "**updated_at**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
