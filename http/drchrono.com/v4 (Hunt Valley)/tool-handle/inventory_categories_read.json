{
  "name": "inventory_categories_read",
  "description": "Retrieve an existing inventory category",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "since": {
        "description": "",
        "title": "since",
        "type": "string"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/inventory_categories/{id}{?since,doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**archived**: If the category is archived or not",
            "**category_type**: Can be one of `\"vaccine\"`, `\"product\"` or `\"service\"`",
            "**created_at**: ",
            "**doctor**: ",
            "**id**: ",
            "**name**: Name of the inventory category",
            "**updated_at**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
