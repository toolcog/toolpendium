{
  "name": "insurances_list",
  "parameters": {
    "type": "object",
    "properties": {
      "cursor": {
        "description": "The pagination cursor value.",
        "title": "Cursor",
        "type": "string"
      },
      "page_size": {
        "description": "Number of results to return per page.",
        "title": "Page size",
        "type": "integer"
      },
      "payer_type": {
        "description": "One of `\"emdeon\"`, `\"gateway\"`, `\"ihcfa\"`",
        "title": "",
        "type": "string"
      },
      "term": {
        "description": "Search term, which can be either a partial name, partial ID or the state.",
        "title": "",
        "type": "string"
      }
    },
    "required": [
      "payer_type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/insurances{?cursor,page_size,payer_type,term}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": ""
        },
        "Paginated Result",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: result data",
            "**next**: Next Paginated page",
            "**previous**: Previous paginated page"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
