{
  "name": "tasks_list",
  "description": "Retrieve or search tasks",
  "parameters": {
    "type": "object",
    "properties": {
      "cursor": {
        "description": "The pagination cursor value.",
        "title": "Cursor",
        "type": "string"
      },
      "page_size": {
        "description": "Number of results to return per page.",
        "title": "Page size",
        "type": "integer"
      },
      "status": {
        "description": "",
        "title": "status",
        "type": "integer"
      },
      "category": {
        "description": "",
        "title": "category",
        "type": "integer"
      },
      "due_at_date": {
        "description": "",
        "title": "due_at_date",
        "type": "string"
      },
      "due_at_unknown": {
        "description": "",
        "title": "due_at_unknown",
        "type": "string"
      },
      "since": {
        "description": "",
        "title": "since",
        "type": "string"
      },
      "due_at_since": {
        "description": "",
        "title": "due_at_since",
        "type": "string"
      },
      "assignee_user": {
        "description": "",
        "title": "assignee_user",
        "type": "integer"
      },
      "assignee_group": {
        "description": "",
        "title": "assignee_group",
        "type": "integer"
      },
      "due_at_range": {
        "description": "",
        "title": "due_at_range",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/tasks{?cursor,page_size,status,category,due_at_date,due_at_unknown,since,due_at_since,assignee_user,assignee_group,due_at_range}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": ""
        },
        "Paginated Result",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: result data",
            "**next**: Next Paginated page",
            "**previous**: Previous paginated page"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
