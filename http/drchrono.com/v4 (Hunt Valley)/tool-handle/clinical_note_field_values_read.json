{
  "name": "clinical_note_field_values_read",
  "description": "Retrieve an existing clinical note field value",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "clinical_note_field": {
        "description": "",
        "title": "clinical_note_field",
        "type": "integer"
      },
      "since": {
        "description": "",
        "title": "since",
        "type": "string"
      },
      "appointment": {
        "description": "",
        "title": "appointment",
        "type": "integer"
      },
      "clinical_note_template": {
        "description": "",
        "title": "clinical_note_template",
        "type": "integer"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/clinical_note_field_values/{id}{?clinical_note_field,since,appointment,clinical_note_template,doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Values entered by doctor when charting a particular appointment",
        "**Key properties:**",
        {
          "$ul": [
            "**appointment**: ID of appointment that the value applies to",
            "**clinical_note_field**: ID of `/api/clinical_note_field_types` object that indicates type of the value",
            "**created_at**: ",
            "**id**: ",
            "**updated_at**: ",
            "**value**: Value of the field. Interpretation varies by field type."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
