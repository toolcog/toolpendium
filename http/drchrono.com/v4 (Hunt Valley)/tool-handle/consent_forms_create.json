{
  "name": "consent_forms_create",
  "description": "Create a patient consent form",
  "parameters": {
    "type": "object",
    "properties": {
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://app.drchrono.com/api/consent_forms{?doctor}"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**archived**: ",
            "**assign_by_default**: If true, consent form will always be automatically assigned to appointments",
            "**created_at**: ",
            "**doctor**: ",
            "**id**: ",
            "**is_mandatory**: If true, consent form must be signed prior to appointment check in",
            "**order**: The order of consent forms that will show in management screen",
            "**title**: ",
            "**updated_at**: ",
            "**uri**: Files are passed using `multipart/form-data` encoding, but returned as URLs."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
