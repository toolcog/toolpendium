{
  "name": "patients_list",
  "description": "Retrieve or search patients",
  "parameters": {
    "type": "object",
    "properties": {
      "cursor": {
        "description": "The pagination cursor value.",
        "title": "Cursor",
        "type": "string"
      },
      "page_size": {
        "description": "Number of results to return per page.",
        "title": "Page size",
        "type": "integer"
      },
      "first_name": {
        "description": "",
        "title": "first_name",
        "type": "string"
      },
      "last_name": {
        "description": "",
        "title": "last_name",
        "type": "string"
      },
      "preferred_language": {
        "description": "",
        "title": "preferred_language",
        "type": "string"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      },
      "gender": {
        "description": "",
        "title": "gender",
        "type": "string"
      },
      "since": {
        "description": "",
        "title": "since",
        "type": "string"
      },
      "date_of_birth": {
        "description": "",
        "title": "date_of_birth",
        "type": "string"
      },
      "race": {
        "description": "",
        "title": "race",
        "type": "string"
      },
      "chart_id": {
        "description": "",
        "title": "chart_id",
        "type": "string"
      },
      "email": {
        "description": "",
        "title": "email",
        "type": "string"
      },
      "ethnicity": {
        "description": "",
        "title": "ethnicity",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/patients{?cursor,page_size,first_name,last_name,preferred_language,doctor,gender,since,date_of_birth,race,chart_id,email,ethnicity}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": ""
        },
        "Paginated Result",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: result data",
            "**next**: Next Paginated page",
            "**previous**: Previous paginated page"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
