{
  "name": "offices_read",
  "description": "Retrieve an existing office",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/offices/{id}{?doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**address**: ",
            "**archived**: Indicates that the object has been soft-deleted and should not be used",
            "**city**: ",
            "**country**: Two-letter country code",
            "**doctor**: ID of the doctor who owns the office",
            "**end_time**: Truncated to the hour. Default is `24:00`",
            "**exam_rooms**: ",
            "**fax_number**: ",
            "**id**: ",
            "**name**: ",
            "**online_scheduling**: Default is `false`",
            "**online_timeslots**: Array of timslots",
            "**phone_number**: ",
            "**start_time**: Truncated to the hour. Default is `00:00:00`",
            "**state**: Two-letter abbreviation",
            "**tax_id_number_professional**: Billing Tax ID #",
            "**zip_code**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
