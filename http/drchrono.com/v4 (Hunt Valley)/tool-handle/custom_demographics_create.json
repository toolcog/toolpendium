{
  "name": "custom_demographics_create",
  "description": "Create custom demographics fields",
  "parameters": {
    "type": "object",
    "properties": {
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://app.drchrono.com/api/custom_demographics{?doctor}"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**allowed_values**: Allowed values if needed. Values must be separated by comma `,`",
            "**archived**: Indicates that the object has been soft-deleted, and should not be used",
            "**description**: ",
            "**doctor**: ",
            "**id**: ",
            "**name**: ",
            "**template_name**: Custom Patient Demographics with `template_name` set can be inserted into clinical notes via Form Builder. Only letters (a-z or A-Z), numbers (0-9) or underscore(_) are allowed for template name."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
