{
  "name": "users_read",
  "description": "Retrieve an existing user, `/api/users/current` can be used to identify logged in user, it will redirect to `/api/users/{current_user_id}`",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/users/{id}{?doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**doctor**: For staff members, this is their primary physician's ID. For doctors, it is their own ID.",
            "**id**: ",
            "**is_doctor**: Mutually exclusive with `is_staff`",
            "**is_staff**: Mutually exclusive with `is_doctor`",
            "**permissions**: Permissions the user has.",
            "**practice_group**: The ID of the practice group this user belongs to. This can be used to identify users in the same practice.",
            "**username**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
