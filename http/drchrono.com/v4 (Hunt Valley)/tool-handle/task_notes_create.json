{
  "name": "task_notes_create",
  "description": "Create a task note",
  "parameters": {
    "type": "object",
    "properties": {
      "task": {
        "description": "",
        "title": "task",
        "type": "integer"
      },
      "since": {
        "description": "",
        "title": "since",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://app.drchrono.com/api/task_notes{?task,since}"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**archived**: If the task note is archived or not",
            "**created_at**: ",
            "**created_by**: ID of the `/api/users` who created the note",
            "**id**: ",
            "**task**: ID of task this note is related with",
            "**text**: Content of the task note",
            "**updated_at**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
