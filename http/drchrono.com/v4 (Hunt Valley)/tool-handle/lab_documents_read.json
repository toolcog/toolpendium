{
  "name": "lab_documents_read",
  "description": "Retrieve an existing lab order document",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "since": {
        "description": "",
        "title": "since",
        "type": "string"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/lab_documents/{id}{?since,doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**document**: ",
            "**id**: ",
            "**lab_order**: ID of the order this document is associated with",
            "**timestamp**: Time at which the document was uploaded",
            "**type**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
