{
  "name": "care_plans_read",
  "description": "Retrieve an existing care plan",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "",
        "title": "",
        "type": "string"
      },
      "patient": {
        "description": "",
        "title": "patient",
        "type": "integer"
      },
      "plan_type": {
        "description": "",
        "title": "plan_type",
        "type": "integer"
      },
      "doctor": {
        "description": "",
        "title": "doctor",
        "type": "integer"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.drchrono.com/api/care_plans/{id}{?patient,plan_type,doctor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**appointment**: ",
            "**code**: ",
            "**code_system**: ",
            "**created_at**: ",
            "**description**: ",
            "**id**: ",
            "**instructions**: ",
            "**patient**: ",
            "**plan_type**: Can be one of the following: `1`(Goal), `2`(Patient education), `3`(Patient decision aid), `4`(Patient clinical instruction), `5`(Referral to other doctor), `6`(Health concerns)",
            "**scheduled_date**: ",
            "**title**: ",
            "**updated_at**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
