{
  "name": "task_statuses_create",
  "description": "Create a task status",
  "parameters": {
    "type": "object",
    "properties": {
      "since": {
        "description": "",
        "title": "since",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://app.drchrono.com/api/task_statuses{?since}"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**archived**: ",
            "**created_at**: ",
            "**id**: ",
            "**name**: ",
            "**practice_group**: ",
            "**status_category**: Can be one of the following `O`(open), `P`(In progress), `H`(On hold), `C`(Complete), default to `O`(Open)",
            "**task_category**: ID of `/api/task_categories`",
            "**updated_at**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
