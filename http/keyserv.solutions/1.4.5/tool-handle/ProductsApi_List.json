{
  "name": "ProductsApi_List",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "body": {
        "nullable": true,
        "oneOf": [
          {
            "$ref": "#/$defs/ApiKey"
          }
        ]
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "ApiKey": {
        "additionalProperties": false,
        "properties": {
          "key": {
            "format": "guid",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://keyserv.solutions/v1/ProductsApi/List{?page}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**custom**",
            "**name**",
            "**serial**",
            "**subscriptions**",
            "**updated**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
