{
  "name": "get_automation_blockers_stats",
  "description": "**Permission required:** view_statistics",
  "parameters": {
    "type": "object",
    "properties": {
      "start_date": {
        "default": "2023-02-28T08:47:25.215200",
        "format": "date-time",
        "type": "string"
      },
      "end_date": {
        "default": "2023-03-30T08:47:25.215228",
        "format": "date-time",
        "type": "string"
      },
      "version_name": {
        "default": "predicted",
        "pattern": "^[A-z0-9_-]+$",
        "type": "string",
        "description": "provide the version name to evaluate"
      },
      "include_detail": {
        "default": false,
        "type": "boolean"
      },
      "inbox_id": {
        "type": "string"
      }
    },
    "required": [
      "inbox_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/stats/{inbox_id}/automation_blockers{?start_date,end_date,version_name,include_detail}"
    }
  },
  "responses": {}
}
