{
  "name": "post_document_process_table_annotation",
  "description": "A fast and convenient annotation tool that converts a table-level annotation to cell-level feedback.\n\n**Permission required:** review",
  "parameters": {
    "type": "object",
    "properties": {
      "document_id": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/TableAnnotationPost"
      }
    },
    "required": [
      "document_id",
      "body"
    ],
    "$defs": {
      "TableAnnotationPost": {
        "properties": {
          "bottom_right": {
            "items": {
              "type": "integer"
            },
            "maxItems": 3,
            "minItems": 3,
            "type": "array"
          },
          "columns": {
            "items": {
              "$ref": "#/$defs/column_bound"
            },
            "type": "array"
          },
          "first_row_y_bounds": {
            "items": {
              "type": "integer"
            },
            "maxItems": 2,
            "minItems": 2,
            "type": "array"
          },
          "header_bottom_y": {
            "type": "integer"
          },
          "name": {
            "type": "string"
          },
          "top_left": {
            "items": {
              "type": "integer"
            },
            "maxItems": 3,
            "minItems": 3,
            "type": "array"
          }
        },
        "type": "object"
      },
      "column_bound": {
        "properties": {
          "field_name": {
            "type": "string"
          },
          "x_bounds": {
            "items": {
              "type": "integer"
            },
            "maxItems": 2,
            "minItems": 2,
            "type": "array"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/documents/{document_id}/process_table_annotation"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
