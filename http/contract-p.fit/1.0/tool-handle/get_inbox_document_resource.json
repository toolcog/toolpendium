{
  "name": "get_inbox_document_resource",
  "description": "**Permission required:** view_list",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      },
      "inbox_id": {
        "type": "string"
      }
    },
    "required": [
      "inbox_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/inboxes/{inbox_id}/documents"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**escalate**",
              {
                "$ul": [
                  "**by**",
                  "**since**",
                  "**value**"
                ]
              }
            ],
            "**feedback**",
            "**files**",
            "**flag_for_review**",
            "**id**",
            "**inbox**",
            "**last_version**",
            [
              "**lock**",
              {
                "$ul": [
                  "**by**",
                  "**since**",
                  "**value**"
                ]
              }
            ],
            "**meta_information**",
            "**opened_by**",
            "**original_filename**",
            "**page_count**",
            "**prediction**",
            [
              "**reject**",
              {
                "$ul": [
                  "**by**",
                  "**since**",
                  "**value**"
                ]
              }
            ],
            [
              "**status_data**",
              {
                "$ul": [
                  "**archived**",
                  "**data**",
                  "**escalate**",
                  "**feedback**",
                  "**lock**",
                  "**ready_accepted**",
                  "**ready_attempts**",
                  "**reject**",
                  "**reject_accepted**",
                  "**reject_attempts**",
                  "**sampling**",
                  "**submit_accepted**",
                  "**submit_attempts**",
                  "**success**"
                ]
              }
            ],
            [
              "**submitted**",
              {
                "$ul": [
                  "**by**",
                  "**since**",
                  "**value**"
                ]
              }
            ],
            [
              "**timings**",
              {
                "$ul": [
                  "**done_time**",
                  "**feedback_time**",
                  "**processing_period**",
                  "**receive_time**",
                  "**start_time**",
                  "**submit_time**"
                ]
              }
            ],
            "**usage_data**",
            "**versions**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
