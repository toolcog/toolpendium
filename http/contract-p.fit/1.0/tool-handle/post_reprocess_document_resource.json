{
  "name": "post_reprocess_document_resource",
  "description": "This endpoint schedules (async) reprocessing for the given document;\nit will call the async processing with the given desired version name,\nsimilar to the async document upload but skipping OCR.\nIt works asynchronously, so results need to be consulted by a separate call (or webhook)\n\n\n**Permission required:** upload",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "default": "reprocessed",
        "type": "string"
      },
      "reviewer": {
        "default": "Machine",
        "enum": [
          "Human",
          "Machine"
        ],
        "type": "string",
        "description": "If human repcrocessing is triggered, the last version will become the passed version. No processing is done."
      },
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      },
      "document_id": {
        "type": "string"
      }
    },
    "required": [
      "document_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/documents/{document_id}/reprocess{?name,reviewer}"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**escalate**",
              {
                "$ul": [
                  "**by**",
                  "**since**",
                  "**value**"
                ]
              }
            ],
            "**feedback**",
            "**files**",
            "**flag_for_review**",
            "**id**",
            "**inbox**",
            "**last_version**",
            [
              "**lock**",
              {
                "$ul": [
                  "**by**",
                  "**since**",
                  "**value**"
                ]
              }
            ],
            "**meta_information**",
            "**opened_by**",
            "**original_filename**",
            "**page_count**",
            "**prediction**",
            [
              "**reject**",
              {
                "$ul": [
                  "**by**",
                  "**since**",
                  "**value**"
                ]
              }
            ],
            [
              "**status_data**",
              {
                "$ul": [
                  "**archived**",
                  "**data**",
                  "**escalate**",
                  "**feedback**",
                  "**lock**",
                  "**ready_accepted**",
                  "**ready_attempts**",
                  "**reject**",
                  "**reject_accepted**",
                  "**reject_attempts**",
                  "**sampling**",
                  "**submit_accepted**",
                  "**submit_attempts**",
                  "**success**"
                ]
              }
            ],
            [
              "**submitted**",
              {
                "$ul": [
                  "**by**",
                  "**since**",
                  "**value**"
                ]
              }
            ],
            [
              "**timings**",
              {
                "$ul": [
                  "**done_time**",
                  "**feedback_time**",
                  "**processing_period**",
                  "**receive_time**",
                  "**start_time**",
                  "**submit_time**"
                ]
              }
            ],
            "**usage_data**",
            "**versions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
