{
  "name": "get_api_keys_inbox_resource",
  "description": "**Permission required:** view_api_keys",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      },
      "inbox_id": {
        "type": "string"
      }
    },
    "required": [
      "inbox_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/auth/api-key/inbox/{inbox_id}"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**: Is the API Key active or not",
            "**expire_at**: The date at which the API Key will expire",
            "**roles**: the user roles",
            "**token**: Token value",
            "**user**: Bearer user"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
