{
  "name": "patch_document_copy_resource",
  "description": "Copy documents in the provided list to another inbox.\n\nIf the destination inbox is the same as the original inbox, the request will be aborted, no duplicates\nwill be created\n\n\n**Permission required:** review",
  "parameters": {
    "type": "object",
    "properties": {
      "inbox_id": {
        "type": "string",
        "description": "The uuid of the inbox where the document(s) should be copied to"
      },
      "body": {
        "$ref": "#/$defs/DocumentIds"
      }
    },
    "required": [
      "inbox_id",
      "body"
    ],
    "$defs": {
      "DocumentIds": {
        "properties": {
          "document_ids": {
            "items": {
              "example": "abcdef123456789abcdef123",
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/documents/{inbox_id}/copy_inbox"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
