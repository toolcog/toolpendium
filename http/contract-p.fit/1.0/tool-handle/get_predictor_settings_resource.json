{
  "name": "get_predictor_settings_resource",
  "description": "**Permission required:** edit_predictor_settings",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/predictor_settings"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**blacklist**",
            "**expected_values**",
            "**fallback**",
            [
              "**key_value_pairs**",
              {
                "$ul": [
                  "**classification_cutoff**",
                  "**rule_config**",
                  "**splitting_cutoff**"
                ]
              }
            ],
            [
              "**table_extraction_settings**",
              {
                "$ul": [
                  [
                    "**field_settings**",
                    {
                      "$ul": [
                        "*****"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**whitelist**",
            "**scope**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
