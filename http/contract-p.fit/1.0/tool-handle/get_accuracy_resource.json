{
  "name": "get_accuracy_resource",
  "description": "**Permission required:** view_statistics",
  "parameters": {
    "type": "object",
    "properties": {
      "start_date": {
        "default": "2023-02-28T08:47:25.213188",
        "format": "date-time",
        "type": "string"
      },
      "end_date": {
        "default": "2023-03-30T08:47:25.213219",
        "format": "date-time",
        "type": "string"
      },
      "stp_files_only": {
        "default": false,
        "type": "boolean"
      },
      "stp_fields_only": {
        "default": false,
        "type": "boolean"
      },
      "version_name": {
        "pattern": "^[A-z0-9_-]+$",
        "type": "string",
        "description": "provide the version name to evaluate"
      },
      "filter_by": {
        "pattern": "^[A-z0-9_-]+$",
        "type": "string",
        "description": "Choose to filter the results by doctype, process label, ..."
      },
      "scope": {
        "type": "string"
      }
    },
    "required": [
      "scope"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/stats/{scope}/accuracy{?start_date,end_date,stp_files_only,stp_fields_only,version_name,filter_by}"
    }
  },
  "responses": {}
}
