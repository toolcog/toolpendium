{
  "name": "patch_predictor_setting_resource",
  "description": "**Permission required:** edit_predictor_settings",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      },
      "scope": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/PredictorSettingModel"
      }
    },
    "required": [
      "scope",
      "body"
    ],
    "$defs": {
      "PredictorSettingModel": {
        "properties": {
          "blacklist": {
            "type": "object"
          },
          "expected_values": {
            "type": "object"
          },
          "fallback": {
            "type": "object"
          },
          "key_value_pairs": {
            "$ref": "#/$defs/KeyValyePairSettingsModel"
          },
          "table_extraction_settings": {
            "$ref": "#/$defs/TableExtractionSettingsModel"
          },
          "whitelist": {
            "type": "object"
          }
        },
        "type": "object"
      },
      "KeyValyePairSettingsModel": {
        "properties": {
          "classification_cutoff": {
            "type": "integer"
          },
          "rule_config": {
            "type": "object"
          },
          "splitting_cutoff": {
            "type": "integer"
          }
        },
        "type": "object"
      },
      "TableExtractionSettingsModel": {
        "properties": {
          "field_settings": {
            "$ref": "#/$defs/FieldSettingsDictModel"
          }
        },
        "type": "object"
      },
      "FieldSettingsDictModel": {
        "properties": {
          "*": {
            "additionalProperties": {
              "$ref": "#/$defs/FieldSettingsModel"
            },
            "type": "object"
          }
        },
        "type": "object"
      },
      "FieldSettingsModel": {
        "properties": {
          "default_value": {
            "type": "string"
          },
          "expected_pattern": {
            "type": "string"
          },
          "nested_field_patterns": {
            "$ref": "#/$defs/SpanDictModel"
          },
          "override_default_patterns": {
            "type": "boolean"
          }
        },
        "type": "object"
      },
      "SpanDictModel": {
        "properties": {
          "*": {
            "additionalProperties": {
              "type": "string"
            },
            "type": "object"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/predictor_settings/{scope}"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**blacklist**",
            "**expected_values**",
            "**fallback**",
            [
              "**key_value_pairs**",
              {
                "$ul": [
                  "**classification_cutoff**",
                  "**rule_config**",
                  "**splitting_cutoff**"
                ]
              }
            ],
            [
              "**table_extraction_settings**",
              {
                "$ul": [
                  [
                    "**field_settings**",
                    {
                      "$ul": [
                        "*****"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**whitelist**",
            "**scope**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
