{
  "name": "patch_integration_resource",
  "description": "Update the given integration attributes\n\n**Permission required:** edit_integrations",
  "parameters": {
    "type": "object",
    "properties": {
      "integration_id": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/IntegrationModelGet"
      }
    },
    "required": [
      "integration_id",
      "body"
    ],
    "$defs": {
      "IntegrationModelGet": {
        "properties": {
          "type": {
            "enum": [
              "webhook",
              "dropbox",
              "office365",
              "email",
              "imap",
              "sftp",
              "smtp",
              "service_bus"
            ],
            "example": "webhook",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/integrations/{integration_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
