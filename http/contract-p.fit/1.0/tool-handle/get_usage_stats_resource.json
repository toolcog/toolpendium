{
  "name": "get_usage_stats_resource",
  "parameters": {
    "type": "object",
    "properties": {
      "inbox_id": {
        "type": "string"
      },
      "start_date": {
        "default": "2023-02-28T08:47:25",
        "type": "string"
      },
      "end_date": {
        "default": "2023-03-30T08:47:25",
        "type": "string"
      },
      "excel_output": {
        "default": false,
        "type": "boolean",
        "description": "Set to 'true' to get data in an excel. (Default output format: JSON)"
      }
    },
    "required": [
      "start_date",
      "end_date"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/stats/usage{?inbox_id,start_date,end_date,excel_output}"
    }
  },
  "responses": {}
}
