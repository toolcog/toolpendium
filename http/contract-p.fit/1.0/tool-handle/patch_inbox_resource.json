{
  "name": "patch_inbox_resource",
  "description": "Update the given inbox with the provided arguments\n\n**Permission required:** view_list",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      },
      "inbox_id": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/InboxModelPatch"
      }
    },
    "required": [
      "inbox_id",
      "body"
    ],
    "$defs": {
      "InboxModelPatch": {
        "properties": {
          "name": {
            "type": "string"
          },
          "project": {
            "example": "abcdef123456789abcdef123",
            "maxLength": 24,
            "minLength": 24,
            "pattern": "[A-z0-9]",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/inboxes/{inbox_id}"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**name**",
            "**project**",
            "**id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
