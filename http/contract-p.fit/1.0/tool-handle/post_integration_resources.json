{
  "name": "post_integration_resources",
  "description": "Create a new integration. Each type of integration has its own initialization logic and thus may\nyield suitable error codes depending on the underlying issue, such as credentials or connectivity issue.\n\n**Permission required:** edit_integrations",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/IntegrationModelGet"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "IntegrationModelGet": {
        "properties": {
          "type": {
            "enum": [
              "webhook",
              "dropbox",
              "office365",
              "email",
              "imap",
              "sftp",
              "smtp",
              "service_bus"
            ],
            "example": "webhook",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/integrations/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
