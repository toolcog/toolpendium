      "body": {
        "$ref": "#/$defs/ReportModelPost"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "ReportModelPost": {
        "properties": {
          "annotations": {
            "$ref": "#/$defs/AnnotationOptions"
          },
          "automation_blockers": {
            "$ref": "#/$defs/SimpleInclude"
          },
          "automation_score": {
            "$ref": "#/$defs/SimpleInclude"
          },
          "automation_what_if": {
            "$ref": "#/$defs/SimpleInclude"
          },
          "cover": {
            "$ref": "#/$defs/SimpleInclude"
          },
          "documents": {
            "items": {
              "example": "abcdef123456789abcdef123",
              "maxLength": 24,
              "minLength": 24,
              "pattern": "[A-z0-9]",
              "type": "string"
            },
            "type": "array"
          },
          "elapse_time": {
            "$ref": "#/$defs/SimpleInclude"
          },
          "evaluations": {
            "$ref": "#/$defs/EvaluationsOptions"
          },
          "field_automation": {
            "$ref": "#/$defs/SimpleInclude"
