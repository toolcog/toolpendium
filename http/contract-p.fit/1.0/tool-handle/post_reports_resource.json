          "$ul": [
            [
              "**annotations**",
              {
                "$ul": [
                  "**add_text** (default: false)",
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**automation_blockers**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**automation_score**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**automation_what_if**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**cover**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**documents**",
            [
              "**elapse_time**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**evaluations**",
              {
                "$ul": [
                  "**additional_column**",
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**field_automation**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**inboxes**",
            [
              "**lines**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**metadata**",
              {
                "$ul": [
                  "**columns**",
                  [
                    "**fields**",
                    {
                      "$ul": [
                        "**annotations**",
                        "**lines**",
                        "**meta_information**"
                      ]
                    }
                  ],
                  "**include** (default: false)",
                  "**rename**"
                ]
              }
            ],
            "**name**",
            [
              "**page_classification**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**projects**",
            [
              "**sections**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**separator** (default: |)",
            "**sources** (default: human,machine)",
            [
              "**text**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**version_name**",
            "**id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
