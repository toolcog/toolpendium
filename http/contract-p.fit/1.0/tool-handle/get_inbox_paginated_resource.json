{
  "name": "get_inbox_paginated_resource",
  "description": "**Permission required:** view_list",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number to fetch"
      },
      "per_page": {
        "default": 20,
        "type": "integer",
        "description": "Results per page"
      },
      "start_receive_date": {
        "format": "date-time",
        "type": "string"
      },
      "end_receive_date": {
        "format": "date-time",
        "type": "string"
      },
      "order_by": {
        "type": "string",
        "description": "comma seperated list of fields to order by"
      },
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      },
      "inbox_id": {
        "type": "string"
      }
    },
    "required": [
      "inbox_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/inboxes/{inbox_id}/paginated{?page,per_page,start_receive_date,end_receive_date,order_by}"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**page**: Current page number",
            "**per_page**: Number of results per page",
            "**results**",
            "**total**: Total number of results"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
