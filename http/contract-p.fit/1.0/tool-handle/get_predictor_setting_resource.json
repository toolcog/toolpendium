                ]
              }
            ],
            [
              "**table_extraction_settings**",
              {
                "$ul": [
                  [
                    "**field_settings**",
                    {
                      "$ul": [
                        "*****"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**whitelist**",
            "**scope**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
