{
  "name": "post_generate_report_id_resource",
  "description": "**Note**: start_date and end_date filter on status update\n\n**Permission required:** read_reports",
  "parameters": {
    "type": "object",
    "properties": {
      "delivery_method": {
        "enum": [
          "download",
          "email"
        ],
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "start_date": {
        "format": "date-time",
        "type": "string"
      },
      "end_date": {
        "format": "date-time",
        "type": "string"
      },
      "report_id": {
        "type": "string"
      }
    },
    "required": [
      "delivery_method",
      "report_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/reports/{report_id}/generate{?delivery_method,email,start_date,end_date}"
    }
  },
  "responses": {}
}
