{
  "name": "get_reports_resource",
  "description": "Return the list of reports\n\n**Permission required:** read_reports",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/reports"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**annotations**",
              {
                "$ul": [
                  "**add_text** (default: false)",
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**automation_blockers**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**automation_score**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**automation_what_if**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**cover**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**documents**",
            [
              "**elapse_time**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**evaluations**",
              {
                "$ul": [
                  "**additional_column**",
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**field_automation**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**inboxes**",
            [
              "**lines**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            [
              "**metadata**",
              {
                "$ul": [
                  "**columns**",
                  [
                    "**fields**",
                    {
                      "$ul": [
                        "**annotations**",
                        "**lines**",
                        "**meta_information**"
                      ]
                    }
                  ],
                  "**include** (default: false)",
                  "**rename**"
                ]
              }
            ],
            "**name**",
            [
              "**page_classification**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**projects**",
            [
              "**sections**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**separator** (default: |)",
            "**sources** (default: human,machine)",
            [
              "**text**",
              {
                "$ul": [
                  "**include** (default: false)"
                ]
              }
            ],
            "**version_name**",
            "**id**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
