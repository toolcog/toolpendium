{
  "name": "get_evaluated_versions",
  "description": "If filter_on_version_date is True, the document\nversions are filtered by timestamp, otherwise, documents are filtered by receive_time.\n\n**Permission required:** view_statistics",
  "parameters": {
    "type": "object",
    "properties": {
      "start_date": {
        "default": "2023-02-28T08:47:25.214049",
        "format": "date-time",
        "type": "string"
      },
      "end_date": {
        "default": "2023-03-30T08:47:25.214078",
        "format": "date-time",
        "type": "string"
      },
      "filter_versions": {
        "default": false,
        "type": "boolean"
      },
      "last_one_only": {
        "default": true,
        "type": "boolean"
      },
      "version_name": {
        "pattern": "^[A-z0-9_-]+$",
        "type": "string",
        "description": "filter the version by name. If not specified, all version names are considered"
      },
      "inbox_id": {
        "type": "string"
      }
    },
    "required": [
      "inbox_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/stats/{inbox_id}/evaluated_versions{?start_date,end_date,filter_versions,last_one_only,version_name}"
    }
  },
  "responses": {}
}
