{
  "name": "post_oauth_login_resource",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/SsoModel"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "SsoModel": {
        "properties": {
          "token": {
            "description": "the access token (JWT for oauth or SAML response)",
            "type": "string"
          },
          "type": {
            "description": "the type of SSO token",
            "enum": [
              "oauth",
              "saml"
            ],
            "example": "oauth",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/sso/login"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
