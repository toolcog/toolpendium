{
  "name": "patch_api_key_resource",
  "description": "**roles** can't be modified after creation: you need to create a new token if you want other roles.\n\n**Permission required:** edit_backend_settings",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Fields": {
        "format": "mask",
        "type": "string",
        "description": "An optional fields mask"
      },
      "key": {
        "type": "string"
      }
    },
    "required": [
      "key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//cfportal.contract-p.fit/api/auth/api-key/{key}"
    },
    "headers": {
      "X-Fields": {
        "$": "X-Fields"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**: Is the API Key active or not",
            "**expire_at**: The date at which the API Key will expire",
            "**roles**: the user roles",
            "**token**: Token value",
            "**user**: Bearer user"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
