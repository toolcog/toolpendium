{
  "name": "searchConnections",
  "description": "Search connections",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/ConnectionSearch"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "ConnectionSearch": {
        "properties": {
          "connectionId": {
            "$ref": "#/$defs/ConnectionId"
          },
          "destination": {
            "$ref": "#/$defs/DestinationSearch"
          },
          "destinationId": {
            "$ref": "#/$defs/DestinationId"
          },
          "name": {
            "type": "string"
          },
          "namespaceDefinition": {
            "$ref": "#/$defs/NamespaceDefinitionType"
          },
          "namespaceFormat": {
            "default": null,
            "description": "Used when namespaceDefinition is 'customformat'. If blank then behaves like namespaceDefinition = 'destination'. If \"${SOURCE_NAMESPACE}\" then behaves like namespaceDefinition = 'source'.",
            "example": "${SOURCE_NAMESPACE}",
            "type": "string"
          },
          "prefix": {
            "description": "Prefix that will be prepended to the name of each stream when it is written to the destination.",
            "type": "string"
          },
          "schedule": {
            "$ref": "#/$defs/ConnectionSchedule"
          },
          "scheduleData": {
            "$ref": "#/$defs/ConnectionScheduleData"
          },
          "scheduleType": {
            "$ref": "#/$defs/ConnectionScheduleType"
          },
          "source": {
            "$ref": "#/$defs/SourceSearch"
          },
          "sourceId": {
            "$ref": "#/$defs/SourceId"
          },
          "status": {
            "$ref": "#/$defs/ConnectionStatus"
          }
        },
        "type": "object"
      },
      "ConnectionId": {
        "format": "uuid",
        "type": "string"
      },
      "DestinationSearch": {
        "properties": {
          "connectionConfiguration": {
            "$ref": "#/$defs/DestinationConfiguration"
          },
          "destinationDefinitionId": {
            "$ref": "#/$defs/DestinationDefinitionId"
          },
          "destinationId": {
            "$ref": "#/$defs/DestinationId"
          },
          "destinationName": {
            "type": "string"
          },
          "name": {
            "type": "string"
          },
          "workspaceId": {
            "$ref": "#/$defs/WorkspaceId"
          }
        },
        "type": "object"
      },
      "DestinationConfiguration": {
        "description": "The values required to configure the destination. The schema for this must match the schema return by destination_definition_specifications/get for the destinationDefinition.",
        "example": {
          "user": "charles"
        }
      },
      "DestinationDefinitionId": {
        "format": "uuid",
        "type": "string"
      },
      "DestinationId": {
        "format": "uuid",
        "type": "string"
      },
      "WorkspaceId": {
        "format": "uuid",
        "type": "string"
      },
      "NamespaceDefinitionType": {
        "description": "Method used for computing final namespace in destination",
        "enum": [
          "source",
          "destination",
          "customformat"
        ],
        "type": "string"
      },
      "ConnectionSchedule": {
        "description": "if null, then no schedule is set.",
        "properties": {
          "timeUnit": {
            "enum": [
              "minutes",
              "hours",
              "days",
              "weeks",
              "months"
            ],
            "type": "string"
          },
          "units": {
            "format": "int64",
            "type": "integer"
          }
        },
        "required": [
          "units",
          "timeUnit"
        ],
        "type": "object"
      },
      "ConnectionScheduleData": {
        "description": "schedule for when the the connection should run, per the schedule type",
        "properties": {
          "basicSchedule": {
            "properties": {
              "timeUnit": {
                "enum": [
                  "minutes",
                  "hours",
                  "days",
                  "weeks",
                  "months"
                ],
                "type": "string"
              },
              "units": {
                "format": "int64",
                "type": "integer"
              }
            },
            "required": [
              "timeUnit",
              "units"
            ],
            "type": "object"
          },
          "cron": {
            "properties": {
              "cronExpression": {
                "type": "string"
              },
              "cronTimeZone": {
                "type": "string"
              }
            },
            "required": [
              "cronExpression",
              "cronTimeZone"
            ],
            "type": "object"
          }
        },
        "type": "object"
      },
      "ConnectionScheduleType": {
        "description": "determine how the schedule data should be interpreted",
        "enum": [
          "manual",
          "basic",
          "cron"
        ],
        "type": "string"
      },
      "SourceSearch": {
        "properties": {
          "connectionConfiguration": {
            "$ref": "#/$defs/SourceConfiguration"
          },
          "name": {
            "type": "string"
          },
          "sourceDefinitionId": {
            "$ref": "#/$defs/SourceDefinitionId"
          },
          "sourceId": {
            "$ref": "#/$defs/SourceId"
          },
          "sourceName": {
            "type": "string"
          },
          "workspaceId": {
            "$ref": "#/$defs/WorkspaceId"
          }
        },
        "type": "object"
      },
      "SourceConfiguration": {
        "description": "The values required to configure the source. The schema for this must match the schema return by source_definition_specifications/get for the source.",
        "example": {
          "user": "charles"
        }
      },
      "SourceDefinitionId": {
        "format": "uuid",
        "type": "string"
      },
      "SourceId": {
        "format": "uuid",
        "type": "string"
      },
      "ConnectionStatus": {
        "description": "Active means that data is flowing through the connection. Inactive means it is not. Deprecated means the connection is off and cannot be re-activated. the schema field describes the elements of the schema that will be synced.",
        "enum": [
          "active",
          "inactive",
          "deprecated"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://airbyte.local/v1/connections/search"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**connections**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**exceptionClassName**",
            "**exceptionStack**",
            "**message**",
            "**validationErrors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
