{
  "name": "setInstancewideDestinationOauthParams",
  "description": "Sets instancewide variables to be used for the oauth flow when creating this destination. When set, these variables will be injected into a connector's configuration before any interaction with the connector image itself. This enables running oauth flows with consistent variables e.g: the company's Google Ads developer_token, client_id, and client_secret without the user having to know about these variables.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/SetInstancewideDestinationOauthParamsRequestBody"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "SetInstancewideDestinationOauthParamsRequestBody": {
        "properties": {
          "destinationDefinitionId": {
            "$ref": "#/$defs/DestinationDefinitionId"
          },
          "params": {
            "additionalProperties": true,
            "type": "object"
          }
        },
        "required": [
          "destinationDefinitionId",
          "params"
        ],
        "type": "object"
      },
      "DestinationDefinitionId": {
        "format": "uuid",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://airbyte.local/v1/destination_oauths/oauth_params/create"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
