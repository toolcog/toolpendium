{
  "name": "webBackendCheckUpdates",
  "description": "Returns a summary of source and destination definitions that could be updated.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://airbyte.local/v1/web_backend/check_updates"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Summary of source and destination definitions that could be updated",
        "**Key properties:**",
        {
          "$ul": [
            "**destinationDefinitions**",
            "**sourceDefinitions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
