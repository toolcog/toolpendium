{
  "name": "listPrivateSourceDefinitions",
  "description": "List all private, non-custom sourceDefinitions, and for each indicate whether the given workspace has a grant for using the definition. Used by admins to view and modify a given workspace's grants.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/WorkspaceIdRequestBody"
      }
    },
    "$defs": {
      "WorkspaceIdRequestBody": {
        "properties": {
          "workspaceId": {
            "$ref": "#/$defs/WorkspaceId"
          }
        },
        "required": [
          "workspaceId"
        ],
        "type": "object"
      },
      "WorkspaceId": {
        "format": "uuid",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://airbyte.local/v1/source_definitions/list_private"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**sourceDefinitions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
