{
  "name": "searchSources",
  "description": "Search sources",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/SourceSearch"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "SourceSearch": {
        "properties": {
          "connectionConfiguration": {
            "$ref": "#/$defs/SourceConfiguration"
          },
          "name": {
            "type": "string"
          },
          "sourceDefinitionId": {
            "$ref": "#/$defs/SourceDefinitionId"
          },
          "sourceId": {
            "$ref": "#/$defs/SourceId"
          },
          "sourceName": {
            "type": "string"
          },
          "workspaceId": {
            "$ref": "#/$defs/WorkspaceId"
          }
        },
        "type": "object"
      },
      "SourceConfiguration": {
        "description": "The values required to configure the source. The schema for this must match the schema return by source_definition_specifications/get for the source.",
        "example": {
          "user": "charles"
        }
      },
      "SourceDefinitionId": {
        "format": "uuid",
        "type": "string"
      },
      "SourceId": {
        "format": "uuid",
        "type": "string"
      },
      "WorkspaceId": {
        "format": "uuid",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://airbyte.local/v1/sources/search"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**sources**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**exceptionClassName**",
            "**exceptionStack**",
            "**message**",
            "**validationErrors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
