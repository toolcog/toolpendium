{
  "name": "campaigns_update",
  "description": "Updates a campaign",
  "parameters": {
    "type": "object",
    "properties": {
      "accountId": {
        "type": "string",
        "description": "Account to apply operations to"
      },
      "campaignId": {
        "type": "string",
        "description": "ID of campaign"
      }
    },
    "required": [
      "accountId",
      "campaignId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.sakari.io/v1/accounts/{accountId}/campaigns/{campaignId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**id**"
                ]
              }
            ],
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
