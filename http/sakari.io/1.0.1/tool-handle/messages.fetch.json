{
  "name": "messages_fetch",
  "description": "Returns a single messag",
  "parameters": {
    "type": "object",
    "properties": {
      "accountId": {
        "type": "string",
        "description": "Account to apply operations to"
      },
      "messageId": {
        "type": "string",
        "description": "ID of message to return"
      }
    },
    "required": [
      "accountId",
      "messageId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.sakari.io/v1/accounts/{accountId}/messages/{messageId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  [
                    "**contact**",
                    {
                      "$ul": [
                        "**email**",
                        "**firstName**",
                        "**id**",
                        "**lastName**",
                        [
                          "**mobile**",
                          {
                            "$ul": [
                              "**country**",
                              "**number**"
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  [
                    "**conversation**",
                    {
                      "$ul": [
                        "**id**"
                      ]
                    }
                  ],
                  [
                    "**created**",
                    {
                      "$ul": [
                        "**at**",
                        [
                          "**by**",
                          {
                            "$ul": [
                              "**firstName**",
                              "**id**",
                              "**lastName**"
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  [
                    "**error**",
                    {
                      "$ul": [
                        "**code**",
                        "**description**"
                      ]
                    }
                  ],
                  "**id**",
                  "**media**: List of media objects attached to message",
                  "**message**",
                  "**outgoing**",
                  "**phoneNumber**",
                  "**price**",
                  "**read**",
                  "**segments**",
                  "**status**",
                  "**template**",
                  [
                    "**updated**",
                    {
                      "$ul": [
                        "**at**",
                        [
                          "**by**",
                          {
                            "$ul": [
                              "**firstName**",
                              "**id**",
                              "**lastName**"
                            ]
                          }
                        ]
                      ]
                    }
                  ]
                ]
              }
            ],
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
