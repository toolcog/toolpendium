{
  "name": "createPodcastEpisode",
  "description": "Create podcast episode",
  "parameters": {
    "type": "object",
    "properties": {
      "podcastKey": {
        "type": "string"
      },
      "body": {
        "properties": {
          "episode": {
            "$ref": "#/$defs/PodcastEpisode"
          },
          "file_logo": {
            "format": "binary",
            "type": "string"
          },
          "file_media": {
            "format": "binary",
            "type": "string"
          }
        },
        "required": [
          "episode",
          "file_logo",
          "file_media"
        ],
        "type": "object"
      }
    },
    "required": [
      "podcastKey"
    ],
    "$defs": {
      "PodcastEpisode": {
        "description": "PodcastEpisode model",
        "properties": {
          "author": {
            "type": "string"
          },
          "block": {
            "type": "boolean"
          },
          "description": {
            "type": "string"
          },
          "duration": {
            "format": "int64",
            "type": "integer"
          },
          "episode": {
            "format": "int64",
            "type": "integer"
          },
          "episodeType": {
            "type": "string"
          },
          "explicit": {
            "type": "boolean"
          },
          "fileUrl": {
            "type": "string"
          },
          "image": {
            "type": "string"
          },
          "key": {
            "type": "string"
          },
          "link": {
            "type": "string"
          },
          "publishDate": {
            "format": "date-time",
            "type": "string"
          },
          "season": {
            "format": "int64",
            "type": "integer"
          },
          "size": {
            "format": "int64",
            "type": "integer"
          },
          "subtitle": {
            "type": "string"
          },
          "summary": {
            "type": "string"
          },
          "tags": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "uniqueItems": true
          },
          "title": {
            "type": "string"
          }
        },
        "required": [
          "description",
          "publishDate",
          "summary",
          "title"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.zeno.fm/api/v2/podcasts/{podcastKey}/episodes/create"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "PodcastEpisode model",
        "**Key properties:**",
        {
          "$ul": [
            "**author**",
            "**block**",
            "**description**",
            "**duration**",
            "**episode**",
            "**episodeType**",
            "**explicit**",
            "**fileUrl**",
            "**image**",
            "**key**",
            "**link**",
            "**publishDate**",
            "**season**",
            "**size**",
            "**subtitle**",
            "**summary**",
            "**tags**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
