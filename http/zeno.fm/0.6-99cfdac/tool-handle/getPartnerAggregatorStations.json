{
  "name": "getPartnerAggregatorStations",
  "description": "List stations",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": "1",
        "minimum": 1,
        "type": "string"
      },
      "hitsPerPage": {
        "default": "10",
        "maximum": 1000,
        "minimum": 1,
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.zeno.fm/api/v2/stations/list{?page,hitsPerPage}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of Station",
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
