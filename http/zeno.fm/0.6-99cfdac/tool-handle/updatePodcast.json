{
  "name": "updatePodcast",
  "description": "Update podcast",
  "parameters": {
    "type": "object",
    "properties": {
      "podcastKey": {
        "type": "string"
      },
      "body": {
        "properties": {
          "file_logo": {
            "format": "binary",
            "type": "string"
          },
          "podcast": {
            "$ref": "#/$defs/Podcast"
          }
        },
        "required": [
          "podcast"
        ],
        "type": "object"
      }
    },
    "required": [
      "podcastKey"
    ],
    "$defs": {
      "Podcast": {
        "description": "Podcast model",
        "properties": {
          "author": {
            "type": "string"
          },
          "block": {
            "type": "boolean"
          },
          "categories": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "uniqueItems": true
          },
          "copyright": {
            "type": "string"
          },
          "country": {
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "explicit": {
            "type": "boolean"
          },
          "image": {
            "type": "string"
          },
          "key": {
            "type": "string"
          },
          "keywords": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "uniqueItems": true
          },
          "language": {
            "type": "string"
          },
          "link": {
            "type": "string"
          },
          "ownerEmail": {
            "type": "string"
          },
          "ownerName": {
            "type": "string"
          },
          "showType": {
            "type": "string"
          },
          "subtitle": {
            "type": "string"
          },
          "summary": {
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "required": [
          "categories",
          "description",
          "language",
          "summary",
          "title"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.zeno.fm/api/v2/podcasts/{podcastKey}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Podcast model",
        "**Key properties:**",
        {
          "$ul": [
            "**author**",
            "**block**",
            "**categories**",
            "**copyright**",
            "**country**",
            "**description**",
            "**explicit**",
            "**image**",
            "**key**",
            "**keywords**",
            "**language**",
            "**link**",
            "**ownerEmail**",
            "**ownerName**",
            "**showType**",
            "**subtitle**",
            "**summary**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
