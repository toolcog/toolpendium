{
  "name": "getPodcastEpisode",
  "description": "Get podcast episode",
  "parameters": {
    "type": "object",
    "properties": {
      "podcastKey": {
        "type": "string"
      },
      "episodeKey": {
        "type": "string"
      }
    },
    "required": [
      "podcastKey",
      "episodeKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.zeno.fm/api/v2/podcasts/{podcastKey}/episodes/{episodeKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "PodcastEpisode model",
        "**Key properties:**",
        {
          "$ul": [
            "**author**",
            "**block**",
            "**description**",
            "**duration**",
            "**episode**",
            "**episodeType**",
            "**explicit**",
            "**fileUrl**",
            "**image**",
            "**key**",
            "**link**",
            "**publishDate**",
            "**season**",
            "**size**",
            "**subtitle**",
            "**summary**",
            "**tags**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
