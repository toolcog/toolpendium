{
  "name": "getPodcastEpisodes",
  "description": "Get podcast episodes",
  "parameters": {
    "type": "object",
    "properties": {
      "podcastKey": {
        "type": "string"
      },
      "limit": {
        "default": "10",
        "maximum": 1000,
        "minimum": 1,
        "type": "string"
      },
      "offset": {
        "default": "0",
        "minimum": 0,
        "type": "string"
      }
    },
    "required": [
      "podcastKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.zeno.fm/api/v2/podcasts/{podcastKey}/episodes{?limit,offset}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of PodcastEpisode",
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
