{
  "name": "getPodcast",
  "description": "Get podcast",
  "parameters": {
    "type": "object",
    "properties": {
      "podcastKey": {
        "type": "string"
      }
    },
    "required": [
      "podcastKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.zeno.fm/api/v2/podcasts/{podcastKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Podcast model",
        "**Key properties:**",
        {
          "$ul": [
            "**author**",
            "**block**",
            "**categories**",
            "**copyright**",
            "**country**",
            "**description**",
            "**explicit**",
            "**image**",
            "**key**",
            "**keywords**",
            "**language**",
            "**link**",
            "**ownerEmail**",
            "**ownerName**",
            "**showType**",
            "**subtitle**",
            "**summary**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
