{
  "name": "methods",
  "description": "Get all generic methods metadata",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/methods"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            [
              "**data**",
              {
                "$ul": [
                  [
                    "**methods**: List of available generic methods",
                    {
                      "$ul": [
                        "**agents**: List of agents compatible with this method",
                        "**category**: Category of this technique",
                        [
                          "**condition**: Conditions generated by this method",
                          {
                            "$ul": [
                              "**parameter**: Id of the parameter used to generate condition",
                              "**prefix**: Prefix used to generate condition"
                            ]
                          }
                        ],
                        [
                          "**deprecated**: Is this method deprecated?",
                          {
                            "$ul": [
                              "**info**: Information notice about the deprecation, especially how to replace it",
                              "**replacedBy**: Id of the method replacing this method"
                            ]
                          }
                        ],
                        "**desc**: Description of this method",
                        "**documentation**: Full documentation of this method",
                        "**id**: Method id",
                        "**name**: Method name",
                        "**parameters**: Parameters for this technique",
                        "**version**: Version of this technique"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
