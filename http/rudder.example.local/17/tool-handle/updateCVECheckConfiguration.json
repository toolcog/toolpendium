{
  "name": "updateCVECheckConfiguration",
  "description": "Update cve check config",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "description": "CVE check config",
        "properties": {
          "apiKey": {
            "description": "Token used by to contact the API to check CVE",
            "type": "string"
          },
          "url": {
            "description": "Url used to check CVE",
            "example": "https://api.rudder.io/cve/v1/",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/cve/check/config"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            [
              "**data**",
              {
                "$ul": [
                  "**apiKey**: Token used by to contact the API to check CVE",
                  "**url**: Url used to check CVE"
                ]
              }
            ],
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
