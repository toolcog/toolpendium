{
  "name": "acceptChangeRequest",
  "description": "Accept a change request",
  "parameters": {
    "type": "object",
    "properties": {
      "changeRequestId": {
        "description": "Change request id",
        "example": 37,
        "type": "integer"
      },
      "body": {
        "properties": {
          "status": {
            "description": "New status of the change request",
            "enum": [
              "pending deployment",
              "deployed"
            ],
            "example": "deployed",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "changeRequestId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/changeRequests/{changeRequestId}/accept"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            [
              "**data**",
              {
                "$ul": [
                  "**rules**"
                ]
              }
            ],
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
