{
  "name": "getDirectiveComplianceId",
  "description": "Get current compliance of a directive of a Rudder server",
  "parameters": {
    "type": "object",
    "properties": {
      "format": {
        "type": "string",
        "description": "format of export"
      },
      "directiveId": {
        "example": "9a1773c9-0889-40b6-be89-f6504443ac1b",
        "format": "uuid",
        "type": "string",
        "description": "Id of the directive"
      }
    },
    "required": [
      "directiveId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/compliance/directives/{directiveId}{?format}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
