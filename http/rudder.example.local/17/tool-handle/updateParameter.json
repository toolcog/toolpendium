{
  "name": "updateParameter",
  "description": "Update the properties of a parameter",
  "parameters": {
    "type": "object",
    "properties": {
      "parameterId": {
        "example": "rudder_file_edit_header",
        "type": "string",
        "description": "Id of the parameter to manage"
      }
    },
    "required": [
      "parameterId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/parameters/{parameterId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            [
              "**data**: Parameters",
              {
                "$ul": [
                  "**parameters**"
                ]
              }
            ],
            "**id**: Id of the parameter",
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
