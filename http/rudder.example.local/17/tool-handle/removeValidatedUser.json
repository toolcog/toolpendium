{
  "name": "removeValidatedUser",
  "description": "The user is again subject to workflow validation",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "example": "JaneDoe",
        "type": "string",
        "description": "Username of an user (unique)"
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/validatedUsers/{username}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            "**data**: the user removed from validated list",
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
