{
  "name": "getLastCVECheck",
  "description": "Get last CVE check result",
  "parameters": {
    "type": "object",
    "properties": {
      "groupId": {
        "type": "string",
        "description": "Id of node groups you want to get from last CVE check"
      },
      "nodeId": {
        "type": "string",
        "description": "Id of nodes you want to get from last CVE check"
      },
      "cveId": {
        "type": "string",
        "description": "Id of CVE you want to get from last CVE check"
      },
      "package": {
        "type": "string",
        "description": "Name of packages you want to get from last CVE check"
      },
      "severity": {
        "enum": [
          "critical",
          "high",
          "medium",
          "low",
          "none",
          "unknown"
        ],
        "type": "string",
        "description": "Severity of the CVE you want to get from last CVE check"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/cve/check/last{?groupId,nodeId,cveId,package,severity}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            [
              "**data**",
              {
                "$ul": [
                  "**CVEChecks**"
                ]
              }
            ],
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
