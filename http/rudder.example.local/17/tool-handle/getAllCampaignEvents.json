{
  "name": "getAllCampaignEvents",
  "description": "Get all campaign events",
  "parameters": {
    "type": "object",
    "properties": {
      "campaignType": {
        "enum": [
          "system-update",
          "software-update"
        ],
        "example": "system-update",
        "type": "string",
        "description": "Type of the campaigns we want"
      },
      "state": {
        "enum": [
          "scheduled",
          "running",
          "finished",
          "skipped"
        ],
        "example": "enabled",
        "type": "string",
        "description": "Status of the campaign events we want"
      },
      "campaignId": {
        "enum": [
          "system-update",
          "software-update"
        ],
        "example": "system-update",
        "type": "string",
        "description": "id of the campaigns we want"
      },
      "limit": {
        "type": "integer",
        "description": "Max number of elements in response"
      },
      "offset": {
        "type": "integer",
        "description": "Offset of data in response (skip X elements)"
      },
      "before": {
        "format": "date",
        "type": "string"
      },
      "after": {
        "format": "date",
        "type": "string"
      },
      "order": {
        "type": "string"
      },
      "asc": {
        "enum": [
          "asc",
          "desc"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/campaigns/events{?campaignType,state,campaignId,limit,offset,before,after,order,asc}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            [
              "**data**",
              {
                "$ul": [
                  "**campaignEvents**"
                ]
              }
            ],
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
