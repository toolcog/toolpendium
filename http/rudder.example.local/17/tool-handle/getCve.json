{
  "name": "getCve",
  "description": "Get a CVE details",
  "parameters": {
    "type": "object",
    "properties": {
      "cveId": {
        "example": "CVE-2022-25235",
        "format": "uuid",
        "type": "string",
        "description": "Id of the CVE"
      }
    },
    "required": [
      "cveId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/cve/{cveId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            [
              "**data**",
              {
                "$ul": [
                  "**CVEs**"
                ]
              }
            ],
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
