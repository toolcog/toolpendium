{
  "name": "apiSubInformations",
  "description": "Get all endpoints in the given section with their supported version.",
  "parameters": {
    "type": "object",
    "properties": {
      "sectionId": {
        "example": "nodes",
        "type": "string",
        "description": "Id of the API section"
      }
    },
    "required": [
      "sectionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rudder.example.local/rudder/api/latest/info/{sectionId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**: The id of the action",
            [
              "**data**",
              {
                "$ul": [
                  "**availableVersions**",
                  "**documentation**: Link to Rudder API description",
                  "**endpoints**"
                ]
              }
            ],
            "**result**: Result of the request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
