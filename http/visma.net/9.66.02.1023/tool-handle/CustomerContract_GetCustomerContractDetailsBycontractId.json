            "**currentSetup**",
            "**effectiveFrom**",
            "**lines**",
            "**pendingRecurring**",
            "**pendingRenewal**",
            "**pendingSetup**",
            [
              "**promoCode**",
              {
                "$ul": [
                  "**description**: Click on the magnifier. &gt; The description.",
                  "**id**: Click on the magnifier. &gt; The identifier."
                ]
              }
            ],
            "**totalPending**"
          ]
        },
        {
          "$lang": "json",
