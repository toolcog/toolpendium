{
  "name": "GetDomain",
  "description": "Details of a domain",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "The domain name",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "domain_name",
      "domainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/domains/{domainName}{?domain_name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**can_toggle_renew**: Indication if the domain renew state can be changed.",
            "**domain_name**: The domain name",
            "**expiration_date**: Expiration date of the domain",
            "**name_servers**: Nameservers of the domain",
            [
              "**registrant**",
              {
                "$ul": [
                  "**address**: Address of the registrant.",
                  "**city**: City of the registrant.",
                  "**company_name**: Company name of the registrant.<br />\r",
                  "**country_code**: Country code of the registrant.\r",
                  "**email**: Email of the registrant.",
                  "**enterprise_number**: Enterprise number of the registrant.<br />\r",
                  "**fax**: Fax of the registrant.\r",
                  "**first_name**: First name of the registrant.",
                  "**language_code**: Language code of the registrant.\r",
                  "**last_name**: Last name of the registrant.",
                  "**phone**: Phone of the registrant.<br />\r",
                  "**postal_code**: Postal code of the registrant."
                ]
              }
            ],
            "**will_renew**: Indication of renewal.<br />\r"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
