{
  "name": "DeleteMailbox",
  "description": "Delete a mailbox",
  "parameters": {
    "type": "object",
    "properties": {
      "mailbox_name": {
        "description": "Mailbox name.",
        "type": "string"
      },
      "mailboxName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "mailbox_name",
      "mailboxName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/v2/mailboxes/{mailboxName}{?mailbox_name}"
    }
  },
  "responses": {}
}
