{
  "name": "GetAccounts",
  "description": "Overview of accounts",
  "parameters": {
    "type": "object",
    "properties": {
      "skip": {
        "description": "The number of items to skip in the resultset.",
        "format": "int32",
        "type": "integer"
      },
      "take": {
        "description": "The number of items to return in the resultset. The returned count can be equal or less than this number.",
        "format": "int32",
        "type": "integer"
      },
      "asset_type": {
        "$ref": "#/components/schemas/AssetType",
        "description": "Filters the list, returning only accounts containing the specified asset type."
      },
      "identifier": {
        "description": "Return only accounts, matching the specified identifier.",
        "nullable": true,
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/accounts{?skip,take,asset_type,identifier}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**: The id of the account",
            "**identifier**: Identifier for the account",
            "**servicepack_id**: The servicepack id that defines the account."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
