{
  "name": "DeleteAlias",
  "description": "Delete a alias",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Mail zone domain name.",
        "type": "string"
      },
      "email_address": {
        "description": "Alias e-mail address.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      },
      "emailAddress": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "domain_name",
      "email_address",
      "domainName",
      "emailAddress"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/v2/mailzones/{domainName}/aliases/{emailAddress}{?domain_name,email_address}"
    }
  },
  "responses": {}
}
