{
  "name": "ChangeMailboxPassword",
  "description": "Change password for mailbox",
  "parameters": {
    "type": "object",
    "properties": {
      "mailbox_name": {
        "description": "Mailbox name.",
        "type": "string"
      },
      "mailboxName": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "$ref": "#/components/schemas/UpdateMailboxPasswordRequest",
        "description": "Contains the new password."
      }
    },
    "required": [
      "mailbox_name",
      "mailboxName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/v2/mailboxes/{mailboxName}/password{?mailbox_name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
