{
  "name": "GetSslCertificateRequests",
  "description": "Overview of SSL certificate requests",
  "parameters": {
    "type": "object",
    "properties": {
      "skip": {
        "description": "The number of items to skip in the resultset.",
        "format": "int32",
        "type": "integer"
      },
      "take": {
        "description": "The number of items to return in the resultset. The returned count can be equal or less than this number.",
        "format": "int32",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/sslcertificaterequests{?skip,take}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**certificate_type**: The type of the certificate:\r",
            "**common_name**: The common name of the certificate request.",
            "**id**: The id of the certificate request.",
            "**order_code**: The order code of the certificate request.",
            "**validation_level**: The validation level of the certificate:\r",
            "**vendor**: The vendor of the certificate."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
