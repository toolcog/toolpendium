{
  "name": "GetSslCertificates",
  "description": "Overview of SSL certificates",
  "parameters": {
    "type": "object",
    "properties": {
      "skip": {
        "description": "The number of items to skip in the resultset.",
        "format": "int32",
        "type": "integer"
      },
      "take": {
        "description": "The number of items to return in the resultset. The returned count can be equal or less than this number.",
        "format": "int32",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/sslcertificates{?skip,take}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**common_name**: The common name (e.g. domain.com) of the certificate.",
            "**expires_after**: The exact time the certificate will expire.",
            "**sha1_fingerprint**: The SHA-1 fingerprint of the certificate.<br />\r",
            "**type**: The type of the certificate:\r",
            "**validation_level**: The validation level of the certificate:\r"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
