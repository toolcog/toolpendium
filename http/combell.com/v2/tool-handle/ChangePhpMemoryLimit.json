{
  "name": "ChangePhpMemoryLimit",
  "description": "Configure PHP memory limit",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Linux hosting domain name.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "$ref": "#/components/schemas/UpdatePhpMemoryLimitRequest",
        "description": "Memory limit config"
      }
    },
    "required": [
      "domain_name",
      "domainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/v2/linuxhostings/{domainName}/phpsettings/memorylimit{?domain_name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
