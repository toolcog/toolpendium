{
  "name": "GetLinuxHostings",
  "description": "Overview of linux hostings",
  "parameters": {
    "type": "object",
    "properties": {
      "skip": {
        "description": "The number of items to skip in the resultset.",
        "format": "int32",
        "type": "integer"
      },
      "take": {
        "description": "The number of items to return in the resultset. The returned count can be equal or less than this number.",
        "format": "int32",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/linuxhostings{?skip,take}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**domain_name**: Domain name for the Linux hosting account.",
            "**servicepack_id**: Id of Linux hosting service package."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
