{
  "name": "GetDomains",
  "description": "Overviews of domains",
  "parameters": {
    "type": "object",
    "properties": {
      "skip": {
        "description": "The number of items to skip in the resultset.",
        "format": "int32",
        "type": "integer"
      },
      "take": {
        "description": "The number of items to return in the resultset. The returned count can be equal or less than this number.",
        "format": "int32",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/domains{?skip,take}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**domain_name**: The domain name",
            "**expiration_date**: Expiration date of the domain",
            "**will_renew**: Indication of renewal.<br />\r"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
