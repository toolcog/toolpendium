{
  "name": "ConfigureHttp2",
  "description": "Configure HTTP/2",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Linux hosting domain name.",
        "type": "string"
      },
      "site_name": {
        "description": "Site name where HTTP/2 should be configured.<br />\r\nFor HTTP/2 to work correctly, the site must have ssl enabled.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      },
      "siteName": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "$ref": "#/components/schemas/Http2Configuration",
        "description": ""
      }
    },
    "required": [
      "domain_name",
      "site_name",
      "domainName",
      "siteName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/v2/linuxhostings/{domainName}/sites/{siteName}/http2/configuration{?domain_name,site_name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
