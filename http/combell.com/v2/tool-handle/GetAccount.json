{
  "name": "GetAccount",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "account_id": {
        "description": "The id of the account.",
        "format": "int32",
        "type": "integer"
      },
      "accountId": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "account_id",
      "accountId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/accounts/{accountId}{?account_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A detailed representation of an account.",
        "**Key properties:**",
        {
          "$ul": [
            "**addons**: A list of addons applied to the account.",
            "**id**: The id of the account",
            "**identifier**: Identifier for the account",
            [
              "**servicepack**: Servicepack information.",
              {
                "$ul": [
                  "**id**: The id of the service pack",
                  "**name**: The name of the service pack"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
