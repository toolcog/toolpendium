{
  "name": "DeleteSmtpDomain",
  "description": "Delete an extra smtp domain",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Mail zone domain name.",
        "type": "string"
      },
      "hostname": {
        "description": "Smtp domain name.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "domain_name",
      "hostname",
      "domainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/v2/mailzones/{domainName}/smtpdomains/{hostname}{?domain_name}"
    }
  },
  "responses": {}
}
