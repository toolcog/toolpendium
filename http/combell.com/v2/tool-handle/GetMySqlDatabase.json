{
  "name": "GetMySqlDatabase",
  "description": "Get a specific database",
  "parameters": {
    "type": "object",
    "properties": {
      "database_name": {
        "type": "string"
      },
      "databaseName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "database_name",
      "databaseName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/mysqldatabases/{databaseName}{?database_name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**account_id**: The account id for the database.",
            "**actual_size**: The actual size in MB for the database.",
            "**hostname**: Hostname",
            "**max_size**: The maximim size in MB for the database.",
            "**name**: Database name",
            "**user_count**: The number of users."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
