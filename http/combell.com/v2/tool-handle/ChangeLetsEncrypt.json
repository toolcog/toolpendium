{
  "name": "ChangeLetsEncrypt",
  "description": "Configure let's encrypt",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Linux hosting domain name.",
        "type": "string"
      },
      "hostname": {
        "description": "Specific hostname.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "$ref": "#/components/schemas/LetsEncryptConfig",
        "description": "Let's encrypt config."
      }
    },
    "required": [
      "domain_name",
      "hostname",
      "domainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/v2/linuxhostings/{domainName}/sslsettings/{hostname}/letsencrypt{?domain_name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
