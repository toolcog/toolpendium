{
  "name": "GetAvailablePhpVersions",
  "description": "Get the available PHP versions.",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Linux hosting domain name.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "domain_name",
      "domainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/linuxhostings/{domainName}/phpsettings/availableversions{?domain_name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**version**: Php version"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
