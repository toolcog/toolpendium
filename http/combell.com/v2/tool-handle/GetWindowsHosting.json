{
  "name": "GetWindowsHosting",
  "description": "Windows hosting detail",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "The Windows hosting domain name.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "domain_name",
      "domainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/windowshostings/{domainName}{?domain_name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**actual_size**: Used webspace size in MB",
            [
              "**application_pool**: The application pool for the hosting account.",
              {
                "$ul": [
                  "**installed_net_core_runtimes**: The installed .NET Core runtimes for the hosting account.",
                  "**pipeline_mode**: The active pipeline mode for the hosting account.",
                  "**runtime**: The active runtime for the hosting account."
                ]
              }
            ],
            "**domain_name**: Domain name for the Windows hosting account.",
            "**ftp_username**: Ftp username",
            "**ip**: Windows hosting IP address",
            "**ip_type**: Type of the hosting IP address (dedicated or shared)",
            "**max_size**: Maximum webspace size in MB",
            "**mssql_database_names**: A list of mssql databases linked to the hosting account.<br />",
            "**servicepack_id**: Id of Windows hosting service package.",
            "**sites**: A list of websites on the hosting account."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
