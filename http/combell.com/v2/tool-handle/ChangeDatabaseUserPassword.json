{
  "name": "ChangeDatabaseUserPassword",
  "description": "Change password for mysql user",
  "parameters": {
    "type": "object",
    "properties": {
      "database_name": {
        "description": "Name of the database.",
        "type": "string"
      },
      "user_name": {
        "description": "Name of the user.",
        "type": "string"
      },
      "databaseName": {
        "type": "string",
        "description": "Automatically added"
      },
      "userName": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "$ref": "#/components/schemas/UpdateUserPasswordRequest",
        "description": "Contains the new password."
      }
    },
    "required": [
      "database_name",
      "user_name",
      "databaseName",
      "userName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/v2/mysqldatabases/{databaseName}/users/{userName}/password{?database_name,user_name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
