{
  "name": "DeleteSshKey",
  "description": "Delete a SSH key",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Linux hosting domain name.",
        "type": "string"
      },
      "fingerprint": {
        "description": "Fingerprint of public key.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "domain_name",
      "fingerprint",
      "domainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/v2/linuxhostings/{domainName}/ssh/keys/{fingerprint}{?domain_name}"
    }
  },
  "responses": {}
}
