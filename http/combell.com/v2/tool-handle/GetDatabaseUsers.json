{
  "name": "GetDatabaseUsers",
  "description": "Overview of mysql users",
  "parameters": {
    "type": "object",
    "properties": {
      "database_name": {
        "description": "Name of the database.",
        "type": "string"
      },
      "databaseName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "database_name",
      "databaseName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/mysqldatabases/{databaseName}/users{?database_name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**enabled**: User status",
            "**name**: User name",
            "**rights**: User rights"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
