{
  "name": "CreateCatchAll",
  "description": "Create a catch-all on the mail zone",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Mail zone domain name.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "$ref": "#/components/schemas/CreateCatchAllRequest",
        "description": "Contains the catch-all information."
      }
    },
    "required": [
      "domain_name",
      "domainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/v2/mailzones/{domainName}/catchall{?domain_name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
