      "database_name",
      "databaseName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/v2/mysqldatabases/{databaseName}{?database_name}"
    }
  },
  "responses": {}
}
