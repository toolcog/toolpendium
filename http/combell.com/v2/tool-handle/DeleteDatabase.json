  "description": "Delete a mysql database",
  "parameters": {
    "type": "object",
    "properties": {
      "database_name": {
        "description": "Name of the database.",
        "type": "string"
      },
      "databaseName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
