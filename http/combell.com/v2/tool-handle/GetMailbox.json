{
  "name": "GetMailbox",
  "description": "Get a specific mailbox",
  "parameters": {
    "type": "object",
    "properties": {
      "mailbox_name": {
        "description": "Mailbox name.",
        "type": "string"
      },
      "mailboxName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "mailbox_name",
      "mailboxName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v2/mailboxes/{mailboxName}{?mailbox_name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**actual_size**: Used size in MB",
            [
              "**auto_forward**",
              {
                "$ul": [
                  "**copy_to_myself**: Copy to myself",
                  "**email_addresses**: E-mail addresses to which e-mails are forwarded",
                  "**enabled**: Enabled"
                ]
              }
            ],
            [
              "**auto_reply**",
              {
                "$ul": [
                  "**enabled**: Enabled",
                  "**message**: Message",
                  "**subject**: Subject"
                ]
              }
            ],
            "**login**: Login to connect with the mailbox",
            "**max_size**: Maximum size in MB",
            "**name**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
