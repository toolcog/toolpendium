{
  "name": "DeleteSubsite",
  "description": "Delete a subsite",
  "parameters": {
    "type": "object",
    "properties": {
      "domain_name": {
        "description": "Linux hosting domain name.",
        "type": "string"
      },
      "site_name": {
        "description": "Name of the site on the linux hosting.",
        "type": "string"
      },
      "domainName": {
        "type": "string",
        "description": "Automatically added"
      },
      "siteName": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "domain_name",
      "site_name",
      "domainName",
      "siteName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/v2/linuxhostings/{domainName}/subsites/{siteName}{?domain_name,site_name}"
    }
  },
  "responses": {}
}
