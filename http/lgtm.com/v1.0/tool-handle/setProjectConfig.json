  "name": "setProjectConfig",
  "description": "Set the administrator configuration for a specific project using the numeric project identifier.",
  "parameters": {
    "type": "object",
    "properties": {
      "project-id": {
        "format": "int64",
        "type": "integer",
        "description": "The numeric project identifier"
      }
    },
    "required": [
      "project-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://lgtm.com/api/v1.0/projects/{project-id}/settings/analysis-configuration"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
