{
  "name": "getAllTransfers",
  "description": "Returns the entire list of transfers created by your sub-partners.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "111",
        "type": "string",
        "description": "int or array of int (optional)"
      },
      "status": {
        "example": "CREATED",
        "type": "string",
        "description": "string or array of string \n\"WAITING\"/\"CREATED\"/\"FINISHED\"/\"REJECTED\"\n(optional)"
      },
      "limit": {
        "example": "10",
        "type": "string",
        "description": "(optional) default 10"
      },
      "offset": {
        "example": "0",
        "type": "string",
        "description": "(optional) default 0"
      },
      "order": {
        "example": "ASC",
        "type": "string",
        "description": "ASC / DESC (optional) default ASC"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nowpayments.io/v1/sub-partner/transfers{?id,status,limit,offset,order}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
