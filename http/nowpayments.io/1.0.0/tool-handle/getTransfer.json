{
  "name": "getTransfer",
  "description": "Get the actual information about the transfer. You need to provide the transfer ID in the request.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nowpayments.io/v1/sub-partner/transfer/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  "**amount**",
                  "**created_at**",
                  "**currency**",
                  "**from_sub_id**",
                  "**id**",
                  "**status**",
                  "**to_sub_id**",
                  "**updated_at**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
