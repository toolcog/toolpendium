{
  "name": "getSubPartners",
  "description": "This method returns the entire list of your sub-partners.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "111",
        "type": "string",
        "description": "int or array of int (optional)"
      },
      "offset": {
        "example": "0",
        "type": "string",
        "description": "(optional) default 0"
      },
      "limit": {
        "example": "10",
        "type": "string",
        "description": "(optional) default 10"
      },
      "order": {
        "example": "DESC",
        "type": "string",
        "description": "ASC / DESC (optional) default ASC"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nowpayments.io/v1/sub-partner{?id,offset,limit,order}"
    }
  },
  "responses": {}
}
