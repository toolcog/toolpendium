{
  "name": "getManyRecurringPayments",
  "description": "The method allows you to view the entire list of recurring payments filtered by payment status and/or payment plan id",
  "parameters": {
    "type": "object",
    "properties": {
      "status": {
        "example": "PARTIALLY_PAID",
        "type": "string",
        "description": "\"WAITING_PAY\" / \"PAID\" /  \"PARTIALLY_PAID\" / \"EXPIRED\""
      },
      "subscription_plan_id": {
        "example": "111394288",
        "type": "string"
      },
      "is_active": {
        "example": "false",
        "type": "string",
        "description": "true / false"
      },
      "limit": {
        "example": "10",
        "type": "string"
      },
      "offset": {
        "example": "0",
        "type": "string"
      },
      "x-api-key": {
        "example": "{{your_api_key}}",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nowpayments.io/v1/subscriptions{?status,subscription_plan_id,is_active,limit,offset}"
    },
    "headers": {
      "x-api-key": {
        "$": "x-api-key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
