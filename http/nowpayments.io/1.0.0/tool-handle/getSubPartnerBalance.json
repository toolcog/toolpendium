{
  "name": "getSubPartnerBalance",
  "description": "This request can be made only from a whitelisted IP.  \nIf IP whitelisting is disabled, this request can be made by any user that has an API key.",
  "parameters": {
    "type": "object",
    "properties": {
      "x-api-key": {
        "example": "{{your_api_key}}",
        "type": "string"
      },
      "id": {
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nowpayments.io/v1/sub-partner/balance/{id}"
    },
    "headers": {
      "x-api-key": {
        "$": "x-api-key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**result**",
              {
                "$ul": [
                  [
                    "**balances**",
                    {
                      "$ul": [
                        [
                          "**usddtrc20**",
                          {
                            "$ul": [
                              "**amount**",
                              "**pendingAmount**"
                            ]
                          }
                        ],
                        [
                          "**usdtbsc**",
                          {
                            "$ul": [
                              "**amount**",
                              "**pendingAmount**"
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  "**subPartnerId**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
