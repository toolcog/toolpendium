{
  "name": "market-statistics",
  "description": "Returns aggregate rental statistics and listing trends for a single US zip code.",
  "parameters": {
    "type": "object",
    "properties": {
      "zipCode": {
        "default": "29611",
        "type": "string",
        "description": "A valid 5-digit US zip code"
      },
      "historyRange": {
        "default": 6,
        "format": "int32",
        "type": "integer",
        "description": "The time range for historical record entries, in months. Defaults to 12 if not provided"
      }
    },
    "required": [
      "zipCode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
