{
  "name": "listReports",
  "description": "Lists all reports asociated with the client or user requesting.",
  "parameters": {
    "type": "object",
    "properties": {
      "start_key": {
        "type": "string",
        "description": "Start value for pagination."
      },
      "username": {
        "type": "string",
        "description": "filter reports created by the specified username"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.truora.com/v1/reports{?start_key,username}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents the result of report searches",
        "**Key properties:**",
        {
          "$ul": [
            "**next**: Next page path",
            "**reports**: Report list",
            "**self**: Current page path"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
