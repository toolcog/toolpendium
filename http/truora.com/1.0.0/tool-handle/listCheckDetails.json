{
  "name": "listCheckDetails",
  "description": "Lists all details associated with a Check. It can be paginated.",
  "parameters": {
    "type": "object",
    "properties": {
      "start_key": {
        "type": "string",
        "description": "Start key value for the pagination"
      },
      "lang": {
        "type": "string",
        "description": "This parameter is used to specify the language wanted for details, if not specified details will come in their original language."
      },
      "check_id": {
        "type": "string",
        "description": "ID of the Check"
      }
    },
    "required": [
      "check_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.truora.com/v1/checks/{check_id}/details{?start_key,lang}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents a list of background check details",
        "**Key properties:**",
        {
          "$ul": [
            "**details**: Detail list",
            "**next**: Next page path",
            "**self**: Current page path"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
