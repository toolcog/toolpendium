{
  "name": "listScoreConfigs",
  "description": "Lists the custom score configurations of the associated account.",
  "parameters": {
    "type": "object",
    "properties": {
      "start_key": {
        "type": "string",
        "description": "The key to start the pagination"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.truora.com/v1/config{?start_key}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The result of an operation that returns multiple score configs",
        "**Key properties:**",
        {
          "$ul": [
            "**score_configs**: The list of score configs"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
