{
  "name": "listChecks",
  "description": "Lists the existing checks in the account or in a specified report.",
  "parameters": {
    "type": "object",
    "properties": {
      "start_key": {
        "type": "string",
        "description": "Start key value for the pagination"
      },
      "report_id": {
        "type": "string",
        "description": "Report id checks to be returned"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.truora.com/v1/checks{?start_key,report_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents the results of a background check search",
        "**Key properties:**",
        {
          "$ul": [
            "**checks**: Background check list in the page",
            "**next**: Next page path",
            "**self**: Current page path"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
