{
  "name": "ListContinuousChecks",
  "description": "Lists all continuous checks",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.truora.com/v1/continuous-checks"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents to ListContinuousChecksOutput",
        "**Key properties:**",
        {
          "$ul": [
            "**continuous_checks**: List of continuous checks in current page\r",
            "**next**: Next page URL",
            "**self**: Current page URL"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
