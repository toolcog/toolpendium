{
  "name": "DeleteCustomType",
  "description": "Allows deleting a custom type. Person, vehicle, and company types cannot be deleted",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "description": "Name of the custom type to be deleted"
      },
      "country": {
        "enum": [
          "BR",
          "CL",
          "CO",
          "CR",
          "EC",
          "MX",
          "PE",
          "ALL"
        ],
        "type": "string",
        "description": "Country where the custom type is valid"
      }
    },
    "required": [
      "type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.truora.com/v1/config{?type,country}"
    }
  },
  "responses": {}
}
