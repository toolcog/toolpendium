{
  "name": "PATCH_api_keys-api_key_id",
  "description": "**This endpoint allows you to update the name of an existing API Key.**\n\nYou must pass this endpoint a JSON request body with a `name` property, which will be used to rename the key associated with the `api_key_id` passed in the URL.",
  "parameters": {
    "type": "object",
    "properties": {
      "on-behalf-of": {
        "default": "The subuser's username. This header generates the API call as if the subuser account was making the call.",
        "type": "string"
      },
      "api_key_id": {
        "type": "string",
        "description": ""
      },
      "body": {
        "example": {
          "name": "A New Hope"
        },
        "properties": {
          "name": {
            "description": "The new name of the API Key.",
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      }
    },
    "required": [
      "api_key_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/api_keys/{api_key_id}"
    },
    "headers": {
      "on-behalf-of": {
        "$": "on-behalf-of"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "API Key Name and ID"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**api_key_id**: The ID of your API Key. ",
            "**name**: The name of your API Key."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
