{
  "name": "GET_v3-teammates-username",
  "description": "**This endpoint allows you to retrieve a specific Teammate by username.**\n\nYou can retrieve the username's for each of your Teammates using the \"Retrieve all Teammates\" endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "on-behalf-of": {
        "default": "The subuser's username. This header generates the API call as if the subuser account was making the call.",
        "type": "string"
      },
      "username": {
        "type": "string",
        "description": "The username of the teammate that you want to retrieve."
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/teammates/{username}"
    },
    "headers": {
      "on-behalf-of": {
        "$": "on-behalf-of"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**address**: (optional) Teammate's address",
            "**address2**: (optional) Teammate's address",
            "**city**: (optional) Teammate's city",
            "**country**: (optional) Teammate's country",
            "**email**: Teammate's email",
            "**first_name**: Teammate's first name",
            "**is_admin**: Set to true if teammate has admin privileges",
            "**last_name**: Teammate's last name",
            "**phone**: (optional) Teammate's phone number",
            "**scopes**: Scopes associated to teammate",
            "**state**: (optional) Teammate's state",
            "**user_type**: Indicate the type of user: account owner, teammate admin user, or normal teammate",
            "**username**: Teammate's username",
            "**website**: (optional) Teammate's website",
            "**zip**: (optional) Teammate's zip"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
