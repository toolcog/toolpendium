{
  "name": "DELETE_mc-lists-id-contacts",
  "description": "**This endpoint allows you to remove contacts from a given list.**\n\nThe contacts will not be deleted. Only their list membership will be changed.",
  "parameters": {
    "type": "object",
    "properties": {
      "contact_ids": {
        "minLength": 1,
        "type": "string",
        "description": "comma separated list of contact ids"
      },
      "id": {
        "type": "string"
      }
    },
    "required": [
      "contact_ids",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/marketing/lists/{id}/contacts{?contact_ids}"
    }
  },
  "responses": {
    "202": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The removal is accepted and processing.",
        "**Key properties:**",
        {
          "$ul": [
            "**job_id**: job_id of the async job"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error_id**",
            "**field**",
            "**message**",
            "**parameter**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
