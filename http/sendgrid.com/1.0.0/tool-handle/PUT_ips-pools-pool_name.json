{
  "name": "PUT_ips-pools-pool_name",
  "description": "**This endpoint allows you to update the name of an IP pool.**",
  "parameters": {
    "type": "object",
    "properties": {
      "pool_name": {
        "type": "string",
        "description": "The name of the IP pool that you want to retrieve the IP addresses for."
      },
      "body": {
        "example": {
          "name": "new_pool_name"
        },
        "properties": {
          "name": {
            "description": "The new name for your IP pool.",
            "maxLength": 64,
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "pool_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/ips/pools/{pool_name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "IP Pools: Pool Resp"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**name**: The name of the IP pool."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
