{
  "name": "DELETE_suppression-bounces-email",
  "description": "**This endpoint allows you to remove an email address from your bounce list.**",
  "parameters": {
    "type": "object",
    "properties": {
      "email_address": {
        "format": "email",
        "type": "string",
        "description": "The email address you would like to remove from the bounce list."
      },
      "on-behalf-of": {
        "default": "The subuser's username. This header generates the API call as if the subuser account was making the call.",
        "type": "string"
      },
      "email": {
        "type": "string"
      },
      "body": {
        "nullable": true
      }
    },
    "required": [
      "email_address",
      "email"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/suppression/bounces/{email}{?email_address}"
    },
    "headers": {
      "on-behalf-of": {
        "$": "on-behalf-of"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "204": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
