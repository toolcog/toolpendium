      },
      "campaign_id": {
        "type": "integer"
      }
    },
    "required": [
      "campaign_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/campaigns/{campaign_id}/schedules"
    },
    "headers": {
      "on-behalf-of": {
        "$": "on-behalf-of"
      }
    }
  },
  "responses": {
