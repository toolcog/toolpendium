{
  "name": "POST_ips",
  "description": "**This endpoint is for adding a(n) IP Address(es) to your account.**",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "example": {
          "count": 90323478,
          "subusers": [
            "subuser1",
            "subuser2"
          ],
          "user_can_send": true,
          "warmup": true
        },
        "properties": {
          "count": {
            "description": "The amount of IPs to add to the account.",
            "type": "integer"
          },
          "subusers": {
            "description": "Array of usernames to be assigned a send IP.",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "warmup": {
            "default": false,
            "description": "Whether or not to warmup the IPs being added.",
            "type": "boolean"
          }
        },
        "required": [
          "count"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/ips"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ips**: List of IP objects.",
            "**remaining_ips**: The number of IPs that can still be added to the user.",
            "**warmup**: Whether or not the IPs are being warmed up."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Global Error Response Schema"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**",
            "**id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
