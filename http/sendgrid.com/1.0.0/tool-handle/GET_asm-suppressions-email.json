{
  "name": "GET_asm-suppressions-email",
  "description": "**This endpoint returns a list of all groups from which the given email address has been unsubscribed.**",
  "parameters": {
    "type": "object",
    "properties": {
      "on-behalf-of": {
        "default": "The subuser's username. This header generates the API call as if the subuser account was making the call.",
        "type": "string"
      },
      "email": {
        "type": "string",
        "description": "The email address that you want to search suppression groups for."
      }
    },
    "required": [
      "email"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/asm/suppressions/{email}"
    },
    "headers": {
      "on-behalf-of": {
        "$": "on-behalf-of"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**suppressions**: The array of suppression groups."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
