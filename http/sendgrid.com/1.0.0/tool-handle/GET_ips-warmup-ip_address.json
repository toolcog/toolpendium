{
  "name": "GET_ips-warmup-ip_address",
  "description": "**This endpoint allows you to retrieve the warmup status for a specific IP address.**\n\nYou can retrieve all of your warming IPs using the \"Retrieve all IPs currently in warmup\" endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "ip_address": {
        "type": "string",
        "description": "The IP address that you want to retrieve the warmup status for."
      }
    },
    "required": [
      "ip_address"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/ips/warmup/{ip_address}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "IP Warmup: IP"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ip**: The IP address.",
            "**start_date**: A Unix timestamp indicating when the IP address entered warmup mode."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**: The errors that were encountered."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
