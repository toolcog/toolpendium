{
  "name": "GET_ips-warmup",
  "description": "**This endpoint allows you to retrieve all of your IP addresses that are currently warming up.**",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/ips/warmup"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "IP Warmup: IP"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ip**: The IP address.",
            "**start_date**: A Unix timestamp indicating when the IP address entered warmup mode."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
