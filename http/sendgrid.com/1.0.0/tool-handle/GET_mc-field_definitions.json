{
  "name": "GET_mc-field_definitions",
  "description": "**This endpoint retrieves all defined Custom Fields and Reserved Fields.**",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/marketing/field_definitions"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_metadata**",
              {
                "$ul": [
                  "**count**",
                  "**next**",
                  "**prev**",
                  "**self**"
                ]
              }
            ],
            "**custom_fields**",
            "**reserved_fields**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
