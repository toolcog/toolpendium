{
  "name": "DELETE_v3-teammates-pending-token",
  "description": "**This endpoint allows you to delete a pending teammate invite.**",
  "parameters": {
    "type": "object",
    "properties": {
      "on-behalf-of": {
        "default": "The subuser's username. This header generates the API call as if the subuser account was making the call.",
        "type": "string"
      },
      "token": {
        "type": "string",
        "description": "The token for the invite you want to delete."
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/teammates/pending/{token}"
    },
    "headers": {
      "on-behalf-of": {
        "$": "on-behalf-of"
      }
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
