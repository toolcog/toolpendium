{
  "name": "GET_ips-pools-pool_name",
  "description": "**This endpoint allows you to get all of the IP addresses that are in a specific IP pool.**",
  "parameters": {
    "type": "object",
    "properties": {
      "pool_name": {
        "type": "string",
        "description": "The name of the IP pool that you want to retrieve the IP addresses for."
      }
    },
    "required": [
      "pool_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.sendgrid.com/v3/ips/pools/{pool_name}"
    }
  },
  "responses": {
