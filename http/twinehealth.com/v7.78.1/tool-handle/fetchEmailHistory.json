{
  "name": "fetchEmailHistory",
  "description": "Get an email history by id",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Email history identifier"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.twinehealth.com/pub/email_history/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  [
                    "**attributes**",
                    {
                      "$ul": [
                        "**email_address**: Address email was sent to.",
                        "**email_type**: Type of email.",
                        "**send_time**: Time email was sent.",
                        [
                          "**status_times**: Status of email. Multiple statuses may be defined. The current status is the one with the most recent date.",
                          {
                            "$ul": [
                              "**bounce**: Time email was bounced.",
                              "**click**: Time email was clicked.",
                              "**deferred**: Time email was deferred.",
                              "**delivered**: Time email was delivered.",
                              "**dropped**: Time email was dropped.",
                              "**open**: Time email was opened.",
                              "**processed**: Time email was processed.",
                              "**spamreport**: Time email was reported as spam.",
                              "**unsubscribe**: Time email was unsubscribed from."
                            ]
                          }
                        ],
                        "**twine_email_id**: Unique identifier for this email."
                      ]
                    }
                  ],
                  "**id**",
                  [
                    "**relationships**",
                    {
                      "$ul": [
                        [
                          "**receiver**",
                          {
                            "$ul": [
                              [
                                "**data**",
                                {
                                  "$ul": [
                                    "**id**",
                                    "**type**"
                                  ]
                                }
                              ]
                            ]
                          }
                        ],
                        [
                          "**sender**",
                          {
                            "$ul": [
                              [
                                "**data**",
                                {
                                  "$ul": [
                                    "**id**",
                                    "**type**"
                                  ]
                                }
                              ]
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  "**type**"
                ]
              }
            ],
            [
              "**meta**",
              {
                "$ul": [
                  "**count**",
                  "**req_id**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
