{
  "name": "getDomains",
  "description": "Gets a list of domains registered for the authenticating user.",
  "parameters": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "description": "A team id in case you want to act on behalf of a team"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.zeit.co/v4/domains{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**domains**: An Array of domains for the given account"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
