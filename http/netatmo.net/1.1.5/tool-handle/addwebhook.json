{
  "name": "addwebhook",
  "description": "Links a callback url to a user.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "url": {
        "type": "string",
        "description": "Your webhook callback url"
      },
      "app_type": {
        "type": "string",
        "description": "Webhooks are only available for Welcome, enter app_camera."
      }
    },
    "required": [
      "url",
      "app_type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.netatmo.net/api/addwebhook{?url,app_type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status**",
            "**time_exec**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
