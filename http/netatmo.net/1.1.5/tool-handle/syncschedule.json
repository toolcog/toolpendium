{
  "name": "syncschedule",
  "description": "The method syncschedule changes the Thermostat weekly schedule.",
  "parameters": {
    "type": "object",
    "properties": {
      "device_id": {
        "type": "string",
        "description": "The relay id"
      },
      "module_id": {
        "type": "string",
        "description": "The thermostat id"
      }
    },
    "required": [
      "device_id",
      "module_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.netatmo.net/api/syncschedule{?device_id,module_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**body**",
            "**status**",
            "**time_exec**",
            "**time_server**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
