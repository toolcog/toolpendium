{
  "name": "getlasteventof",
  "description": "Returns most recent events.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "home_id": {
        "type": "string",
        "description": "ID of the Home you're interested in"
      },
      "person_id": {
        "type": "string",
        "description": "Your request will retrieve all events of the given home until the most recent event of the given person"
      },
      "offset": {
        "type": "integer",
        "description": "Number of events to retrieve. Default is 30."
      }
    },
    "required": [
      "home_id",
      "person_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.netatmo.net/api/getlasteventof{?home_id,person_id,offset}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**body**",
              {
                "$ul": [
                  "**events_list**"
                ]
              }
            ],
            "**status**",
            "**time_exec**",
            "**time_server**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
