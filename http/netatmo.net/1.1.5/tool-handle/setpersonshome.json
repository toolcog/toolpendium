{
  "name": "setpersonshome",
  "description": "Sets a person as 'At home'.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "home_id": {
        "type": "string",
        "description": "ID of the Home you're interested in"
      },
      "person_ids": {
        "type": "string",
        "description": "List of persons IDs"
      }
    },
    "required": [
      "home_id",
      "person_ids"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.netatmo.net/api/setpersonshome{?home_id,person_ids}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status**",
            "**time_exec**",
            "**time_server**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
