{
  "name": "listFeatures",
  "description": "Return a collection of Feature.",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "default": "netflix-monthly",
        "type": "string",
        "description": "The namespace of the feature type."
      },
      "date": {
        "default": "2018-09-15",
        "pattern": "date",
        "type": "string",
        "description": "Date of the collection of feature items."
      },
      "start": {
        "default": "2018-09-15",
        "pattern": "date",
        "type": "string",
        "description": "Start date for a range of features."
      },
      "end": {
        "default": "2018-10-15",
        "pattern": "date",
        "type": "string",
        "description": "End date for a range of features."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://tv.api.pressassociation.io/v2/feature{?type,date,start,end}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
