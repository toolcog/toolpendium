{
  "name": "getCatalogueAssetDetail",
  "description": "Return the content of the selected catalogue asset.",
  "parameters": {
    "type": "object",
    "properties": {
      "catalogueId": {
        "type": "string",
        "description": "The identifier for the selected catalogue."
      },
      "assetId": {
        "type": "string",
        "description": "The identifier for the selected catalogue asset."
      }
    },
    "required": [
      "catalogueId",
      "assetId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://tv.api.pressassociation.io/v2/catalogue/{catalogueId}/asset/{assetId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
