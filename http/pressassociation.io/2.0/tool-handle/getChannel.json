{
  "name": "getChannel",
  "description": "Return the content of the selected channel.",
  "parameters": {
    "type": "object",
    "properties": {
      "channelId": {
        "type": "string",
        "description": "The identifier for the selected channel."
      },
      "aliases": {
        "default": true,
        "type": "boolean",
        "description": "Flag to display Legacy and Provider Ids."
      }
    },
    "required": [
      "channelId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://tv.api.pressassociation.io/v2/channel/{channelId}{?aliases}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
