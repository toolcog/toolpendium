              {
                "$ul": [
                  [
                    "**device**",
                    {
                      "$ul": [
                        "**display_name**",
                        "**id**",
                        "**name**",
                        "**url**"
                      ]
                    }
                  ],
                  "**id**",
                  "**is_connected**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**tags**"
          ]
