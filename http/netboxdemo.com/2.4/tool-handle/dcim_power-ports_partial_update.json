          },
          "device": {
            "title": "Device",
            "type": "integer"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "power_outlet": {
            "title": "Power outlet",
            "type": "integer"
          },
          "tags": {
