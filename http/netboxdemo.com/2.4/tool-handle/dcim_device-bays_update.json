{
  "name": "dcim_device-bays_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this device bay."
      },
      "body": {
        "$ref": "#/$defs/WritableDeviceBay"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritableDeviceBay": {
        "properties": {
          "device": {
            "title": "Device",
            "type": "integer"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "installed_device": {
            "title": "Installed device",
            "type": "integer"
          },
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "tags": {
            "items": {
              "minLength": 1,
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "device",
          "name"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://netboxdemo.com/api/dcim/device-bays/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**device**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**id**",
            [
              "**installed_device**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**name**",
            "**tags**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
