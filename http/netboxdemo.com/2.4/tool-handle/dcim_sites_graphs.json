{
  "name": "dcim_sites_graphs",
  "description": "A convenience method for rendering graphs for a particular site.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this site."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://netboxdemo.com/api/dcim/sites/{id}/graphs/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**asn**",
            "**comments**",
            "**contact_email**",
            "**contact_name**",
            "**contact_phone**",
            "**count_circuits**",
            "**count_devices**",
            "**count_prefixes**",
            "**count_racks**",
            "**count_vlans**",
            "**created**",
            "**custom_fields**",
            "**description**",
            "**facility**",
            "**id**",
            "**last_updated**",
            "**latitude**",
            "**longitude**",
            "**name**",
            "**physical_address**",
            [
              "**region**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**shipping_address**",
            "**slug**",
            [
              "**status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            [
              "**tenant**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**time_zone**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
