{
  "name": "dcim_interfaces_graphs",
  "description": "A convenience method for rendering graphs for a particular interface.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this interface."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://netboxdemo.com/api/dcim/interfaces/{id}/graphs/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**circuit_termination**",
              {
                "$ul": [
                  [
                    "**circuit**",
                    {
                      "$ul": [
                        "**cid**",
                        "**id**",
                        "**url**"
                      ]
                    }
                  ],
                  "**id**",
                  "**port_speed**",
                  "**pp_info**",
                  "**term_side**",
                  "**upstream_speed**: Upstream speed, if different from port speed",
                  "**xconnect_id**"
                ]
              }
            ],
            "**description**",
            [
              "**device**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**enabled**",
            [
              "**form_factor**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**id**",
            "**interface_connection**",
            "**is_connected**",
            [
              "**lag**",
              {
                "$ul": [
                  [
                    "**device**",
                    {
                      "$ul": [
                        "**display_name**",
                        "**id**",
                        "**name**",
                        "**url**"
                      ]
                    }
                  ],
                  "**id**",
                  "**is_connected**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**mac_address**",
            "**mgmt_only**: This interface is used only for out-of-band management",
            [
              "**mode**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**mtu**",
            "**name**",
            "**tagged_vlans**",
            "**tags**",
            [
              "**untagged_vlan**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**",
                  "**vid**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
