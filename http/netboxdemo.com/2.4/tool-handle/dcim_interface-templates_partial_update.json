{
  "name": "dcim_interface-templates_partial_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this interface template."
      },
      "body": {
        "$ref": "#/$defs/WritableInterfaceTemplate"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritableInterfaceTemplate": {
        "properties": {
          "device_type": {
            "title": "Device type",
            "type": "integer"
          },
          "form_factor": {
            "enum": [
              0,
              200,
              800,
              1000,
              1150,
              1170,
              1050,
              1100,
              1200,
              1300,
              1310,
              1320,
              1350,
              1400,
              1500,
              1510,
              1520,
              1550,
              1600,
              2600,
              2610,
              2620,
              2630,
              2640,
              6100,
              6200,
              6300,
              6400,
              6500,
              6600,
              6700,
              3010,
              3020,
              3040,
              3080,
              3160,
              3320,
              4000,
              4010,
              4040,
              4050,
              5000,
              5050,
              5100,
              5150,
              5200,
              5300,
              5310,
              5320,
              5330,
              32767
            ],
            "title": "Form factor",
            "type": "integer"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "mgmt_only": {
            "title": "Management only",
            "type": "boolean"
          },
          "name": {
            "maxLength": 64,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          }
        },
        "required": [
          "device_type",
          "name"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "http://netboxdemo.com/api/dcim/interface-templates/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**device_type**",
              {
                "$ul": [
                  "**id**",
                  [
                    "**manufacturer**",
                    {
                      "$ul": [
                        "**id**",
                        "**name**",
                        "**slug**",
                        "**url**"
                      ]
                    }
                  ],
                  "**model**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            [
              "**form_factor**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**id**",
            "**mgmt_only**",
            "**name**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
