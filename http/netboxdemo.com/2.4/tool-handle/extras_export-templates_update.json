{
  "name": "extras_export-templates_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this export template."
      },
      "body": {
        "$ref": "#/$defs/ExportTemplate"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "ExportTemplate": {
        "properties": {
          "content_type": {
            "title": "Content type",
            "type": "integer"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "file_extension": {
            "maxLength": 15,
            "title": "File extension",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "mime_type": {
            "maxLength": 15,
            "title": "Mime type",
            "type": "string"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "template_code": {
            "minLength": 1,
            "title": "Template code",
            "type": "string"
          }
        },
        "required": [
          "content_type",
          "name",
          "template_code"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://netboxdemo.com/api/extras/export-templates/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**content_type**",
            "**description**",
            "**file_extension**",
            "**id**",
            "**mime_type**",
            "**name**",
            "**template_code**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
