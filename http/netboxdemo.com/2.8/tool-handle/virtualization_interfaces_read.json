{
  "name": "virtualization_interfaces_read",
  "description": "Call to super to allow for caching",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this interface."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/virtualization/interfaces/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**description**",
            "**enabled**",
            "**id**",
            "**mac_address**",
            [
              "**mode**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**mtu**",
            "**name**",
            "**tagged_vlans**",
            "**tags**",
            [
              "**type** (default: [object Object])",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            [
              "**untagged_vlan**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**",
                  "**vid**"
                ]
              }
            ],
            [
              "**virtual_machine**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
