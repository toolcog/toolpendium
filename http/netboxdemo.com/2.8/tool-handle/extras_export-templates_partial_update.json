{
  "name": "extras_export-templates_partial_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this export template."
      },
      "body": {
        "$ref": "#/$defs/WritableExportTemplate"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritableExportTemplate": {
        "properties": {
          "content_type": {
            "title": "Content type",
            "type": "string"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "file_extension": {
            "description": "Extension to append to the rendered filename",
            "maxLength": 15,
            "title": "File extension",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "mime_type": {
            "description": "Defaults to <code>text/plain</code>",
            "maxLength": 50,
            "title": "MIME type",
            "type": "string"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "template_code": {
            "description": "The list of objects being exported is passed as a context variable named <code>queryset</code>.",
            "minLength": 1,
            "title": "Template code",
            "type": "string"
          },
          "template_language": {
            "enum": [
              "django",
              "jinja2"
            ],
            "title": "Template language",
            "type": "string"
          }
        },
        "required": [
          "content_type",
          "name",
          "template_code"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://netboxdemo.com/api/extras/export-templates/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**content_type**",
            "**description**",
            "**file_extension**: Extension to append to the rendered filename",
            "**id**",
            "**mime_type**: Defaults to <code>text/plain</code>",
            "**name**",
            "**template_code**: The list of objects being exported is passed as a context variable named <code>queryset</code>.",
            [
              "**template_language** (default: [object Object])",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
