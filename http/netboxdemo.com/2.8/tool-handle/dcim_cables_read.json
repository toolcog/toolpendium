{
  "name": "dcim_cables_read",
  "description": "Call to super to allow for caching",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this cable."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/dcim/cables/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**color**",
            "**id**",
            "**label**",
            "**length**",
            [
              "**length_unit**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            [
              "**status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**termination_a**",
            "**termination_a_id**",
            "**termination_a_type**",
            "**termination_b**",
            "**termination_b_id**",
            "**termination_b_type**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
