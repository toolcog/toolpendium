                        "**value**"
                      ]
                    }
                  ],
                  [
                    "**device**",
                    {
                      "$ul": [
                        "**display_name**",
                        "**id**",
                        "**name**",
                        "**url**"
                      ]
                    }
                  ],
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**tags**",
            [
              "**type**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
