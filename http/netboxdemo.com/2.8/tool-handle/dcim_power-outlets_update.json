  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://netboxdemo.com/api/dcim/power-outlets/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**cable**",
              {
                "$ul": [
                  "**id**",
                  "**label**",
                  "**url**"
                ]
              }
            ],
            "**connected_endpoint**: ",
            "**connected_endpoint_type**",
            [
              "**connection_status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**description**",
            [
              "**device**",
              {
                "$ul": [
                  "**display_name**",
