{
  "name": "extras_tags_create",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/Tag"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "Tag": {
        "properties": {
          "color": {
            "maxLength": 6,
            "minLength": 1,
            "pattern": "^[0-9a-f]{6}$",
            "title": "Color",
            "type": "string"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "slug": {
            "format": "slug",
            "maxLength": 100,
            "minLength": 1,
            "pattern": "^[-a-zA-Z0-9_]+$",
            "title": "Slug",
            "type": "string"
          },
          "tagged_items": {
            "readOnly": true,
            "title": "Tagged items",
            "type": "integer"
          }
        },
        "required": [
          "name",
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://netboxdemo.com/api/extras/tags/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**color**",
            "**description**",
            "**id**",
            "**name**",
            "**slug**",
            "**tagged_items**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
