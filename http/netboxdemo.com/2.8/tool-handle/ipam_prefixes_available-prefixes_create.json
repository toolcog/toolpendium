{
  "name": "ipam_prefixes_available-prefixes_create",
  "description": "The advisory lock decorator uses a PostgreSQL advisory lock to prevent this API from being\ninvoked in parallel, which results in a race condition where multiple insertions can occur.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this prefix."
      },
      "body": {
        "$ref": "#/$defs/WritablePrefix"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritablePrefix": {
        "properties": {
          "created": {
            "format": "date",
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "custom_fields": {
            "default": {},
            "title": "Custom fields",
            "type": "object"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "family": {
            "readOnly": true,
            "title": "Family",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "is_pool": {
            "description": "All IP addresses within this prefix are considered usable",
            "title": "Is a pool",
            "type": "boolean"
          },
          "last_updated": {
            "format": "date-time",
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "prefix": {
            "description": "IPv4 or IPv6 network with mask",
            "title": "Prefix",
            "type": "string"
          },
          "role": {
            "description": "The primary function of this prefix",
            "nullable": true,
            "title": "Role",
            "type": "integer"
          },
          "site": {
            "nullable": true,
            "title": "Site",
            "type": "integer"
          },
          "status": {
            "description": "Operational status of this prefix",
            "enum": [
              "container",
              "active",
              "reserved",
              "deprecated"
            ],
            "title": "Status",
            "type": "string"
          },
          "tags": {
            "items": {
              "minLength": 1,
              "type": "string"
            },
            "type": "array"
          },
          "tenant": {
            "nullable": true,
            "title": "Tenant",
            "type": "integer"
          },
          "vlan": {
            "nullable": true,
            "title": "VLAN",
            "type": "integer"
          },
          "vrf": {
            "nullable": true,
            "title": "VRF",
            "type": "integer"
          }
        },
        "required": [
          "prefix"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://netboxdemo.com/api/ipam/prefixes/{id}/available-prefixes/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**family**",
            "**prefix**",
            [
              "**vrf**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**prefix_count**",
                  "**rd**: Unique route distinguisher (as defined in RFC 4364)",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
