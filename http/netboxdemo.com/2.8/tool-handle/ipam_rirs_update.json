{
  "name": "ipam_rirs_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this RIR."
      },
      "body": {
        "$ref": "#/$defs/RIR"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "RIR": {
        "properties": {
          "aggregate_count": {
            "readOnly": true,
            "title": "Aggregate count",
            "type": "integer"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "is_private": {
            "description": "IP space managed by this RIR is considered private",
            "title": "Private",
            "type": "boolean"
          },
          "name": {
            "maxLength": 50,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "slug": {
            "format": "slug",
            "maxLength": 50,
            "minLength": 1,
            "pattern": "^[-a-zA-Z0-9_]+$",
            "title": "Slug",
            "type": "string"
          }
        },
        "required": [
          "name",
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://netboxdemo.com/api/ipam/rirs/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**aggregate_count**",
            "**description**",
            "**id**",
            "**is_private**: IP space managed by this RIR is considered private",
            "**name**",
            "**slug**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
