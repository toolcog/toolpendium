{
  "name": "circuits_circuit-terminations_read",
  "description": "Call to super to allow for caching",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this circuit termination."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/circuits/circuit-terminations/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**cable**",
              {
                "$ul": [
                  "**id**",
                  "**label**",
                  "**url**"
                ]
              }
            ],
            [
              "**circuit**",
              {
                "$ul": [
                  "**cid**",
                  "**id**",
                  "**url**"
                ]
              }
            ],
            "**connected_endpoint**: ",
            "**connected_endpoint_type**",
            [
              "**connection_status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**description**",
            "**id**",
            "**port_speed**",
            "**pp_info**",
            [
              "**site**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**term_side**",
            "**upstream_speed**: Upstream speed, if different from port speed",
            "**xconnect_id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
