{
  "name": "dcim_connected-device_list",
  "description": "This endpoint allows a user to determine what device (if any) is connected to a given peer device and peer\ninterface. This is useful in a situation where a device boots with no configuration, but can detect its neighbors\nvia a protocol such as LLDP. Two query parameters must be included in the request:\n\n* `peer_device`: The name of the peer device\n* `peer_interface`: The name of the peer interface",
  "parameters": {
    "type": "object",
    "properties": {
      "peer_device": {
        "type": "string",
        "description": "The name of the peer device"
      },
      "peer_interface": {
        "type": "string",
        "description": "The name of the peer interface"
      }
    },
    "required": [
      "peer_device",
      "peer_interface"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/dcim/connected-device/{?peer_device,peer_interface}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**asset_tag**: A unique tag used to identify this device",
            [
              "**cluster**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**url**",
                  "**virtualmachine_count**"
                ]
              }
            ],
            "**comments**",
            "**created**",
            "**custom_fields** (default: [object Object])",
            [
              "**device_role**",
              {
                "$ul": [
                  "**device_count**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**",
                  "**virtualmachine_count**"
                ]
              }
            ],
            [
              "**device_type**",
              {
                "$ul": [
                  "**device_count**",
                  "**display_name**",
                  "**id**",
                  [
                    "**manufacturer**",
                    {
                      "$ul": [
                        "**devicetype_count**",
                        "**id**",
                        "**name**",
                        "**slug**",
                        "**url**"
                      ]
                    }
                  ],
                  "**model**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**display_name**",
            [
              "**face**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**id**",
            "**last_updated**",
            "**local_context_data**",
            "**name**",
            [
              "**parent_device**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            [
              "**platform**",
              {
                "$ul": [
                  "**device_count**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**",
                  "**virtualmachine_count**"
                ]
              }
            ],
            "**position**: The lowest-numbered unit occupied by the device",
            [
              "**primary_ip**",
              {
                "$ul": [
                  "**address**: IPv4 or IPv6 address (with mask)",
                  "**family**",
                  "**id**",
                  "**url**"
                ]
              }
            ],
            [
              "**primary_ip4**",
              {
                "$ul": [
                  "**address**: IPv4 or IPv6 address (with mask)",
                  "**family**",
                  "**id**",
                  "**url**"
                ]
              }
            ],
            [
              "**primary_ip6**",
              {
                "$ul": [
                  "**address**: IPv4 or IPv6 address (with mask)",
                  "**family**",
                  "**id**",
                  "**url**"
                ]
              }
            ],
            [
              "**rack**",
              {
                "$ul": [
                  "**device_count**",
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**serial**",
            [
              "**site**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            [
              "**status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            [
              "**tenant**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**vc_position**",
            "**vc_priority**",
            [
              "**virtual_chassis**",
              {
                "$ul": [
                  "**id**",
                  [
                    "**master**",
                    {
                      "$ul": [
                        "**display_name**",
                        "**id**",
                        "**name**",
                        "**url**"
                      ]
                    }
                  ],
                  "**member_count**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
