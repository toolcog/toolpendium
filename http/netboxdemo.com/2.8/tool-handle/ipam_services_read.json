{
  "name": "ipam_services_read",
  "description": "Call to super to allow for caching",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this service."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/ipam/services/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**description**",
            [
              "**device**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**id**",
            "**ipaddresses**",
            "**last_updated**",
            "**name**",
            "**port**",
            [
              "**protocol**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            [
              "**virtual_machine**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
