{
  "name": "ipam_vrfs_read",
  "description": "Call to super to allow for caching",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this VRF."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/ipam/vrfs/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**description**",
            "**display_name**",
            "**enforce_unique**: Prevent duplicate prefixes/IP addresses within this VRF",
            "**id**",
            "**ipaddress_count**",
            "**last_updated**",
            "**name**",
            "**prefix_count**",
            "**rd**: Unique route distinguisher (as defined in RFC 4364)",
            "**tags**",
            [
              "**tenant**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
