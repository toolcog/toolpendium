{
  "name": "dcim_device-types_read",
  "description": "Call to super to allow for caching",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this device type."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/dcim/device-types/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**comments**",
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**device_count**",
            "**display_name**",
            "**front_image**",
            "**id**",
            "**is_full_depth**: Device consumes both front and rear rack faces",
            "**last_updated**",
            [
              "**manufacturer**",
              {
                "$ul": [
                  "**devicetype_count**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**model**",
            "**part_number**: Discrete part number (optional)",
            "**rear_image**",
            "**slug**",
            [
              "**subdevice_role**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            "**u_height**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
