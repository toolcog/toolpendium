{
  "name": "ipam_prefixes_read",
  "description": "Call to super to allow for caching",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this prefix."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/ipam/prefixes/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**description**",
            [
              "**family**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**id**",
            "**is_pool**: All IP addresses within this prefix are considered usable",
            "**last_updated**",
            "**prefix**: IPv4 or IPv6 network with mask",
            [
              "**role**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**prefix_count**",
                  "**slug**",
                  "**url**",
                  "**vlan_count**"
                ]
              }
            ],
            [
              "**site**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            [
              "**status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            [
              "**tenant**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            [
              "**vlan**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**",
                  "**vid**"
                ]
              }
            ],
            [
              "**vrf**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**prefix_count**",
                  "**rd**: Unique route distinguisher (as defined in RFC 4364)",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
