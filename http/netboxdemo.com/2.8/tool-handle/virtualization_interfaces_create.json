              "ieee802.11ad",
              "ieee802.11ax",
              "gsm",
              "cdma",
              "lte",
              "sonet-oc3",
              "sonet-oc12",
              "sonet-oc48",
              "sonet-oc192",
              "sonet-oc768",
              "sonet-oc1920",
              "sonet-oc3840",
              "1gfc-sfp",
              "2gfc-sfp",
              "4gfc-sfp",
              "8gfc-sfpp",
              "16gfc-sfpp",
              "32gfc-sfp28",
              "128gfc-sfp28",
              "infiniband-sdr",
              "infiniband-ddr",
              "infiniband-qdr",
              "infiniband-fdr10",
              "infiniband-fdr",
              "infiniband-edr",
              "infiniband-hdr",
              "infiniband-ndr",
              "infiniband-xdr",
              "t1",
              "e1",
              "t3",
              "e3",
              "cisco-stackwise",
              "cisco-stackwise-plus",
