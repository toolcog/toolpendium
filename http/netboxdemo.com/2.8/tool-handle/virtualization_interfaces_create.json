{
  "name": "virtualization_interfaces_create",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/WritableVirtualMachineInterface"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "WritableVirtualMachineInterface": {
        "properties": {
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "enabled": {
            "title": "Enabled",
            "type": "boolean"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "mac_address": {
            "nullable": true,
            "title": "MAC Address",
            "type": "string"
          },
          "mode": {
            "enum": [
              "access",
              "tagged",
              "tagged-all"
            ],
            "title": "Mode",
            "type": "string"
          },
          "mtu": {
            "maximum": 65536,
            "minimum": 1,
            "nullable": true,
            "title": "MTU",
            "type": "integer"
          },
          "name": {
            "maxLength": 64,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "tagged_vlans": {
            "items": {
              "title": "Tagged VLANs",
              "type": "integer"
            },
            "type": "array",
            "uniqueItems": true
          },
          "tags": {
            "items": {
              "minLength": 1,
              "type": "string"
            },
            "type": "array"
          },
          "type": {
