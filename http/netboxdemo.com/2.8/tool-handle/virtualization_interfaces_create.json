  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://netboxdemo.com/api/virtualization/interfaces/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**description**",
            "**enabled**",
            "**id**",
            "**mac_address**",
            [
              "**mode**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**mtu**",
            "**name**",
            "**tagged_vlans**",
            "**tags**",
            [
              "**type** (default: [object Object])",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            [
              "**untagged_vlan**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**",
                  "**vid**"
                ]
              }
            ],
            [
              "**virtual_machine**",
              {
                "$ul": [
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
