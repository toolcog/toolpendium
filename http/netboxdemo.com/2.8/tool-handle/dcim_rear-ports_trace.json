{
  "name": "dcim_rear-ports_trace",
  "description": "Trace a complete cable path and return each segment as a three-tuple of (termination, cable, termination).",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this rear port."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/dcim/rear-ports/{id}/trace/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**cable**",
              {
                "$ul": [
                  "**id**",
                  "**label**",
                  "**url**"
                ]
              }
            ],
            "**description**",
            [
              "**device**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**id**",
            "**name**",
            "**positions**",
            "**tags**",
            [
              "**type**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
