{
  "name": "dcim_power-ports_read",
  "description": "Call to super to allow for caching",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this power port."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://netboxdemo.com/api/dcim/power-ports/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**allocated_draw**: Allocated power draw (watts)",
            [
              "**cable**",
              {
                "$ul": [
                  "**id**",
                  "**label**",
                  "**url**"
                ]
              }
            ],
            "**connected_endpoint**: ",
            "**connected_endpoint_type**",
            [
              "**connection_status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**description**",
            [
              "**device**",
              {
                "$ul": [
                  "**display_name**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**id**",
            "**maximum_draw**: Maximum power draw (watts)",
            "**name**",
            "**tags**",
            [
              "**type**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
