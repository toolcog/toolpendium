{
  "name": "client",
  "description": "Retrieve a list of clients.\n",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://connect.authentiq.io/client"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Client object",
        "**Key properties:**",
        {
          "$ul": [
            "**application_type**",
            "**client_id**",
            "**client_name**",
            "**client_uri**",
            "**contacts**",
            "**default_max_age**",
            "**default_scopes**",
            "**grant_types**",
            "**logo_uri**",
            "**policy_uri**",
            "**redirect_uris**",
            "**response_types**",
            "**tos_uri**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Error Response defined as in Section 5.2 of OAuth 2.0 [RFC6749].\n",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**error_description**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
