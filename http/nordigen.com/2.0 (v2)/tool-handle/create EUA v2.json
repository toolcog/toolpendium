{
  "name": "create_EUA_v2",
  "description": "API endpoints related to end-user agreements.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/EndUserAgreementRequest"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "EndUserAgreementRequest": {
        "description": "Represents an end-user agreement.",
        "properties": {
          "access_scope": {
            "default": [
              "balances",
              "details",
              "transactions"
            ],
            "description": "Array containing one or several values of ['balances', 'details', 'transactions']",
            "items": {
              "items": {
                "default": [
                  "balances",
                  "details",
                  "transactions"
                ]
              },
              "title": "Level of information to access (by default all)",
              "type": "array"
            },
            "title": "Level of information to access (by default all)",
            "type": "array"
          },
          "access_valid_for_days": {
            "default": 90,
            "description": "Number of days from acceptance that the access can be used.",
            "maximum": 90,
            "minimum": 1,
            "title": "Access Valid For (Days)",
            "type": "integer"
          },
          "institution_id": {
            "description": "an Institution ID for this EUA",
            "minLength": 1,
            "type": "string"
          },
          "max_historical_days": {
            "default": 90,
            "description": "Maximum number of days of transaction data to retrieve.",
            "maximum": 730,
            "minimum": 1,
            "title": "Maximum Historical Days",
            "type": "integer"
          }
        },
        "required": [
          "institution_id"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://ob.nordigen.com/api/v2/agreements/enduser/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an end-user agreement.",
        "**Key properties:**",
        {
          "$ul": [
            "**accepted**: The date & time at which the end user accepted the agreement.",
            "**access_scope**: Array containing one or several values of ['balances', 'details', 'transactions'] (default: balances,details,transactions)",
            "**access_valid_for_days**: Number of days from acceptance that the access can be used. (default: 90)",
            "**created**: The date & time at which the end user agreement was created.",
            "**id**: The ID of this End User Agreement, used to refer to this end user agreement in other API calls.",
            "**institution_id**: an Institution ID for this EUA",
            "**max_historical_days**: Maximum number of days of transaction data to retrieve. (default: 90)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
