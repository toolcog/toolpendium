  "name": "retrieve_all_requisitions",
  "description": "Retrieve all requisitions belonging to the company",
  "parameters": {
    "type": "object",
    "properties": {
      "limit": {
        "default": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Number of results to return per page."
      },
      "offset": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "The initial index from which to return the results."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ob.nordigen.com/api/v2/requisitions/{?limit,offset}"
