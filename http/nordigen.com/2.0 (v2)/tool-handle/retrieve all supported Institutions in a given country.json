{
  "name": "retrieve_all_supported_Institutions_in_a_given_country",
  "description": "List all available institutions",
  "parameters": {
    "type": "object",
    "properties": {
      "country": {
        "type": "string",
        "description": "ISO 3166 two-character country code"
      },
      "payments_enabled": {
        "type": "string",
        "description": "Boolean value, indicating if payments are enabled"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ob.nordigen.com/api/v2/institutions/{?country,payments_enabled}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Represents an Integration.",
        "**Key properties:**",
        {
          "$ul": [
            "**bic**",
            "**countries**",
            "**id**",
            "**logo**",
            "**name**",
            "**transaction_total_days** (default: 90)"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Unspecified response body",
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Unspecified response body",
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Unspecified response body",
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Unspecified response body",
        {
          "$lang": "json",
          "$code": {
