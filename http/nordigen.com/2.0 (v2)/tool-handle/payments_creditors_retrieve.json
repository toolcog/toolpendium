{
  "name": "payments_creditors_retrieve",
  "description": "API endpoints related to creditor accounts.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "Unique entry ID",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ob.nordigen.com/api/v2/payments/creditors/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Creditor account read serializer.",
        "**Key properties:**",
        {
          "$ul": [
            "**account**: Creditor account type identifier",
            "**address_country**: Creditor account address country",
            "**currency**: Creditor account currency",
            "**id**: Unique entry ID",
            "**name**: Creditor account name",
            "**type**: Creditor account type (default: IBAN)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
