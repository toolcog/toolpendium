{
  "name": "payments_creditors_list",
  "description": "API endpoints related to creditor accounts.",
  "parameters": {
    "type": "object",
    "properties": {
      "account": {
        "type": "string"
      },
      "address_country": {
        "type": "string"
      },
      "agent": {
        "type": "string"
      },
      "currency": {
        "type": "string"
      },
      "limit": {
        "default": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Number of results to return per page."
      },
      "name": {
        "type": "string"
      },
      "offset": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "The initial index from which to return the results."
      },
      "type": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ob.nordigen.com/api/v2/payments/creditors/{?account,address_country,agent,currency,limit,name,offset,type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**",
            "**next**",
            "**previous**",
            "**results**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
