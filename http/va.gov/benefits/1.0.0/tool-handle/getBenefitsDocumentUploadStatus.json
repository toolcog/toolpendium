{
  "name": "getBenefitsDocumentUploadStatus",
  "description": "Get status for a previous benefits document upload",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "ID as returned by a previous create upload request"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://sandbox-api.va.gov/services/vba_documents/v1/uploads/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**: Status record for a previously initiated document submission.",
              {
                "$ul": [
                  [
                    "**attributes**",
                    {
                      "$ul": [
                        "**code**: Unambiguous status code. Only present if status = \"error\"",
                        "**detail**: Human readable error detail. Only present if status = \"error\"",
                        "**guid**: The document upload identifier",
                        "**message**: Human readable error description. Only present if status = \"error\"",
                        "**status**: Document upload status.",
                        "**updated_at**: The last time the submission was updated",
                        [
                          "**uploaded_pdf**",
                          {
                            "$ul": [
                              [
                                "**content**",
                                {
                                  "$ul": [
                                    "**attachments**",
                                    [
                                      "**dimensions**",
                                      {
                                        "$ul": [
                                          "**height**: The document height",
                                          "**oversized_pdf**: Indicates if this is an oversized PDF (greater than 21x21)",
                                          "**width**: The document width"
                                        ]
                                      }
                                    ],
                                    "**page_count**: The total number of pages solely in this PDF document"
                                  ]
                                }
                              ],
                              "**total_documents**: The total number of documents contained in this upload",
                              "**total_pages**: The total number of pages contained in this upload"
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  "**id**: JSON API identifier",
                  "**type**: JSON API type specification"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
