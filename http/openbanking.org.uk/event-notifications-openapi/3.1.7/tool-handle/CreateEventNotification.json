{
  "name": "CreateEventNotification",
  "description": "Send an event notification",
  "parameters": {
    "type": "object",
    "properties": {
      "x-fapi-financial-id": {
        "type": "string",
        "description": "The unique id of the ASPSP to which the request is issued. The unique id will be issued by OB."
      },
      "x-fapi-interaction-id": {
        "type": "string",
        "description": "An RFC4122 UID used as a correlation id."
      }
    },
    "required": [
      "x-fapi-financial-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://openbanking.org.uk/event-notifications"
    },
    "headers": {
      "x-fapi-financial-id": {
        "$": "x-fapi-financial-id"
      },
      "x-fapi-interaction-id": {
        "$": "x-fapi-interaction-id"
      }
    }
  },
  "responses": {}
}
