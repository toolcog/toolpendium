{
  "name": "list_shipment_rates",
  "description": "Get Rates for the shipment information associated with the shipment ID",
  "parameters": {
    "type": "object",
    "properties": {
      "created_at_start": {
        "format": "date-time",
        "type": "string",
        "description": "Used to create a filter for when a resource was created (ex. A shipment that was created after a certain time)"
      },
      "shipment_id": {
        "$ref": "#/components/schemas/se_id",
        "description": "Shipment ID"
      }
    },
    "required": [
      "shipment_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shipengine.com/v1/shipments/{shipment_id}/rates{?created_at_start}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "rates_information"
        },
        "A rates information resource",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**: When the rate was created",
            "**errors**",
            "**invalid_rates**: An array of invalid shipment rates (default: )",
            "**rate_request_id**: A string that uniquely identifies the rate request",
            "**rates**: An array of shipment rates",
            "**shipment_id**: A string that uniquely identifies the shipment",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
