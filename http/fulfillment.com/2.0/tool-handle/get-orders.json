{
  "name": "get-orders",
  "description": "Retrieve many orders at once",
  "parameters": {
    "type": "object",
    "properties": {
      "fromDate": {
        "type": "string",
        "description": "Date-time in ISO 8601 format for selecting orders after, or at, the specified time"
      },
      "toDate": {
        "type": "string",
        "description": "Date-time in ISO 8601 format for selecting orders before, or at, the specified time"
      },
      "merchantIds": {
        "items": {
          "type": "integer"
        },
        "type": "array",
        "description": "A CSV of merchant id, '123' or '1,2,3'"
      },
      "warehouseIds": {
        "items": {
          "type": "integer"
        },
        "type": "array",
        "description": "A CSV of warehouse id, '123' or '1,2,3'"
      },
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "A multiplier of the number of items (limit parameter) to skip before returning results"
      },
      "limit": {
        "default": 80,
        "maximum": 200,
        "minimum": 1,
        "type": "integer",
        "description": "The numbers of items to return"
      },
      "hydrate": {
        "items": {
          "enum": [
            "integrator",
            "lineItems",
            "trackingNumbers.carrier"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "Adds additional information to the response, uses a CSV format for multiple values.'"
      }
    },
    "required": [
      "fromDate",
      "toDate"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fulfillment.com/v2/orders{?fromDate,toDate,merchantIds,warehouseIds,page,limit,hydrate}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ErrorStandard.v2"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
