{
  "name": "get-track",
  "description": "Get uniformed tracking events for any package, this response is carrier independent. Please note, an API Key is required for throttling purposes, please contact your success manager for details.",
  "parameters": {
    "type": "object",
    "properties": {
      "trackingNumber": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fulfillment.com/v2/track{?trackingNumber}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "trackingResponse"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**destination**: GeoJSon Feature",
              {
                "$ul": [
                  "**bbox**",
                  [
                    "**centerline**: GeoJSon geometry",
                    {
                      "$ul": [
                        "**coordinates**",
                        "**type**: the geometry type"
                      ]
                    }
                  ],
                  [
                    "**geometry**: GeoJSon geometry",
                    {
                      "$ul": [
                        "**coordinates**",
                        "**type**: the geometry type"
                      ]
                    }
                  ],
                  "**id**",
                  [
                    "**properties**",
                    {
                      "$ul": [
                        "**name**"
                      ]
                    }
                  ],
                  "**title**",
                  "**type**"
                ]
              }
            ],
            "**fdcOrderId**",
            "**firstCheckedDateTime**",
            "**firstTransitEvent**",
            "**lastCheckedDateTime**",
            "**lastUpdatedDateTime**",
            [
              "**origin**: GeoJSon Feature",
              {
                "$ul": [
                  "**bbox**",
                  [
                    "**centerline**: GeoJSon geometry",
                    {
                      "$ul": [
                        "**coordinates**",
                        "**type**: the geometry type"
                      ]
                    }
                  ],
                  [
                    "**geometry**: GeoJSon geometry",
                    {
                      "$ul": [
                        "**coordinates**",
                        "**type**: the geometry type"
                      ]
                    }
                  ],
                  "**id**",
                  [
                    "**properties**",
                    {
                      "$ul": [
                        "**name**"
                      ]
                    }
                  ],
                  "**title**",
                  "**type**"
                ]
              }
            ],
            "**status**",
            "**statusCategoryCode**",
            "**statusDateTime**",
            "**statusMessage**",
            "**trackedEvents**",
            [
              "**trackingNumber**",
              {
                "$ul": [
                  "**barcodeScanValue**: The computer readable alternative to the human readable",
                  [
                    "**carrier**",
                    {
                      "$ul": [
                        "**id**"
                      ]
                    }
                  ],
                  "**value**: The human readable tracking number recipients (consignee) expect"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
