{
  "name": "Seat_Details",
  "description": "A description of all available seat details by aircraft type. You can retrieve the full set or details for a particular aircraft type.",
  "parameters": {
    "type": "object",
    "properties": {
      "aircraftCode": {
        "type": "string",
        "description": "Aircraft type. 3-character IATA equipment code"
      },
      "Accept": {
        "type": "string",
        "description": "http header: application/json or application/xml (Acceptable values are: \"application/json\", \"application/xml\")"
      },
      "cabinCode": {
        "default": "C",
        "type": "string",
        "description": "Cabin class: M, E, C, F (Acceptable values are: \"\", \"M\", \"E\", \"C\", \"F\")"
      },
      "lang": {
        "type": "string",
        "description": "2-letter ISO 3166-1 language code"
      }
    },
    "required": [
      "aircraftCode",
      "Accept",
      "cabinCode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.lufthansa.com/v1/references/seatdetails/{aircraftCode}/{cabinCode}{?lang}"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
