{
  "name": "ReferencesAirlinesByAirlineCodeGet",
  "description": "List all airlines or one specific airline.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept": {
        "type": "string",
        "description": "http header: application/json or application/xml (Acceptable values are: \"application/json\", \"application/xml\")"
      },
      "airlineCode": {
        "default": "LH",
        "type": "string",
        "description": "2-character IATA airline/carrier code"
      },
      "limit": {
        "default": "20",
        "type": "string",
        "description": "Number of records returned per request. Defaults to 20, maximum is 100 (if a value bigger than 100 is given, 100 will be taken)"
      },
      "offset": {
        "default": "0",
        "type": "string",
        "description": "Number of records skipped. Defaults to 0"
      }
    },
    "required": [
      "Accept",
      "airlineCode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.lufthansa.com/v1/references/airlines/{airlineCode}{?limit,offset}"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
