{
  "name": "get-zipcodes",
  "description": "Returns a list of zip codes. Results are always paginated. Visit the [Zip Code Data API](https://www.metadapi.com/API-Products/API-Product-Details/zip-code-api) product page for information on how to get an API key.",
  "parameters": {
    "type": "object",
    "properties": {
      "offset": {
        "example": 10,
        "minimum": 0,
        "type": "number",
        "description": "Offset is the position in the dataset to start retrieval of records."
      },
      "limit": {
        "example": 10,
        "maximum": 50,
        "minimum": 1,
        "type": "number",
        "description": "Number of records to return in each page."
      },
      "zipcode": {
        "example": "90210,33162",
        "type": "string",
        "description": "5 Digit Zip Code query parameter. Can have multiple values (separated by comma)."
      },
      "uspsMainCityKey": {
        "example": "Z20259",
        "type": "string",
        "description": "Parameter for USPS City / Town key identifier for the main city of the zip code."
      },
      "zipClassificationCode": {
        "example": "P",
        "maxLength": 1,
        "minLength": 1,
        "type": "string",
        "description": "Parameter for zipClassificationCode"
      },
      "uspsFacilityCode": {
        "example": "B",
        "type": "string",
        "description": "Parameter for facility code."
      },
      "uspsDeliveryCode": {
        "type": "string",
        "description": "Parameter for delivery code."
      },
      "uspsCarrierRouteSortCode": {
        "type": "string",
        "description": "Parameter for carrier route sort code."
      },
      "uniqueZipNameInd": {
        "type": "boolean",
        "description": "Parameter for unique zip name indicator."
      },
      "uspsFinanceNumber": {
        "type": "string",
        "description": "Parameter for finance number."
      },
      "stateCode": {
        "example": "CA",
        "maxLength": 2,
        "minLength": 2,
        "type": "string",
        "description": "Parameter for state code."
      },
      "stateFipsCode": {
        "example": "06",
        "type": "string",
        "description": "Parameter for State FIPS code."
      },
      "countyFipsCode": {
        "example": "037",
        "type": "string",
        "description": "Parameter for county FIPS code."
      },
      "divisionCode": {
        "example": "9",
        "type": "string",
        "description": "Parameter for division code. "
      },
      "regionCode": {
        "example": "4",
        "type": "string",
        "description": "Parameter for region code. "
      },
      "msaCode": {
        "example": "31080",
        "type": "string",
        "description": "Parameter for msaCode."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://global.metadapi.com/zipc/v1/zipcodes{?offset,limit,zipcode,uspsMainCityKey,zipClassificationCode,uspsFacilityCode,uspsDeliveryCode,uspsCarrierRouteSortCode,uniqueZipNameInd,uspsFinanceNumber,stateCode,stateFipsCode,countyFipsCode,divisionCode,regionCode,msaCode}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ZipCodeResponse"
        },
        "Succesful response of the ",
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**links**: API Response Links",
              {
                "$ul": [
                  "**first**: Link to the first group of records from API call.",
                  "**last**: Link to last set of records from API call. ",
                  "**next**: Link to next set of results from API call.",
                  "**prev**: Link to the previous set of records from API call.",
                  "**self**: Link to this response."
                ]
              }
            ],
            [
              "**meta**: Response Metadata. Provides additional context about the data response.",
              {
                "$ul": [
                  "**count**: Total number of records of the response. Note: the data object always has a limit number of records retrieved.",
                  "**limit**: Total number of records returned in the data array. ",
                  "**offset**: Offset position of where the first record returned."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
