{
  "name": "get-msagroups",
  "description": "This end point lists all the Metropolitan and Micropolitan Statistical Areas in the United States with the corresponding states and counties that make up the group. ",
  "parameters": {
    "type": "object",
    "properties": {
      "limit": {
        "maximum": 50,
        "minimum": 1,
        "type": "integer",
        "description": "Number of records to return in each page. Max value: 50."
      },
      "offset": {
        "minimum": 0,
        "type": "integer",
        "description": "Offset is the position in the dataset to start retrieval of records."
      },
      "stateCode": {
        "example": "CA",
        "maxLength": 2,
        "minLength": 2,
        "type": "string",
        "description": "Parameter for state code."
      }
    },
    "required": [
      "limit",
      "offset"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://global.metadapi.com/zipc/v1/msagroups{?limit,offset,stateCode}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Response",
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**links**: Response Link Navigation object",
              {
                "$ul": [
                  "**first**: Link to the first group of records from API call.",
                  "**next**: Link to next set of results from API call.",
                  "**prev**: Link to the previous set of records from API call.",
                  "**self**: Link to this response."
                ]
              }
            ],
            [
              "**meta**: Response Metadata Information",
              {
                "$ul": [
                  "**count**: Total number of records of the response. Note: the data object always has a limit number of records retrieved.",
                  "**limit**: Total number of records returned in the data array. Specified by limit query parameter.",
                  "**offset**: Offset position of where the first record returned."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of Errors generated by the request.",
        "**Key properties:**",
        {
          "$ul": [
            "**errors**: Errors Array"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
