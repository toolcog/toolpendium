{
  "name": "get-distance",
  "description": "Gets the distance (in miles and kilometers) between 2 zip codes passed as parameters. There are 2 mandatory query parameters (zipCode1 and zipCode2). ",
  "parameters": {
    "type": "object",
    "properties": {
      "zipCode1": {
        "example": "90210",
        "type": "string",
        "description": "Zip Code 1"
      },
      "zipCode2": {
        "example": "33162",
        "type": "string",
        "description": "Zip Code 2"
      }
    },
    "required": [
      "zipCode1",
      "zipCode2"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://global.metadapi.com/zipc/v1/distance{?zipCode1,zipCode2}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**distanceKm**",
                  "**distanceMi**",
                  "**zipCode1**",
                  "**zipCode2**"
                ]
              }
            ],
            [
              "**links**",
              {
                "$ul": [
                  "**self**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of Errors generated by the request.",
        "**Key properties:**",
        {
          "$ul": [
            "**errors**: Errors Array"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
