{
  "name": "get-msagroups-msacode",
  "description": "Gets the details of a single Metropolitan Statistical Area code passed as a path parameter.",
  "parameters": {
    "type": "object",
    "properties": {
      "msaCode": {
        "example": "11580",
        "maxLength": 5,
        "minLength": 5,
        "type": "string",
        "description": "5 digit MSA (Metropolitan Statistical Area) code."
      }
    },
    "required": [
      "msaCode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://global.metadapi.com/zipc/v1/msagroups/{msaCode}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**: Response Data array.",
              {
                "$ul": [
                  "**msaCode**: 5 digit MSA (Metropolitan Statistical Area) code.",
                  "**msaName**: Name of the defined MSA code.",
                  "**states**: Array with states and counties that make up MSA Group"
                ]
              }
            ],
            [
              "**links**: Response Link Navigation object",
              {
                "$ul": [
                  "**self**: Link to this response."
                ]
              }
            ],
            [
              "**meta**: Response Metadata Information",
              {
                "$ul": [
                  "**count**: Total number of records of the response. Note: the data object always has a limit number of records retrieved."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
