{
  "name": "getEmailLists",
  "description": "List all email groups for authenticated user",
  "parameters": {
    "type": "object",
    "properties": {
      "ev-api-key": {
        "type": "string",
        "description": "API Key required to make the API call."
      },
      "ev-access-token": {
        "type": "string",
        "description": "Access token required to make the API call."
      },
      "include": {
        "type": "string",
        "description": "Related record types to include in the response. Valid option is `ownerUser`"
      }
    },
    "required": [
      "ev-api-key",
      "ev-access-token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://accountname.exavault.com/api/v2/email-lists{?include}"
    },
    "headers": {
      "ev-api-key": {
        "$": "ev-api-key"
      },
      "ev-access-token": {
        "$": "ev-access-token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "EmailListCollectionResponse"
        },
        "Response object for list of email lists",
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**included**",
            "**responseStatus**: Http status of response",
            "**returnedResults**",
            "**totalResults**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
