{
  "name": "getFormEntries",
  "description": "Returns the form data entries for a specific form for a receive. Optional parameters can be included in the call to manage larger data sets.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "ev-api-key": {
        "type": "string",
        "description": "API Key required to make the API call. "
      },
      "ev-access-token": {
        "type": "string",
        "description": "Access token required to make the API call."
      },
      "id": {
        "type": "integer",
        "description": "ID of the form to retrieve entries for."
      },
      "limit": {
        "type": "integer",
        "description": "Limit of records to be returned (for pagination)"
      },
      "offset": {
        "type": "integer",
        "description": "Current offset of records (for pagination)"
      }
    },
    "required": [
      "ev-api-key",
      "ev-access-token",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://accountname.exavault.com/api/v2/forms/entries/{id}{?limit,offset}"
    },
    "headers": {
      "ev-api-key": {
        "$": "ev-api-key"
      },
      "ev-access-token": {
        "$": "ev-access-token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "FormEntryResponse"
        },
        "Response object of the form data.",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Object submissions data for form.",
            "**responseStatus**: Http status code of the response. ",
            "**returnedResults**: Number of returned results. ",
            "**totalResults**: Total results found. "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
