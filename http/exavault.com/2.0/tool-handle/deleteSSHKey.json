{
  "name": "deleteSSHKey",
  "description": "Delete the specified SSH key. This will not delete or deactivate the user tied to the key.",
  "parameters": {
    "type": "object",
    "properties": {
      "ev-api-key": {
        "type": "string",
        "description": "API key required to make the API call."
      },
      "ev-access-token": {
        "type": "string",
        "description": "Access token required to make the API call."
      },
      "id": {
        "type": "string"
      }
    },
    "required": [
      "ev-api-key",
      "ev-access-token",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://accountname.exavault.com/api/v2/ssh-keys/{id}"
    },
    "headers": {
      "ev-api-key": {
        "$": "ev-api-key"
      },
      "ev-access-token": {
        "$": "ev-access-token"
      }
    }
  },
  "responses": {}
}
