{
  "name": "ListLicensees",
  "description": "Returns a list of licensees for a key.",
  "parameters": {
    "type": "object",
    "properties": {
      "key": {
        "$ref": "#/$defs/ApiKeyParam"
      },
      "starting_after": {
        "format": "int32",
        "type": "integer",
        "description": "Specify ID of the licensee after which you would like to list results"
      },
      "user_token": {
        "$ref": "#/$defs/UserTokenParam"
      },
      "limit": {
        "$ref": "#/components/schemas/LimitParam",
        "description": "Specify the maximum number of results to return per page. Default and maximum is `100`."
      },
      "query": {
        "type": "string",
        "description": "Filter result by licensee name. Query can be shortened to `q=`"
      }
    },
    "required": [
      "key"
    ],
    "$defs": {
      "ApiKeyParam": {
        "description": "Your API Key. Typically beings `ak_`.\n\nAvailable from your dashboard\n",
        "example": "ak_test",
        "title": "API Key",
        "type": "string"
      },
      "UserTokenParam": {
        "description": "A secret key used for sensitive operations on your account and API Keys.\n\nYour user token can be retrieved and managed from your [accounts page](https://ideal-postcodes.co.uk/account).\n\nTypically beings `uk_...`\n",
        "example": "uk_B59ScW1p1HHouf1VqclEPZUx",
        "title": "User Token",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ideal-postcodes.co.uk/v1/keys/{key}/licensees{?starting_after,user_token,limit,query}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Licensee List Response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**",
            [
              "**result**: List of licensees",
              {
                "$ul": [
                  "**hasMore**: Returns true if there are more licensees listed after the maximum number of results as implied by `limit`",
                  "**licensees**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Basic Error Response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: `400X` type error response code",
            "**message**: Bad request error description",
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
