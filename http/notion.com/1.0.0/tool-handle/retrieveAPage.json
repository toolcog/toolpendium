{
  "name": "retrieveAPage",
  "description": "Retrieves a Page object using the ID in the request path. This endpoint exposes page properties, not page content. ",
  "parameters": {
    "type": "object",
    "properties": {
      "Notion-Version": {
        "example": "{{NOTION_VERSION}}",
        "type": "string"
      },
      "": {
        "example": "",
        "type": "string"
      },
      "id": {
        "example": "{{PAGE_ID}}",
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.notion.com/v1/pages/{id}"
    },
    "headers": {
      "Notion-Version": {
        "$": "Notion-Version"
      },
      "": {
        "$": ""
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**archived**",
            "**cover**",
            [
              "**created_by**",
              {
                "$ul": [
                  "**id**",
                  "**object**"
                ]
              }
            ],
            "**created_time**",
            [
              "**icon**",
              {
                "$ul": [
                  "**emoji**",
                  "**type**"
                ]
              }
            ],
            "**id**",
            [
              "**last_edited_by**",
              {
                "$ul": [
                  "**id**",
                  "**object**"
                ]
              }
            ],
            "**last_edited_time**",
            "**object**",
            [
              "**parent**",
              {
                "$ul": [
                  "**page_id**",
                  "**type**"
                ]
              }
            ],
            [
              "**properties**",
              {
                "$ul": [
                  [
                    "**title**",
                    {
                      "$ul": [
                        "**id**",
                        "**title**",
                        "**type**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
