{
  "name": "retrieveBlockChildren",
  "description": "Retrieve block children",
  "parameters": {
    "type": "object",
    "properties": {
      "page_size": {
        "example": "100",
        "type": "string"
      },
      "Notion-Version": {
        "example": "{{NOTION_VERSION}}",
        "type": "string"
      },
      "id": {
        "example": "{{PAGE_ID}}",
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.notion.com/v1/blocks/{id}/children{?page_size}"
    },
    "headers": {
      "Notion-Version": {
        "$": "Notion-Version"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**has_more**",
            "**next_cursor**",
            "**object**",
            "**results**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
