{
  "name": "GroupV2_IndividualGroupInvite",
  "description": "Invite a user to join this group.",
  "parameters": {
    "type": "object",
    "properties": {
      "groupId": {
        "format": "int64",
        "type": "integer",
        "description": "ID of the group you would like to join."
      },
      "membershipId": {
        "format": "int64",
        "type": "integer",
        "description": "Membership id of the account being invited."
      },
      "membershipType": {
        "format": "int32",
        "type": "integer",
        "description": "MembershipType of the account being invited."
      }
    },
    "required": [
      "groupId",
      "membershipId",
      "membershipType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.bungie.net/Platform/GroupV2/{groupId}/Members/IndividualInvite/{membershipType}/{membershipId}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            [
              "**Response**",
              {
                "$ul": [
                  "**resolution**"
                ]
              }
            ],
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
