{
  "name": "Fireteam_GetMyClanFireteams",
  "description": "Gets a listing of all fireteams that caller is an applicant, a member, or an alternate of.",
  "parameters": {
    "type": "object",
    "properties": {
      "groupFilter": {
        "type": "boolean",
        "description": "If true, filter by clan. Otherwise, ignore the clan and show all of the user's fireteams."
      },
      "groupId": {
        "format": "int64",
        "type": "integer",
        "description": "The group id of the clan. (This parameter is ignored unless the optional query parameter groupFilter is true)."
      },
      "includeClosed": {
        "type": "boolean",
        "description": "If true, return fireteams that have been closed."
      },
      "langFilter": {
        "type": "string",
        "description": "An optional language filter."
      },
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "Deprecated parameter, ignored."
      },
      "platform": {
        "format": "uint8",
        "type": "integer",
        "description": "The platform filter."
      }
    },
    "required": [
      "groupId",
      "includeClosed",
      "page",
      "platform"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Fireteam/Clan/{groupId}/My/{platform}/{includeClosed}/{page}/{?groupFilter,langFilter}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            [
              "**Response**",
              {
                "$ul": [
                  "**hasMore**",
                  [
                    "**query**",
                    {
                      "$ul": [
                        "**currentPage**",
                        "**itemsPerPage**",
                        "**requestContinuationToken**"
                      ]
                    }
                  ],
                  "**replacementContinuationToken**",
                  "**results**",
                  "**totalResults**",
                  "**useTotalResults**: If useTotalResults is true, then totalResults represents an accurate count.\r"
                ]
              }
            ],
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
