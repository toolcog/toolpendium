{
  "name": "Social_IssueFriendRequest",
  "description": "Requests a friend relationship with the target user. Any of the target user's linked membership ids are valid inputs.",
  "parameters": {
    "type": "object",
    "properties": {
      "membershipId": {
        "type": "string",
        "description": "The membership id of the user you wish to add."
      }
    },
    "required": [
      "membershipId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Social/Friends/Add/{membershipId}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
