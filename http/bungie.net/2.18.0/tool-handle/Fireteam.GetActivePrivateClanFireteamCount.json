{
  "name": "Fireteam_GetActivePrivateClanFireteamCount",
  "description": "Gets a count of all active non-public fireteams for the specified clan. Maximum value returned is 25.",
  "parameters": {
    "type": "object",
    "properties": {
      "groupId": {
        "format": "int64",
        "type": "integer",
        "description": "The group id of the clan."
      }
    },
    "required": [
      "groupId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Fireteam/Clan/{groupId}/ActiveCount/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
