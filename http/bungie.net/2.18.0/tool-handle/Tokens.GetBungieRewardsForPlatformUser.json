{
  "name": "Tokens_GetBungieRewardsForPlatformUser",
  "description": "Returns the bungie rewards for the targeted user when a platform membership Id and Type are used.",
  "parameters": {
    "type": "object",
    "properties": {
      "membershipId": {
        "format": "int64",
        "type": "integer",
        "description": "users platform membershipId for requested user rewards. If not self, elevated permissions are required."
      },
      "membershipType": {
        "format": "int32",
        "type": "integer",
        "description": "The target Destiny 2 membership type."
      }
    },
    "required": [
      "membershipId",
      "membershipType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Tokens/Rewards/GetRewardsForPlatformUser/{membershipId}/{membershipType}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
