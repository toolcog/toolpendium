{
  "name": "GroupV2_BanMember",
  "description": "Bans the requested member from the requested group for the specified period of time.",
  "parameters": {
    "type": "object",
    "properties": {
      "groupId": {
        "format": "int64",
        "type": "integer",
        "description": "Group ID that has the member to ban."
      },
      "membershipId": {
        "format": "int64",
        "type": "integer",
        "description": "Membership ID of the member to ban from the group."
      },
      "membershipType": {
        "format": "int32",
        "type": "integer",
        "description": "Membership type of the provided membership ID."
      }
    },
    "required": [
      "groupId",
      "membershipId",
      "membershipType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.bungie.net/Platform/GroupV2/{groupId}/Members/{membershipType}/{membershipId}/Ban/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
