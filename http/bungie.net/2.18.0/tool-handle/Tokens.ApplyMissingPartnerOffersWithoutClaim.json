{
  "name": "Tokens_ApplyMissingPartnerOffersWithoutClaim",
  "description": "Apply a partner offer to the targeted user. This endpoint does not claim a new offer, but any already claimed offers will be applied to the game if not already.",
  "parameters": {
    "type": "object",
    "properties": {
      "partnerApplicationId": {
        "format": "int32",
        "type": "integer",
        "description": "The partner application identifier."
      },
      "targetBnetMembershipId": {
        "format": "int64",
        "type": "integer",
        "description": "The bungie.net user to apply missing offers to. If not self, elevated permissions are required."
      }
    },
    "required": [
      "partnerApplicationId",
      "targetBnetMembershipId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Tokens/Partner/ApplyMissingOffers/{partnerApplicationId}/{targetBnetMembershipId}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
