{
  "name": "Forum_GetForumTagSuggestions",
  "description": "Gets tag suggestions based on partial text entry, matching them with other tags previously used in the forums.",
  "parameters": {
    "type": "object",
    "properties": {
      "partialtag": {
        "type": "string",
        "description": "The partial tag input to generate suggestions from."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Forum/GetForumTagSuggestions/{?partialtag}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
