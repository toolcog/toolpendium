{
  "name": "GroupV2_AbdicateFoundership",
  "description": "An administrative method to allow the founder of a group or clan to give up their position to another admin permanently.",
  "parameters": {
    "type": "object",
    "properties": {
      "founderIdNew": {
        "format": "int64",
        "type": "integer",
        "description": "The new founder for this group. Must already be a group admin."
      },
      "groupId": {
        "format": "int64",
        "type": "integer",
        "description": "The target group id."
      },
      "membershipType": {
        "format": "int32",
        "type": "integer",
        "description": "Membership type of the provided founderIdNew."
      }
    },
    "required": [
      "founderIdNew",
      "groupId",
      "membershipType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.bungie.net/Platform/GroupV2/{groupId}/Admin/AbdicateFoundership/{membershipType}/{founderIdNew}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
