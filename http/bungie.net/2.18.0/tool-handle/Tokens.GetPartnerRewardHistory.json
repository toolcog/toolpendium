{
  "name": "Tokens_GetPartnerRewardHistory",
  "description": "Returns the partner rewards history of the targeted user, both partner offers and Twitch drops.",
  "parameters": {
    "type": "object",
    "properties": {
      "partnerApplicationId": {
        "format": "int32",
        "type": "integer",
        "description": "The partner application identifier."
      },
      "targetBnetMembershipId": {
        "format": "int64",
        "type": "integer",
        "description": "The bungie.net user to return reward history for."
      }
    },
    "required": [
      "partnerApplicationId",
      "targetBnetMembershipId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Tokens/Partner/History/{targetBnetMembershipId}/Application/{partnerApplicationId}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            [
              "**Response**",
              {
                "$ul": [
                  "**PartnerOffers**",
                  "**TwitchDrops**"
                ]
              }
            ],
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
