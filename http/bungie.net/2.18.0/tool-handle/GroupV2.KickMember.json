{
  "name": "GroupV2_KickMember",
  "description": "Kick a member from the given group, forcing them to reapply if they wish to re-join the group. You must have suitable permissions in the group to perform this operation.",
  "parameters": {
    "type": "object",
    "properties": {
      "groupId": {
        "format": "int64",
        "type": "integer",
        "description": "Group ID to kick the user from."
      },
      "membershipId": {
        "format": "int64",
        "type": "integer",
        "description": "Membership ID to kick."
      },
      "membershipType": {
        "format": "int32",
        "type": "integer",
        "description": "Membership type of the provided membership ID."
      }
    },
    "required": [
      "groupId",
      "membershipId",
      "membershipType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.bungie.net/Platform/GroupV2/{groupId}/Members/{membershipType}/{membershipId}/Kick/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            [
              "**Response**",
              {
                "$ul": [
                  [
                    "**group**",
                    {
                      "$ul": [
                        "**about**",
                        "**allowChat**",
                        "**avatarImageIndex**",
                        "**avatarPath**",
                        "**banExpireDate**",
                        "**bannerPath**",
                        "**chatSecurity**",
                        [
                          "**clanInfo**: The same as GroupV2ClanInfo, but includes any investment data.",
                          {
                            "$ul": [
                              [
                                "**clanBannerData**",
                                {
                                  "$ul": [
                                    "**decalBackgroundColorId**",
                                    "**decalColorId**",
                                    "**decalId**",
                                    "**gonfalonColorId**",
                                    "**gonfalonDetailColorId**",
                                    "**gonfalonDetailId**",
                                    "**gonfalonId**"
                                  ]
                                }
                              ],
                              "**clanCallsign**",
                              "**d2ClanProgressions**"
                            ]
                          }
                        ],
                        "**conversationId**",
                        "**creationDate**",
                        "**defaultPublicity**",
                        "**enableInvitationMessagingForAdmins**",
                        [
                          "**features**",
                          {
                            "$ul": [
                              "**capabilities**",
                              "**hostGuidedGamePermissionOverride**: Minimum Member Level allowed to host guided games\r",
                              "**invitePermissionOverride**: Minimum Member Level allowed to invite new members to group\r",
                              "**joinLevel**: Level to join a member at when accepting an invite, application, or joining an open clan\r",
                              "**maximumMembers**",
                              "**maximumMembershipsOfGroupType**: Maximum number of groups of this type a typical membership may join. For example, a user may join about 50 General groups with their Bungie.net account. They may join one clan per Destiny membership.",
                              "**membershipTypes**",
                              "**updateBannerPermissionOverride**: Minimum Member Level allowed to update banner\r",
                              "**updateCulturePermissionOverride**: Minimum Member Level allowed to update group culture\r"
                            ]
                          }
                        ],
                        "**groupId**",
                        "**groupType**",
                        "**homepage**",
                        "**isDefaultPostPublic**",
                        "**isPublic**",
                        "**isPublicTopicAdminOnly**",
                        "**locale**",
                        "**memberCount**",
                        "**membershipIdCreated**",
                        "**membershipOption**",
                        "**modificationDate**",
                        "**motto**",
                        "**name**",
                        "**tags**",
                        "**theme**"
                      ]
                    }
                  ],
                  "**groupDeleted**"
                ]
              }
            ],
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
