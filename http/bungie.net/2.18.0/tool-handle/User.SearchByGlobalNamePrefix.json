{
  "name": "User_SearchByGlobalNamePrefix",
  "description": "[OBSOLETE] Do not use this to search users, use SearchByGlobalNamePost instead.",
  "parameters": {
    "type": "object",
    "properties": {
      "displayNamePrefix": {
        "type": "string",
        "description": "The display name prefix you're looking for."
      },
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "The zero-based page of results you desire."
      }
    },
    "required": [
      "displayNamePrefix",
      "page"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/User/Search/Prefix/{displayNamePrefix}/{page}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            [
              "**Response**",
              {
                "$ul": [
                  "**hasMore**",
                  "**page**",
                  "**searchResults**"
                ]
              }
            ],
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
