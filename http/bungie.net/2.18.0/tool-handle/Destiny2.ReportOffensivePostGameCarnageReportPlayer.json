{
  "name": "Destiny2_ReportOffensivePostGameCarnageReportPlayer",
  "description": "Report a player that you met in an activity that was engaging in ToS-violating activities. Both you and the offending player must have played in the activityId passed in. Please use this judiciously and only when you have strong suspicions of violation, pretty please.",
  "parameters": {
    "type": "object",
    "properties": {
      "activityId": {
        "format": "int64",
        "type": "integer",
        "description": "The ID of the activity where you ran into the brigand that you're reporting."
      }
    },
    "required": [
      "activityId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Destiny2/Stats/PostGameCarnageReport/{activityId}/Report/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
