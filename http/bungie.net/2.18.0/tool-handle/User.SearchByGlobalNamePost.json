{
  "name": "User_SearchByGlobalNamePost",
  "description": "Given the prefix of a global display name, returns all users who share that name.",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "The zero-based page of results you desire."
      }
    },
    "required": [
      "page"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.bungie.net/Platform/User/Search/GlobalName/{page}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            [
              "**Response**",
              {
                "$ul": [
                  "**hasMore**",
                  "**page**",
                  "**searchResults**"
                ]
              }
            ],
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
