{
  "name": "GroupV2_GetGroupOptionalConversations",
  "description": "Gets a list of available optional conversation channels and their settings.",
  "parameters": {
    "type": "object",
    "properties": {
      "groupId": {
        "format": "int64",
        "type": "integer",
        "description": "Requested group's id."
      }
    },
    "required": [
      "groupId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/GroupV2/{groupId}/OptionalConversations/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            "**Response**",
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
