{
  "name": "Fireteam_GetClanFireteam",
  "description": "Gets a specific fireteam.",
  "parameters": {
    "type": "object",
    "properties": {
      "fireteamId": {
        "format": "int64",
        "type": "integer",
        "description": "The unique id of the fireteam."
      },
      "groupId": {
        "format": "int64",
        "type": "integer",
        "description": "The group id of the clan."
      }
    },
    "required": [
      "fireteamId",
      "groupId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Fireteam/Clan/{groupId}/Summary/{fireteamId}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            [
              "**Response**",
              {
                "$ul": [
                  "**Alternates**",
                  "**Members**",
                  [
                    "**Summary**",
                    {
                      "$ul": [
                        "**activityType**",
                        "**alternateSlotCount**",
                        "**availableAlternateSlotCount**",
                        "**availablePlayerSlotCount**",
                        "**dateCreated**",
                        "**dateModified**",
                        "**datePlayerModified**",
                        "**fireteamId**",
                        "**groupId**",
                        "**isImmediate**",
                        "**isPublic**",
                        "**isValid**",
                        "**locale**",
                        "**ownerCurrentGuardianRankSnapshot**",
                        "**ownerHighestLifetimeGuardianRankSnapshot**",
                        "**ownerMembershipId**",
                        "**ownerTotalCommendationScoreSnapshot**",
                        "**platform**",
                        "**playerSlotCount**",
                        "**scheduledTime**",
                        "**title**",
                        "**titleBeforeModeration**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
