{
  "name": "Fireteam_SearchPublicAvailableClanFireteams",
  "description": "Gets a listing of all public fireteams starting now with open slots. Caller is not checked for join criteria so caching is maximized.",
  "parameters": {
    "type": "object",
    "properties": {
      "activityType": {
        "format": "int32",
        "type": "integer",
        "description": "The activity type to filter by."
      },
      "dateRange": {
        "format": "uint8",
        "type": "integer",
        "description": "The date range to grab available fireteams."
      },
      "excludeImmediate": {
        "type": "boolean",
        "description": "If you wish the result to exclude immediate fireteams, set this to true. Immediate-only can be forced using the dateRange enum."
      },
      "langFilter": {
        "type": "string",
        "description": "An optional language filter."
      },
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "Zero based page"
      },
      "platform": {
        "format": "uint8",
        "type": "integer",
        "description": "The platform filter."
      },
      "slotFilter": {
        "format": "uint8",
        "type": "integer",
        "description": "Filters based on available slots"
      }
    },
    "required": [
      "activityType",
      "dateRange",
      "page",
      "platform",
      "slotFilter"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.bungie.net/Platform/Fireteam/Search/Available/{platform}/{activityType}/{dateRange}/{slotFilter}/{page}/{?excludeImmediate,langFilter}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DetailedErrorTrace**",
            "**ErrorCode**",
            "**ErrorStatus**",
            "**Message**",
            "**MessageData**",
            [
              "**Response**",
              {
                "$ul": [
                  "**hasMore**",
                  [
                    "**query**",
                    {
                      "$ul": [
                        "**currentPage**",
                        "**itemsPerPage**",
                        "**requestContinuationToken**"
                      ]
                    }
                  ],
                  "**replacementContinuationToken**",
                  "**results**",
                  "**totalResults**",
                  "**useTotalResults**: If useTotalResults is true, then totalResults represents an accurate count.\r"
                ]
              }
            ],
            "**ThrottleSeconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
