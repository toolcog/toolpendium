{
  "name": "setListeNoire",
  "description": "Ajoute un numero en liste noire. Une fois ajouté, les requêtes d'envoi de SMS marketing vers ce numéro seront refusées.",
  "parameters": {
    "type": "object",
    "properties": {
      "keyid": {
        "format": "string",
        "type": "string",
        "description": "Clé API"
      },
      "setlisteNoire": {
        "enum": [
          "1"
        ],
        "type": "string",
        "description": "Doit valoir \"1\""
      },
      "num": {
        "format": "string",
        "type": "string",
        "description": "numéro de mobile à insérer en liste noire"
      }
    },
    "required": [
      "keyid",
      "setlisteNoire",
      "num"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://apirest.isendpro.com/cgi-bin/setlistenoire{?keyid,setlisteNoire,num}"
    }
  },
