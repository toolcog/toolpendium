{
  "name": "getCredit",
  "description": "Retourne le credit existant associe au compte.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "keyid": {
        "format": "string",
        "type": "string",
        "description": "Clé API"
      },
      "credit": {
        "enum": [
          "1",
          "2"
        ],
        "type": "string",
        "description": "Type de reponse demandée, 1 pour euro, 2 pour euro + estimation quantité"
      }
    },
    "required": [
      "keyid",
      "credit"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://apirest.isendpro.com/cgi-bin/credit{?keyid,credit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**etat**",
              {
                "$ul": [
                  "**credit**: Montant du crédit restant",
                  "**quantite**: Equivalence du crédit en nombre de SMS vers la France Métropolitaine."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**etat**",
              {
                "$ul": [
                  "**etat**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
