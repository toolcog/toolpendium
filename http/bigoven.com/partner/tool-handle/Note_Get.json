{
  "name": "Note_Get",
  "description": "Get a given note. Make sure you're passing authentication information in the header for the user who owns the note.",
  "parameters": {
    "type": "object",
    "properties": {
      "recipeId": {
        "format": "int32",
        "type": "integer",
        "description": "recipe identifier (integer)"
      },
      "noteId": {
        "format": "int32",
        "type": "integer",
        "description": "The note ID (note -- it's not the RecipeID)"
      }
    },
    "required": [
      "recipeId",
      "noteId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api2.bigoven.com/recipe/{recipeId}/note/{noteId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**CreationDate**",
            "**Date**",
            "**DateDT**",
            "**GUID**",
            "**ID**",
            "**Notes**",
            "**People**",
            "**RecipeID**",
            "**UserID**",
            "**Variations**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
