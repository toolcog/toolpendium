{
  "name": "Recipe_AutoComplete",
  "description": "Given a query, return recipe titles starting with query. Query must be at least 3 chars in length.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "type": "string",
        "description": ""
      },
      "limit": {
        "format": "int32",
        "type": "integer",
        "description": ""
      }
    },
    "required": [
      "query"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api2.bigoven.com/recipe/autocomplete{?query,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
