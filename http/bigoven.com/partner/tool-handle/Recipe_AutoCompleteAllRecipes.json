{
  "name": "Recipe_AutoCompleteAllRecipes",
  "description": "Automatics the complete all recipes.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "type": "string",
        "description": "The query."
      },
      "limit": {
        "format": "int32",
        "type": "integer",
        "description": "The limit."
      }
    },
    "required": [
      "query",
      "limit"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api2.bigoven.com/recipe/autocomplete/all{?query,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ImageURL**",
            "**QualityScore**",
            "**RecipeID**",
            "**Servings**",
            "**Title**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
