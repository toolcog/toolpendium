{
  "name": "Note_Delete",
  "description": "Delete a review\r\n                do a DELETE Http request of /note/{ID}",
  "parameters": {
    "type": "object",
    "properties": {
      "recipeId": {
        "format": "int32",
        "type": "integer",
        "description": "recipeId (int)"
      },
      "noteId": {
        "format": "int32",
        "type": "integer",
        "description": "noteId (int)"
      }
    },
    "required": [
      "recipeId",
      "noteId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api2.bigoven.com/recipe/{recipeId}/note/{noteId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
