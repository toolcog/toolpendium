      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**expires**: For public preview only",
            "**expires_only**: For public preview only (default: false)",
            "**sections**: Preview sections"
          ]
        },
