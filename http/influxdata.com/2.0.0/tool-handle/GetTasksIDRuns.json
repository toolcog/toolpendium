{
  "name": "GetTasksIDRuns",
  "description": "List runs for a task",
  "parameters": {
    "type": "object",
    "properties": {
      "Zap-Trace-Span": {
        "type": "string",
        "description": "OpenTracing span context"
      },
      "taskID": {
        "type": "string",
        "description": "The ID of the task to get runs for."
      },
      "after": {
        "type": "string",
        "description": "Returns runs after a specific ID."
      },
      "limit": {
        "default": 100,
        "maximum": 500,
        "minimum": 1,
        "type": "integer",
        "description": "The number of runs to return"
      },
      "afterTime": {
        "format": "date-time",
        "type": "string",
        "description": "Filter runs to those scheduled after this time, RFC3339"
      },
      "beforeTime": {
        "format": "date-time",
        "type": "string",
        "description": "Filter runs to those scheduled before this time, RFC3339"
      }
    },
    "required": [
      "taskID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v2/tasks/{taskID}/runs{?after,limit,afterTime,beforeTime}"
    },
    "headers": {
      "Zap-Trace-Span": {
        "$": "Zap-Trace-Span"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**links**",
              {
                "$ul": [
                  "**next**: URI of resource.",
                  "**prev**: URI of resource.",
                  "**self**: URI of resource."
                ]
              }
            ],
            "**runs**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: code is the machine-readable error code.",
            "**err**: err is a stack of errors that occurred during processing of the request. Useful for debugging.",
            "**message**: message is a human-readable message.",
            "**op**: op describes the logical code operation during error. Useful for debugging."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
