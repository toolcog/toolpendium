{
  "name": "GetRoutes",
  "description": "List all top level routes",
  "parameters": {
    "type": "object",
    "properties": {
      "Zap-Trace-Span": {
        "type": "string",
        "description": "OpenTracing span context"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v2/"
    },
    "headers": {
      "Zap-Trace-Span": {
        "$": "Zap-Trace-Span"
      }
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**authorizations**",
            "**buckets**",
            "**dashboards**",
            [
              "**external**",
              {
                "$ul": [
                  "**statusFeed**"
                ]
              }
            ],
            "**flags**",
            "**me**",
            "**orgs**",
            [
              "**query**",
              {
                "$ul": [
                  "**analyze**",
                  "**ast**",
                  "**self**",
                  "**suggestions**"
                ]
              }
            ],
            "**setup**",
            "**signin**",
            "**signout**",
            "**sources**",
            [
              "**system**",
              {
                "$ul": [
                  "**debug**",
                  "**health**",
                  "**metrics**"
                ]
              }
            ],
            "**tasks**",
            "**telegrafs**",
            "**users**",
            "**variables**",
            "**write**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
