{
  "name": "GetScrapers",
  "description": "List all scraper targets",
  "parameters": {
    "type": "object",
    "properties": {
      "Zap-Trace-Span": {
        "type": "string",
        "description": "OpenTracing span context"
      },
      "name": {
        "type": "string",
        "description": "Specifies the name of the scraper target."
      },
      "id": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "List of scraper target IDs to return. If both `id` and `owner` are specified, only `id` is used."
      },
      "orgID": {
        "type": "string",
        "description": "Specifies the organization ID of the scraper target."
      },
      "org": {
        "type": "string",
        "description": "Specifies the organization name of the scraper target."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v2/scrapers{?name,id,orgID,org}"
    },
    "headers": {
      "Zap-Trace-Span": {
        "$": "Zap-Trace-Span"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**configurations**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
