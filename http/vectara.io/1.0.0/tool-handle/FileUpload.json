{
  "name": "FileUpload",
  "description": "File Upload",
  "parameters": {
    "type": "object",
    "properties": {
      "c": {
        "type": "integer",
        "description": "Customer ID"
      },
      "o": {
        "type": "integer",
        "description": "Corpus ID"
      },
      "d": {
        "type": "boolean",
        "description": "If true, the server returns the extracted document that was indexed"
      },
      "body": {
        "properties": {
          "doc_metadata": {
            "description": "A JSON string of any additional metadata you want attached to the file.",
            "type": "string"
          },
          "file": {
            "description": "The file to be indexed into Vectara.",
            "format": "binary",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "c",
      "o"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vectara.io/v1/upload{?c,o,d}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**response**",
              {
                "$ul": [
                  [
                    "**quotaConsumed**",
                    {
                      "$ul": [
                        "**numChars**: The number of characters Vectara indexed from the file uploaded.",
                        "**numMetadataChars**: The number of metadata characters Vectara indexed from the file uploaded."
                      ]
                    }
                  ],
                  "**status**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**httpCode**: Returned HTTP code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
