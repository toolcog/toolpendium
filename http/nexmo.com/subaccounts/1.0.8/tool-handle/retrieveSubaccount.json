{
  "name": "retrieveSubaccount",
  "description": "Get the information of a subaccount specified with its API key.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "ID of the primary account"
      },
      "subaccount_key": {
        "type": "string",
        "description": "ID of the subaccount"
      }
    },
    "required": [
      "api_key",
      "subaccount_key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nexmo.com/accounts/{api_key}/subaccounts/{subaccount_key}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**api_key**: Unique subaccount ID.",
            "**balance**: Balance of the subAccount. Value is null if balance is shared with primary account.",
            "**created_at**: Subaccount creation date and time.",
            "**credit_limit**: Credit limit of the subAccount. Value is null if balance is shared with primary account.",
            "**name**: Name of the subaccount.",
            "**primary_account_api_key**: Unique primary account ID.",
            "**suspended**: Subaccount suspension status.",
            "**use_primary_account_balance**: Flag showing if balance is shared with primary account."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Unprovisioned",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Invalid API Key",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
