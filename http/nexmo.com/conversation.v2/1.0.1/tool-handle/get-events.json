{
  "name": "get-events",
  "description": "List Events",
  "parameters": {
    "type": "object",
    "properties": {
      "conversation_id": {
        "type": "string",
        "description": "The ID of the conversation"
      },
      "page_size": {
        "default": 10,
        "maximum": 100,
        "type": "integer",
        "description": "The number of results returned per page.   The default value is `10`. The maximum value is `100`."
      },
      "order": {
        "default": "asc",
        "enum": [
          "asc",
          "desc"
        ],
        "type": "string",
        "description": "Show the most (`desc`) / least (`asc`) recently created entries first"
      },
      "cursor": {
        "type": "string",
        "description": "The cursor to start returning results from.\n\nYou are not expected to provide this manually, but to follow the url provided in `_links.next.href` in the response which contains a `cursor` value\n"
      },
      "start_id": {
        "type": "string",
        "description": "The ID to start returning events at"
      },
      "end_id": {
        "type": "string",
        "description": "The ID to end returning events at"
      },
      "event_type": {
        "type": "string",
        "description": "The type of event to search for. Does not currently support custom events"
      }
    },
    "required": [
      "conversation_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nexmo.com/v0.2/conversations/{conversation_id}/events{?page_size,order,cursor,start_id,end_id,event_type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_embedded**",
              {
                "$ul": [
                  [
                    "**data**",
                    {
                      "$ul": [
                        "**events**: List of events matching the provided filter"
                      ]
                    }
                  ]
                ]
              }
            ],
            [
              "**_links**",
              {
                "$ul": [
                  [
                    "**first**",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**next**",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**prev**",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**self**",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**page_size**: The number of results returned on this page."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
