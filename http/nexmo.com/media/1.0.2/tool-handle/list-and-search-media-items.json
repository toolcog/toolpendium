{
  "name": "list-and-search-media-items",
  "description": "Retrieve information about multiple media items with the ability to search and paginate.",
  "parameters": {
    "type": "object",
    "properties": {
      "order": {
        "default": "descending",
        "enum": [
          "ascending",
          "descending"
        ],
        "example": "ascending",
        "type": "string",
        "description": "The order of search results."
      },
      "page_index": {
        "default": 0,
        "example": 1,
        "type": "integer",
        "description": "Which page to retrieve in pagination"
      },
      "page_size": {
        "default": 20,
        "example": 50,
        "type": "integer",
        "description": "How many items at most per page"
      },
      "start_time": {
        "default": "1 week ago",
        "example": "2020-01-01T14:00:00.000Z",
        "type": "string",
        "description": "Retrieve results created on or after this timestap."
      },
      "end_time": {
        "example": "2020-01-01T14:00:00.000Z",
        "type": "string",
        "description": "Retrieve results created on or before this timestamp."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nexmo.com/v3/media/{?order,page_index,page_size,start_time,end_time}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_embedded**: A collection of media items. See [retrieve a media item](#retrieve-a-media-item) for a description of the returned fields",
              {
                "$ul": [
                  "**media**"
                ]
              }
            ],
            [
              "**_links**",
              {
                "$ul": [
                  [
                    "**first**",
                    {
                      "$ul": [
                        "**href** (default: )"
                      ]
                    }
                  ],
                  [
                    "**last**",
                    {
                      "$ul": [
                        "**href** (default: )"
                      ]
                    }
                  ],
                  [
                    "**self**",
                    {
                      "$ul": [
                        "**href** (default: )"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**count**: The total number of records returned by your request. (default: 0)",
            "**page_index**: The `page_index` used in your request. (default: 0)",
            "**page_size**: The amount of records returned in this response. (default: 0)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
