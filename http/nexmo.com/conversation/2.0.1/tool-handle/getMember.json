{
  "name": "getMember",
  "description": "Retrieve a member",
  "parameters": {
    "type": "object",
    "properties": {
      "conversation_id": {
        "type": "string",
        "description": "Conversation ID"
      },
      "member_id": {
        "type": "string",
        "description": "Member ID"
      }
    },
    "required": [
      "conversation_id",
      "member_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nexmo.com/v0.1/conversations/{conversation_id}/members/{member_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**href**: A link towards a member included in Conversation API",
            "**id**: Member ID"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
