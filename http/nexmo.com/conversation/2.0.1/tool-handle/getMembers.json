{
  "name": "getMembers",
  "description": "This endpoint is **DEPRECATED**. Please use [/v0.2/members](/api/conversation.v2#get-members).",
  "parameters": {
    "type": "object",
    "properties": {
      "conversation_id": {
        "type": "string",
        "description": "Conversation ID"
      }
    },
    "required": [
      "conversation_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nexmo.com/v0.1/conversations/{conversation_id}/members"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**name**: Unique name for a user",
            "**state**: The state that the member is in. Possible values are `invited`, `joined`, `left`, or `unknown`",
            "**user_id**: User ID",
            "**user_name**: Unique name for a user"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
