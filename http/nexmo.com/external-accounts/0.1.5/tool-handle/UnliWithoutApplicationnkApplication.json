{
  "name": "UnliWithoutApplicationnkApplication",
  "description": "Unlink application from an account",
  "parameters": {
    "type": "object",
    "properties": {
      "provider": {
        "enum": [
          "messenger",
          "viber_service_msg",
          "whatsapp"
        ],
        "type": "string",
        "description": "Provider of the account you want to unlink an application from"
      },
      "external_id": {
        "type": "string",
        "description": "External id of the account you want to unlink an application from"
      },
      "application_id": {
        "type": "string",
        "description": "Id of the application you want to unlink"
      }
    },
    "required": [
      "provider",
      "external_id",
      "application_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.nexmo.com/beta/chatapp-accounts/{provider}/{external_id}/applications/{application_id}"
    }
  },
