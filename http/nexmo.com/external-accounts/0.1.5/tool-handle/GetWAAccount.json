{
  "name": "GetWAAccount",
  "description": "Retrieve a Whatsapp account",
  "parameters": {
    "type": "object",
    "properties": {
      "external_id": {
        "type": "string",
        "description": "External id of the account you want to retrieve. In this case it will be the WhatsApp number."
      }
    },
    "required": [
      "external_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.nexmo.com/beta/chatapp-accounts/whatsapp/{external_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**api_key**: The external api key for this account",
            "**applications**: The array of associated application ids",
            "**external_id**: The external identifier for this account",
            "**name**: The account name",
            "**provider**: The provider (will be `whatsapp`)."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
