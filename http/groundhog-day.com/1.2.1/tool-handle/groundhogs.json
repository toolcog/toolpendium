{
  "name": "groundhogs",
  "description": "Returns all prognosticating animals with their known predictions.",
  "parameters": {
    "type": "object",
    "properties": {
      "country": {
        "example": "Canada or USA",
        "type": "string",
        "description": "Filter groundhogs by country of origin (USA or Canada)."
      },
      "isGroundhog": {
        "enum": [
          "1",
          "0",
          "true",
          "false"
        ],
        "type": "string",
        "description": "Filter groundhogs by type (actual, alive groundhogs, or other prognosticators)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://virtserver.swaggerhub.com/pcraig3/groundhog-day-api/1.2.1/api/v1/groundhogs{?country,isGroundhog}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**groundhogs**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
