{
  "name": "loadService",
  "description": "Loads a service description into the OSDB Service Registry",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "service_description_url": {
            "description": "The URL of the resource containing the service description to load.",
            "type": "string"
          },
          "service_moniker": {
            "description": "Service ID to be used to uniquely identify the service. (Optional: Required for anonymous services or to override the service name in the service description.)",
            "type": "string"
          }
        },
        "required": [
          "service_description_url"
        ],
        "type": "object",
        "description": "Service to register with OSDB"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://osdb.openlinksw.com/osdb/api/v1/services"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**api**: The path of the REST API method",
            "**method**: The name of the REST API method",
            "**response**: Confirmation message",
            "**status** (default: success)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**api**: The path of the REST API method reporting the error",
            "**method**: The name of the REST API method reporting the error",
            "**response**: A message describing the error",
            "**status** (default: error)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
