{
  "name": "search",
  "description": "Search for products",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "type": "string",
        "description": "Query string to search for items."
      },
      "price_min": {
        "type": "number",
        "description": "The minimum price to filter by."
      },
      "price_max": {
        "type": "number",
        "description": "The maximum price to filter by."
      },
      "similar_to_id": {
        "type": "string",
        "description": "A product id that you want to find similar products for. (Only include one)"
      },
      "num_results": {
        "type": "string",
        "description": "How many results to return. Defaults to 5. It can be a number between 1 and 10."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://server.shop.app/openai/search{?query,price_min,price_max,similar_to_id,num_results}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**results**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
