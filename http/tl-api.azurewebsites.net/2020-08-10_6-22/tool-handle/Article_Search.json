{
  "name": "Article_Search",
  "description": "Search articles\nIt will only return basic information of article\n            ",
  "parameters": {
    "type": "object",
    "properties": {
      "searchText": {
        "nullable": true,
        "type": "string",
        "description": "part of article name"
      },
      "gymId": {
        "default": -1,
        "format": "int32",
        "type": "integer",
        "description": "-1 for all gyms "
      },
      "type": {
        "default": "all",
        "nullable": true,
        "type": "string",
        "description": "filter article type. default is 'all'"
      },
      "orderBy": {
        "default": "1",
        "nullable": true,
        "type": "string",
        "description": "order by column.!-- invalid column will give internal server error"
      },
      "limit": {
        "default": 100,
        "format": "int32",
        "type": "integer",
        "description": "number of recode in result and default is 100. use negative numbers to order by desc"
      },
      "offset": {
        "default": 0,
        "format": "int32",
        "type": "integer",
        "description": "number of recodes to skip"
      },
      "activeStatus": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "Active Status 1 : Active, 2: Inactive, 3: All, Default : 1"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://tl-api.azurewebsites.net/api/Article/Search{?searchText,gymId,type,orderBy,limit,offset,activeStatus}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The DefaultResponseDTO Class.\nContains fields of DefaultResponse DTO in application.\n            ",
        "**Key properties:**",
        {
          "$ul": [
            "**isError**",
            "**message**",
            "**responseException**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
