{
  "name": "Package_Search",
  "description": "Search packages\n            ",
  "parameters": {
    "type": "object",
    "properties": {
      "searchText": {
        "nullable": true,
        "type": "string",
        "description": "part of package name"
      },
      "gymId": {
        "default": -1,
        "format": "int32",
        "type": "integer",
        "description": "primary key of TL gym entity"
      },
      "type": {
        "default": "all",
        "nullable": true,
        "type": "string",
        "description": "filter package type.!-- default is 'all'"
      },
      "orderBy": {
        "default": "1",
        "nullable": true,
        "type": "string",
        "description": "order by column.!-- invalid column will give internal server error"
      },
      "limit": {
        "default": 100,
        "format": "int32",
        "type": "integer",
        "description": "number of recode in result and default is 100. use negative numbers to order by desc"
      },
      "offset": {
        "default": 0,
        "format": "int32",
        "type": "integer",
        "description": "number of recodes to skip"
      },
      "activeStatus": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "active status active : 1, inactive : 2, all 3, deafult : 1"
      },
      "categoryId": {
        "default": -1,
        "format": "int32",
        "type": "integer",
        "description": "Packge Category Id"
      },
      "startpPrice": {
        "default": 0,
        "format": "decimal",
        "type": "number",
        "description": "Start price of the price Range"
      },
      "endPrice": {
        "default": 9999999,
        "format": "decimal",
        "type": "number",
        "description": "End Price of the price Range"
      },
      "requestSource": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "1 : MRM, 2 : Mobile "
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://tl-api.azurewebsites.net/api/Package/Search{?searchText,gymId,type,orderBy,limit,offset,activeStatus,categoryId,startpPrice,endPrice,requestSource}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "The DefaultResponseDTO Class.\nContains fields of DefaultResponse DTO in application.\n            ",
        "**Key properties:**",
        {
          "$ul": [
            "**isError**",
            "**message**",
            "**responseException**",
            "**result**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The DefaultResponseDTO Class.\nContains fields of DefaultResponse DTO in application.\n            ",
        "**Key properties:**",
        {
          "$ul": [
            "**isError**",
            "**message**",
            "**responseException**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**InnerException**",
            "**Message**",
            "**Source**",
            "**StackTrace**",
            "**customError**",
            "**errors**",
            "**isCustomErrorObject**",
            "**isModelValidatonError**",
            "**referenceDocumentLink**",
            "**referenceErrorCode**",
            "**statusCode**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
