{
  "name": "findTextBroadcasts",
  "description": "Searches for all text broadcasts created by user. Can query on label, name, and the current running status of the campaign. Returns a paged list of text broadcasts",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "A name of text broadcast"
      },
      "label": {
        "type": "string",
        "description": "A label of a text broadcast"
      },
      "running": {
        "type": "boolean",
        "description": "Returns broadcasts only in running state."
      },
      "scheduled": {
        "type": "boolean",
        "description": "Specify whether the campaigns should be scheduled or not"
      },
      "intervalBegin": {
        "format": "int64",
        "type": "integer",
        "description": "Start of the find time interval, formatted in unix time milliseconds. Example: 1473781817000 for Sat, 05 Jan 1985 14:03:37 GMT"
      },
      "intervalEnd": {
        "format": "int64",
        "type": "integer",
        "description": "End of the find time interval, formatted in unix time milliseconds. Example: 1473781817000 for Sat, 05 Jan 1985 14:03:37 GMT"
      },
      "limit": {
        "default": 10,
        "format": "int32",
        "type": "integer",
        "description": "To set the maximum number of records to return in a paged list response. The default is 100"
      },
      "offset": {
        "default": 0,
        "format": "int32",
        "type": "integer",
        "description": "Offset to the start of a given page. The default is 0. Check [pagination](https://developers.callfire.com/docs.html#pagination) page for more information about pagination in CallFire API."
      },
      "fields": {
        "type": "string",
        "description": "Limit fields received in response. E.g. fields: id, name or fields items (id, name), see more at [partial response](https://developers.callfire.com/docs.html#partial-response) page."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.callfire.com/v2/texts/broadcasts{?name,label,running,scheduled,intervalBegin,intervalEnd,limit,offset,fields}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents a page with results returned by query operation. Check [pagination](https://developers.callfire.com/docs.html#pagination) page for more information about pagination in CallFire API.",
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**limit**: A maximum number of returned items. If items.size() < limit assume no more items",
            "**offset**: An offset from a start of paging source",
            "**totalCount**: Total count of available results. -1 if unknown"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
