{
  "name": "getCallRecordings",
  "description": "Returns a list of recordings metadata of particular call. Metadata contains link to a MP3 recording",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "An id of a call"
      },
      "fields": {
        "type": "string",
        "description": "Limit fields received in response. E.g. fields: id, name or fields items (id, name), see more at [partial response](https://developers.callfire.com/docs.html#partial-response) page."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.callfire.com/v2/calls/{id}/recordings{?fields}"
    }
  },
