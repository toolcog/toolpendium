{
  "name": "createContactListFromFile",
  "description": "Creates a contact list to be used with campaigns through uploading a .csv file. Returns the id of created list",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "file": {
            "description": "CSV file to be uploaded",
            "format": "binary",
            "type": "string"
          },
          "name": {
            "description": "A name of a contact list",
            "type": "string"
          },
          "useCustomFields": {
            "description": "A flag to indicate how to define property names for contacts. If true, uses the field and property names exactly as defined. If false will assign custom properties and fields to A, B, C, etc",
            "type": "boolean"
          }
        },
        "required": [
          "file"
        ],
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.callfire.com/v2/contacts/lists/upload"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
