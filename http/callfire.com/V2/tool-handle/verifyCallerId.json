  "name": "verifyCallerId",
  "description": "With the verification code received from the Create caller id endpoint, a call to this endpoint is required to finish verification. For authentication use api credentials.",
  "parameters": {
    "type": "object",
    "properties": {
      "callerid": {
        "type": "string",
        "description": "A phone number in E.164 format (11-digit) which needs to be verified. Example: 12132000384"
      },
      "body": {
        "$ref": "#/components/schemas/CallerIdVerificationRequest",
        "description": "request"
      }
    },
    "required": [
      "callerid"
    ]
  },
  "handler": "http",
  "request": {
