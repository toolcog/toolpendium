{
  "name": "getCreditUsage",
  "description": "Find credit usage for the user. Returns credits usage for time period specified or if unspecified then total for all time. For authentication use api credentials.",
  "parameters": {
    "type": "object",
    "properties": {
      "intervalBegin": {
        "format": "int64",
        "type": "integer",
        "description": "Beginning of usage period formatted in unix time milliseconds. Example: 1473781817000"
      },
      "intervalEnd": {
        "format": "int64",
        "type": "integer",
        "description": "End of usage period formatted in unix time milliseconds. Example: 1473781817000"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.callfire.com/v2/me/billing/credit-usage{?intervalBegin,intervalEnd}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Contains statistics of billing plan usage",
        "**Key properties:**",
        {
          "$ul": [
            "**callsDurationMinutes**: Sum of calls duration rounded to nearest whole minute",
            "**creditsUsed**: Total credits used calls and texts.",
            "**intervalBegin**: Start of usage period formatted in unix time milliseconds. Example: 1473781817000 for Sat, 05 Jan 1985 14:03:37 GMT",
            "**intervalEnd**: End of usage period formatted in unix time milliseconds. Example: 1473781817000 for Sat, 05 Jan 1985 14:03:37 GMT",
            "**textsSent**: Number of texts sent"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
