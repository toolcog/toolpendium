{
  "name": "orderKeywords",
  "description": "Purchase keywords. Send a list of available keywords into this API to purchase them using CallFire credits. Make sure the account has enough credits before trying to purchase the keywords. Keyword should only consist of uppercase and lowercase letters and numbers. Number of characters must be greater than 2, but less than 65.",
  "parameters": {
    "type": "object",
    "properties": {
      "fields": {
        "type": "string",
        "description": "Limit fields received in response. E.g. fields: id, name or fields items (id, name), see more at [partial response](https://developers.callfire.com/docs.html#partial-response) page."
      },
      "body": {
        "$ref": "#/components/schemas/KeywordPurchaseRequest",
        "description": "Request object which contains a list of keywords to buy"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.callfire.com/v2/orders/keywords{?fields}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
