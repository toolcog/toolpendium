{
  "name": "getUniversalDoNotContacts",
  "description": "Searches for a UniversalDoNotContact object for a given phone number. Shows whether inbound/outbound actions are allowed for a given number",
  "parameters": {
    "type": "object",
    "properties": {
      "toNumber": {
        "type": "string",
        "description": "A required destination phone number in E.164 format (11-digit). Example: 12132000384"
      },
      "fromNumber": {
        "type": "string",
        "description": "An optional destination/source number for DNC, specified in E.164 format (11-digit). Example: 12132000384"
      },
      "fields": {
        "type": "string",
        "description": "Limit fields received in response. E.g. fields: id, name or fields items (id, name), see more at [partial response](https://developers.callfire.com/docs.html#partial-response) page."
      }
    },
    "required": [
      "toNumber"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.callfire.com/v2/contacts/dncs/universals/{toNumber}{?fromNumber,fields}"
    }
  },
