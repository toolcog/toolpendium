{
  "name": "removeContactListItems",
  "description": "Deletes contacts from a contact list. List the contact ids in request to delete multiple contacts with one request.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "A id of a contact list"
      },
      "contactId": {
        "items": {
          "format": "int64",
          "type": "integer"
        },
        "type": "array",
        "description": "An id of a contact entity in the CallFire system"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.callfire.com/v2/contacts/lists/{id}/items{?contactId}"
