{
  "name": "getCallRecordingMp3ByName",
  "description": "Returns a MP3 recording of a particular call, response contains binary data, content type is 'audio/mpeg'",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "An id of a call"
      },
      "name": {
        "type": "string",
        "description": "A name of a recording"
      }
    },
    "required": [
      "id",
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
