{
  "name": "findNumbersTollfree",
  "description": "Searches for the toll free numbers which are available for purchase in the CallFire catalog",
  "parameters": {
    "type": "object",
    "properties": {
      "pattern": {
        "type": "string",
        "description": "Filter toll free numbers by prefix, pattern must be 3 char long and should end with '*'. Examples: 8**, 85*, 87* (but 855 will fail because pattern must end with '*')."
      },
      "limit": {
        "default": 100,
        "format": "int32",
        "type": "integer",
        "description": "To set the maximum number of records to return in a paged list response. The default is 100"
      },
      "fields": {
        "type": "string",
        "description": "Limit fields received in response. E.g. fields: id, name or fields items (id, name), see more at [partial response](https://developers.callfire.com/docs.html#partial-response) page."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.callfire.com/v2/numbers/tollfree{?pattern,limit,fields}"
    }
  },
