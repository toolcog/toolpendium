{
  "name": "Fetch",
  "description": "The `Fetch` operation looks up and returns vectors, by ID, from a single namespace. The returned vectors include the vector data and/or metadata.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/FetchRequest"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "FetchRequest": {
        "properties": {
          "ids": {
            "items": {
              "$ref": "#/$defs/VectorId"
            },
            "maxLength": 1000,
            "type": "array"
          },
          "namespace": {
            "$ref": "#/$defs/NamespaceName"
          }
        },
        "required": [
          "ids"
        ],
        "type": "object"
      },
      "VectorId": {
        "description": "The unique ID of a vector",
        "example": "vector-0",
        "format": "VectorId",
        "type": "string"
      },
      "NamespaceName": {
        "description": "An index namespace name",
        "example": "namespace-0",
        "format": "NamespaceName",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://example-abcd1234.svc.us-east1-gcp.pinecone.io/vectors/fetch"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**namespace**: An index namespace name",
            "**vectors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
