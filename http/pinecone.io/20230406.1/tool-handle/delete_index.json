{
  "name": "delete_index",
  "description": "This operation deletes an existing index.",
  "parameters": {
    "type": "object",
    "properties": {
      "indexName": {
        "$ref": "#/$defs/IndexName"
      }
    },
    "required": [
      "indexName"
    ],
    "$defs": {
      "IndexName": {
        "description": "The unique name of an index.",
        "example": "example",
        "format": "IndexName",
        "maxLength": 45,
        "minLength": 1,
        "nullable": false,
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://controller.us-east1-gcp.pinecone.io/databases/{indexName}"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "500": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
