{
  "name": "getObservationsByCodes",
  "description": "Given a User ID and search parameters, retrieve a page of observations.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "format": "int64",
        "type": "integer",
        "description": "userId"
      },
      "code": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "uniqueItems": false,
        "description": "code"
      },
      "limit": {
        "format": "int64",
        "type": "integer",
        "description": "limit"
      },
      "offset": {
        "format": "int64",
        "type": "integer",
        "description": "offset"
      },
      "orderDirection": {
        "type": "string",
        "description": "orderDirection"
      }
    },
    "required": [
      "userId",
      "code",
      "limit",
      "offset",
      "orderDirection"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.patientview.org//user/{userId}/observations{?code,limit,offset,orderDirection}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**totalElements**",
            "**totalPages**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
