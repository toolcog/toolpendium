{
  "name": "getPatientEnteredObservationsByCode",
  "description": "Given a User ID and observation code, retrieve patient entered observations.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "format": "int64",
        "type": "integer",
        "description": "userId"
      },
      "code": {
        "type": "string",
        "description": "code"
      }
    },
    "required": [
      "userId",
      "code"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.patientview.org//user/{userId}/observations/{code}/patiententered"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**applies**",
            "**bodySite**",
            "**comments**",
            "**comparator**",
            "**diagram**",
            [
              "**group**",
              {
                "$ul": [
                  "**address1**",
                  "**address2**",
                  "**address3**",
                  "**childGroups**",
                  "**code**",
                  "**contactPoints**",
                  "**created**",
                  "**fhirResourceId**",
                  "**groupFeatures**",
                  [
                    "**groupType**",
                    {
                      "$ul": [
                        "**created**",
                        "**description**",
                        "**descriptionFriendly**",
                        "**displayOrder**",
                        "**id**",
                        "**lastUpdate**",
                        [
                          "**lookupType**",
                          {
                            "$ul": [
                              "**created**",
                              "**description**",
                              "**id**",
                              "**lastUpdate**",
                              "**type**"
                            ]
                          }
                        ],
                        "**value**"
                      ]
                    }
                  ],
                  "**id**",
                  "**lastImportDate**",
                  "**lastUpdate**",
                  "**links**",
                  "**locations**",
                  "**name**",
                  "**parentGroups**",
                  "**postcode**",
                  "**sftpUser**",
                  "**shortName**",
                  "**visible**",
                  "**visibleToJoin**"
                ]
              }
            ],
            "**id**",
            "**identifier**",
            "**location**",
            "**name**",
            "**temporaryUuid**",
            "**units**",
            "**value**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
