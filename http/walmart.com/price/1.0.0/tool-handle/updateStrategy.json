{
  "name": "updateStrategy",
  "description": "Updates the existing strategy",
  "parameters": {
    "type": "object",
    "properties": {
      "WM_SEC.ACCESS_TOKEN": {
        "type": "string",
        "description": "The access token retrieved in the Token API call"
      },
      "WM_CONSUMER.CHANNEL.TYPE": {
        "type": "string",
        "description": "A unique ID to track the consumer request by channel. Use the Consumer Channel Type received during onboarding"
      },
      "WM_QOS.CORRELATION_ID": {
        "type": "string",
        "description": "A unique ID which identifies each API call and used to track and debug issues; use a random generated GUID for this ID"
      },
      "WM_SVC.NAME": {
        "type": "string",
        "description": "Walmart Service Name"
      },
      "strategyCollectionId": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "properties": {
          "enableRepricerForPromotion": {
            "description": "Status of item on promotions to be enable on repricer",
            "example": true,
            "type": "boolean"
          },
          "enabled": {
            "description": "Status of the Strategy Collection",
            "example": true,
            "type": "boolean"
          },
          "repricerStrategy": {
            "description": "Name of the Strategy Collection",
            "example": "Buy Box Strategy For testing",
            "type": "string"
          },
          "strategies": {
            "description": "Strategies",
            "items": {
              "properties": {
                "adjustmentType": {
                  "enum": [
                    "UNIT",
                    "PERCENTAGE"
                  ],
                  "type": "string"
                },
                "adjustmentValue": {
                  "example": 1.2,
                  "format": "double",
                  "type": "number"
                },
                "strategyType": {
                  "enum": [
                    "Buy Box Price",
                    "External Price",
                    "Competitive Price"
                  ],
                  "type": "string"
                }
              },
              "type": "object"
            },
            "type": "array"
          }
        },
        "type": "object",
        "description": "The request body will have the strategy related information"
      }
    },
    "required": [
      "WM_SEC.ACCESS_TOKEN",
      "WM_QOS.CORRELATION_ID",
      "WM_SVC.NAME",
      "strategyCollectionId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://marketplace.walmartapis.com/v3/repricer/strategy/{strategyCollectionId}"
    },
    "headers": {
      "WM_SEC.ACCESS_TOKEN": {
        "$": "WM_SEC.ACCESS_TOKEN"
      },
      "WM_CONSUMER.CHANNEL.TYPE": {
        "$": "WM_CONSUMER.CHANNEL.TYPE"
      },
      "WM_QOS.CORRELATION_ID": {
        "$": "WM_QOS.CORRELATION_ID"
      },
      "WM_SVC.NAME": {
        "$": "WM_SVC.NAME"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**enableRepricerForPromotion**",
            "**enabled**",
            "**repricerStrategy**",
            "**strategies**",
            "**strategyCollectionId**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
