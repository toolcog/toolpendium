{
  "name": "getStrategies",
  "description": "Get the list of strategies",
  "parameters": {
    "type": "object",
    "properties": {
      "WM_SEC.ACCESS_TOKEN": {
        "type": "string",
        "description": "The access token retrieved in the Token API call"
      },
      "WM_CONSUMER.CHANNEL.TYPE": {
        "type": "string",
        "description": "A unique ID to track the consumer request by channel. Use the Consumer Channel Type received during onboarding"
      },
      "WM_QOS.CORRELATION_ID": {
        "type": "string",
        "description": "A unique ID which identifies each API call and used to track and debug issues; use a random generated GUID for this ID"
      },
      "WM_SVC.NAME": {
        "type": "string",
        "description": "Walmart Service Name"
      }
    },
    "required": [
      "WM_SEC.ACCESS_TOKEN",
      "WM_QOS.CORRELATION_ID",
      "WM_SVC.NAME"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://marketplace.walmartapis.com/v3/repricer/strategies"
    },
    "headers": {
      "WM_SEC.ACCESS_TOKEN": {
        "$": "WM_SEC.ACCESS_TOKEN"
      },
      "WM_CONSUMER.CHANNEL.TYPE": {
        "$": "WM_CONSUMER.CHANNEL.TYPE"
      },
      "WM_QOS.CORRELATION_ID": {
        "$": "WM_QOS.CORRELATION_ID"
      },
      "WM_SVC.NAME": {
        "$": "WM_SVC.NAME"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**strategyCollections**",
            "**totalElements**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
