{
  "name": "getIngredientSubstitutes",
  "description": "Search for substitutes for a given ingredient.",
  "parameters": {
    "type": "object",
    "properties": {
      "ingredientName": {
        "example": "butter",
        "type": "string",
        "description": "The name of the ingredient you want to replace."
      }
    },
    "required": [
      "ingredientName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/ingredients/substitutes{?ingredientName}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**ingredient**",
            "**message**",
            "**substitutes**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
