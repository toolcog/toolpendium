{
  "name": "getComparableProducts",
  "description": "Find comparable products to the given one.",
  "parameters": {
    "type": "object",
    "properties": {
      "upc": {
        "example": 33698816271,
        "type": "number",
        "description": "The UPC of the product for which you want to find comparable products."
      }
    },
    "required": [
      "upc"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/products/upc/{upc}/comparable"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**comparableProducts**",
              {
                "$ul": [
                  "**calories**",
                  "**likes**",
                  "**price**",
                  "**protein**",
                  "**spoonacularScore**",
                  "**sugar**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
