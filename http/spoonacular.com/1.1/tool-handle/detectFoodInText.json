{
  "name": "detectFoodInText",
  "description": "Take any text and find all mentions of food contained within it. This task is also called Named Entity Recognition (NER). In this case, the entities are foods. Either dishes, such as pizza or cheeseburger, or ingredients, such as cucumber or almonds.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "enum": [
          "application/x-www-form-urlencoded",
          "application/json",
          "multipart/form-data"
        ],
        "example": "application/json",
        "type": "string",
        "description": "The content type."
      },
      "body": {
        "type": "string"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.spoonacular.com/food/detect"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      }
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**annotations**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
