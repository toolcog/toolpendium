{
  "name": "clearMealPlanDay",
  "description": "Delete all planned items from the user's meal plan for a specific day.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "example": "dsky",
        "type": "string",
        "description": "The username."
      },
      "date": {
        "example": "2020-06-01",
        "type": "string",
        "description": "The date in the format yyyy-mm-dd."
      },
      "hash": {
        "type": "string",
        "description": "The private hash for the username."
      }
    },
    "required": [
      "username",
      "date",
      "hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.spoonacular.com/mealplanner/{username}/day/{date}{?hash}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
