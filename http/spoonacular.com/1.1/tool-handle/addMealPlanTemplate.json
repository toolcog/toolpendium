{
  "name": "addMealPlanTemplate",
  "description": "Add a meal plan template for a user.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "example": "dsky",
        "type": "string",
        "description": "The username."
      },
      "hash": {
        "example": "4b5v4398573406",
        "type": "string",
        "description": "The private hash for the username."
      }
    },
    "required": [
      "username",
      "hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.spoonacular.com/mealplanner/{username}/templates{?hash}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**name**",
            "**publishAsPublic**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
