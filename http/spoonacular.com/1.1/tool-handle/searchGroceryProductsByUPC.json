{
  "name": "searchGroceryProductsByUPC",
  "description": "Get information about a packaged food using its UPC.",
  "parameters": {
    "type": "object",
    "properties": {
      "upc": {
        "example": 41631000564,
        "type": "number",
        "description": "The product's UPC."
      }
    },
    "required": [
      "upc"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/products/upc/{upc}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**badges**",
            "**breadcrumbs**",
            "**generatedText**",
            "**id**",
            "**imageType**",
            "**importantBadges**",
            "**ingredientCount**",
            "**ingredientList**",
            "**ingredients**",
            "**likes**",
            [
              "**nutrition**",
              {
                "$ul": [
                  [
                    "**caloricBreakdown**",
                    {
                      "$ul": [
                        "**percentCarbs**",
                        "**percentFat**",
                        "**percentProtein**"
                      ]
                    }
                  ],
                  "**nutrients**"
                ]
              }
            ],
            "**price**",
            [
              "**servings**",
              {
                "$ul": [
                  "**number**",
                  "**size**",
                  "**unit**"
                ]
              }
            ],
            "**spoonacularScore**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
