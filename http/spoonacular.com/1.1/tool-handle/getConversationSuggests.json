{
  "name": "getConversationSuggests",
  "description": "This endpoint returns suggestions for things the user can say or ask the chatbot.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "example": "tell",
        "type": "string",
        "description": "A (partial) query from the user. The endpoint will return if it matches topics it can talk about."
      },
      "number": {
        "example": 5,
        "type": "number",
        "description": "The number of suggestions to return (between 1 and 25)."
      }
    },
    "required": [
      "query"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/converse/suggest{?query,number}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**suggests**",
              {
                "$ul": [
                  "**_**"
                ]
              }
            ],
            "**words**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
