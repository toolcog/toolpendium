{
  "name": "menuItemNutritionLabelWidget",
  "description": "Visualize a menu item's nutritional label information as HTML including CSS.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": 342313,
        "type": "number",
        "description": "The menu item id."
      },
      "defaultCss": {
        "default": true,
        "example": false,
        "type": "boolean",
        "description": "Whether the default CSS should be added to the response."
      },
      "showOptionalNutrients": {
        "example": false,
        "type": "boolean",
        "description": "Whether to show optional nutrients."
      },
      "showZeroValues": {
        "example": false,
        "type": "boolean",
        "description": "Whether to show zero values."
      },
      "showIngredients": {
        "example": false,
        "type": "boolean",
        "description": "Whether to show a list of ingredients."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/menuItems/{id}/nutritionLabel{?defaultCss,showOptionalNutrients,showZeroValues,showIngredients}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
