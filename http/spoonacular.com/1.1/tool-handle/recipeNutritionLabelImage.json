{
  "name": "recipeNutritionLabelImage",
  "description": "Get a recipe's nutrition label as an image.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": 641166,
        "type": "number",
        "description": "The recipe id."
      },
      "showOptionalNutrients": {
        "example": false,
        "type": "boolean",
        "description": "Whether to show optional nutrients."
      },
      "showZeroValues": {
        "example": false,
        "type": "boolean",
        "description": "Whether to show zero values."
      },
      "showIngredients": {
        "example": false,
        "type": "boolean",
        "description": "Whether to show a list of ingredients."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/recipes/{id}/nutritionLabel.png{?showOptionalNutrients,showZeroValues,showIngredients}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
