{
  "name": "deleteFromMealPlan",
  "description": "Delete an item from the user's meal plan.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "example": "dsky",
        "type": "string",
        "description": "The username."
      },
      "id": {
        "example": 15678,
        "type": "number",
        "description": "The shopping list item id."
      },
      "hash": {
        "type": "string",
        "description": "The private hash for the username."
      }
    },
    "required": [
      "username",
      "id",
      "hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.spoonacular.com/mealplanner/{username}/items/{id}{?hash}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
