{
  "name": "computeGlycemicLoad",
  "description": "Retrieve the glycemic index for a list of ingredients and compute the individual and total glycemic load.",
  "parameters": {
    "type": "object",
    "properties": {
      "language": {
        "enum": [
          "en",
          "de"
        ],
        "example": "en",
        "type": "string",
        "description": "The language of the input. Either 'en' or 'de'."
      },
      "body": {
        "description": "",
        "properties": {
          "ingredients": {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "ingredients"
        ],
        "type": "object",
        "x-examples": {
          "example-1": {
            "ingredients": [
              "1 kiwi",
              "2 cups rice",
              "2 glasses of water"
            ]
          }
        }
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.spoonacular.com/food/ingredients/glycemicLoad{?language}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**ingredients**",
            "**totalGlycemicLoad**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
