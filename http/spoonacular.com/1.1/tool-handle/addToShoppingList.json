{
  "name": "addToShoppingList",
  "description": "Add an item to the current shopping list of a user.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "example": "dsky",
        "type": "string",
        "description": "The username."
      },
      "hash": {
        "type": "string",
        "description": "The private hash for the username."
      },
      "body": {
        "description": "",
        "properties": {
          "aisle": {
            "minLength": 1,
            "type": "string"
          },
          "item": {
            "minLength": 1,
            "type": "string"
          },
          "parse": {
            "type": "boolean"
          }
        },
        "required": [
          "item",
          "aisle",
          "parse"
        ],
        "type": "object",
        "x-examples": {
          "example-1": {
            "aisle": "Baking",
            "item": "1 package baking powder",
            "parse": true
          }
        }
      }
    },
    "required": [
      "username",
      "hash",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.spoonacular.com/mealplanner/{username}/shopping-list/items{?hash}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**aisles**",
            "**cost**",
            "**endDate**",
            "**startDate**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
