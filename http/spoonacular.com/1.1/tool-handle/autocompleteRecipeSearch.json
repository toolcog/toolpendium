{
  "name": "autocompleteRecipeSearch",
  "description": "Autocomplete a partial input to suggest possible recipe names.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "example": "burger",
        "type": "string",
        "description": "The (natural language) search query."
      },
      "number": {
        "default": 10,
        "example": 10,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "The maximum number of items to return (between 1 and 100). Defaults to 10."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/recipes/autocomplete{?query,number}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**imageType**",
            "**title**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.title"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
