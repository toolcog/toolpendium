{
  "name": "classifyGroceryProduct",
  "description": "This endpoint allows you to match a packaged food to a basic category, e.g. a specific brand of milk to the category milk.",
  "parameters": {
    "type": "object",
    "properties": {
      "locale": {
        "enum": [
          "en_US",
          "en_GB"
        ],
        "example": "en_US",
        "type": "string",
        "description": "The display name of the returned category, supported is en_US (for American English) and en_GB (for British English)."
      },
      "body": {
        "description": "",
        "properties": {
          "plu_code": {
            "type": "string"
          },
          "title": {
            "minLength": 1,
            "type": "string"
          },
          "upc": {
            "type": "string"
          }
        },
        "required": [
          "title",
          "upc",
          "plu_code"
        ],
        "type": "object",
        "x-examples": {
          "example-1": {
            "plu_code": "",
            "title": "Kroger Vitamin A & D Reduced Fat 2% Milk",
            "upc": ""
          }
        }
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.spoonacular.com/food/products/classify{?locale}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**breadcrumbs**",
            "**category**",
            "**cleanTitle**",
            "**image**",
            "**usdaCode**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
