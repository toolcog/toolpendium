{
  "name": "analyzeARecipeSearchQuery",
  "description": "Parse a recipe search query to find out its intention.",
  "parameters": {
    "type": "object",
    "properties": {
      "q": {
        "example": "salmon with fusilli and no nuts",
        "type": "string",
        "description": "The recipe search query."
      }
    },
    "required": [
      "q"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/recipes/queries/analyze{?q}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**cuisines**",
            "**dishes**",
            "**ingredients**",
            "**modifiers**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
