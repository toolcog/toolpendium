{
  "name": "imageAnalysisByURL",
  "description": "Analyze a food image. The API tries to classify the image, guess the nutrition, and find a matching recipes.",
  "parameters": {
    "type": "object",
    "properties": {
      "imageUrl": {
        "example": "https://spoonacular.com/recipeImages/635350-240x150.jpg",
        "type": "string",
        "description": "The URL of the image to be analyzed."
      }
    },
    "required": [
      "imageUrl"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/images/analyze{?imageUrl}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**category**",
              {
                "$ul": [
                  "**name**",
                  "**probability**"
                ]
              }
            ],
            [
              "**nutrition**",
              {
                "$ul": [
                  [
                    "**calories**",
                    {
                      "$ul": [
                        [
                          "**confidenceRange95Percent**",
                          {
                            "$ul": [
                              "**max**",
                              "**min**"
                            ]
                          }
                        ],
                        "**standardDeviation**",
                        "**unit**",
                        "**value**"
                      ]
                    }
                  ],
                  [
                    "**carbs**",
                    {
                      "$ul": [
                        [
                          "**confidenceRange95Percent**",
                          {
                            "$ul": [
                              "**max**",
                              "**min**"
                            ]
                          }
                        ],
                        "**standardDeviation**",
                        "**unit**",
                        "**value**"
                      ]
                    }
                  ],
                  [
                    "**fat**",
                    {
                      "$ul": [
                        [
                          "**confidenceRange95Percent**",
                          {
                            "$ul": [
                              "**max**",
                              "**min**"
                            ]
                          }
                        ],
                        "**standardDeviation**",
                        "**unit**",
                        "**value**"
                      ]
                    }
                  ],
                  [
                    "**protein**",
                    {
                      "$ul": [
                        [
                          "**confidenceRange95Percent**",
                          {
                            "$ul": [
                              "**max**",
                              "**min**"
                            ]
                          }
                        ],
                        "**standardDeviation**",
                        "**unit**",
                        "**value**"
                      ]
                    }
                  ],
                  "**recipesUsed**"
                ]
              }
            ],
            "**recipes**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
