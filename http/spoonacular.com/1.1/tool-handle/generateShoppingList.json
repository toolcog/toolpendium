{
  "name": "generateShoppingList",
  "description": "Generate the shopping list for a user from the meal planner in a given time frame.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "example": "dsky",
        "type": "string",
        "description": "The username."
      },
      "start-date": {
        "example": "2020-06-01",
        "type": "string",
        "description": "The start date in the format yyyy-mm-dd."
      },
      "end-date": {
        "example": "2020-06-07",
        "type": "string",
        "description": "The end date in the format yyyy-mm-dd."
      },
      "hash": {
        "type": "string",
        "description": "The private hash for the username."
      }
    },
    "required": [
      "username",
      "start-date",
      "end-date",
      "hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.spoonacular.com/mealplanner/{username}/shopping-list/{start-date}/{end-date}{?hash}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**aisles**",
            "**cost**",
            "**endDate**",
            "**startDate**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
