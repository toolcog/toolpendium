{
  "name": "autocompleteMenuItemSearch",
  "description": "Generate suggestions for menu items based on a (partial) query. The matches will be found by looking in the title only.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "example": "chicke",
        "type": "string",
        "description": "The (partial) search query."
      },
      "number": {
        "example": 10,
        "maximum": 25,
        "minimum": 1,
        "type": "number",
        "description": "The number of results to return (between 1 and 25)."
      }
    },
    "required": [
      "query"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/menuItems/suggest{?query,number}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**results**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
