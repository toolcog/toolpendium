{
  "name": "analyzeRecipeInstructions",
  "description": "This endpoint allows you to break down instructions into atomic steps. Furthermore, each step will contain the ingredients and equipment required. Additionally, all ingredients and equipment from the recipe's instructions will be extracted independently of the step they're used in.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "enum": [
          "application/x-www-form-urlencoded",
          "application/json",
          "multipart/form-data"
        ],
        "example": "application/json",
        "type": "string",
        "description": "The content type."
      },
      "body": {
        "type": "string"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.spoonacular.com/recipes/analyzeInstructions"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      }
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**equipment**",
            "**ingredients**",
            "**parsedInstructions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
