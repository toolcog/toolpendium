{
  "name": "classifyGroceryProductBulk",
  "description": "Provide a set of product jsons, get back classified products.",
  "parameters": {
    "type": "object",
    "properties": {
      "locale": {
        "example": "en_US",
        "type": "string",
        "description": "The display name of the returned category, supported is en_US (for American English) and en_GB (for British English)."
      },
      "body": {
        "description": "",
        "items": {
          "properties": {
            "plu_code": {
              "type": "string"
            },
            "title": {
              "minLength": 1,
              "type": "string"
            },
            "upc": {
              "type": "string"
            }
          },
          "required": [
            "title",
            "upc",
            "plu_code"
          ],
          "type": "object"
        },
        "minItems": 0,
        "type": "array",
        "uniqueItems": true,
        "x-examples": {
          "example-1": [
            {
              "plu_code": "",
              "title": "Kroger Vitamin A & D Reduced Fat 2% Milk",
              "upc": ""
            }
          ]
        }
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.spoonacular.com/food/products/classifyBatch{?locale}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**breadcrumbs**",
            "**category**",
            "**cleanTitle**",
            "**image**",
            "**usdaCode**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
