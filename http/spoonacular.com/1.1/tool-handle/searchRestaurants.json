{
  "name": "searchRestaurants",
  "description": "Search through thousands of restaurants (in North America) by location, cuisine, budget, and more.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "example": "beach cafe",
        "type": "string",
        "description": "The search query."
      },
      "lat": {
        "example": 37.7786357,
        "type": "number",
        "description": "The latitude of the user's location."
      },
      "lng": {
        "example": -122.3918135,
        "type": "number",
        "description": "The longitude of the user's location.\"."
      },
      "distance": {
        "example": 2,
        "type": "number",
        "description": "The distance around the location in miles."
      },
      "budget": {
        "example": 20,
        "type": "number",
        "description": "The user's budget for a meal in USD."
      },
      "cuisine": {
        "example": "italian",
        "type": "string",
        "description": "The cuisine of the restaurant."
      },
      "min-rating": {
        "example": 4.4,
        "type": "number",
        "description": "The minimum rating of the restaurant between 0 and 5."
      },
      "is-open": {
        "example": true,
        "type": "boolean",
        "description": "Whether the restaurant must be open at the time of search."
      },
      "sort": {
        "example": "distance",
        "type": "string",
        "description": "How to sort the results, one of the following 'cheapest', 'fastest', 'rating', 'distance' or the default 'relevance'."
      },
      "page": {
        "example": 0,
        "type": "number",
        "description": "The page number of results."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/restaurants/search{?query,lat,lng,distance,budget,cuisine,min-rating,is-open,sort,page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**restaurants**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
