{
  "name": "searchAllFood",
  "description": "Search all food content with one call. That includes recipes, grocery products, menu items, simple foods (ingredients), and food videos.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "example": "apple",
        "type": "string",
        "description": "The search query."
      },
      "offset": {
        "maximum": 900,
        "minimum": 0,
        "type": "integer",
        "description": "The number of results to skip (between 0 and 900)."
      },
      "number": {
        "default": 10,
        "example": 10,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "The maximum number of items to return (between 1 and 100). Defaults to 10."
      }
    },
    "required": [
      "query"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/search{?query,offset,number}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**limit**",
            "**offset**",
            "**query**",
            "**searchResults**",
            "**totalResults**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
