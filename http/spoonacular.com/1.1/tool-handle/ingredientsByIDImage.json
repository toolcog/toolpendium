{
  "name": "ingredientsByIDImage",
  "description": "Visualize a recipe's ingredient list.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": 1082038,
        "type": "number",
        "description": "The recipe id."
      },
      "measure": {
        "enum": [
          "us",
          "metric"
        ],
        "example": "metric",
        "type": "string",
        "description": "Whether the the measures should be 'us' or 'metric'."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/recipes/{id}/ingredientWidget.png{?measure}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
