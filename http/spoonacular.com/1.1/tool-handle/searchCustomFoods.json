{
  "name": "searchCustomFoods",
  "description": "Search custom foods in a user's account.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "example": "burger",
        "type": "string",
        "description": "The (natural language) search query."
      },
      "username": {
        "example": "dsky",
        "type": "string",
        "description": "The username."
      },
      "hash": {
        "example": "4b5v4398573406",
        "type": "string",
        "description": "The private hash for the username."
      },
      "offset": {
        "maximum": 900,
        "minimum": 0,
        "type": "integer",
        "description": "The number of results to skip (between 0 and 900)."
      },
      "number": {
        "default": 10,
        "example": 10,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "The maximum number of items to return (between 1 and 100). Defaults to 10."
      }
    },
    "required": [
      "username",
      "hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/customFoods/search{?query,username,hash,offset,number}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**customFoods**",
            "**number**",
            "**offset**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
