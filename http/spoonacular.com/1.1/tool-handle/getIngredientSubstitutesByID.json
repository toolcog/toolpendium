{
  "name": "getIngredientSubstitutesByID",
  "description": "Search for substitutes for a given ingredient.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": 1001,
        "type": "number",
        "description": "The id of the ingredient you want substitutes for."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/ingredients/{id}/substitutes"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**ingredient**",
            "**message**",
            "**substitutes**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
