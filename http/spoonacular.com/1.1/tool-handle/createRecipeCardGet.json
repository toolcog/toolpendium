{
  "name": "createRecipeCardGet",
  "description": "Generate a recipe card for a recipe.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": 4632,
        "type": "number",
        "description": "The recipe id."
      },
      "mask": {
        "example": "ellipseMask",
        "type": "string",
        "description": "The mask to put over the recipe image (\"ellipseMask\", \"diamondMask\", \"starMask\", \"heartMask\", \"potMask\", \"fishMask\")."
      },
      "backgroundImage": {
        "example": "background1",
        "type": "string",
        "description": "The background image (\"none\",\"background1\", or \"background2\")."
      },
      "backgroundColor": {
        "example": "ffffff",
        "type": "string",
        "description": "The background color for the recipe card as a hex-string."
      },
      "fontColor": {
        "example": "333333",
        "type": "string",
        "description": "The font color for the recipe card as a hex-string."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/recipes/{id}/card{?mask,backgroundImage,backgroundColor,fontColor}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
