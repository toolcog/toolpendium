{
  "name": "imageClassificationByURL",
  "description": "Classify a food image.",
  "parameters": {
    "type": "object",
    "properties": {
      "imageUrl": {
        "example": "https://spoonacular.com/recipeImages/635350-240x150.jpg",
        "type": "string",
        "description": "The URL of the image to be classified."
      }
    },
    "required": [
      "imageUrl"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/images/classify{?imageUrl}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**category**",
            "**probability**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
