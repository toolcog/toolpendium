{
  "name": "getMealPlanTemplate",
  "description": "Get information about a meal plan template.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "example": "dsky",
        "type": "string",
        "description": "The username."
      },
      "id": {
        "example": 15678,
        "type": "number",
        "description": "The shopping list item id."
      },
      "hash": {
        "type": "string",
        "description": "The private hash for the username."
      }
    },
    "required": [
      "username",
      "id",
      "hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/mealplanner/{username}/templates/{id}{?hash}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**days**",
            "**id**",
            "**name**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
