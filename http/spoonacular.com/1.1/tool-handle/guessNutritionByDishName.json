{
  "name": "guessNutritionByDishName",
  "description": "Estimate the macronutrients of a dish based on its title.",
  "parameters": {
    "type": "object",
    "properties": {
      "title": {
        "example": "Spaghetti Aglio et Olio",
        "type": "string",
        "description": "The title of the dish."
      }
    },
    "required": [
      "title"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/recipes/guessNutrition{?title}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**calories**",
              {
                "$ul": [
                  [
                    "**confidenceRange95Percent**",
                    {
                      "$ul": [
                        "**max**",
                        "**min**"
                      ]
                    }
                  ],
                  "**standardDeviation**",
                  "**unit**",
                  "**value**"
                ]
              }
            ],
            [
              "**carbs**",
              {
                "$ul": [
                  [
                    "**confidenceRange95Percent**",
                    {
                      "$ul": [
                        "**max**",
                        "**min**"
                      ]
                    }
                  ],
                  "**standardDeviation**",
                  "**unit**",
                  "**value**"
                ]
              }
            ],
            [
              "**fat**",
              {
                "$ul": [
                  [
                    "**confidenceRange95Percent**",
                    {
                      "$ul": [
                        "**max**",
                        "**min**"
                      ]
                    }
                  ],
                  "**standardDeviation**",
                  "**unit**",
                  "**value**"
                ]
              }
            ],
            [
              "**protein**",
              {
                "$ul": [
                  [
                    "**confidenceRange95Percent**",
                    {
                      "$ul": [
                        "**max**",
                        "**min**"
                      ]
                    }
                  ],
                  "**standardDeviation**",
                  "**unit**",
                  "**value**"
                ]
              }
            ],
            "**recipesUsed**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
