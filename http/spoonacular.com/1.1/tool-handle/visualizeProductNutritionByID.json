{
  "name": "visualizeProductNutritionByID",
  "description": "Visualize a product's nutritional information as HTML including CSS.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": 7657,
        "type": "number",
        "description": "The id of the product."
      },
      "defaultCss": {
        "default": true,
        "example": false,
        "type": "boolean",
        "description": "Whether the default CSS should be added to the response."
      },
      "Accept": {
        "enum": [
          "application/json",
          "text/html",
          "media/*"
        ],
        "example": "application/json",
        "type": "string",
        "description": "Accept header."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/products/{id}/nutritionWidget{?defaultCss}"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
