{
  "name": "autocompleteIngredientSearch",
  "description": "Autocomplete the entry of an ingredient.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "example": "burger",
        "type": "string",
        "description": "The (natural language) search query."
      },
      "number": {
        "default": 10,
        "example": 10,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "The maximum number of items to return (between 1 and 100). Defaults to 10."
      },
      "metaInformation": {
        "example": false,
        "type": "boolean",
        "description": "Whether to return more meta information about the ingredients."
      },
      "intolerances": {
        "example": "egg",
        "type": "string",
        "description": "A comma-separated list of intolerances. All recipes returned must not contain ingredients that are not suitable for people with the intolerances entered. See a full list of supported intolerances."
      },
      "language": {
        "enum": [
          "en",
          "de"
        ],
        "example": "en",
        "type": "string",
        "description": "The language of the input. Either 'en' or 'de'."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/food/ingredients/autocomplete{?query,number,metaInformation,intolerances,language}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**aisle**",
            "**id**",
            "**image**",
            "**name**",
            "**possibleUnits**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
