{
  "name": "quickAnswer",
  "description": "Answer a nutrition related natural language question.",
  "parameters": {
    "type": "object",
    "properties": {
      "q": {
        "example": "How much vitamin c is in 2 apples?",
        "type": "string",
        "description": "The nutrition related question."
      }
    },
    "required": [
      "q"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.spoonacular.com/recipes/quickAnswer{?q}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**answer**",
            "**image**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
