{
  "name": "queryBatchStatusV3",
  "description": "<p>Use v4 instead</p>\nFetch the status of a specific batch of payees. The batch is fully processed when status is ACCEPTED and pendingCount is 0 ( 200 - OK, 404 - batch not found ).\n",
  "parameters": {
    "type": "object",
    "properties": {
      "batchId": {
        "format": "uuid",
        "type": "string",
        "description": "Batch Id"
      }
    },
    "required": [
      "batchId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.sandbox.velopayments.com//v3/payees/batch/{batchId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**failureCount**",
            "**failures**",
            "**pendingCount**",
            "**status**: Batch Status. One of the following values: SUBMITTED, ACCEPTED"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
