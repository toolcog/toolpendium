{
  "name": "listPaymentChanges",
  "description": "Deprecated (use /v4/payments/deltas instead)",
  "parameters": {
    "type": "object",
    "properties": {
      "payorId": {
        "format": "uuid",
        "type": "string",
        "description": "The Payor ID to find associated Payments"
      },
      "updatedSince": {
        "format": "date-time",
        "type": "string",
        "description": "The updatedSince filter in the format YYYY-MM-DDThh:mm:ss+hh:mm"
      },
      "page": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "Page number. Default is 1."
      },
      "pageSize": {
        "default": 100,
        "format": "int32",
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "The number of results to return in a page"
      }
    },
    "required": [
      "payorId",
      "updatedSince"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.sandbox.velopayments.com//v1/deltas/payments{?payorId,updatedSince,page,pageSize}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List Payment Changes Response Object",
        "**Key properties:**",
        {
          "$ul": [
            "**content**",
            "**links**",
            [
              "**page**",
              {
                "$ul": [
                  "**numberOfElements**",
                  "**page**",
                  "**pageSize**",
                  "**totalElements**",
                  "**totalPages**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
