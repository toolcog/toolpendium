{
  "name": "getPaymentsForPayoutV3",
  "description": "Retrieve payments for a payout",
  "parameters": {
    "type": "object",
    "properties": {
      "payoutId": {
        "format": "uuid",
        "type": "string",
        "description": "Id of the payout"
      },
      "status": {
        "enum": [
          "ACCEPTED",
          "REJECTED",
          "WITHDRAWN",
          "WITHDRAWABLE"
        ],
        "type": "string",
        "description": "Payment Status\n* ACCEPTED: any payment which was accepted at submission time (status may have changed since)\n* REJECTED: any payment rejected by initial submission processing\n* WITHDRAWN: any payment which has been withdrawn\n* WITHDRAWABLE: any payment eligible for withdrawal\n"
      },
      "remoteId": {
        "type": "string",
        "description": "The remote id of the payees."
      },
      "payorPaymentId": {
        "type": "string",
        "description": "Payor's Id of the Payment"
      },
      "sourceAccountName": {
        "type": "string",
        "description": "Physical Account Name"
      },
      "transmissionType": {
        "enum": [
          "ACH",
          "SAME_DAY_ACH",
          "WIRE"
        ],
        "type": "string",
        "description": "Transmission Type\n* ACH\n* SAME_DAY_ACH\n* WIRE\n"
      },
      "paymentMemo": {
        "type": "string",
        "description": "Payment Memo of the Payment"
      },
      "pageSize": {
        "default": 25,
        "format": "int32",
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "The number of results to return in a page"
      },
      "page": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "Page number. Default is 1."
      }
    },
    "required": [
      "payoutId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.sandbox.velopayments.com//v3/payouts/{payoutId}/payments{?status,remoteId,payorPaymentId,sourceAccountName,transmissionType,paymentMemo,pageSize,page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List Payees Response Object",
        "**Key properties:**",
        {
          "$ul": [
            "**content**",
            "**links**",
            [
              "**page**",
              {
                "$ul": [
                  "**numberOfElements**",
                  "**page**",
                  "**pageSize**",
                  "**totalElements**",
                  "**totalPages**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**correlationId**: a unique identifier to track a request or related sequence of requests",
            "**errors**: one or more errors",
            "**httpStatusCode**: this will mirror the Status-Code part of the Status-Line http response header and is included for extra clarity"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
