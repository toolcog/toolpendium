  "name": "getFundingByIdV1",
  "description": "Get Funding by Id",
  "parameters": {
    "type": "object",
    "properties": {
      "fundingId": {
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "fundingId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
