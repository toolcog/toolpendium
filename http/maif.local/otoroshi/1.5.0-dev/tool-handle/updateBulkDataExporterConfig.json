{
  "name": "updateBulkDataExporterConfig",
  "description": "Update a data exporter configs",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://otoroshi-api.oto.tools//api/data-exporter-configs/_bulk"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "The bulk response",
        "**Key properties:**",
        {
          "$ul": [
            "**id**: Data exporter id",
            "**status**: Status",
            "**updated**: Whether the action was carried out correctly or not"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
