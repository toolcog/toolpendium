{
  "name": "health",
  "description": "Import the full state of Otoroshi as a file",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://otoroshi-api.oto.tools//health"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The structure that represent current Otoroshi health",
        "**Key properties:**",
        {
          "$ul": [
            "**datastore**",
            "**otoroshi**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
