{
  "name": "deleteApiKeyFromGroup",
  "description": "Delete an api key for a specified service group",
  "parameters": {
    "type": "object",
    "properties": {
      "groupId": {
        "type": "string",
        "description": "The api key group id"
      },
      "clientId": {
        "type": "string",
        "description": "the api key id"
      }
    },
    "required": [
      "groupId",
      "clientId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://otoroshi-api.oto.tools//api/groups/{groupId}/apikeys/{clientId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**deleted**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
