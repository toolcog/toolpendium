{
  "name": "findAllGlobalJwtVerifiers",
  "description": "Get all global JWT verifiers",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://otoroshi-api.oto.tools//api/verifiers"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "A JWT verifier used by multiple service descriptor",
        "**Key properties:**",
        {
          "$ul": [
            "**algoSettings**",
            "**desc**: Verifier description",
            "**enabled**: Is it enabled",
            "**id**: Verifier id",
            "**name**: Verifier name",
            "**source**",
            "**strategy**",
            "**strict**: Does it fail if JWT not found"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
