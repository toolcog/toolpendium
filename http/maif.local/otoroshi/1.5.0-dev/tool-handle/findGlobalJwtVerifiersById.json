{
  "name": "findGlobalJwtVerifiersById",
  "description": "Get one global JWT verifiers",
  "parameters": {
    "type": "object",
    "properties": {
      "verifierId": {
        "type": "string",
        "description": "The jwt verifier id"
      }
    },
    "required": [
      "verifierId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://otoroshi-api.oto.tools//api/verifiers/{verifierId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A JWT verifier used by multiple service descriptor",
        "**Key properties:**",
        {
          "$ul": [
            "**algoSettings**",
            "**desc**: Verifier description",
            "**enabled**: Is it enabled",
            "**id**: Verifier id",
            "**name**: Verifier name",
            "**source**",
            "**strategy**",
            "**strict**: Does it fail if JWT not found"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
