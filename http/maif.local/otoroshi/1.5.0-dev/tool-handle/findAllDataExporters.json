{
  "name": "findAllDataExporters",
  "description": "Get all data exporter configs",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://otoroshi-api.oto.tools//api/data-exporter-configs"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Settings to export Otorshi events",
        "**Key properties:**",
        {
          "$ul": [
            "**bufferSize**: buffer size",
            "**config**: Data Exporter config",
            "**desc**: Description",
            "**enabled**: Boolean",
            [
              "**filtering**",
              {
                "$ul": [
                  "**exclude**: Excluding pattern",
                  "**include**: Including pattern"
                ]
              }
            ],
            "**groupDuration**: duration",
            "**groupSize**: Group size",
            "**id**: Id",
            "**jsonWorkers**: nb workers",
            [
              "**location**",
              {
                "$ul": [
                  "**teams**: Team ids",
                  "**tenant**: Tenant id"
                ]
              }
            ],
            "**metadata**: Metadata",
            "**name**: Name",
            "**projection**: projection",
            "**sendWorkers**: send workers",
            "**typ**: Type of data exporter"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
