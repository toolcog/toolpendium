{
  "name": "DataExporterTemplate",
  "description": "Get all data exporter configs",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "description": "The data exporter config type"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://otoroshi-api.oto.tools//api/data-exporter-configs/_template{?type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Settings to export Otorshi events",
        "**Key properties:**",
        {
          "$ul": [
            "**bufferSize**: buffer size",
            "**config**: Data Exporter config",
            "**desc**: Description",
            "**enabled**: Boolean",
            [
              "**filtering**",
              {
                "$ul": [
                  "**exclude**: Excluding pattern",
                  "**include**: Including pattern"
                ]
              }
            ],
            "**groupDuration**: duration",
            "**groupSize**: Group size",
            "**id**: Id",
            "**jsonWorkers**: nb workers",
            [
              "**location**",
              {
                "$ul": [
                  "**teams**: Team ids",
                  "**tenant**: Tenant id"
                ]
              }
            ],
            "**metadata**: Metadata",
            "**name**: Name",
            "**projection**: projection",
            "**sendWorkers**: send workers",
            "**typ**: Type of data exporter"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
