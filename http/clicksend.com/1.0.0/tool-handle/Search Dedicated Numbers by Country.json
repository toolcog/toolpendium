      }
    },
    "required": [
      "country",
      "search",
      "search_type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rest.clicksend.com/v3/numbers/search/{country}?{search}=1&{search_type}=2"
    }
  },
  "responses": {}
}
