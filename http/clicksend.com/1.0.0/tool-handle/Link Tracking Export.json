{
  "name": "Link_Tracking_Export",
  "description": "Compile all link tracking for the specified campaign into a CSV for download.",
  "parameters": {
    "type": "object",
    "properties": {
      "campaign_id": {
        "type": "number",
        "description": "Your campaign id."
      },
      "filename": {
        "type": "string",
        "description": "Your export filename."
      }
    },
    "required": [
      "campaign_id",
      "filename"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rest.clicksend.com/v3/sms-campaigns/{campaign_id}/link-export?filename={filename}"
    }
  },
  "responses": {}
}
