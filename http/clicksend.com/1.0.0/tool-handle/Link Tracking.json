{
  "name": "Link_Tracking",
  "description": "This endpoint gives you a paginated overview of all the links in a particular campaign, and who has and hasn't visited them.",
  "parameters": {
    "type": "object",
    "properties": {
      "campaign_id": {
        "type": "number",
        "description": "Your campaign id."
      }
    },
    "required": [
      "campaign_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://rest.clicksend.com/v3/sms-campaigns/{campaign_id}/link-tracking"
    }
  },
  "responses": {}
}
