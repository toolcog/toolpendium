{
  "name": "Create_Email_to_SMS_Allowed_Address",
  "description": "Create an allowed email address.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "example": {
          "email_address": "Cv3p0@gmail.com",
          "from": "+17128845887"
        },
        "properties": {
          "email_address": {
            "description": "Your email address.",
            "type": "string"
          },
          "from": {
            "description": "Your sender id - [more info](http://help.clicksend.com/SMS/what-is-a-sender-id-or-sender-number).",
            "type": "string"
          }
        },
        "required": [
          "email_address"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://rest.clicksend.com/v3/sms/email-sms"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
