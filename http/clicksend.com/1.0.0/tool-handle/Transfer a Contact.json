{
  "name": "Transfer_a_Contact",
  "description": "Transfers a specific contact to another list.",
  "parameters": {
    "type": "object",
    "properties": {
      "from_list_id": {
        "type": "number",
        "description": "From list id."
      },
      "contact_id": {
        "type": "number",
        "description": "Contact ID."
      },
      "to_list_id": {
        "type": "number",
        "description": "To list id."
      }
    },
    "required": [
      "from_list_id",
      "contact_id",
      "to_list_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://rest.clicksend.com/v3/lists/{from_list_id}/contacts/{contact_id}/{to_list_id}"
    }
  },
  "responses": {}
}
