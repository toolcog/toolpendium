{
  "name": "getTransactionsByPayee",
  "description": "Returns all transactions for a specified payee",
  "parameters": {
    "type": "object",
    "properties": {
      "budget_id": {
        "type": "string",
        "description": "The id of the budget. \"last-used\" can be used to specify the last used budget and \"default\" can be used if default budget selection is enabled (see: https://api.ynab.com/#oauth-default-budget)."
      },
      "payee_id": {
        "type": "string",
        "description": "The id of the payee"
      },
      "since_date": {
        "format": "date",
        "type": "string",
        "description": "If specified, only transactions on or after this date will be included.  The date should be ISO formatted (e.g. 2016-12-30)."
      },
      "type": {
        "enum": [
          "uncategorized",
          "unapproved"
        ],
        "type": "string",
        "description": "If specified, only transactions of the specified type will be included. \"uncategorized\" and \"unapproved\" are currently supported."
      },
      "last_knowledge_of_server": {
        "format": "int64",
        "type": "integer",
        "description": "The starting server knowledge.  If provided, only entities that have changed since `last_knowledge_of_server` will be included."
      }
    },
    "required": [
      "budget_id",
      "payee_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ynab.com/v1/budgets/{budget_id}/payees/{payee_id}/transactions{?since_date,type,last_knowledge_of_server}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**server_knowledge**: The knowledge of the server",
                  "**transactions**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**detail**",
                  "**id**",
                  "**name**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
