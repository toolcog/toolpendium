      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "UpdateSubscriptionsBulkRequest": {
        "description": "Request model for updating subscriptions",
        "properties": {
          "isSubscribed": {
            "description": "Creates or deletes a subscription on each item in an array of objects.",
            "type": "boolean"
          },
          "objectIds": {
            "description": "List of ids",
            "items": {
              "description": "List of ids",
              "format": "int64",
              "type": "integer"
            },
            "type": "array"
          }
        },
        "required": [
          "isSubscribed",
          "objectIds"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/api/v4/user/subscriptions/download_shares"
    },
    "headers": {
