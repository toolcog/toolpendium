    "type": "object",
    "properties": {
      "emergency_code": {
        "type": "string"
      },
      "X-Sds-Auth-Token": {
        "type": "string",
        "description": "Authentication token"
      }
    },
    "required": [
      "emergency_code"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v4/user/account/mfa{?emergency_code}"
    },
    "headers": {
