{
  "name": "requestDownloadShareQr",
  "description": "### Description:  \nRetrieve detailed information about one Download Share.\n\n### Precondition:\nUser with <span style='padding: 3px; background-color: #F6F7F8; border: 1px solid #000; border-radius: 5px; display: inline;'>&#128275; manage download share</span> permissions on target node.\n\n### Postcondition:\nDownload Share is returned\n\n### Further Information:\nNone.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Sds-Date-Format": {
        "enum": [
          "UTC",
          "LOCAL",
          "OFFSET",
          "EPOCH",
          "LEET"
        ],
        "type": "string",
        "description": "Date time format (cf. [RFC 3339](https://www.ietf.org/rfc/rfc3339.txt) & [leettime.de](http://leettime.de/))"
      },
      "share_id": {
        "format": "int64",
        "type": "integer",
        "description": "Share ID"
      },
      "X-Sds-Auth-Token": {
        "type": "string",
        "description": "Authentication token"
      }
    },
    "required": [
      "share_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v4/shares/downloads/{share_id}/qr"
    },
    "headers": {
      "X-Sds-Date-Format": {
        "$": "X-Sds-Date-Format"
      },
      "X-Sds-Auth-Token": {
        "$": "X-Sds-Auth-Token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Download Share information",
        "**Key properties:**",
        {
          "$ul": [
            "**accessKey**: Share access key to generate secure link",
            "**classification**: &#128679; Deprecated since v4.11.0",
            "**cntDownloads**: Downloads counter (incremented on each download)",
            "**createdAt**: Creation date",
            [
              "**createdBy**: User information",
              {
                "$ul": [
                  "**avatarUuid**: &#128640; Since v4.11.0",
                  "**displayName**: &#128679; Deprecated since v4.11.0",
                  "**email**: &#128640; Since v4.11.0",
                  "**firstName**: &#128640; Since v4.11.0",
                  "**id**: Unique identifier for the user",
                  "**lastName**: &#128640; Since v4.11.0",
                  "**title**: &#128679; Deprecated since v4.18.0",
                  "**userName**: &#128640; Since v4.13.0",
                  "**userType**: &#128640; Since v4.11.0"
                ]
              }
            ],
            "**dataUrl**: Path to shared download node",
            "**expireAt**: Expiration date",
            "**id**: Share ID",
            "**internalNotes**: &#128640; Since v4.11.0",
            "**isEncrypted**: Encrypted share",
            "**isProtected**: Is share protected by password",
            "**maxDownloads**: Max allowed downloads",
            "**name**: Alias name",
            "**nodeId**: Source node ID",
            "**nodePath**: Path to shared download node",
            "**nodeType**: Node type",
            "**notes**: User notes",
            "**notifyCreator**: &#128679; Deprecated since v4.20.0",
            "**recipients**: &#128679; Deprecated since v4.11.0",
            "**showCreatorName**: Show creator first and last name.",
            "**showCreatorUsername**: Show creator email address.",
            "**smsRecipients**: &#128679; Deprecated since v4.11.0",
            "**updatedAt**: Modification date",
            [
              "**updatedBy**: User information",
              {
                "$ul": [
                  "**avatarUuid**: &#128640; Since v4.11.0",
                  "**displayName**: &#128679; Deprecated since v4.11.0",
                  "**email**: &#128640; Since v4.11.0",
                  "**firstName**: &#128640; Since v4.11.0",
                  "**id**: Unique identifier for the user",
                  "**lastName**: &#128640; Since v4.11.0",
                  "**title**: &#128679; Deprecated since v4.18.0",
                  "**userName**: &#128640; Since v4.13.0",
                  "**userType**: &#128640; Since v4.11.0"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
