{
  "name": "filterFileDataStoppings",
  "description": "Filter the stopping words from the provided input file",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "description": "provide the type of filtering required stopping/swear"
      },
      "body": {
        "properties": {
          "file": {
            "format": "binary",
            "type": "string"
          }
        },
        "type": "object",
        "description": "Filter the stopping words from the provided input file"
      }
    },
    "required": [
      "type",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://exude-api.herokuapp.com/exude/{type}/file"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
