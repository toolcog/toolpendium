{
  "name": "Preprocessing_GetPageAngle",
  "description": "Analyzes a photo or image of a document and identifies the rotation angle of the page.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "imageFile": {
            "description": "Image file to perform OCR on.  Common file formats such as PNG, JPEG are supported.",
            "format": "binary",
            "type": "string"
          }
        },
        "required": [
          "imageFile"
        ],
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.cloudmersive.com/ocr/preprocessing/image/get-page-angle"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Result of performing a get-page-angle operation",
        "**Key properties:**",
        {
          "$ul": [
            "**Angle**: Angle of the page in radians; 0 represents perfectly horizontal",
            "**Successful**: True if the operation was successful, false otherwise"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
