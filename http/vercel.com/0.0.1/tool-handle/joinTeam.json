{
  "name": "joinTeam",
  "description": "Join a team with a provided invite code or team ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "properties": {
          "inviteCode": {
            "description": "The invite code to join the team.",
            "examples": [
              "fisdh38aejkeivn34nslfore9vjtn4ls"
            ],
            "type": "string"
          },
          "teamId": {
            "description": "The team ID.",
            "examples": [
              "team_3oNwMKqLHqEBh02CTPsrbNbe"
            ],
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "teamId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vercel.com/v1/teams/{teamId}/members/teams/join"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Successfully joined a team.",
        "**Key properties:**",
        {
          "$ul": [
            "**from**: The origin of how the user joined.",
            "**name**: The name of the team the user joined.",
            "**slug**: The slug of the team the user joined.",
            "**teamId**: The ID of the team the user joined."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
