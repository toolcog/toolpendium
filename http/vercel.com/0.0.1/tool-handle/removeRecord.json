{
  "name": "removeRecord",
  "description": "Removes an existing DNS record from a domain name.",
  "parameters": {
    "type": "object",
    "properties": {
      "domain": {
        "examples": [
          "example.com"
        ],
        "type": "string"
      },
      "recordId": {
        "examples": [
          "rec_V0fra8eEgQwEpFhYG2vTzC3K"
        ],
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      }
    },
    "required": [
      "domain",
      "recordId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.vercel.com/v2/domains/{domain}/records/{recordId}{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
