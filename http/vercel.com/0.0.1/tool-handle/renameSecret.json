{
  "name": "renameSecret",
  "description": "Enables to edit the name of a user's secret. The name has to be unique to that user's secrets.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "The name of the secret.",
        "examples": [
          "my-api-key"
        ],
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "name": {
            "description": "The name of the new secret.",
            "examples": [
              "my-api-key"
            ],
            "maximum": 100,
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      }
    },
    "required": [
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.vercel.com/v2/secrets/{name}{?teamId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**: Enables basic storage and retrieval of dates and times.",
            "**name**",
            "**oldName**",
            "**uid**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
