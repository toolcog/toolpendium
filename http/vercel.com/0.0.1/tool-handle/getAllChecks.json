{
  "name": "getAllChecks",
  "description": "List all of the checks created for a deployment.",
  "parameters": {
    "type": "object",
    "properties": {
      "deploymentId": {
        "description": "The deployment to get all checks for",
        "examples": [
          "dpl_2qn7PZrx89yxY34vEZPD31Y9XVj6"
        ],
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      }
    },
    "required": [
      "deploymentId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vercel.com/v1/deployments/{deploymentId}/checks{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**checks**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
