{
  "name": "createEdgeConfig",
  "description": "Creates an Edge Config.",
  "parameters": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      },
      "body": {
        "properties": {
          "items": {
            "additionalProperties": {
              "oneOf": [
                {
                  "oneOf": [
                    {
                      "type": "string"
                    },
                    {
                      "type": "number"
                    },
                    {
                      "type": "boolean"
                    },
                    {
                      "type": "null"
                    },
                    {
                      "type": "object"
                    }
                  ]
                },
                {
                  "items": {
                    "oneOf": [
                      {
                        "type": "string"
                      },
                      {
                        "type": "number"
                      },
                      {
                        "type": "boolean"
                      },
                      {
                        "type": "null"
                      },
                      {
                        "type": "object"
                      }
                    ]
                  },
                  "type": "array"
                }
              ]
            },
            "propertyNames": {
              "maxLength": 256,
              "pattern": "^[\\\\w-]+$",
              "type": "string"
            },
            "type": "object"
          },
          "slug": {
            "maxLength": 32,
            "pattern": "^[\\\\w-]+$",
            "type": "string"
          }
        },
        "required": [
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vercel.com/v1/edge-config{?teamId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "An Edge Config",
        "**Key properties:**",
        {
          "$ul": [
            "**connectedAt**: Timestamp (in milliseconds) of when the configuration was connected.",
            "**itemCount**",
            "**lastReceivedWebhookEvent**: Timestamp (in milliseconds) of when the last webhook event was received from WorkOS.",
            "**sizeInBytes**",
            "**state**: Current state of the connection.",
            "**status**: Current status of the connection.",
            "**type**: The Identity Provider \"type\", for example Okta."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
