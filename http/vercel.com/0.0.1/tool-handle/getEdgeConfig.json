{
  "name": "getEdgeConfig",
  "description": "Returns an Edge Config.",
  "parameters": {
    "type": "object",
    "properties": {
      "edgeConfigId": {
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      }
    },
    "required": [
      "edgeConfigId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vercel.com/v1/edge-config/{edgeConfigId}{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The EdgeConfig.",
        "**Key properties:**",
        {
          "$ul": [
            "**createdAt**",
            "**creator**",
            "**domain**",
            "**id**",
            "**itemCount**",
            "**name**",
            "**recordType**",
            "**sizeInBytes**",
            "**ttl**",
            "**type**",
            "**value**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
