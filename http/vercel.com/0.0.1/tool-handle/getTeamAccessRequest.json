{
  "name": "getTeamAccessRequest",
  "description": "Check the status of a join request. It'll respond with a 404 if the request has been declined. If no `userId` path segment was provided, this endpoint will instead return the status of the authenticated user.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "userId",
      "teamId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vercel.com/v1/teams/{teamId}/request/{userId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accessRequestedAt**: Timestamp in milliseconds when the user requested access to the team.",
            [
              "**bitbucket**: Map of the connected Bitbucket account.",
              {
                "$ul": [
                  "**login**"
                ]
              }
            ],
            "**confirmed**: Current status of the membership. Will be `true` if confirmed, if pending it'll be `false`.",
            [
              "**github**: Map of the connected GitHub account.",
              {
                "$ul": [
                  "**login**"
                ]
              }
            ],
            [
              "**gitlab**: Map of the connected GitLab account.",
              {
                "$ul": [
                  "**login**"
                ]
              }
            ],
            [
              "**joinedFrom**: A map that describes the origin from where the user joined.",
              {
                "$ul": [
                  "**commitId**",
                  "**dsyncConnectedAt**",
                  "**dsyncUserId**",
                  "**gitUserId**",
                  "**gitUserLogin**",
                  "**idpUserId**",
                  "**origin**",
                  "**repoId**",
                  "**repoPath**",
                  "**ssoConnectedAt**",
                  "**ssoUserId**"
                ]
              }
            ],
            "**teamName**: The name of the team.",
            "**teamSlug**: The slug of the team."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
