{
  "name": "deleteDomain",
  "description": "Delete a previously registered domain name from Vercel. Deleting a domain will automatically remove any associated aliases.",
  "parameters": {
    "type": "object",
    "properties": {
      "domain": {
        "description": "The name of the domain.",
        "examples": [
          "example.com"
        ],
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      }
    },
    "required": [
      "domain"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.vercel.com/v6/domains/{domain}{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**uid**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
