{
  "name": "updateEdgeConfig",
  "description": "Updates an Edge Config.",
  "parameters": {
    "type": "object",
    "properties": {
      "edgeConfigId": {
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      },
      "body": {
        "properties": {
          "slug": {
            "maxLength": 32,
            "pattern": "^[\\\\w-]+$",
            "type": "string"
          }
        },
        "required": [
          "slug"
        ],
        "type": "object"
      }
    },
    "required": [
      "edgeConfigId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.vercel.com/v1/edge-config/{edgeConfigId}{?teamId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "An Edge Config",
        "**Key properties:**",
        {
          "$ul": [
            "**connectedAt**: Timestamp (in milliseconds) of when the configuration was connected.",
            "**itemCount**",
            "**lastReceivedWebhookEvent**: Timestamp (in milliseconds) of when the last webhook event was received from WorkOS.",
            "**sizeInBytes**",
            "**state**: Current state of the connection.",
            "**status**: Current status of the connection.",
            "**type**: The Identity Provider \"type\", for example Okta."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
