{
  "name": "requestAccessToTeam",
  "description": "Request access to a team as a member. An owner has to approve the request. Only 10 users can request access to a team at the same time.",
  "parameters": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "joinedFrom": {
            "additionalProperties": false,
            "properties": {
              "commitId": {
                "description": "The commit sha if the origin is a git provider.",
                "examples": [
                  "f498d25d8bd654b578716203be73084b31130cd7"
                ],
                "type": "string"
              },
              "gitUserId": {
                "description": "The ID of the Git account of the user who requests access.",
                "examples": [
                  103053343
                ],
                "oneOf": [
                  {
                    "type": "string"
                  },
                  {
                    "type": "number"
                  }
                ]
              },
              "gitUserLogin": {
                "description": "The login name for the Git account of the user who requests access.",
                "examples": [
                  "jane-doe"
                ],
                "type": "string"
              },
              "origin": {
                "description": "The origin of the request.",
                "enum": [
                  "import",
                  "teams",
                  "github",
                  "gitlab",
                  "bitbucket",
                  "feedback",
                  "organization-teams"
                ],
                "examples": [
                  "github"
                ],
                "type": "string"
              },
              "repoId": {
                "description": "The ID of the repository for the given Git provider.",
                "examples": [
                  "67753070"
                ],
                "type": "string"
              },
              "repoPath": {
                "description": "The path to the repository for the given Git provider.",
                "examples": [
                  "jane-doe/example"
                ],
                "type": "string"
              }
            },
            "required": [
              "origin"
            ],
            "type": "object"
          }
        },
        "required": [
          "joinedFrom"
        ],
        "type": "object"
      }
    },
    "required": [
      "teamId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vercel.com/v1/teams/{teamId}/request"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accessRequestedAt**",
            [
              "**bitbucket**",
              {
                "$ul": [
                  "**login**"
                ]
              }
            ],
            "**confirmed**",
            [
              "**github**",
              {
                "$ul": [
                  "**login**"
                ]
              }
            ],
            [
              "**gitlab**",
              {
                "$ul": [
                  "**login**"
                ]
              }
            ],
            [
              "**joinedFrom**",
              {
                "$ul": [
                  "**commitId**",
                  "**dsyncConnectedAt**",
                  "**dsyncUserId**",
                  "**gitUserId**",
                  "**gitUserLogin**",
                  "**idpUserId**",
                  "**origin**",
                  "**repoId**",
                  "**repoPath**",
                  "**ssoConnectedAt**",
                  "**ssoUserId**"
                ]
              }
            ],
            "**teamName**",
            "**teamSlug**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
