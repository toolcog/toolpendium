{
  "name": "deleteAlias",
  "description": "Delete an Alias with the specified ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "aliasId": {
        "description": "The ID or alias that will be removed",
        "examples": [
          "2WjyKQmM8ZnGcJsPWMrHRHrE"
        ],
        "oneOf": [
          {
            "type": "string"
          }
        ]
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      }
    },
    "required": [
      "aliasId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.vercel.com/v2/aliases/{aliasId}{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
