{
  "name": "createSecret",
  "description": "Allows to create a new secret.",
  "parameters": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      },
      "name": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "decryptable": {
            "description": "Whether the secret value can be decrypted after it has been created.",
            "examples": [
              true
            ],
            "type": "boolean"
          },
          "name": {
            "description": "The name of the secret (max 100 characters).",
            "examples": [
              "my-api-key"
            ],
            "maximum": 100,
            "type": "string"
          },
          "projectId": {
            "deprecated": true,
            "description": "Associate a secret to a project.",
            "examples": [
              "prj_2WjyKQmM8ZnGcJsPWMrHRHrE"
            ],
            "type": "string"
          },
          "value": {
            "description": "The value of the new secret.",
            "examples": [
              "some secret value"
            ],
            "type": "string"
          }
        },
        "required": [
          "name",
          "value"
        ],
        "type": "object"
      }
    },
    "required": [
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vercel.com/v2/secrets/{name}{?teamId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**: The date when the secret was created.",
            "**createdAt**: Timestamp for when the secret was created.",
            "**decryptable**: Indicates whether the secret value can be decrypted after it has been created.",
            "**name**: The name of the secret.",
            "**projectId**: The unique identifier of the project which the secret belongs to.",
            "**teamId**: The unique identifier of the team the secret was created for.",
            "**uid**: The unique identifier of the secret.",
            "**userId**: The unique identifier of the user who created the secret.",
            [
              "**value**",
              {
                "$ul": [
                  "**data**",
                  "**type**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
