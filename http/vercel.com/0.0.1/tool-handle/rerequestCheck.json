{
  "name": "rerequestCheck",
  "description": "Rerequest a selected check that has failed.",
  "parameters": {
    "type": "object",
    "properties": {
      "deploymentId": {
        "description": "The deployment to rerun the check for.",
        "examples": [
          "dpl_2qn7PZrx89yxY34vEZPD31Y9XVj6"
        ],
        "type": "string"
      },
      "checkId": {
        "description": "The check to rerun",
        "examples": [
          "check_2qn7PZrx89yxY34vEZPD31Y9XVj6"
        ],
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      }
    },
    "required": [
      "deploymentId",
      "checkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vercel.com/v1/deployments/{deploymentId}/checks/{checkId}/rerequest{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
