{
  "name": "updateRecord",
  "description": "Updates an existing DNS record for a domain name.",
  "parameters": {
    "type": "object",
    "properties": {
      "recordId": {
        "description": "The id of the DNS record",
        "examples": [
          "rec_2qn7pzrx89yxy34vezpd31y9"
        ],
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      },
      "body": {
        "properties": {
          "additionalProperties": false,
          "mxPriority": {
            "description": "The MX priority value of the DNS record",
            "type": [
              "integer",
              "null"
            ]
          },
          "name": {
            "description": "The name of the DNS record",
            "examples": [
              "example-1"
            ],
            "type": [
              "string",
              "null"
            ]
          },
          "srv": {
            "additionalProperties": false,
            "properties": {
              "port": {
                "description": "",
                "type": [
                  "integer",
                  "null"
                ]
              },
              "priority": {
                "description": "",
                "type": [
                  "integer",
                  "null"
                ]
              },
              "target": {
                "description": "",
                "examples": [
                  "example2.com."
                ],
                "maxLength": 255,
                "type": [
                  "string",
                  "null"
                ]
              },
              "weight": {
                "description": "",
                "type": [
                  "integer",
                  "null"
                ]
              }
            },
            "required": [
              "target",
              "weight",
              "port",
              "priority"
            ],
            "type": [
              "object",
              "null"
            ]
          },
          "ttl": {
            "description": "The Time to live (TTL) value of the DNS record",
            "examples": [
              "60"
            ],
            "maximum": 2147483647,
            "minimum": 60,
            "type": [
              "integer",
              "null"
            ]
          },
          "type": {
            "description": "The type of the DNS record",
            "enum": [
              "A",
              "AAAA",
              "ALIAS",
              "CAA",
              "CNAME",
              "MX",
              "SRV",
              "TXT",
              "NS"
            ],
            "examples": [
              "A"
            ],
            "maxLength": 255,
            "type": [
              "string",
              "null"
            ]
          },
          "value": {
            "description": "The value of the DNS record",
            "examples": [
              "google.com"
            ],
            "type": [
              "string",
              "null"
            ]
          }
        },
        "type": "object"
      }
    },
    "required": [
      "recordId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.vercel.com/v1/domains/records/{recordId}{?teamId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "From T, pick a set of properties whose keys are in the union K",
        "**Key properties:**",
        {
          "$ul": [
            "**createdAt**",
            "**creator**",
            "**domain**",
            "**id**",
            "**name**",
            "**recordType**",
            "**ttl**",
            "**type**",
            "**value**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
