{
  "name": "artifactQuery",
  "description": "Query information about an array of artifacts.",
  "parameters": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      },
      "body": {
        "properties": {
          "hashes": {
            "description": "artifact hashes",
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "hashes"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vercel.com/v8/artifacts{?teamId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
