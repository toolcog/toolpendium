{
  "name": "buyDomain",
  "description": "Allows to purchase the specified domain.",
  "parameters": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "expectedPrice": {
            "description": "The price you expect to be charged for the purchase.",
            "examples": [
              10
            ],
            "type": "number"
          },
          "name": {
            "description": "The domain name to purchase.",
            "examples": [
              "example.com"
            ],
            "type": "string"
          },
          "renew": {
            "description": "Indicates whether the domain should be automatically renewed.",
            "examples": [
              true
            ],
            "type": "boolean"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vercel.com/v4/domains/buy{?teamId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**domain**",
              {
                "$ul": [
                  "**created**",
                  "**ns**",
                  "**pending**",
                  "**uid**",
                  "**verified**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
