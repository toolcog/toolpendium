{
  "name": "createEdgeConfigToken",
  "description": "Adds a token to an existing Edge Config.",
  "parameters": {
    "type": "object",
    "properties": {
      "edgeConfigId": {
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "label": {
            "maxLength": 52,
            "type": "string"
          }
        },
        "required": [
          "label"
        ],
        "type": "object"
      }
    },
    "required": [
      "edgeConfigId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.vercel.com/v1/edge-config/{edgeConfigId}/token{?teamId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**token**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
