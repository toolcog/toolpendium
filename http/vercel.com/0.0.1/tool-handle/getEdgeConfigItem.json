{
  "name": "getEdgeConfigItem",
  "description": "Returns a specific Edge Config Item.",
  "parameters": {
    "type": "object",
    "properties": {
      "edgeConfigId": {
        "type": "string"
      },
      "edgeConfigItemKey": {
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      }
    },
    "required": [
      "edgeConfigId",
      "edgeConfigItemKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vercel.com/v1/edge-config/{edgeConfigId}/item/{edgeConfigItemKey}{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The EdgeConfig.",
        "**Key properties:**",
        {
          "$ul": [
            "**createdAt**",
            "**edgeConfigId**",
            "**key**",
            "**updatedAt**",
            "**value**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
