{
  "name": "getProjectEnv",
  "description": "Retrieve the environment variable for a given project.",
  "parameters": {
    "type": "object",
    "properties": {
      "idOrName": {
        "description": "The unique project identifier or the project name",
        "examples": [
          "prj_XLKmu1DyR1eY7zq8UgeRKbA7yVLA"
        ],
        "type": "string"
      },
      "id": {
        "description": "The unique ID for the environment variable to get the decrypted value.",
        "type": "string"
      },
      "teamId": {
        "type": "string",
        "description": "The Team identifier or slug to perform the request on behalf of."
      }
    },
    "required": [
      "idOrName",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vercel.com/v1/projects/{idOrName}/env/{id}{?teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**configurationId**",
            "**createdAt**",
            "**createdBy**",
            "**decrypted**: Whether `value` is decrypted.",
            "**edgeConfigId**",
            "**edgeConfigTokenId**",
            "**gitBranch**",
            "**id**",
            "**key**",
            "**target**",
            "**type**",
            "**updatedAt**",
            "**updatedBy**",
            "**value**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
