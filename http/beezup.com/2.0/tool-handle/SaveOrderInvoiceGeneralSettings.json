{
  "name": "SaveOrderInvoiceGeneralSettings",
  "description": "Save Order Invoice general settings",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/orderInvoiceGeneralSettings"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "orderInvoiceGeneralSettings": {
        "description": "Order Invoice General Settings",
        "properties": {
          "cultureName": {
            "$ref": "#/$defs/cultureName"
          },
          "invoicePrefix": {
            "$ref": "#/$defs/invoicePrefix"
          },
          "invoiceStartingSequenceNumber": {
            "$ref": "#/$defs/invoiceSequenceNumber"
          },
          "productVATPercent": {
            "$ref": "#/$defs/productVATPercent"
          },
          "shippingVATPercent": {
            "$ref": "#/$defs/shippingVATPercent"
          }
        },
        "required": [
          "cultureName",
          "productVATPercent",
          "shippingVATPercent",
          "invoicePrefix",
          "invoiceStartingSequenceNumber"
        ],
        "type": "object"
      },
      "cultureName": {
        "description": "If the error is translated, the culture name will be indicated",
        "example": "en",
        "type": "string",
        "x-exclude": true,
        "x-lov": "Go2CultureName"
      },
      "invoicePrefix": {
        "description": "Invoice Prefix. Can contain 1 to 50 characters, with alphanumeric characters in lowercase uppercase and #, _, -",
        "example": "TOTO",
        "pattern": "^[a-zA-Z0-9#_-]{1,50}$",
        "type": "string",
        "x-pattern": "^[a-zA-Z0-9#_-]{1,50}$"
      },
      "invoiceSequenceNumber": {
        "description": "Invoice Sequence Number",
        "example": "879",
        "minimum": 0,
        "type": "integer"
      },
      "productVATPercent": {
        "description": "Product VAT in percent",
        "example": "4.0",
        "maximum": 100,
        "minimum": 0,
        "type": "number"
      },
      "shippingVATPercent": {
        "description": "Shipping cost VAT in percent",
        "example": "8.0",
        "maximum": 100,
        "minimum": 0,
        "type": "number"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/marketplaces/orders/invoices/settings/general"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
