{
  "name": "SetMerchantOrderInfoV3",
  "description": "Set an Order's merchant information",
  "parameters": {
    "type": "object",
    "properties": {
      "marketplaceTechnicalCode": {
        "type": "string",
        "description": "The marketplace technical code"
      },
      "accountId": {
        "format": "int32",
        "type": "integer"
      },
      "beezUPOrderId": {
        "format": "uuid",
        "type": "string",
        "description": "The BeezUP Order identifier"
      },
      "testMode": {
        "default": "false",
        "type": "boolean",
        "description": "If true, the operation will be not be sent to marketplace. But the validation will be taken in account."
      },
      "body": {
        "$ref": "#/$defs/setMerchantOrderInfoRequest"
      }
    },
    "required": [
      "marketplaceTechnicalCode",
      "accountId",
      "beezUPOrderId",
      "body"
    ],
    "$defs": {
      "setMerchantOrderInfoRequest": {
        "properties": {
          "order_MerchantECommerceSoftwareName": {
            "$ref": "#/$defs/order_MerchantECommerceSoftwareName"
          },
          "order_MerchantECommerceSoftwareVersion": {
            "$ref": "#/$defs/order_MerchantECommerceSoftwareVersion"
          },
          "order_MerchantOrderId": {
            "$ref": "#/$defs/order_MerchantOrderId"
          }
        },
        "required": [
          "order_MerchantOrderId",
          "order_MerchantECommerceSoftwareName",
          "order_MerchantECommerceSoftwareVersion"
        ],
        "type": "object"
      },
      "order_MerchantECommerceSoftwareName": {
        "description": "The e-commerce software name of the merchant",
        "example": "Prestashop",
        "type": "string"
      },
      "order_MerchantECommerceSoftwareVersion": {
        "description": "The e-commece software version of the merchant",
        "example": "123.0.1",
        "type": "string"
      },
      "order_MerchantOrderId": {
        "description": "The order merchant identifier",
        "example": "MyOrderMerchantId",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/orders/v3/{marketplaceTechnicalCode}/{accountId}/{beezUPOrderId}/setMerchantOrderInfo{?testMode}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
