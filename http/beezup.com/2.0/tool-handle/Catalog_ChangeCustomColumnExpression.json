{
  "name": "Catalog_ChangeCustomColumnExpression",
  "description": "Change custom column expression",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "Your store identifier"
      },
      "columnId": {
        "format": "guid",
        "type": "string",
        "description": "The custom column identifier"
      },
      "body": {
        "$ref": "#/$defs/changeCustomColumnExpressionRequest"
      }
    },
    "required": [
      "storeId",
      "columnId",
      "body"
    ],
    "$defs": {
      "changeCustomColumnExpressionRequest": {
        "description": "The request message to change the custom column expression",
        "properties": {
          "encryptedBlocklyExpression": {
            "$ref": "#/$defs/encryptedBlocklyExpression"
          },
          "encryptedExpression": {
            "$ref": "#/$defs/encryptedExpression"
          }
        },
        "required": [
          "encryptedExpression",
          "encryptedBlocklyExpression"
        ],
        "type": "object"
      },
      "encryptedBlocklyExpression": {
        "description": "The encrypted XML Blockly representation of the expression",
        "example": "apokpoa,opz,sixsoisiosnoisn",
        "type": "string"
      },
      "encryptedExpression": {
        "description": "The encrypted excel expression of the column",
        "example": "uziushdczaniodnndonisodndsiondsoidsndoin",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/catalogs/{storeId}/customColumns/{columnId}/expression"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
