{
  "name": "ClearMerchantOrderInfoV3",
  "description": "Clear an Order's merchant information",
  "parameters": {
    "type": "object",
    "properties": {
      "marketplaceTechnicalCode": {
        "type": "string",
        "description": "The marketplace technical code"
      },
      "accountId": {
        "format": "int32",
        "type": "integer"
      },
      "beezUPOrderId": {
        "format": "uuid",
        "type": "string",
        "description": "The BeezUP Order identifier"
      },
      "testMode": {
        "default": "false",
        "type": "boolean",
        "description": "If true, the operation will be not be sent to marketplace. But the validation will be taken in account."
      }
    },
    "required": [
      "marketplaceTechnicalCode",
      "accountId",
      "beezUPOrderId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/orders/v3/{marketplaceTechnicalCode}/{accountId}/{beezUPOrderId}/clearMerchantOrderInfo{?testMode}"
    }
  },
