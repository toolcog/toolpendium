{
  "name": "ConfigureChannelCatalogGeneralSettings",
  "description": "Configure channel catalog general settings",
  "parameters": {
    "type": "object",
    "properties": {
      "channelCatalogId": {
        "format": "guid",
        "type": "string",
        "description": "The channel catalog identifier"
      },
      "body": {
        "$ref": "#/$defs/generalSettings"
      }
    },
    "required": [
      "channelCatalogId",
      "body"
    ],
    "$defs": {
      "generalSettings": {
        "description": "Represents the general settings of your channel catalog",
        "properties": {
          "acceptToPublishInfo": {
            "description": "If true then you authorize disclosure of my statistics generated from clicks and sales",
            "example": "true",
            "type": "boolean"
          },
          "activeBeezUPTracking": {
            "default": "true",
            "description": "Activate BeezUP tracking for my statistics (checked by default)",
            "example": "true",
            "type": "boolean"
          },
          "doNotExportOutOfStockProducts": {
            "description": "Do not export \"out of stock\" products. Note: this option is not taken into account by the counter.\n",
            "example": "true",
            "type": "boolean"
          }
        },
        "required": [
          "acceptToPublishInfo",
          "doNotExportOutOfStockProducts",
          "activeBeezUPTracking"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/channelCatalogs/{channelCatalogId}/settings/general"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
