{
  "name": "GetOrderHistory",
  "description": "[DEPRECATED] Get an Order's harvest and change history",
  "parameters": {
    "type": "object",
    "properties": {
      "marketplaceTechnicalCode": {
        "type": "string",
        "description": "The marketplace technical code"
      },
      "accountId": {
        "format": "int32",
        "type": "integer"
      },
      "beezUPOrderId": {
        "format": "uuid",
        "type": "string",
        "description": "The BeezUP Order identifier"
      },
      "If-None-Match": {
        "type": "string",
        "description": "ETag value to identify the last known version of requested resource.\\\nTo avoid useless exchange, we recommend you to indicate the ETag you previously got from this operation.\\\nIf the ETag value does not match the response will be 200 to give you a new content, otherwise the response will be: 304 Not Modified, without any content.\\\nFor more details go to this link: http://tools.ietf.org/html/rfc7232#section-2.3\n"
      }
    },
    "required": [
      "marketplaceTechnicalCode",
      "accountId",
      "beezUPOrderId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/marketplaces/orders/{marketplaceTechnicalCode}/{accountId}/{beezUPOrderId}/history"
    },
    "headers": {
      "If-None-Match": {
        "$": "If-None-Match"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Describe the history related to an order",
        "**Key properties:**",
        {
          "$ul": [
            "**changeOrderReportings**: The list of change order operation",
            "**harvestOrderReportings**",
            "**lastModificationUtcDate**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
