{
  "name": "UpdateStore",
  "description": "Update some store's information. FYI, you cannot change the country.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "Your store identifier"
      },
      "body": {
        "$ref": "#/$defs/updateStoreRequest"
      }
    },
    "required": [
      "storeId",
      "body"
    ],
    "$defs": {
      "updateStoreRequest": {
        "properties": {
          "name": {
            "$ref": "#/$defs/storeName"
          },
          "sectors": {
            "$ref": "#/$defs/storeSectors"
          },
          "url": {
            "$ref": "#/$defs/storeUrl"
          }
        },
        "required": [
          "name",
          "url",
          "sectors"
        ],
        "type": "object"
      },
      "storeName": {
        "description": "The store name. Must be unique.",
        "example": "My Store",
        "type": "string"
      },
      "storeSectors": {
        "description": "The store's sectors based on the list of values /user/lov/ParamSector",
        "example": [
          "ANIMALERIE",
          "AUTOMOTO"
        ],
        "items": {
          "$ref": "#/$defs/storeSector"
        },
        "type": "array"
      },
      "storeSector": {
        "description": "The store's sector based on the list of values /user/lov/ParamSector",
        "example": "ANIMALERIE",
        "type": "string",
        "x-lov": "ParamSector"
      },
      "storeUrl": {
        "description": "The url of your store",
        "example": "http://www.mystore.com",
        "pattern": "^(https?:\\/\\/)?([\\da-z\\.-]+)\\.([a-z\\.]{2,6})([\\/\\w \\.-]*)*\\/?$",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/customer/stores/{storeId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "409": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
