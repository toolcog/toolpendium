{
  "name": "GetRule",
  "description": "Gets the rule",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "Your store identifier"
      },
      "ruleId": {
        "format": "guid",
        "type": "string",
        "description": "Your rule identifier"
      }
    },
    "required": [
      "storeId",
      "ruleId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/analytics/{storeId}/rules/{ruleId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**actionName**: The optimisation action",
            "**enabled**: Is the rule enabled",
            "**lastExecutionStatus**: The status of the last execution",
            "**lastExecutionUtcDate**: The utc date of the last execution",
            [
              "**links**: Links to retrieve/action on other entities",
              {
                "$ul": [
                  "**delete**",
                  "**disable**",
                  "**enable**",
                  "**movedown**",
                  "**moveup**",
                  "**reportFilter**",
                  "**run**",
                  "**self**",
                  "**update**"
                ]
              }
            ],
            "**position**: Rule execution position",
            "**reportFilterId**: Report filter identifier linked to the rule",
            "**ruleId**: The identifier of the rule",
            "**ruleName**: The name of the rule",
            "**validityEndUtcDate**: Rule validity end utc date",
            "**validityStartUtcDate**: Rule validity start utc date"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
