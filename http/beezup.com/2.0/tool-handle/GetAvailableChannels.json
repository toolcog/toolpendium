{
  "name": "GetAvailableChannels",
  "description": "List all available channel for this store",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "The store identifier"
      }
    },
    "required": [
      "storeId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/channels/{?storeId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "The available channel",
        "**Key properties:**",
        {
          "$ul": [
            "**channelId**: The channel identifier",
            "**channelLogoUrl**: The URL <a href=\"https://en.wikipedia.org/wiki/URL\">https://en.wikipedia.org/wiki/URL</a>",
            "**channelName**: The channel name",
            [
              "**links**: The links related to an available channel",
              {
                "$ul": [
                  "**self**"
                ]
              }
            ],
            "**types**: The type list related to a channel"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
