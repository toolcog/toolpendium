{
  "name": "Importation_SaveCustomColumn",
  "description": "Create or replace a custom column",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "Your store identifier"
      },
      "executionId": {
        "format": "guid",
        "type": "string",
        "description": "The execution identifier of you catalog importation"
      },
      "columnId": {
        "format": "guid",
        "type": "string",
        "description": "The custom column identifier"
      },
      "body": {
        "$ref": "#/$defs/changeCustomColumnRequest"
      }
    },
    "required": [
      "storeId",
      "executionId",
      "columnId",
      "body"
    ],
    "$defs": {
      "changeCustomColumnRequest": {
        "description": "The message request to change a custom column expression an user name",
        "properties": {
          "encryptedBlocklyExpression": {
            "$ref": "#/$defs/encryptedBlocklyExpression"
          },
          "encryptedExpression": {
            "$ref": "#/$defs/encryptedExpression"
          },
          "userColumName": {
            "$ref": "#/$defs/userColumName"
          }
        },
        "required": [
          "encryptedExpression",
          "encryptedBlocklyExpression",
          "userColumName"
        ],
        "type": "object"
      },
      "encryptedBlocklyExpression": {
        "description": "The encrypted XML Blockly representation of the expression",
        "example": "apokpoa,opz,sixsoisiosnoisn",
        "type": "string"
      },
      "encryptedExpression": {
        "description": "The encrypted excel expression of the column",
        "example": "uziushdczaniodnndonisodndsiondsoidsndoin",
        "type": "string"
      },
      "userColumName": {
        "description": "Column named by the user",
        "example": "My SKU",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/catalogs/{storeId}/importations/{executionId}/customColumns/{columnId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "409": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
