{
  "name": "HarvestAccount",
  "description": "Send harvest request for an Account",
  "parameters": {
    "type": "object",
    "properties": {
      "marketplaceTechnicalCode": {
        "type": "string",
        "description": "The marketplace technical code"
      },
      "accountId": {
        "format": "int32",
        "type": "integer"
      },
      "marketplaceOrderId": {
        "type": "string"
      },
      "beezUPOrderId": {
        "type": "string"
      }
    },
    "required": [
      "marketplaceTechnicalCode",
      "accountId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/orders/v3/{marketplaceTechnicalCode}/{accountId}/harvest{?marketplaceOrderId,beezUPOrderId}"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "409": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
