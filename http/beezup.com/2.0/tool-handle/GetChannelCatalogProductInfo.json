{
  "name": "GetChannelCatalogProductInfo",
  "description": "Get channel catalog product information",
  "parameters": {
    "type": "object",
    "properties": {
      "channelCatalogId": {
        "format": "guid",
        "type": "string",
        "description": "The channel catalog identifier"
      },
      "productId": {
        "format": "guid",
        "type": "string",
        "description": "The product identifier"
      }
    },
    "required": [
      "channelCatalogId",
      "productId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/channelCatalogs/{channelCatalogId}/products/{productId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The basic information related to a product",
        "**Key properties:**",
        {
          "$ul": [
            "**productExists**: Indicates if the product still exists in your catalog",
            "**productId**: The product identifier",
            "**productImageUrl**: The product image Url",
            "**productSku**: The product SKU",
            "**productTitle**: The product tile",
            "**disabled**: Indicates if the product has been disabled or not (default: false)",
            "**excluded**: Indicates if the product has been excluded by a exclusion filter (default: false)",
            "**excludedBy**",
            [
              "**links**: Indicates the differents actions you can do on a channel product",
              {
                "$ul": [
                  "**disable**",
                  "**override**",
                  "**reenable**",
                  "**self**"
                ]
              }
            ],
            "**overrides**: The key is the channel column identifier",
            "**uncategorized**: Indicates if the product's category has been NOT mapped to a channel category"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
