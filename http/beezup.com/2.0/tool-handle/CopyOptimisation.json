{
  "name": "CopyOptimisation",
  "description": "Copy product optimisations between 2 channels",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "Your store identifier"
      },
      "body": {
        "$ref": "#/$defs/copyOptimisationRequest"
      }
    },
    "required": [
      "storeId",
      "body"
    ],
    "$defs": {
      "copyOptimisationRequest": {
        "description": "Copy the optimisation between 2 channels",
        "properties": {
          "channelIdSource": {
            "$ref": "#/$defs/BeezUP.Common.ChannelId"
          },
          "channelIdTarget": {
            "$ref": "#/$defs/BeezUP.Common.ChannelId"
          },
          "keepExistingOptimisation": {
            "description": "If true the existing optimisation will be kept",
            "example": "false",
            "type": "boolean"
          }
        },
        "required": [
          "channelIdSource",
          "channelIdTarget",
          "keepExistingOptimisation"
        ],
        "type": "object"
      },
      "BeezUP.Common.ChannelId": {
        "description": "The channel identifier",
        "example": "2dc136a7-0d3d-4cc9-a825-a28a42c53e28",
        "format": "guid",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/analytics/{storeId}/optimisations/copy"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Products optimisatisation copied",
        "**Key properties:**",
        {
          "$ul": [
            "**catalogProductCount**: The catalog product count",
            [
              "**channel**: The basic info related to a channel",
              {
                "$ul": [
                  "**channelId**: The channel identifier",
                  "**channelImageUrl**: The URL <a href=\"https://en.wikipedia.org/wiki/URL\">https://en.wikipedia.org/wiki/URL</a>",
                  "**channelName**: The channel name"
                ]
              }
            ],
            "**enabledProductCount**: The enabled product count"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
