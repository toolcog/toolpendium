{
  "name": "GetChannelColumns",
  "description": "Get channel columns",
  "parameters": {
    "type": "object",
    "properties": {
      "channelId": {
        "format": "guid",
        "type": "string",
        "description": "The channel identifier"
      },
      "Accept-Encoding": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Indicates that the client accepts that the response will be compressed to reduce traffic size."
      },
      "body": {
        "items": {
          "$ref": "#/$defs/BeezUP.Common.ChannelColumnId"
        },
        "type": "array",
        "description": "Allow you to filter the channel column identifier list your want to get"
      }
    },
    "required": [
      "channelId",
      "Accept-Encoding"
    ],
    "$defs": {
      "BeezUP.Common.ChannelColumnId": {
        "description": "The channel column identifier",
        "example": "8a76f06a-fefc-4c0d-bcfe-b210f1482977",
        "format": "guid",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/channels/{channelId}/columns"
    },
    "headers": {
      "Accept-Encoding": {
        "$": "Accept-Encoding"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "The channel column",
        "**Key properties:**",
        {
          "$ul": [
            "**channelColumnDescription**: The channel column description",
            "**channelColumnId**: The channel column identifier",
            "**channelColumnName**: The channel column name",
            [
              "**configuration**: The channel column configuration. The BeezUP Column Name is optional. If the BeezUP column is mapped we will indicate the data type and column importance of the BeezUP column.",
              {
                "$ul": [
                  "**beezUPColumnName**: The BeezUP column name",
                  "**columnDataType**: Data type of the column, will be used for parsing and for consolidation proces (default: String)",
                  "**columnImportance**: Importance of the column (default: Optional)"
                ]
              }
            ],
            "**position**: The position of the column starting from 1",
            "**restrictedValues**: The restricted values. The key will be the restricted value identifier (format:guid) and the value will be the restriced value",
            "**showInMapping**: Indicates if this column must shown in the mapping page or not"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
