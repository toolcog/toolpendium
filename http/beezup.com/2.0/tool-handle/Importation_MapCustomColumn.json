{
  "name": "Importation_MapCustomColumn",
  "description": "Map custom column to a BeezUP column",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "Your store identifier"
      },
      "executionId": {
        "format": "guid",
        "type": "string",
        "description": "The execution identifier of you catalog importation"
      },
      "columnId": {
        "format": "guid",
        "type": "string",
        "description": "The custom column identifier"
      },
      "body": {
        "$ref": "#/$defs/mapBeezUPColumnRequest"
      }
    },
    "required": [
      "storeId",
      "executionId",
      "columnId",
      "body"
    ],
    "$defs": {
      "mapBeezUPColumnRequest": {
        "properties": {
          "beezUPColumnName": {
            "$ref": "#/$defs/BeezUP.Common.BeezUPColumnName"
          }
        },
        "required": [
          "beezUPColumnName"
        ],
        "type": "object"
      },
      "BeezUP.Common.BeezUPColumnName": {
        "description": "The BeezUP column name",
        "example": "CategoryFirstLevel",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/catalogs/{storeId}/importations/{executionId}/customColumns/{columnId}/map"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "409": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
