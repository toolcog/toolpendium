{
  "name": "ShareStore",
  "description": "Share a store to another user",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "Your store identifier"
      },
      "body": {
        "$ref": "#/components/schemas/friendEmail",
        "description": "Your friend's email"
      }
    },
    "required": [
      "storeId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/customer/stores/{storeId}/shares"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
