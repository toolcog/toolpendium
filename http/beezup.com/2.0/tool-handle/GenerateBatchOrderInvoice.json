{
  "name": "GenerateBatchOrderInvoice",
  "description": "Generate an Order Invoice batch",
  "parameters": {
    "type": "object",
    "properties": {
      "userName": {
        "type": "string",
        "description": "Sometimes the user in the e-commerce application is not the same as user associated with the current subscription key. We recommend providing your application's user login."
      },
      "body": {
        "$ref": "#/$defs/generateBatchOrderInvoiceRequest"
      }
    },
    "required": [
      "userName",
      "body"
    ],
    "$defs": {
      "generateBatchOrderInvoiceRequest": {
        "items": {
          "$ref": "#/$defs/generateBatchOrderInvoiceRequestItem"
        },
        "type": "array"
      },
      "generateBatchOrderInvoiceRequestItem": {
        "properties": {
          "accountId": {
            "$ref": "#/$defs/marketplaceAccountId"
          },
          "beezUPOrderUUID": {
            "$ref": "#/$defs/beezUPOrderUUID"
          },
          "invoiceSequenceNumber": {
            "$ref": "#/$defs/invoiceSequenceNumber"
          },
          "marketplaceTechnicalCode": {
            "$ref": "#/$defs/marketplaceTechnicalCode"
          }
        },
        "required": [
          "marketplaceTechnicalCode",
          "accountId",
          "beezUPOrderUUID",
          "invoiceSequenceNumber"
        ],
        "type": "object"
      },
      "marketplaceAccountId": {
        "description": "Marketplace Account Identifier",
        "example": "50",
        "type": "integer"
      },
      "beezUPOrderUUID": {
        "description": "beezUPOrderUUID",
        "type": "string"
      },
      "invoiceSequenceNumber": {
        "description": "Invoice Sequence Number",
        "example": "879",
        "minimum": 0,
        "type": "integer"
      },
      "marketplaceTechnicalCode": {
        "description": "Marketplace Technical Code",
        "example": "CDISCOUNT",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/marketplaces/orders/invoices/generate{?userName}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
