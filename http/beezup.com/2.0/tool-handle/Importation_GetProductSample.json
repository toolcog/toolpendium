{
  "name": "Importation_GetProductSample",
  "description": "Get the product sample related to this importation with all columns (catalog and custom)",
  "parameters": {
    "type": "object",
    "properties": {
      "storeId": {
        "format": "guid",
        "type": "string",
        "description": "Your store identifier"
      },
      "executionId": {
        "format": "guid",
        "type": "string",
        "description": "The execution identifier of you catalog importation"
      },
      "productSampleIndex": {
        "format": "int32",
        "maximum": 99,
        "minimum": 0,
        "type": "integer",
        "description": "Index of the product sample. Starting from 0 to 99."
      }
    },
    "required": [
      "storeId",
      "executionId",
      "productSampleIndex"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/catalogs/{storeId}/importations/{executionId}/productSamples/{productSampleIndex}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Describe a product sample",
        "**Key properties:**",
        {
          "$ul": [
            "**productValues**: The key is the column identifier"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
