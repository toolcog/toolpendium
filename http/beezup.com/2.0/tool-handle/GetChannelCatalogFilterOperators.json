{
  "name": "GetChannelCatalogFilterOperators",
  "description": "Get channel catalog filter operators",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/channelCatalogs/filterOperators"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "The filter operator",
        "**Key properties:**",
        {
          "$ul": [
            "**expectedChannelColumnDataType**: Data type of the operator (default: string)",
            "**expectedValueDataType**: Data type of the operator (default: string)",
            "**name**: The exclusion filter operator name",
            "**valueRequired**: This operator requires a value"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
