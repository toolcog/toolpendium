{
  "name": "GetChannelInfo",
  "description": "Get channel information",
  "parameters": {
    "type": "object",
    "properties": {
      "channelId": {
        "format": "guid",
        "type": "string",
        "description": "The channel identifier"
      }
    },
    "required": [
      "channelId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/channels/{channelId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**beezUPOffer**: Indicate the BeezUP offer",
            "**channelDescription**: Indicae a description to the channel",
            "**channelId**: The channel identifier",
            "**channelLogoUrl**: The URL <a href=\"https://en.wikipedia.org/wiki/URL\">https://en.wikipedia.org/wiki/URL</a>",
            "**channelName**: The channel name",
            [
              "**details**",
              {
                "$ul": [
                  "**businessModel**: The business model of the channel",
                  "**category**: The channel category",
                  "**channelType**: The channel type",
                  "**costs**: The cost's description of the channel",
                  "**homeUrl**: The home url of the channel",
                  "**subscriptionLink**: The subscription link to the channel",
                  "**trackingType**: The tracking type"
                ]
              }
            ],
            [
              "**keyNumbers**",
              {
                "$ul": [
                  "**categories**: The estimated category count of the channel",
                  "**products**: The estimated product count of the channel",
                  "**stores**: The estimated store count of the channel",
                  "**viewsPerMonth**: The estimated view count per month of the channel"
                ]
              }
            ],
            [
              "**salesContact**",
              {
                "$ul": [
                  "**email**: Indicate the sales contact email",
                  "**name**: Indicate the BeezUP offer",
                  "**phoneNumber**: The sales contact phone number"
                ]
              }
            ],
            [
              "**technicalContact**",
              {
                "$ul": [
                  "**email**: The technical contact email",
                  "**name**: The technical contact name",
                  "**phoneNumber**: The technical contact phone number"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
