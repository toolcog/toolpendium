          }
        },
        "type": "object"
      },
      "invoiceSequenceNumber": {
        "description": "Invoice Sequence Number",
        "example": "879",
        "minimum": 0,
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
