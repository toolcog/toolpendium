{
  "name": "GetOrderInvoicePreview",
  "description": "View a preview an Order Invoice",
  "parameters": {
    "type": "object",
    "properties": {
      "marketplaceTechnicalCode": {
        "format": "MarketplaceTechnicalCode",
        "type": "string",
        "description": "The Marketplace Technical Code"
      },
      "accountId": {
        "format": "MarketplaceAccountId",
        "type": "string",
        "description": "The Account Identifier"
      },
      "beezUPOrderUUID": {
        "format": "BeezUPOrderUUID",
        "type": "string",
        "description": "The BeezUP Order UUID"
      },
      "Accept-Encoding": {
        "items": {
          "type": "string"
        },
        "type": "string",
        "description": "Allows the client to indicate wether it accepts a compressed encoding to reduce traffic size"
      },
      "body": {
        "$ref": "#/$defs/previewOrderInvoiceRequest"
      }
    },
    "required": [
      "marketplaceTechnicalCode",
      "accountId",
      "beezUPOrderUUID",
      "Accept-Encoding",
      "body"
    ],
