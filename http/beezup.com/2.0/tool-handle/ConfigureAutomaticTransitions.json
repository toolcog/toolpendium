{
  "name": "ConfigureAutomaticTransitions",
  "description": "Configure new or existing automatic Order status transition",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/configureAutomaticTransitionRequest"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "configureAutomaticTransitionRequest": {
        "properties": {
          "automaticTransitions": {
            "items": {
              "$ref": "#/$defs/automaticTransition"
            },
            "minLength": 1,
            "type": "array"
          }
        },
        "required": [
          "automaticTransitions"
        ],
        "type": "object"
      },
      "automaticTransition": {
        "properties": {
          "accountId": {
            "$ref": "#/$defs/accountId"
          },
          "enabled": {
            "example": "false",
            "type": "boolean"
          },
          "marketplaceTechnicalCode": {
            "$ref": "#/$defs/BeezUP.Common.MarketplaceTechnicalCode"
          },
          "orderStatusTransitionId": {
            "example": "1234",
            "format": "int32",
            "type": "integer"
          }
        },
        "required": [
          "marketplaceTechnicalCode",
          "accountId",
          "orderStatusTransitionId",
          "enabled"
        ],
        "type": "object"
      },
      "accountId": {
        "description": "The marketplace account identifier in BeezUP. This account identifier is based on your api settings.",
        "example": "1234",
        "format": "int32",
        "type": "integer"
      },
      "BeezUP.Common.MarketplaceTechnicalCode": {
        "description": "The technical code of the marketplace.",
        "example": "Amazon",
        "type": "string",
        "x-lov": "OMMarketplaceTechnicalCode"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.beezup.com/v2/user/marketplaces/orders/automaticTransitions"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
