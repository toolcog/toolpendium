{
  "name": "article_file_details",
  "description": "File by id",
  "parameters": {
    "type": "object",
    "properties": {
      "article_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer",
        "description": "Article Unique identifier"
      },
      "file_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer",
        "description": "File Unique identifier"
      }
    },
    "required": [
      "article_id",
      "file_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.figshare.com/v2/articles/{article_id}/files/{file_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**computed_md5**: File computed md5",
            "**download_url**: Url for file download",
            "**id**: File id",
            "**is_link_only**: True if file is hosted somewhere else",
            "**name**: File name",
            "**size**: File size",
            "**supplied_md5**: File supplied md5"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
