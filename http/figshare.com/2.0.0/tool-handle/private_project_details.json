{
  "name": "private_project_details",
  "description": "View a private project",
  "parameters": {
    "type": "object",
    "properties": {
      "project_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer",
        "description": "Project unique identifier"
      }
    },
    "required": [
      "project_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.figshare.com/v2/account/projects/{project_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**: Project id",
            "**published_date**: Date when project was published",
            "**title**: Project title",
            "**url**: Api endpoint"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: A machine friendly error code, used by the dev team to identify the error.",
            "**message**: A human friendly message explaining the error."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
