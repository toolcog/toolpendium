{
  "name": "file_download",
  "description": "Starts the download of a file",
  "parameters": {
    "type": "object",
    "properties": {
      "file_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer"
      }
    },
    "required": [
      "file_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.figshare.com/v2/file/download/{file_id}"
    }
  },
  "responses": {}
}
