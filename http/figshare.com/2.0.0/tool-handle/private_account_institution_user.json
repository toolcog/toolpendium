{
  "name": "private_account_institution_user",
  "description": "Retrieve institution user information using the account_id",
  "parameters": {
    "type": "object",
    "properties": {
      "account_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer",
        "description": "Account identifier the user is associated to"
      }
    },
    "required": [
      "account_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.figshare.com/v2/account/institution/users/{account_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**first_name**: First Name",
            "**id**: User id",
            "**is_active**: Account activity status",
            "**is_public**: Account public status",
            "**job_title**: User Job title",
            "**last_name**: Last Name",
            "**name**: Full Name",
            "**orcid_id**: Orcid associated to this User",
            "**url_name**: Name that appears in website url"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: A machine friendly error code, used by the dev team to identify the error.",
            "**message**: A human friendly message explaining the error."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
