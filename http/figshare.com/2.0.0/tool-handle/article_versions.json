{
  "name": "article_versions",
  "description": "List public article versions",
  "parameters": {
    "type": "object",
    "properties": {
      "article_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer",
        "description": "Article Unique identifier"
      }
    },
    "required": [
      "article_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.figshare.com/v2/articles/{article_id}/versions"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**url**: Api endpoint for the item version",
            "**version**: Version number"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
