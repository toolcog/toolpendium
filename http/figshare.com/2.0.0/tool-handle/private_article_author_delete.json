{
  "name": "private_article_author_delete",
  "description": "De-associate author from article",
  "parameters": {
    "type": "object",
    "properties": {
      "article_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer",
        "description": "Article unique identifier"
      },
      "author_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer",
        "description": "Article Author unique identifier"
      }
    },
    "required": [
      "article_id",
      "author_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.figshare.com/v2/account/articles/{article_id}/authors/{author_id}"
    }
  },
  "responses": {
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: A machine friendly error code, used by the dev team to identify the error.",
            "**message**: A human friendly message explaining the error."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
