{
  "name": "project_details",
  "description": "View a project",
  "parameters": {
    "type": "object",
    "properties": {
      "project_id": {
        "format": "int64",
        "minimum": 1,
        "type": "integer",
        "description": "Project Unique identifier"
      }
    },
    "required": [
      "project_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.figshare.com/v2/projects/{project_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**: Project id",
            "**published_date**: Date when project was published",
            "**title**: Project title",
            "**url**: Api endpoint"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
