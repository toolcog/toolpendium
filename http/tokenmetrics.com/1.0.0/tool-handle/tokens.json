{
  "name": "tokens",
  "description": "Tokens",
  "parameters": {
    "type": "object",
    "properties": {
      "token_ids": {
        "example": "3375, 3306",
        "type": "string"
      },
      "token_names": {
        "example": "Hivemapper, Sei_Network, Layer_Zero, Lyra Huobi",
        "type": "string"
      },
      "token_symbols": {
        "example": "bds, bds, btc",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.tokenmetrics.com/v1/tokens{?token_ids,token_names,token_symbols}"
    }
  },
  "responses": {}
}
