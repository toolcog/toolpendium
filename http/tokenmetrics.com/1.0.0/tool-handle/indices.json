{
  "name": "indices",
  "description": "Indices",
  "parameters": {
    "type": "object",
    "properties": {
      "exchanges": {
        "example": "binance",
        "type": "string"
      },
      "timeHorizon": {
        "example": "daily",
        "type": "string"
      },
      "lowCap": {
        "example": "true",
        "type": "string"
      },
      "startDate": {
        "example": "2023-01-10",
        "type": "string"
      },
      "endDate": {
        "example": "2023-01-11",
        "type": "string"
      },
      "limit": {
        "example": "1000",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.tokenmetrics.com/v1/indices{?exchanges,timeHorizon,lowCap,startDate,endDate,limit}"
    }
  },
  "responses": {}
}
