{
  "name": "addImageWatermarkV1",
  "description": "Add an image watermark to each page of a PDF document.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "file": {
            "description": "The input PDF document",
            "format": "binary",
            "type": "string"
          },
          "image": {
            "description": "The image to add as a watermark. The format of the image can be either PNG or JPEG.",
            "format": "binary",
            "type": "string"
          },
          "margin": {
            "default": 1,
            "description": "The distance in inches from the border of the page to the image watermark.",
            "example": 1,
            "format": "float",
            "minimum": 0,
            "type": "number"
          },
          "transparency": {
            "default": 50,
            "description": "The transparency level for the image watermark from 0 (opaque) to 100 (transparent).",
            "example": 50,
            "format": "int32",
            "maximum": 100,
            "minimum": 0,
            "type": "integer"
          }
        },
        "required": [
          "file",
          "image"
        ],
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.pdfblocks.com/v1/add_watermark/image"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "4XX": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
