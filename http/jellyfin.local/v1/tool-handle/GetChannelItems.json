{
  "name": "GetChannelItems",
  "description": "Get channel items.",
  "parameters": {
    "type": "object",
    "properties": {
      "channelId": {
        "description": "Channel Id.",
        "format": "uuid",
        "type": "string"
      },
      "folderId": {
        "description": "Optional. Folder Id.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "userId": {
        "description": "Optional. User Id.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "startIndex": {
        "description": "Optional. The record index to start at. All items with a lower index will be dropped from the results.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "limit": {
        "description": "Optional. The maximum number of records to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "sortOrder": {
        "description": "Optional. Sort Order - Ascending,Descending.",
        "nullable": true,
        "type": "string"
      },
      "filters": {
        "description": "Optional. Specify additional filters to apply.",
        "items": {
          "$ref": "#/$defs/ItemFilter"
        },
        "nullable": true,
        "type": "array"
      },
      "sortBy": {
        "description": "Optional. Specify one or more sort orders, comma delimited. Options: Album, AlbumArtist, Artist, Budget, CommunityRating, CriticRating, DateCreated, DatePlayed, PlayCount, PremiereDate, ProductionYear, SortName, Random, Revenue, Runtime.",
        "nullable": true,
        "type": "string"
      },
      "fields": {
        "description": "Optional. Specify additional fields of information to return in the output.",
        "items": {
          "$ref": "#/$defs/ItemFields"
        },
        "nullable": true,
        "type": "array"
      }
    },
    "required": [
      "channelId"
    ],
    "$defs": {
      "ItemFilter": {
        "description": "Enum ItemFilter.",
        "enum": [
          "IsFolder",
          "IsNotFolder",
          "IsUnplayed",
          "IsPlayed",
          "IsFavorite",
          "IsResumable",
          "Likes",
          "Dislikes",
          "IsFavoriteOrLikes"
        ],
        "type": "string"
      },
      "ItemFields": {
        "description": "Used to control the data that gets attached to DtoBaseItems.",
        "enum": [
          "AirTime",
          "CanDelete",
          "CanDownload",
          "ChannelInfo",
          "Chapters",
          "ChildCount",
          "CumulativeRunTimeTicks",
          "CustomRating",
          "DateCreated",
          "DateLastMediaAdded",
          "DisplayPreferencesId",
          "Etag",
          "ExternalUrls",
          "Genres",
          "HomePageUrl",
          "ItemCounts",
          "MediaSourceCount",
          "MediaSources",
          "OriginalTitle",
          "Overview",
          "ParentId",
          "Path",
          "People",
          "PlayAccess",
          "ProductionLocations",
          "ProviderIds",
          "PrimaryImageAspectRatio",
          "RecursiveItemCount",
          "Settings",
          "ScreenshotImageTags",
          "SeriesPrimaryImage",
          "SeriesStudio",
          "SortName",
          "SpecialEpisodeNumbers",
          "Studios",
          "BasicSyncInfo",
          "SyncInfo",
          "Taglines",
          "Tags",
          "RemoteTrailers",
          "MediaStreams",
          "SeasonUserData",
          "ServiceName",
          "ThemeSongIds",
          "ThemeVideoIds",
          "ExternalEtag",
          "PresentationUniqueKey",
          "InheritedParentalRatingValue",
          "ExternalSeriesId",
          "SeriesPresentationUniqueKey",
          "DateLastRefreshed",
          "DateLastSaved",
          "RefreshState",
          "ChannelImage",
          "EnableMediaSourceDisplay",
          "Width",
          "Height",
          "ExtraIds",
          "LocalTrailerCount",
          "IsHD",
          "SpecialFeatureCount"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Channels/{channelId}/Items{?folderId,userId,startIndex,limit,sortOrder,filters,sortBy,fields}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
