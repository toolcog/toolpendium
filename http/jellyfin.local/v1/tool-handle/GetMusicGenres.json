{
  "name": "GetMusicGenres",
  "description": "Gets all music genres from a given item, folder, or the entire library.",
  "parameters": {
    "type": "object",
    "properties": {
      "startIndex": {
        "description": "Optional. The record index to start at. All items with a lower index will be dropped from the results.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "limit": {
        "description": "Optional. The maximum number of records to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "searchTerm": {
        "description": "The search term.",
        "nullable": true,
        "type": "string"
      },
      "parentId": {
        "description": "Specify this to localize the search to a specific item or folder. Omit to use the root.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "fields": {
        "description": "Optional. Specify additional fields of information to return in the output.",
        "items": {
          "$ref": "#/$defs/ItemFields"
        },
        "nullable": true,
        "type": "array"
      },
      "excludeItemTypes": {
        "description": "Optional. If specified, results will be filtered out based on item type. This allows multiple, comma delimited.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      },
      "includeItemTypes": {
        "description": "Optional. If specified, results will be filtered in based on item type. This allows multiple, comma delimited.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      },
      "isFavorite": {
        "description": "Optional filter by items that are marked as favorite, or not.",
        "nullable": true,
        "type": "boolean"
      },
      "imageTypeLimit": {
        "description": "Optional, the max number of images to return, per image type.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "enableImageTypes": {
        "description": "Optional. The image types to include in the output.",
        "items": {
          "$ref": "#/$defs/ImageType"
        },
        "nullable": true,
        "type": "array"
      },
      "userId": {
        "description": "User id.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "nameStartsWithOrGreater": {
        "description": "Optional filter by items whose name is sorted equally or greater than a given input string.",
        "nullable": true,
        "type": "string"
      },
      "nameStartsWith": {
        "description": "Optional filter by items whose name is sorted equally than a given input string.",
        "nullable": true,
        "type": "string"
      },
      "nameLessThan": {
        "description": "Optional filter by items whose name is equally or lesser than a given input string.",
        "nullable": true,
        "type": "string"
      },
      "enableImages": {
        "default": true,
        "description": "Optional, include image information in output.",
        "nullable": true,
        "type": "boolean"
      },
      "enableTotalRecordCount": {
        "default": true,
        "description": "Optional. Include total record count.",
        "type": "boolean"
      }
    },
    "$defs": {
      "ItemFields": {
        "description": "Used to control the data that gets attached to DtoBaseItems.",
        "enum": [
          "AirTime",
          "CanDelete",
          "CanDownload",
          "ChannelInfo",
          "Chapters",
          "ChildCount",
          "CumulativeRunTimeTicks",
          "CustomRating",
          "DateCreated",
          "DateLastMediaAdded",
          "DisplayPreferencesId",
          "Etag",
          "ExternalUrls",
          "Genres",
          "HomePageUrl",
          "ItemCounts",
          "MediaSourceCount",
          "MediaSources",
          "OriginalTitle",
          "Overview",
          "ParentId",
          "Path",
          "People",
          "PlayAccess",
          "ProductionLocations",
          "ProviderIds",
          "PrimaryImageAspectRatio",
          "RecursiveItemCount",
          "Settings",
          "ScreenshotImageTags",
          "SeriesPrimaryImage",
          "SeriesStudio",
          "SortName",
          "SpecialEpisodeNumbers",
          "Studios",
          "BasicSyncInfo",
          "SyncInfo",
          "Taglines",
          "Tags",
          "RemoteTrailers",
          "MediaStreams",
          "SeasonUserData",
          "ServiceName",
          "ThemeSongIds",
          "ThemeVideoIds",
          "ExternalEtag",
          "PresentationUniqueKey",
          "InheritedParentalRatingValue",
          "ExternalSeriesId",
          "SeriesPresentationUniqueKey",
          "DateLastRefreshed",
          "DateLastSaved",
          "RefreshState",
          "ChannelImage",
          "EnableMediaSourceDisplay",
          "Width",
          "Height",
          "ExtraIds",
          "LocalTrailerCount",
          "IsHD",
          "SpecialFeatureCount"
        ],
        "type": "string"
      },
      "ImageType": {
        "description": "Enum ImageType.",
        "enum": [
          "Primary",
          "Art",
          "Backdrop",
          "Banner",
          "Logo",
          "Thumb",
          "Disc",
          "Box",
          "Screenshot",
          "Menu",
          "Chapter",
          "BoxRear",
          "Profile"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/MusicGenres{?startIndex,limit,searchTerm,parentId,fields,excludeItemTypes,includeItemTypes,isFavorite,imageTypeLimit,enableImageTypes,userId,nameStartsWithOrGreater,nameStartsWith,nameLessThan,enableImages,enableTotalRecordCount}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
