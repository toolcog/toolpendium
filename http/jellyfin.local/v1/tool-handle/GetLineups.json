{
  "name": "GetLineups",
  "description": "Gets available lineups.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "Provider id.",
        "nullable": true,
        "type": "string"
      },
      "type": {
        "description": "Provider type.",
        "nullable": true,
        "type": "string"
      },
      "location": {
        "description": "Location.",
        "nullable": true,
        "type": "string"
      },
      "country": {
        "description": "Country.",
        "nullable": true,
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/LiveTv/ListingProviders/Lineups{?id,type,location,country}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Id**: Gets or sets the identifier.",
            "**Name**: Gets or sets the name."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
