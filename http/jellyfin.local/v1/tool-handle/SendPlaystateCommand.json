{
  "name": "SendPlaystateCommand",
  "description": "Issues a playstate command to a client.",
  "parameters": {
    "type": "object",
    "properties": {
      "sessionId": {
        "description": "The session id.",
        "type": "string"
      },
      "command": {
        "$ref": "#/components/schemas/PlaystateCommand",
        "description": "The MediaBrowser.Model.Session.PlaystateCommand."
      },
      "seekPositionTicks": {
        "description": "The optional position ticks.",
        "format": "int64",
        "nullable": true,
        "type": "integer"
      },
      "controllingUserId": {
        "description": "The optional controlling user id.",
        "nullable": true,
        "type": "string"
      }
    },
    "required": [
      "sessionId",
      "command"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Sessions/{sessionId}/Playing/{command}{?seekPositionTicks,controllingUserId}"
    }
  },
  "responses": {}
}
