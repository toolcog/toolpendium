{
  "name": "SendSystemCommand",
  "description": "Issues a system command to a client.",
  "parameters": {
    "type": "object",
    "properties": {
      "sessionId": {
        "description": "The session id.",
        "type": "string"
      },
      "command": {
        "$ref": "#/components/schemas/GeneralCommandType",
        "description": "The command to send."
      }
    },
    "required": [
      "sessionId",
      "command"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Sessions/{sessionId}/System/{command}"
    }
  },
  "responses": {}
}
