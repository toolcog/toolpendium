{
  "name": "AddUserToSession",
  "description": "Adds an additional user to a session.",
  "parameters": {
    "type": "object",
    "properties": {
      "sessionId": {
        "description": "The session id.",
        "type": "string"
      },
      "userId": {
        "description": "The user id.",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "sessionId",
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Sessions/{sessionId}/User/{userId}"
    }
  },
  "responses": {}
}
