{
  "name": "GetRepositories",
  "description": "Gets all package repositories.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Repositories"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Class RepositoryInfo.",
        "**Key properties:**",
        {
          "$ul": [
            "**Enabled**: Gets or sets a value indicating whether the repository is enabled.",
            "**Name**: Gets or sets the name.",
            "**Url**: Gets or sets the URL."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
