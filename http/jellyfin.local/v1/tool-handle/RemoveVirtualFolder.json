{
  "name": "RemoveVirtualFolder",
  "description": "Removes a virtual folder.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "The name of the folder.",
        "nullable": true,
        "type": "string"
      },
      "refreshLibrary": {
        "default": false,
        "description": "Whether to refresh the library.",
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://jellyfin.local/Library/VirtualFolders{?name,refreshLibrary}"
    }
  },
  "responses": {}
}
