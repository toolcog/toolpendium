{
  "name": "GetMediaInfoImages",
  "description": "Get all media info images.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Images/MediaInfo"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Context**: Gets or sets the context.",
            "**FileLength**: Gets or sets the length of the file.",
            "**Format**: Gets or sets the format.",
            "**Name**: Gets or sets the name.",
            "**Theme**: Gets or sets the theme."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
