{
  "name": "UploadSubtitle",
  "description": "Upload an external subtitle file.",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "description": "The item the subtitle belongs to.",
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/UploadSubtitleDto",
        "description": "The request body."
      }
    },
    "required": [
      "itemId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Videos/{itemId}/Subtitles"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
