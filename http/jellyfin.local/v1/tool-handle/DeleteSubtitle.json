{
  "name": "DeleteSubtitle",
  "description": "Deletes an external subtitle file.",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "description": "The item id.",
        "format": "uuid",
        "type": "string"
      },
      "index": {
        "description": "The index of the subtitle file.",
        "format": "int32",
        "type": "integer"
      }
    },
    "required": [
      "itemId",
      "index"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://jellyfin.local/Videos/{itemId}/Subtitles/{index}"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
