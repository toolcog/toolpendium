{
  "name": "SyncPlayGetGroups",
  "description": "Gets all SyncPlay groups.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/SyncPlay/List"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Class GroupInfoDto.",
        "**Key properties:**",
        {
          "$ul": [
            "**GroupId**: Gets the group identifier.",
            "**GroupName**: Gets the group name.",
            "**LastUpdatedAt**: Gets the date when this DTO has been created.",
            "**Participants**: Gets the participants.",
            "**State**: Enum GroupState."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
