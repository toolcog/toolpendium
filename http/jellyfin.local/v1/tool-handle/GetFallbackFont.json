{
  "name": "GetFallbackFont",
  "description": "Gets a fallback font file.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "The name of the fallback font file to get.",
        "type": "string"
      }
    },
    "required": [
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/FallbackFont/Fonts/{name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
