{
  "name": "GetNamedConfiguration",
  "description": "Gets a named configuration.",
  "parameters": {
    "type": "object",
    "properties": {
      "key": {
        "description": "Configuration key.",
        "type": "string"
      }
    },
    "required": [
      "key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/System/Configuration/{key}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
