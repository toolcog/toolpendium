{
  "name": "GetDescriptionXml",
  "description": "Get Description Xml.",
  "parameters": {
    "type": "object",
    "properties": {
      "serverId": {
        "description": "Server UUID.",
        "type": "string"
      }
    },
    "required": [
      "serverId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Dlna/{serverId}/description"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
