{
  "name": "GetQueryFiltersLegacy",
  "description": "Gets legacy query filters.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "description": "Optional. User id.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "parentId": {
        "description": "Optional. Parent id.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "includeItemTypes": {
        "description": "Optional. If specified, results will be filtered based on item type. This allows multiple, comma delimited.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      },
      "mediaTypes": {
        "description": "Optional. Filter by MediaType. Allows multiple, comma delimited.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Items/Filters{?userId,parentId,includeItemTypes,mediaTypes}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Genres**",
            "**OfficialRatings**",
            "**Tags**",
            "**Years**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
