{
  "name": "GetThemeVideos",
  "description": "Get theme videos for an item.",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "description": "The item id.",
        "format": "uuid",
        "type": "string"
      },
      "userId": {
        "description": "Optional. Filter by user id, and attach user data.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "inheritFromParent": {
        "default": false,
        "description": "Optional. Determines whether or not parent items should be searched for theme media.",
        "type": "boolean"
      }
    },
    "required": [
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Items/{itemId}/ThemeVideos{?userId,inheritFromParent}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Class ThemeMediaResult.",
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**OwnerId**: Gets or sets the owner id.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
