{
  "name": "PostAddedMovies",
  "description": "Reports that new movies have been added by an external source.",
  "parameters": {
    "type": "object",
    "properties": {
      "tmdbId": {
        "description": "The tmdbId.",
        "nullable": true,
        "type": "string"
      },
      "imdbId": {
        "description": "The imdbId.",
        "nullable": true,
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Library/Movies/Added{?tmdbId,imdbId}"
    }
  },
  "responses": {}
}
