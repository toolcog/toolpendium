{
  "name": "GetTasks",
  "description": "Get tasks.",
  "parameters": {
    "type": "object",
    "properties": {
      "isHidden": {
        "description": "Optional filter tasks that are hidden, or not.",
        "nullable": true,
        "type": "boolean"
      },
      "isEnabled": {
        "description": "Optional filter tasks that are enabled, or not.",
        "nullable": true,
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/ScheduledTasks{?isHidden,isEnabled}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Class TaskInfo.",
        "**Key properties:**",
        {
          "$ul": [
            "**Category**: Gets or sets the category.",
            "**CurrentProgressPercentage**: Gets or sets the progress.",
            "**Description**: Gets or sets the description.",
            "**Id**: Gets or sets the id.",
            "**IsHidden**: Gets or sets a value indicating whether this instance is hidden.",
            "**Key**: Gets or sets the key.",
            [
              "**LastExecutionResult**: Class TaskExecutionInfo.",
              {
                "$ul": [
                  "**EndTimeUtc**: Gets or sets the end time UTC.",
                  "**ErrorMessage**: Gets or sets the error message.",
                  "**Id**: Gets or sets the id.",
                  "**Key**: Gets or sets the key.",
                  "**LongErrorMessage**: Gets or sets the long error message.",
                  "**Name**: Gets or sets the name.",
                  "**StartTimeUtc**: Gets or sets the start time UTC.",
                  "**Status**: Enum TaskCompletionStatus."
                ]
              }
            ],
            "**Name**: Gets or sets the name.",
            "**State**: Enum TaskState.",
            "**Triggers**: Gets or sets the triggers."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
