{
  "name": "RemoveFromCollection",
  "description": "Removes items from a collection.",
  "parameters": {
    "type": "object",
    "properties": {
      "collectionId": {
        "description": "The collection id.",
        "format": "uuid",
        "type": "string"
      },
      "ids": {
        "description": "Item ids, comma delimited.",
        "items": {
          "format": "uuid",
          "type": "string"
        },
        "type": "array"
      }
    },
    "required": [
      "collectionId",
      "ids"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://jellyfin.local/Collections/{collectionId}/Items{?ids}"
    }
  },
  "responses": {}
}
