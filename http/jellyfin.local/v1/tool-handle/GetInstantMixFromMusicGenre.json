{
  "name": "GetInstantMixFromMusicGenre",
  "description": "Creates an instant playlist based on a given song.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "The genre name.",
        "type": "string"
      },
      "userId": {
        "description": "Optional. Filter by user id, and attach user data.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "limit": {
        "description": "Optional. The maximum number of records to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "fields": {
        "description": "Optional. Specify additional fields of information to return in the output.",
        "items": {
          "$ref": "#/$defs/ItemFields"
        },
        "nullable": true,
        "type": "array"
      },
      "enableImages": {
        "description": "Optional. Include image information in output.",
        "nullable": true,
        "type": "boolean"
      },
      "enableUserData": {
        "description": "Optional. Include user data.",
        "nullable": true,
        "type": "boolean"
      },
      "imageTypeLimit": {
        "description": "Optional. The max number of images to return, per image type.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "enableImageTypes": {
        "description": "Optional. The image types to include in the output.",
        "items": {
          "$ref": "#/$defs/ImageType"
        },
        "nullable": true,
        "type": "array"
      }
    },
    "required": [
      "name"
    ],
    "$defs": {
      "ItemFields": {
        "description": "Used to control the data that gets attached to DtoBaseItems.",
        "enum": [
          "AirTime",
          "CanDelete",
          "CanDownload",
          "ChannelInfo",
          "Chapters",
          "ChildCount",
          "CumulativeRunTimeTicks",
          "CustomRating",
          "DateCreated",
          "DateLastMediaAdded",
          "DisplayPreferencesId",
          "Etag",
          "ExternalUrls",
          "Genres",
          "HomePageUrl",
          "ItemCounts",
          "MediaSourceCount",
          "MediaSources",
          "OriginalTitle",
          "Overview",
          "ParentId",
          "Path",
          "People",
          "PlayAccess",
          "ProductionLocations",
          "ProviderIds",
          "PrimaryImageAspectRatio",
          "RecursiveItemCount",
          "Settings",
          "ScreenshotImageTags",
          "SeriesPrimaryImage",
          "SeriesStudio",
          "SortName",
          "SpecialEpisodeNumbers",
          "Studios",
          "BasicSyncInfo",
          "SyncInfo",
          "Taglines",
          "Tags",
          "RemoteTrailers",
          "MediaStreams",
          "SeasonUserData",
          "ServiceName",
          "ThemeSongIds",
          "ThemeVideoIds",
          "ExternalEtag",
          "PresentationUniqueKey",
          "InheritedParentalRatingValue",
          "ExternalSeriesId",
          "SeriesPresentationUniqueKey",
          "DateLastRefreshed",
          "DateLastSaved",
          "RefreshState",
          "ChannelImage",
          "EnableMediaSourceDisplay",
          "Width",
          "Height",
          "ExtraIds",
          "LocalTrailerCount",
          "IsHD",
          "SpecialFeatureCount"
        ],
        "type": "string"
      },
      "ImageType": {
        "description": "Enum ImageType.",
        "enum": [
          "Primary",
          "Art",
          "Backdrop",
          "Banner",
          "Logo",
          "Thumb",
          "Disc",
          "Box",
          "Screenshot",
          "Menu",
          "Chapter",
          "BoxRear",
          "Profile"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/MusicGenres/{name}/InstantMix{?userId,limit,fields,enableImages,enableUserData,imageTypeLimit,enableImageTypes}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
