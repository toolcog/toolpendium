{
  "name": "RemoveMediaPath",
  "description": "Remove a media path.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "The name of the library.",
        "nullable": true,
        "type": "string"
      },
      "path": {
        "description": "The path to remove.",
        "nullable": true,
        "type": "string"
      },
      "refreshLibrary": {
        "default": false,
        "description": "Whether to refresh the library.",
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://jellyfin.local/Library/VirtualFolders/Paths{?name,path,refreshLibrary}"
    }
  },
  "responses": {}
}
