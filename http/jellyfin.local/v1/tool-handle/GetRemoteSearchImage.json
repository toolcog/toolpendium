{
  "name": "GetRemoteSearchImage",
  "description": "Gets a remote image.",
  "parameters": {
    "type": "object",
    "properties": {
      "imageUrl": {
        "description": "The image url.",
        "type": "string"
      },
      "providerName": {
        "description": "The provider name.",
        "type": "string"
      }
    },
    "required": [
      "imageUrl",
      "providerName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Items/RemoteSearch/Image{?imageUrl,providerName}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
