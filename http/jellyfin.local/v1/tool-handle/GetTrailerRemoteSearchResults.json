{
  "name": "GetTrailerRemoteSearchResults",
  "description": "Get trailer remote search.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/TrailerInfoRemoteSearchQuery",
        "description": "Remote search query."
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Items/RemoteSearch/Trailer"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**AlbumArtist**",
            "**Artists**",
            "**ImageUrl**",
            "**IndexNumber**",
            "**IndexNumberEnd**",
            "**Name**: Gets or sets the name.",
            "**Overview**",
            "**ParentIndexNumber**",
            "**PremiereDate**",
            "**ProductionYear**: Gets or sets the year.",
            "**ProviderIds**: Gets or sets the provider ids.",
            "**SearchProviderName**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
