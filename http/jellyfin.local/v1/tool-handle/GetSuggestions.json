{
  "name": "GetSuggestions",
  "description": "Gets suggestions.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "description": "The user id.",
        "format": "uuid",
        "type": "string"
      },
      "mediaType": {
        "description": "The media types.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      },
      "type": {
        "description": "The type.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      },
      "startIndex": {
        "description": "Optional. The start index.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "limit": {
        "description": "Optional. The limit.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "enableTotalRecordCount": {
        "default": false,
        "description": "Whether to enable the total record count.",
        "type": "boolean"
      }
    },
    "required": [
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Users/{userId}/Suggestions{?mediaType,type,startIndex,limit,enableTotalRecordCount}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
