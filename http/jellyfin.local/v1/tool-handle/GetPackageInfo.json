{
  "name": "GetPackageInfo",
  "description": "Gets a package by name or assembly GUID.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "The name of the package.",
        "type": "string"
      },
      "assemblyGuid": {
        "description": "The GUID of the associated assembly.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      }
    },
    "required": [
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Packages/{name}{?assemblyGuid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Class PackageInfo.",
        "**Key properties:**",
        {
          "$ul": [
            "**category**: Gets or sets the category.",
            "**description**: Gets or sets a long description of the plugin containing features or helpful explanations.",
            "**guid**: Gets or sets the guid of the assembly associated with this plugin.\r",
            "**imageUrl**: Gets or sets the image url for the package.",
            "**name**: Gets or sets the name.",
            "**overview**: Gets or sets a short overview of what the plugin does.",
            "**owner**: Gets or sets the owner.",
            "**versions**: Gets or sets the versions."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
