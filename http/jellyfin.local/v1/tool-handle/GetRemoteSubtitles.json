{
  "name": "GetRemoteSubtitles",
  "description": "Gets the remote subtitles.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "The item id.",
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Providers/Subtitles/Subtitles/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
