{
  "name": "GetFallbackFontList",
  "description": "Gets a list of available fallback font files.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/FallbackFont/Fonts"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Class FontFile.",
        "**Key properties:**",
        {
          "$ul": [
            "**DateCreated**: Gets or sets the date created.",
            "**DateModified**: Gets or sets the date modified.",
            "**Name**: Gets or sets the name.",
            "**Size**: Gets or sets the size."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
