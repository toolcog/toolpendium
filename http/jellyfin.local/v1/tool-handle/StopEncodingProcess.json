{
  "name": "StopEncodingProcess",
  "description": "Stops an active encoding.",
  "parameters": {
    "type": "object",
    "properties": {
      "deviceId": {
        "description": "The device id of the client requesting. Used to stop encoding processes when needed.",
        "nullable": true,
        "type": "string"
      },
      "playSessionId": {
        "description": "The play session id.",
        "nullable": true,
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://jellyfin.local/Videos/ActiveEncodings{?deviceId,playSessionId}"
    }
  },
  "responses": {}
}
