{
  "name": "AddMediaPath",
  "description": "Add a media path to a library.",
  "parameters": {
    "type": "object",
    "properties": {
      "refreshLibrary": {
        "default": false,
        "description": "Whether to refresh the library.",
        "type": "boolean"
      },
      "body": {
        "$ref": "#/components/schemas/MediaPathDto",
        "description": "The media path dto."
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Library/VirtualFolders/Paths{?refreshLibrary}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
