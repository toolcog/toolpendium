{
  "name": "DeleteItems",
  "description": "Deletes items from the library and filesystem.",
  "parameters": {
    "type": "object",
    "properties": {
      "ids": {
        "description": "The item ids.",
        "items": {
          "format": "uuid",
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://jellyfin.local/Items{?ids}"
    }
  },
  "responses": {
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
