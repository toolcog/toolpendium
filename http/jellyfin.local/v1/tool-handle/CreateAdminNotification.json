{
  "name": "CreateAdminNotification",
  "description": "Sends a notification to all admins.",
  "parameters": {
    "type": "object",
    "properties": {
      "url": {
        "description": "The URL of the notification.",
        "nullable": true,
        "type": "string"
      },
      "level": {
        "$ref": "#/components/schemas/NotificationLevel",
        "description": "The level of the notification."
      },
      "name": {
        "default": "",
        "description": "The name of the notification.",
        "nullable": true,
        "type": "string"
      },
      "description": {
        "default": "",
        "description": "The description of the notification.",
        "nullable": true,
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Notifications/Admin{?url,level,name,description}"
    }
  },
  "responses": {}
}
