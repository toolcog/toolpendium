{
  "name": "GetStudioImage",
  "description": "Get studio image by name.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "Studio name.",
        "type": "string"
      },
      "imageType": {
        "$ref": "#/components/schemas/ImageType",
        "description": "Image type."
      },
      "tag": {
        "description": "Optional. Supply the cache tag from the item object to receive strong caching headers.",
        "nullable": true,
        "type": "string"
      },
      "format": {
        "$ref": "#/components/schemas/ImageFormat",
        "description": "Determines the output format of the image - original,gif,jpg,png."
      },
      "maxWidth": {
        "description": "The maximum image width to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "maxHeight": {
        "description": "The maximum image height to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "percentPlayed": {
        "description": "Optional. Percent to render for the percent played overlay.",
        "format": "double",
        "nullable": true,
        "type": "number"
      },
      "unplayedCount": {
        "description": "Optional. Unplayed count overlay to render.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "width": {
        "description": "The fixed image width to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "height": {
        "description": "The fixed image height to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "quality": {
        "description": "Optional. Quality setting, from 0-100. Defaults to 90 and should suffice in most cases.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "cropWhitespace": {
        "description": "Optional. Specify if whitespace should be cropped out of the image. True/False. If unspecified, whitespace will be cropped from logos and clear art.",
        "nullable": true,
        "type": "boolean"
      },
      "addPlayedIndicator": {
        "description": "Optional. Add a played indicator.",
        "nullable": true,
        "type": "boolean"
      },
      "blur": {
        "description": "Optional. Blur image.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "backgroundColor": {
        "description": "Optional. Apply a background color for transparent images.",
        "nullable": true,
        "type": "string"
      },
      "foregroundLayer": {
        "description": "Optional. Apply a foreground layer on top of the image.",
        "nullable": true,
        "type": "string"
      },
      "imageIndex": {
        "description": "Image index.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      }
    },
    "required": [
      "name",
      "imageType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Studios/{name}/Images/{imageType}{?tag,format,maxWidth,maxHeight,percentPlayed,unplayedCount,width,height,quality,cropWhitespace,addPlayedIndicator,blur,backgroundColor,foregroundLayer,imageIndex}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
