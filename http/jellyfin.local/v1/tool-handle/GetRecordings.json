{
  "name": "GetRecordings",
  "description": "Gets live tv recordings.",
  "parameters": {
    "type": "object",
    "properties": {
      "channelId": {
        "description": "Optional. Filter by channel id.",
        "nullable": true,
        "type": "string"
      },
      "userId": {
        "description": "Optional. Filter by user and attach user data.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "startIndex": {
        "description": "Optional. The record index to start at. All items with a lower index will be dropped from the results.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "limit": {
        "description": "Optional. The maximum number of records to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "status": {
        "$ref": "#/components/schemas/RecordingStatus",
        "description": "Optional. Filter by recording status."
      },
      "isInProgress": {
        "description": "Optional. Filter by recordings that are in progress, or not.",
        "nullable": true,
        "type": "boolean"
      },
      "seriesTimerId": {
        "description": "Optional. Filter by recordings belonging to a series timer.",
        "nullable": true,
        "type": "string"
      },
      "enableImages": {
        "description": "Optional. Include image information in output.",
        "nullable": true,
        "type": "boolean"
      },
      "imageTypeLimit": {
        "description": "Optional. The max number of images to return, per image type.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "enableImageTypes": {
        "description": "Optional. The image types to include in the output.",
        "items": {
          "$ref": "#/$defs/ImageType"
        },
        "nullable": true,
        "type": "array"
      },
      "fields": {
        "description": "Optional. Specify additional fields of information to return in the output.",
        "items": {
          "$ref": "#/$defs/ItemFields"
        },
        "nullable": true,
        "type": "array"
      },
      "enableUserData": {
        "description": "Optional. Include user data.",
        "nullable": true,
        "type": "boolean"
      },
      "isMovie": {
        "description": "Optional. Filter for movies.",
        "nullable": true,
        "type": "boolean"
      },
      "isSeries": {
        "description": "Optional. Filter for series.",
        "nullable": true,
        "type": "boolean"
      },
      "isKids": {
        "description": "Optional. Filter for kids.",
        "nullable": true,
        "type": "boolean"
      },
      "isSports": {
        "description": "Optional. Filter for sports.",
        "nullable": true,
        "type": "boolean"
      },
      "isNews": {
        "description": "Optional. Filter for news.",
        "nullable": true,
        "type": "boolean"
      },
      "isLibraryItem": {
        "description": "Optional. Filter for is library item.",
        "nullable": true,
        "type": "boolean"
      },
      "enableTotalRecordCount": {
        "default": true,
        "description": "Optional. Return total record count.",
        "type": "boolean"
      }
    },
    "$defs": {
      "ImageType": {
        "description": "Enum ImageType.",
        "enum": [
          "Primary",
          "Art",
          "Backdrop",
          "Banner",
          "Logo",
          "Thumb",
          "Disc",
          "Box",
          "Screenshot",
          "Menu",
          "Chapter",
          "BoxRear",
          "Profile"
        ],
        "type": "string"
      },
      "ItemFields": {
        "description": "Used to control the data that gets attached to DtoBaseItems.",
        "enum": [
          "AirTime",
          "CanDelete",
          "CanDownload",
          "ChannelInfo",
          "Chapters",
          "ChildCount",
          "CumulativeRunTimeTicks",
          "CustomRating",
          "DateCreated",
          "DateLastMediaAdded",
          "DisplayPreferencesId",
          "Etag",
          "ExternalUrls",
          "Genres",
          "HomePageUrl",
          "ItemCounts",
          "MediaSourceCount",
          "MediaSources",
          "OriginalTitle",
          "Overview",
          "ParentId",
          "Path",
          "People",
          "PlayAccess",
          "ProductionLocations",
          "ProviderIds",
          "PrimaryImageAspectRatio",
          "RecursiveItemCount",
          "Settings",
          "ScreenshotImageTags",
          "SeriesPrimaryImage",
          "SeriesStudio",
          "SortName",
          "SpecialEpisodeNumbers",
          "Studios",
          "BasicSyncInfo",
          "SyncInfo",
          "Taglines",
          "Tags",
          "RemoteTrailers",
          "MediaStreams",
          "SeasonUserData",
          "ServiceName",
          "ThemeSongIds",
          "ThemeVideoIds",
          "ExternalEtag",
          "PresentationUniqueKey",
          "InheritedParentalRatingValue",
          "ExternalSeriesId",
          "SeriesPresentationUniqueKey",
          "DateLastRefreshed",
          "DateLastSaved",
          "RefreshState",
          "ChannelImage",
          "EnableMediaSourceDisplay",
          "Width",
          "Height",
          "ExtraIds",
          "LocalTrailerCount",
          "IsHD",
          "SpecialFeatureCount"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/LiveTv/Recordings{?channelId,userId,startIndex,limit,status,isInProgress,seriesTimerId,enableImages,imageTypeLimit,enableImageTypes,fields,enableUserData,isMovie,isSeries,isKids,isSports,isNews,isLibraryItem,enableTotalRecordCount}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
