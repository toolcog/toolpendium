{
  "name": "GetNotificationsSummary",
  "description": "Gets a user's notification summary.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "string"
      }
    },
    "required": [
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Notifications/{userId}/Summary"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The notification summary DTO.",
        "**Key properties:**",
        {
          "$ul": [
            "**MaxUnreadNotificationLevel**",
            "**UnreadCount**: Gets or sets the number of unread notifications."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
