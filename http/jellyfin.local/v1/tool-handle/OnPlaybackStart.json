{
  "name": "OnPlaybackStart",
  "description": "Reports that a user has begun playing an item.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "description": "User id.",
        "format": "uuid",
        "type": "string"
      },
      "itemId": {
        "description": "Item id.",
        "format": "uuid",
        "type": "string"
      },
      "mediaSourceId": {
        "description": "The id of the MediaSource.",
        "nullable": true,
        "type": "string"
      },
      "audioStreamIndex": {
        "description": "The audio stream index.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "subtitleStreamIndex": {
        "description": "The subtitle stream index.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "playMethod": {
        "$ref": "#/components/schemas/PlayMethod",
        "description": "The play method."
      },
      "liveStreamId": {
        "description": "The live stream id.",
        "nullable": true,
        "type": "string"
      },
      "playSessionId": {
        "description": "The play session id.",
        "nullable": true,
        "type": "string"
      },
      "canSeek": {
        "default": false,
        "description": "Indicates if the client can seek.",
        "type": "boolean"
      }
    },
    "required": [
      "userId",
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Users/{userId}/PlayingItems/{itemId}{?mediaSourceId,audioStreamIndex,subtitleStreamIndex,playMethod,liveStreamId,playSessionId,canSeek}"
    }
  },
  "responses": {}
}
