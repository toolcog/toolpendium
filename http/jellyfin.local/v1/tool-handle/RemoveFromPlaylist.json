{
  "name": "RemoveFromPlaylist",
  "description": "Removes items from a playlist.",
  "parameters": {
    "type": "object",
    "properties": {
      "playlistId": {
        "description": "The playlist id.",
        "type": "string"
      },
      "entryIds": {
        "description": "The item ids, comma delimited.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      }
    },
    "required": [
      "playlistId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://jellyfin.local/Playlists/{playlistId}/Items{?entryIds}"
    }
  },
  "responses": {}
}
