{
  "name": "AddToPlaylist",
  "description": "Adds items to a playlist.",
  "parameters": {
    "type": "object",
    "properties": {
      "playlistId": {
        "description": "The playlist id.",
        "format": "uuid",
        "type": "string"
      },
      "ids": {
        "description": "Item id, comma delimited.",
        "items": {
          "format": "uuid",
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      },
      "userId": {
        "description": "The userId.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      }
    },
    "required": [
      "playlistId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Playlists/{playlistId}/Items{?ids,userId}"
    }
  },
  "responses": {}
}
