{
  "name": "PostUpdatedMedia",
  "description": "Reports that new movies have been added by an external source.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "description": "A list of updated media paths.",
        "items": {
          "$ref": "#/$defs/MediaUpdateInfoDto"
        },
        "type": "array"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "MediaUpdateInfoDto": {
        "additionalProperties": false,
        "description": "Media Update Info Dto.",
        "properties": {
          "Path": {
            "description": "Gets or sets media path.",
            "nullable": true,
            "type": "string"
          },
          "UpdateType": {
            "description": "Gets or sets media update type.\r\nCreated, Modified, Deleted.",
            "nullable": true,
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Library/Media/Updated"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
