{
  "name": "SendFullGeneralCommand",
  "description": "Issues a full general command to a client.",
  "parameters": {
    "type": "object",
    "properties": {
      "sessionId": {
        "description": "The session id.",
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/GeneralCommand",
        "description": "The MediaBrowser.Model.Session.GeneralCommand."
      }
    },
    "required": [
      "sessionId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Sessions/{sessionId}/Command"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
