{
  "name": "AddListingProvider",
  "description": "Adds a listings provider.",
  "parameters": {
    "type": "object",
    "properties": {
      "pw": {
        "description": "Password.",
        "nullable": true,
        "type": "string"
      },
      "validateListings": {
        "default": false,
        "description": "Validate listings.",
        "type": "boolean"
      },
      "validateLogin": {
        "default": false,
        "description": "Validate login.",
        "type": "boolean"
      },
      "body": {
        "$ref": "#/components/schemas/ListingsProviderInfo",
        "description": "New listings info."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/LiveTv/ListingProviders{?pw,validateListings,validateLogin}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ChannelMappings**",
            "**Country**",
            "**EnableAllTuners**",
            "**EnabledTuners**",
            "**Id**",
            "**KidsCategories**",
            "**ListingsId**",
            "**MovieCategories**",
            "**MoviePrefix**",
            "**NewsCategories**",
            "**Password**",
            "**Path**",
            "**PreferredLanguage**",
            "**SportsCategories**",
            "**Type**",
            "**UserAgent**",
            "**Username**",
            "**ZipCode**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
