{
  "name": "GetCountries",
  "description": "Gets known countries.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Localization/Countries"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Class CountryInfo.",
        "**Key properties:**",
        {
          "$ul": [
            "**DisplayName**: Gets or sets the display name.",
            "**Name**: Gets or sets the name.",
            "**ThreeLetterISORegionName**: Gets or sets the name of the three letter ISO region.",
            "**TwoLetterISORegionName**: Gets or sets the name of the two letter ISO region."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
