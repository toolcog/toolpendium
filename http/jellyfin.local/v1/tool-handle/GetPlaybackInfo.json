{
  "name": "GetPlaybackInfo",
  "description": "Gets live playback media info for an item.",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "description": "The item id.",
        "format": "uuid",
        "type": "string"
      },
      "userId": {
        "description": "The user id.",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "itemId",
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Items/{itemId}/PlaybackInfo{?userId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Class PlaybackInfoResponse.",
        "**Key properties:**",
        {
          "$ul": [
            "**ErrorCode**",
            "**MediaSources**: Gets or sets the media sources.",
            "**PlaySessionId**: Gets or sets the play session identifier."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
