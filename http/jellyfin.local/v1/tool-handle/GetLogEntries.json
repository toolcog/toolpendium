{
  "name": "GetLogEntries",
  "description": "Gets activity log entries.",
  "parameters": {
    "type": "object",
    "properties": {
      "startIndex": {
        "description": "Optional. The record index to start at. All items with a lower index will be dropped from the results.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "limit": {
        "description": "Optional. The maximum number of records to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "minDate": {
        "description": "Optional. The minimum date. Format = ISO.",
        "format": "date-time",
        "nullable": true,
        "type": "string"
      },
      "hasUserId": {
        "description": "Optional. Filter log entries if it has user id, or not.",
        "nullable": true,
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/System/ActivityLog/Entries{?startIndex,limit,minDate,hasUserId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
