{
  "name": "GetChannelMappingOptions",
  "description": "Get channel mapping options.",
  "parameters": {
    "type": "object",
    "properties": {
      "providerId": {
        "description": "Provider id.",
        "nullable": true,
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/LiveTv/ChannelMappingOptions{?providerId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Channel mapping options dto.",
        "**Key properties:**",
        {
          "$ul": [
            "**Mappings**: Gets or sets list of mappings.",
            "**ProviderChannels**: Gets or sets list of provider channels.",
            "**ProviderName**: Gets or sets provider name.",
            "**TunerChannels**: Gets or sets list of tuner channels."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
