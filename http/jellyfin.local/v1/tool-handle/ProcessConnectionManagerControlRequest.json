{
  "name": "ProcessConnectionManagerControlRequest",
  "description": "Process a connection manager control request.",
  "parameters": {
    "type": "object",
    "properties": {
      "serverId": {
        "description": "Server UUID.",
        "type": "string"
      }
    },
    "required": [
      "serverId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Dlna/{serverId}/ConnectionManager/Control"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
