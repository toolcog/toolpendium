{
  "name": "RenameVirtualFolder",
  "description": "Renames a virtual folder.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "The name of the virtual folder.",
        "nullable": true,
        "type": "string"
      },
      "newName": {
        "description": "The new name.",
        "nullable": true,
        "type": "string"
      },
      "refreshLibrary": {
        "default": false,
        "description": "Whether to refresh the library.",
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Library/VirtualFolders/Name{?name,newName,refreshLibrary}"
    }
  },
