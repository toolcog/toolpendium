{
  "name": "GetNotifications",
  "description": "Gets a user's notifications.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "string"
      }
    },
    "required": [
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Notifications/{userId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A list of notifications with the total record count for pagination.",
        "**Key properties:**",
        {
          "$ul": [
            "**Notifications**: Gets or sets the current page of notifications.",
            "**TotalRecordCount**: Gets or sets the total number of notifications."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
