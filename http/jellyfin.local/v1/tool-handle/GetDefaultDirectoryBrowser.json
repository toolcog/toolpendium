{
  "name": "GetDefaultDirectoryBrowser",
  "description": "Get Default directory browser.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Environment/DefaultDirectoryBrowser"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Default directory browser info.",
        "**Key properties:**",
        {
          "$ul": [
            "**Path**: Gets or sets the path."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
