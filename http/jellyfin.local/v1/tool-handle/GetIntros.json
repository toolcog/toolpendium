{
  "name": "GetIntros",
  "description": "Gets intros to play before the main media item plays.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "description": "User id.",
        "format": "uuid",
        "type": "string"
      },
      "itemId": {
        "description": "Item id.",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "userId",
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Users/{userId}/Items/{itemId}/Intros"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
