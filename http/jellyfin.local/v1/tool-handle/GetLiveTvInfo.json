{
  "name": "GetLiveTvInfo",
  "description": "Gets available live tv services.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/LiveTv/Info"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**EnabledUsers**: Gets or sets the enabled users.",
            "**IsEnabled**: Gets or sets a value indicating whether this instance is enabled.",
            "**Services**: Gets or sets the services."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
