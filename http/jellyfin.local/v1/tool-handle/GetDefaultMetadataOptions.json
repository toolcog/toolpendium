{
  "name": "GetDefaultMetadataOptions",
  "description": "Gets a default MetadataOptions object.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/System/Configuration/MetadataOptions/Default"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Class MetadataOptions.",
        "**Key properties:**",
        {
          "$ul": [
            "**DisabledImageFetchers**",
            "**DisabledMetadataFetchers**",
            "**DisabledMetadataSavers**",
            "**ImageFetcherOrder**",
            "**ItemType**",
            "**LocalMetadataReaderOrder**",
            "**MetadataFetcherOrder**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
