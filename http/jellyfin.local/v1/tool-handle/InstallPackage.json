{
  "name": "InstallPackage",
  "description": "Installs a package.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "Package name.",
        "type": "string"
      },
      "assemblyGuid": {
        "description": "GUID of the associated assembly.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "version": {
        "description": "Optional version. Defaults to latest version.",
        "nullable": true,
        "type": "string"
      },
      "repositoryUrl": {
        "description": "Optional. Specify the repository to install from.",
        "nullable": true,
        "type": "string"
      }
    },
    "required": [
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Packages/Installed/{name}{?assemblyGuid,version,repositoryUrl}"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
