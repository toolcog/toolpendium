{
  "name": "GetQueryFilters",
  "description": "Gets query filters.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "description": "Optional. User id.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "parentId": {
        "description": "Optional. Specify this to localize the search to a specific item or folder. Omit to use the root.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "includeItemTypes": {
        "description": "Optional. If specified, results will be filtered based on item type. This allows multiple, comma delimited.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      },
      "isAiring": {
        "description": "Optional. Is item airing.",
        "nullable": true,
        "type": "boolean"
      },
      "isMovie": {
        "description": "Optional. Is item movie.",
        "nullable": true,
        "type": "boolean"
      },
      "isSports": {
        "description": "Optional. Is item sports.",
        "nullable": true,
        "type": "boolean"
      },
      "isKids": {
        "description": "Optional. Is item kids.",
        "nullable": true,
        "type": "boolean"
      },
      "isNews": {
        "description": "Optional. Is item news.",
        "nullable": true,
        "type": "boolean"
      },
      "isSeries": {
        "description": "Optional. Is item series.",
        "nullable": true,
        "type": "boolean"
      },
      "recursive": {
        "description": "Optional. Search recursive.",
        "nullable": true,
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Items/Filters2{?userId,parentId,includeItemTypes,isAiring,isMovie,isSports,isKids,isNews,isSeries,recursive}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Genres**",
            "**Tags**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
