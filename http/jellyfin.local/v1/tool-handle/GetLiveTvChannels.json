{
  "name": "GetLiveTvChannels",
  "description": "Gets available live tv channels.",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "$ref": "#/components/schemas/ChannelType",
        "description": "Optional. Filter by channel type."
      },
      "userId": {
        "description": "Optional. Filter by user and attach user data.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "startIndex": {
        "description": "Optional. The record index to start at. All items with a lower index will be dropped from the results.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "isMovie": {
        "description": "Optional. Filter for movies.",
        "nullable": true,
        "type": "boolean"
      },
      "isSeries": {
        "description": "Optional. Filter for series.",
        "nullable": true,
        "type": "boolean"
      },
      "isNews": {
        "description": "Optional. Filter for news.",
        "nullable": true,
        "type": "boolean"
      },
      "isKids": {
        "description": "Optional. Filter for kids.",
        "nullable": true,
        "type": "boolean"
      },
      "isSports": {
        "description": "Optional. Filter for sports.",
        "nullable": true,
        "type": "boolean"
      },
      "limit": {
        "description": "Optional. The maximum number of records to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "isFavorite": {
        "description": "Optional. Filter by channels that are favorites, or not.",
        "nullable": true,
        "type": "boolean"
      },
      "isLiked": {
        "description": "Optional. Filter by channels that are liked, or not.",
        "nullable": true,
        "type": "boolean"
      },
      "isDisliked": {
        "description": "Optional. Filter by channels that are disliked, or not.",
        "nullable": true,
        "type": "boolean"
      },
      "enableImages": {
        "description": "Optional. Include image information in output.",
        "nullable": true,
        "type": "boolean"
      },
      "imageTypeLimit": {
        "description": "Optional. The max number of images to return, per image type.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "enableImageTypes": {
        "description": "\"Optional. The image types to include in the output.",
        "items": {
          "$ref": "#/$defs/ImageType"
        },
        "nullable": true,
        "type": "array"
      },
      "fields": {
        "description": "Optional. Specify additional fields of information to return in the output.",
        "items": {
          "$ref": "#/$defs/ItemFields"
        },
        "nullable": true,
        "type": "array"
      },
      "enableUserData": {
        "description": "Optional. Include user data.",
        "nullable": true,
        "type": "boolean"
      },
      "sortBy": {
        "description": "Optional. Key to sort by.",
        "items": {
          "type": "string"
        },
        "nullable": true,
        "type": "array"
      },
      "sortOrder": {
        "$ref": "#/components/schemas/SortOrder",
        "description": "Optional. Sort order."
      },
      "enableFavoriteSorting": {
        "default": false,
        "description": "Optional. Incorporate favorite and like status into channel sorting.",
        "type": "boolean"
      },
      "addCurrentProgram": {
        "default": true,
        "description": "Optional. Adds current program info to each channel.",
        "type": "boolean"
      }
    },
    "$defs": {
      "ImageType": {
        "description": "Enum ImageType.",
        "enum": [
          "Primary",
          "Art",
          "Backdrop",
          "Banner",
          "Logo",
          "Thumb",
          "Disc",
          "Box",
          "Screenshot",
          "Menu",
          "Chapter",
          "BoxRear",
          "Profile"
        ],
        "type": "string"
      },
      "ItemFields": {
        "description": "Used to control the data that gets attached to DtoBaseItems.",
        "enum": [
          "AirTime",
          "CanDelete",
          "CanDownload",
          "ChannelInfo",
          "Chapters",
          "ChildCount",
          "CumulativeRunTimeTicks",
          "CustomRating",
          "DateCreated",
          "DateLastMediaAdded",
          "DisplayPreferencesId",
          "Etag",
          "ExternalUrls",
          "Genres",
          "HomePageUrl",
          "ItemCounts",
          "MediaSourceCount",
          "MediaSources",
          "OriginalTitle",
          "Overview",
          "ParentId",
          "Path",
          "People",
          "PlayAccess",
          "ProductionLocations",
          "ProviderIds",
          "PrimaryImageAspectRatio",
          "RecursiveItemCount",
          "Settings",
          "ScreenshotImageTags",
          "SeriesPrimaryImage",
          "SeriesStudio",
          "SortName",
          "SpecialEpisodeNumbers",
          "Studios",
          "BasicSyncInfo",
          "SyncInfo",
          "Taglines",
          "Tags",
          "RemoteTrailers",
          "MediaStreams",
          "SeasonUserData",
          "ServiceName",
          "ThemeSongIds",
          "ThemeVideoIds",
          "ExternalEtag",
          "PresentationUniqueKey",
          "InheritedParentalRatingValue",
          "ExternalSeriesId",
          "SeriesPresentationUniqueKey",
          "DateLastRefreshed",
          "DateLastSaved",
          "RefreshState",
          "ChannelImage",
          "EnableMediaSourceDisplay",
          "Width",
          "Height",
          "ExtraIds",
          "LocalTrailerCount",
          "IsHD",
          "SpecialFeatureCount"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/LiveTv/Channels{?type,userId,startIndex,isMovie,isSeries,isNews,isKids,isSports,limit,isFavorite,isLiked,isDisliked,enableImages,imageTypeLimit,enableImageTypes,fields,enableUserData,sortBy,sortOrder,enableFavoriteSorting,addCurrentProgram}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
