{
  "name": "DownloadRemoteSubtitles",
  "description": "Downloads a remote subtitle.",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "description": "The item id.",
        "format": "uuid",
        "type": "string"
      },
      "subtitleId": {
        "description": "The subtitle id.",
        "type": "string"
      }
    },
    "required": [
      "itemId",
      "subtitleId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Items/{itemId}/RemoteSearch/Subtitles/{subtitleId}"
    }
  },
  "responses": {}
}
