{
  "name": "GetRemoteImages",
  "description": "Gets available remote images for an item.",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "description": "Item Id.",
        "format": "uuid",
        "type": "string"
      },
      "type": {
        "$ref": "#/components/schemas/ImageType",
        "description": "The image type."
      },
      "startIndex": {
        "description": "Optional. The record index to start at. All items with a lower index will be dropped from the results.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "limit": {
        "description": "Optional. The maximum number of records to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "providerName": {
        "description": "Optional. The image provider to use.",
        "nullable": true,
        "type": "string"
      },
      "includeAllLanguages": {
        "default": false,
        "description": "Optional. Include all languages.",
        "type": "boolean"
      }
    },
    "required": [
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Items/{itemId}/RemoteImages{?type,startIndex,limit,providerName,includeAllLanguages}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Class RemoteImageResult.",
        "**Key properties:**",
        {
          "$ul": [
            "**Images**: Gets or sets the images.",
            "**Providers**: Gets or sets the providers.",
            "**TotalRecordCount**: Gets or sets the total record count."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
