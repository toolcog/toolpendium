{
  "name": "GetParentPath",
  "description": "Gets the parent path of a given path.",
  "parameters": {
    "type": "object",
    "properties": {
      "path": {
        "description": "The path.",
        "type": "string"
      }
    },
    "required": [
      "path"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Environment/ParentPath{?path}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
