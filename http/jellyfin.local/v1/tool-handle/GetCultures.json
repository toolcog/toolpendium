{
  "name": "GetCultures",
  "description": "Gets known cultures.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Localization/Cultures"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Class CultureDto.",
        "**Key properties:**",
        {
          "$ul": [
            "**DisplayName**: Gets or sets the display name.",
            "**Name**: Gets or sets the name.",
            "**ThreeLetterISOLanguageName**: Gets or sets the name of the three letter ISO language.",
            "**ThreeLetterISOLanguageNames**",
            "**TwoLetterISOLanguageName**: Gets or sets the name of the two letter ISO language."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
