{
  "name": "OnPlaybackProgress",
  "description": "Reports a user's playback progress.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "description": "User id.",
        "format": "uuid",
        "type": "string"
      },
      "itemId": {
        "description": "Item id.",
        "format": "uuid",
        "type": "string"
      },
      "mediaSourceId": {
        "description": "The id of the MediaSource.",
        "nullable": true,
        "type": "string"
      },
      "positionTicks": {
        "description": "Optional. The current position, in ticks. 1 tick = 10000 ms.",
        "format": "int64",
        "nullable": true,
        "type": "integer"
      },
      "audioStreamIndex": {
        "description": "The audio stream index.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "subtitleStreamIndex": {
        "description": "The subtitle stream index.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "volumeLevel": {
        "description": "Scale of 0-100.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "playMethod": {
        "$ref": "#/components/schemas/PlayMethod",
        "description": "The play method."
      },
      "liveStreamId": {
        "description": "The live stream id.",
        "nullable": true,
        "type": "string"
      },
      "playSessionId": {
        "description": "The play session id.",
        "nullable": true,
        "type": "string"
      },
      "repeatMode": {
        "$ref": "#/components/schemas/RepeatMode",
        "description": "The repeat mode."
      },
      "isPaused": {
        "default": false,
        "description": "Indicates if the player is paused.",
        "type": "boolean"
      },
      "isMuted": {
        "default": false,
        "description": "Indicates if the player is muted.",
        "type": "boolean"
      }
    },
    "required": [
      "userId",
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Users/{userId}/PlayingItems/{itemId}/Progress{?mediaSourceId,positionTicks,audioStreamIndex,subtitleStreamIndex,volumeLevel,playMethod,liveStreamId,playSessionId,repeatMode,isPaused,isMuted}"
    }
  },
  "responses": {}
}
