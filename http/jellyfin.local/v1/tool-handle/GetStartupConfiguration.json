{
  "name": "GetStartupConfiguration",
  "description": "Gets the initial startup wizard configuration.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Startup/Configuration"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The startup configuration DTO.",
        "**Key properties:**",
        {
          "$ul": [
            "**MetadataCountryCode**: Gets or sets the metadata country code.",
            "**PreferredMetadataLanguage**: Gets or sets the preferred language for the metadata.",
            "**UICulture**: Gets or sets UI language culture."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
