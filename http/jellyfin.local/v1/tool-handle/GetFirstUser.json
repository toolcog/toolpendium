{
  "name": "GetFirstUser",
  "description": "Gets the first user.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Startup/User"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The startup user DTO.",
        "**Key properties:**",
        {
          "$ul": [
            "**Name**: Gets or sets the username.",
            "**Password**: Gets or sets the user's password."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
