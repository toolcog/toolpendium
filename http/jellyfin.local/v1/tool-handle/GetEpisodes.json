{
  "name": "GetEpisodes",
  "description": "Gets episodes for a tv season.",
  "parameters": {
    "type": "object",
    "properties": {
      "seriesId": {
        "description": "The series id.",
        "format": "uuid",
        "type": "string"
      },
      "userId": {
        "description": "The user id.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "fields": {
        "description": "Optional. Specify additional fields of information to return in the output. This allows multiple, comma delimited. Options: Budget, Chapters, DateCreated, Genres, HomePageUrl, IndexOptions, MediaStreams, Overview, ParentId, Path, People, ProviderIds, PrimaryImageAspectRatio, Revenue, SortName, Studios, Taglines, TrailerUrls.",
        "items": {
          "$ref": "#/$defs/ItemFields"
        },
        "nullable": true,
        "type": "array"
      },
      "season": {
        "description": "Optional filter by season number.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "seasonId": {
        "description": "Optional. Filter by season id.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "isMissing": {
        "description": "Optional. Filter by items that are missing episodes or not.",
        "nullable": true,
        "type": "boolean"
      },
      "adjacentTo": {
        "description": "Optional. Return items that are siblings of a supplied item.",
        "nullable": true,
        "type": "string"
      },
      "startItemId": {
        "description": "Optional. Skip through the list until a given item is found.",
        "format": "uuid",
        "nullable": true,
        "type": "string"
      },
      "startIndex": {
        "description": "Optional. The record index to start at. All items with a lower index will be dropped from the results.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "limit": {
        "description": "Optional. The maximum number of records to return.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "enableImages": {
        "description": "Optional, include image information in output.",
        "nullable": true,
        "type": "boolean"
      },
      "imageTypeLimit": {
        "description": "Optional, the max number of images to return, per image type.",
        "format": "int32",
        "nullable": true,
        "type": "integer"
      },
      "enableImageTypes": {
        "description": "Optional. The image types to include in the output.",
        "items": {
          "$ref": "#/$defs/ImageType"
        },
        "nullable": true,
        "type": "array"
      },
      "enableUserData": {
        "description": "Optional. Include user data.",
        "nullable": true,
        "type": "boolean"
      },
      "sortBy": {
        "description": "Optional. Specify one or more sort orders, comma delimited. Options: Album, AlbumArtist, Artist, Budget, CommunityRating, CriticRating, DateCreated, DatePlayed, PlayCount, PremiereDate, ProductionYear, SortName, Random, Revenue, Runtime.",
        "nullable": true,
        "type": "string"
      }
    },
    "required": [
      "seriesId"
    ],
    "$defs": {
      "ItemFields": {
        "description": "Used to control the data that gets attached to DtoBaseItems.",
        "enum": [
          "AirTime",
          "CanDelete",
          "CanDownload",
          "ChannelInfo",
          "Chapters",
          "ChildCount",
          "CumulativeRunTimeTicks",
          "CustomRating",
          "DateCreated",
          "DateLastMediaAdded",
          "DisplayPreferencesId",
          "Etag",
          "ExternalUrls",
          "Genres",
          "HomePageUrl",
          "ItemCounts",
          "MediaSourceCount",
          "MediaSources",
          "OriginalTitle",
          "Overview",
          "ParentId",
          "Path",
          "People",
          "PlayAccess",
          "ProductionLocations",
          "ProviderIds",
          "PrimaryImageAspectRatio",
          "RecursiveItemCount",
          "Settings",
          "ScreenshotImageTags",
          "SeriesPrimaryImage",
          "SeriesStudio",
          "SortName",
          "SpecialEpisodeNumbers",
          "Studios",
          "BasicSyncInfo",
          "SyncInfo",
          "Taglines",
          "Tags",
          "RemoteTrailers",
          "MediaStreams",
          "SeasonUserData",
          "ServiceName",
          "ThemeSongIds",
          "ThemeVideoIds",
          "ExternalEtag",
          "PresentationUniqueKey",
          "InheritedParentalRatingValue",
          "ExternalSeriesId",
          "SeriesPresentationUniqueKey",
          "DateLastRefreshed",
          "DateLastSaved",
          "RefreshState",
          "ChannelImage",
          "EnableMediaSourceDisplay",
          "Width",
          "Height",
          "ExtraIds",
          "LocalTrailerCount",
          "IsHD",
          "SpecialFeatureCount"
        ],
        "type": "string"
      },
      "ImageType": {
        "description": "Enum ImageType.",
        "enum": [
          "Primary",
          "Art",
          "Backdrop",
          "Banner",
          "Logo",
          "Thumb",
          "Disc",
          "Box",
          "Screenshot",
          "Menu",
          "Chapter",
          "BoxRear",
          "Profile"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/Shows/{seriesId}/Episodes{?userId,fields,season,seasonId,isMissing,adjacentTo,startItemId,startIndex,limit,enableImages,imageTypeLimit,enableImageTypes,enableUserData,sortBy}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
