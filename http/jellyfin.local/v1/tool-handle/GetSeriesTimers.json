{
  "name": "GetSeriesTimers",
  "description": "Gets live tv series timers.",
  "parameters": {
    "type": "object",
    "properties": {
      "sortBy": {
        "description": "Optional. Sort by SortName or Priority.",
        "nullable": true,
        "type": "string"
      },
      "sortOrder": {
        "$ref": "#/components/schemas/SortOrder",
        "description": "Optional. Sort in Ascending or Descending order."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://jellyfin.local/LiveTv/SeriesTimers{?sortBy,sortOrder}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Items**: Gets or sets the items.",
            "**StartIndex**: The index of the first record in Items.",
            "**TotalRecordCount**: The total number of records available."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
