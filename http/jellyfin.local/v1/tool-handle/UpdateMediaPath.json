{
  "name": "UpdateMediaPath",
  "description": "Updates a media path.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "description": "The name of the library.",
        "nullable": true,
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/MediaPathInfo",
        "description": "The path info."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://jellyfin.local/Library/VirtualFolders/Paths/Update{?name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
