{
  "name": "Retrievedistributionlinks",
  "description": "Retrieves all the individual links for a given distribution",
  "parameters": {
    "type": "object",
    "properties": {
      "surveyId": {
        "type": "string",
        "description": "ID of the survey (eg: SV_123)"
      },
      "DistributionId": {
        "type": "string",
        "description": "ID of the distribution list"
      }
    },
    "required": [
      "surveyId",
      "DistributionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://fra1.qualtrics.com/API/v3/distributions/{DistributionId}/links{?surveyId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**httpStatus**",
                  "**requestId**"
                ]
              }
            ],
            [
              "**result**",
              {
                "$ul": [
                  "**elements**",
                  "**nextPage**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
