{
  "name": "CreateContactInMailinglist",
  "description": "Creates a contact in a given mailing list",
  "parameters": {
    "type": "object",
    "properties": {
      "DirectoryId": {
        "type": "string",
        "description": "ID of the qualtrics directory to create the contact to"
      },
      "MailingListId": {
        "type": "string",
        "description": "ID of the mailing list"
      },
      "body": {
        "$ref": "#/components/schemas/CreateContactInMailingList",
        "description": "Contact data"
      }
    },
    "required": [
      "DirectoryId",
      "MailingListId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://fra1.qualtrics.com/API/v3/directories/{DirectoryId}/mailinglists/{MailingListId}/contacts"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
