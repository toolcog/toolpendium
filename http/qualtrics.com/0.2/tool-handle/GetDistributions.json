{
  "name": "GetDistributions",
  "description": "Gets all distributions for a given survey",
  "parameters": {
    "type": "object",
    "properties": {
      "surveyId": {
        "type": "string",
        "description": "The survey for which to load the distributions"
      }
    },
    "required": [
      "surveyId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://fra1.qualtrics.com/API/v3/distributions{?surveyId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**httpStatus**",
                  "**requestId**"
                ]
              }
            ],
            [
              "**result**",
              {
                "$ul": [
                  "**elements**",
                  "**nextPage**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
