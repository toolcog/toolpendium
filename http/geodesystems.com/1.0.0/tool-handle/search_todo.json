        "description": "Search text"
      },
      "name": {
        "type": "string",
        "description": "Search name"
      },
      "description": {
        "type": "string",
        "description": "Search description"
      },
      "fromdate": {
        "format": "date-time",
        "type": "string",
        "description": "From date"
      },
      "todate": {
        "format": "date-time",
        "type": "string",
        "description": "To date"
      },
      "createdate.from": {
        "format": "date-time",
        "type": "string",
