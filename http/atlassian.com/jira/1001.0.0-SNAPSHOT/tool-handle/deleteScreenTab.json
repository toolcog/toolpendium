{
  "name": "deleteScreenTab",
  "description": "Deletes a screen tab.\n\n**[Permissions](#permissions) required:** *Administer Jira* [global permission](https://confluence.atlassian.com/x/x4dKLg).",
  "parameters": {
    "type": "object",
    "properties": {
      "screenId": {
        "format": "int64",
        "type": "integer",
        "description": "The ID of the screen."
      },
      "tabId": {
        "format": "int64",
        "type": "integer",
        "description": "The ID of the screen tab."
      }
    },
    "required": [
      "screenId",
      "tabId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://your-domain.atlassian.net/rest/api/3/screens/{screenId}/tabs/{tabId}"
    }
  },
  "responses": {}
}
