{
  "name": "getLicense",
  "description": "Returns licensing information about the Jira instance.\n\n**[Permissions](#permissions) required:** None.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://your-domain.atlassian.net/rest/api/3/instance/license"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Details about a license for the Jira instance.",
        "**Key properties:**",
        {
          "$ul": [
            "**applications**: The applications under this license."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
