{
  "name": "MigrationResource_updateEntityPropertiesValue_put",
  "description": "Updates the values of multiple entity properties for an object, up to 50 updates per request. This operation is for use by Connect apps during app migration.",
  "parameters": {
    "type": "object",
    "properties": {
      "Atlassian-Transfer-Id": {
        "format": "uuid",
        "type": "string",
        "description": "The app migration transfer ID."
      },
      "entityType": {
        "enum": [
          "IssueProperty",
          "CommentProperty",
          "DashboardItemProperty",
          "IssueTypeProperty",
          "ProjectProperty",
          "UserProperty",
          "WorklogProperty",
          "BoardProperty",
          "SprintProperty"
        ],
        "type": "string",
        "description": "The type indicating the object that contains the entity properties."
      },
      "body": {
        "items": {
          "$ref": "#/$defs/EntityPropertyDetails"
        },
        "maxItems": 50,
        "minItems": 1,
        "type": "array"
      }
    },
    "required": [
      "Atlassian-Transfer-Id",
      "entityType",
      "body"
    ],
    "$defs": {
      "EntityPropertyDetails": {
        "properties": {
          "entityId": {
            "description": "The entity property ID.",
            "example": 123,
            "type": "number"
          },
          "key": {
            "description": "The entity property key.",
            "example": "mykey",
            "type": "string"
          },
          "value": {
            "description": "The new value of the entity property.",
            "example": "newValue",
            "type": "string"
          }
        },
        "required": [
          "entityId",
          "key",
          "value"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://your-domain.atlassian.net/rest/atlassian-connect/1/migration/properties/{entityType}"
    },
    "headers": {
      "Atlassian-Transfer-Id": {
        "$": "Atlassian-Transfer-Id"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
