{
  "name": "updatePrecomputations",
  "description": "Update the precomputation value of a function created by a Forge/Connect app.\n\n**[Permissions](#permissions) required:** An API for apps to update their own precomputations.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/JqlFunctionPrecomputationUpdateRequestBean"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "JqlFunctionPrecomputationUpdateRequestBean": {
        "additionalProperties": false,
        "description": "List of pairs (id and value) for precomputation updates.",
        "properties": {
          "values": {
            "items": {
              "$ref": "#/$defs/JqlFunctionPrecomputationUpdateBean"
            },
            "type": "array"
          }
        },
        "type": "object",
        "writeOnly": true
      },
      "JqlFunctionPrecomputationUpdateBean": {
        "additionalProperties": false,
        "description": "Precomputation id and its new value.",
        "properties": {
          "id": {
            "format": "int64",
            "type": "integer",
            "writeOnly": true
          },
          "value": {
            "type": "string",
            "writeOnly": true
          }
        },
        "required": [
          "id",
          "value"
        ],
        "type": "object",
        "writeOnly": true
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://your-domain.atlassian.net/rest/api/3/jql/function/computation"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "204": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
