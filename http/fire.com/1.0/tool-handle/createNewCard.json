{
  "name": "createNewCard",
  "description": "You can create multiple debit cards which can be linked to your fire.com accounts.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "acceptFeesAndCharges": {
            "example": true,
            "type": "boolean"
          },
          "addressType": {
            "enum": [
              "HOME",
              "BUSINESS"
            ],
            "example": "BUSINESS",
            "type": "string"
          },
          "cardPin": {
            "example": 5345,
            "type": "string"
          },
          "eurIcan": {
            "example": 2150,
            "format": "int64",
            "type": "integer"
          },
          "gbpIcan": {
            "example": 2152,
            "format": "int64",
            "type": "integer"
          },
          "userId": {
            "example": 3245,
            "format": "int64",
            "type": "integer"
          }
        },
        "title": "newCard",
        "type": "object",
        "description": "Details of the new card"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.fire.com/business/v1/cards"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "newCardResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**cardId**",
            "**expiryDate**",
            "**maskedPan**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
