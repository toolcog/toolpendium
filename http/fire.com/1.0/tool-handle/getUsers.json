{
  "name": "getUsers",
  "description": "You can retrieve the details of all fire.com users on your acount.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fire.com/business/v1/users"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "user list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**emailAddress**: email address for user",
            "**firstName**: User first name",
            "**id**: The User ID for this User",
            "**lastName**: User second name",
            "**lastlogin**: Timestamp on when user last logged in.",
            [
              "**mobileApplicationDetails**",
              {
                "$ul": [
                  "**OS**: Operating system of device.",
                  "**businessUserId**: Business user ID",
                  "**clientID**: Client ID of user.",
                  "**deviceName**: type of device.",
                  "**deviceOSVersion**: OS version for device.",
                  "**mobileApplicationId**: Mobile application id for user.",
                  "**status**: Status of user"
                ]
              }
            ],
            "**mobileNumber**: User mobile number",
            "**role**: User role",
            "**status**: Status of user",
            "**userCvl**: Users Cvl type ID (shows up when status is LIVE)"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
