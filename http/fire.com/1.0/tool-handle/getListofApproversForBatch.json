{
  "name": "getListofApproversForBatch",
  "description": "Returns a list of approvers for this batch.",
  "parameters": {
    "type": "object",
    "properties": {
      "batchUuid": {
        "description": "The uuid of the batch.",
        "example": "4ADFB67A-0F5B-4A9A-9D74-34437250045C",
        "type": "string"
      }
    },
    "required": [
      "batchUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fire.com/business/v1/batches/{batchUuid}/approvals"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "batchApprovers"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**approvals**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
