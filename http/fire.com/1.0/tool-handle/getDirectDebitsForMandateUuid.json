{
  "name": "getDirectDebitsForMandateUuid",
  "description": "Retrieve all direct debit payments associated with a direct debit mandate.\nThe permision needed to access this endpoint is PERM_BUSINESS_GET_DIRECT_DEBITS\n",
  "parameters": {
    "type": "object",
    "properties": {
      "mandateUuid": {
        "type": "string",
        "description": "The mandate UUID to retrieve"
      }
    },
    "required": [
      "mandateUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fire.com/business/v1/directdebits{?mandateUuid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "directDebits"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**directdebits**",
            "**total**: Number of direct debits found"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
