{
  "name": "getItemsBatchInternationalTransfer",
  "description": "Returns a paginated list of items in the specified batch.",
  "parameters": {
    "type": "object",
    "properties": {
      "batchUuid": {
        "description": "The uuid of the batch.",
        "example": "4ADFB67A-0F5B-4A9A-9D74-34437250045C",
        "type": "string"
      },
      "offset": {
        "description": "The page offset. Defaults to 0. This is the record number that the returned list will start at. E.g. offset = 40 and limit = 20 will return records 40 to 59.",
        "example": "0",
        "format": "int64",
        "type": "integer"
      },
      "limit": {
        "description": "The number of records to return. Defaults to 10 - max is 200.",
        "example": "10",
        "format": "int64",
        "type": "integer"
      }
    },
    "required": [
      "batchUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fire.com/business/v2/batches/{batchUuid}/internationaltransfers{?offset,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "batchItems"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**total**: total number of batches returned"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
