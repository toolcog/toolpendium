{
  "name": "getAccounts",
  "description": "Returns all your fire.com Accounts. Ordered by Alias ascending. Can be paginated.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fire.com/business/v1/accounts"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "accounts"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accounts**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
