{
  "name": "getListofCardTransactions",
  "description": "Returns a list of cards transactions related to your fire.com card.",
  "parameters": {
    "type": "object",
    "properties": {
      "cardId": {
        "description": "The cardid of the card to retrieve the associated transactions",
        "format": "int64",
        "type": "integer"
      },
      "limit": {
        "description": "The number of records to return",
        "format": "int64",
        "type": "integer"
      },
      "offset": {
        "description": "The page of records to return",
        "format": "int64",
        "type": "integer"
      }
    },
    "required": [
      "cardId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fire.com/business/v1/cards/{cardId}/transactions{?limit,offset}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "cardTransactionsv1 list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**dateRangeTo**: milisecond timestamp of date range to value.",
            "**total**: The total number of card transactions in the list.",
            "**transactions**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
