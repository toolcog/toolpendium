{
  "name": "getPayees",
  "description": "Returns all your payee bank accounts. \n\nOrdered by payee name ascending. \n\nCan be paginated.\n",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.fire.com/business/v1/payees"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "payeeBankAccounts"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**fundingSources**",
            "**total**: The total number of payees in the list."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
