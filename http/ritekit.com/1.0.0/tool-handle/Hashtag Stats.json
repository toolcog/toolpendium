{
  "name": "Hashtag_Stats",
  "description": "Returns real-time stats for up to 100 hashtags (updated hourly).",
  "parameters": {
    "type": "object",
    "properties": {
      "tags": {
        "items": {},
        "type": "array",
        "description": "Hashtag(s) without # mark"
      }
    },
    "required": [
      "tags"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ritekit.com/v1/stats/multiple-hashtags{?tags}"
    }
  },
  "responses": {}
}
