{
  "name": "Auto-Hashtag",
  "description": "Returns auto-hashtagged text of the post.",
  "parameters": {
    "type": "object",
    "properties": {
      "post": {
        "type": "string",
        "description": "Text of the post"
      },
      "maxHashtags": {
        "default": 2,
        "type": "integer",
        "description": "Max number of hashtags."
      },
      "hashtagPosition": {
        "default": "auto",
        "type": "string",
        "description": "Position of hashtags: end => at the end, auto => anywhere"
      }
    },
    "required": [
      "post"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ritekit.com/v1/stats/auto-hashtag{?post,maxHashtags,hashtagPosition}"
    }
  },
  "responses": {}
}
