{
  "name": "Emoji_Suggestions",
  "description": "Returns list of emoji suggestions for a given text of the post",
  "parameters": {
    "type": "object",
    "properties": {
      "text": {
        "type": "string",
        "description": "Text of the post"
      }
    },
    "required": [
      "text"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ritekit.com/v1/emoji/suggestions{?text}"
    }
  },
  "responses": {}
}
