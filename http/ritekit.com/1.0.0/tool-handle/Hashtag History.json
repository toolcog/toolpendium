{
  "name": "Hashtag_History",
  "description": "Returns historical stats for a given hashtag from the last 30 days",
  "parameters": {
    "type": "object",
    "properties": {
      "hashtag": {
        "type": "string",
        "description": "Hashtag without # mark"
      }
    },
    "required": [
      "hashtag"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ritekit.com/v1/stats/history/{hashtag}"
    }
  },
  "responses": {}
}
