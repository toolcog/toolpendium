{
  "name": "Shorten_Link",
  "description": "Returns a shorten link with a given CTA.",
  "parameters": {
    "type": "object",
    "properties": {
      "url": {
        "type": "string",
        "description": "URL"
      },
      "cta": {
        "type": "integer",
        "description": "cta id"
      }
    },
    "required": [
      "url",
      "cta"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ritekit.com/v1/link/short-link{?url,cta}"
    }
  },
  "responses": {}
}
