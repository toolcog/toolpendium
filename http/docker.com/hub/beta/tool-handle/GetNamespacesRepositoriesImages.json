{
  "name": "GetNamespacesRepositoriesImages",
  "description": "Gets details on the images in a repository.",
  "parameters": {
    "type": "object",
    "properties": {
      "namespace": {
        "type": "string",
        "description": "Namespace of the repository."
      },
      "repository": {
        "type": "string",
        "description": "Name of the repository."
      },
      "status": {
        "enum": [
          "active",
          "inactive"
        ],
        "type": "string",
        "description": "Filters to only show images of this status."
      },
      "currently_tagged": {
        "type": "boolean",
        "description": "Filters to only show images with:\n- `true`: at least 1 current tag.\n- `false`: no current tags.\n"
      },
      "ordering": {
        "enum": [
          "last_activity",
          "-last_activity",
          "digest",
          "-digest"
        ],
        "type": "string",
        "description": "Orders the results by this property.\n\nPrefixing with `-` sorts by descending order.\n"
      },
      "active_from": {
        "type": "string",
        "description": "Sets the time from which an image must have been pushed or pulled to\nbe counted as active.\n\nDefaults to 1 month before the current time.\n"
      },
      "page": {
        "type": "integer",
        "description": "Page number to get. Defaults to 1."
      },
      "page_size": {
        "type": "integer",
        "description": "Number of images to get per page. Defaults to 10. Max of 100."
      }
    },
    "required": [
      "namespace",
      "repository"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://hub.docker.com//v2/namespaces/{namespace}/repositories/{repository}/images{?status,currently_tagged,ordering,active_from,page,page_size}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Paginated list of images in a repository.",
        "**Key properties:**",
        {
          "$ul": [
            "**count**: Total count of images in the repository.",
            "**next**: Link to the next page with the same query parameters if there are more images.",
            "**previous**: Link to the previous page with the same query parameters if not on first page.",
            "**results**: Image details."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**errinfo**: Context information for an error used for diagnostics.",
              {
                "$ul": [
                  "**api_call_docker_id**: ID of docker user.",
                  "**api_call_name**: Name of the API operation called.",
                  "**api_call_start**: Date/time of call start.",
                  "**api_call_txnid**: Unique ID for this call."
                ]
              }
            ],
            "**message**: The error message.",
            "**txnid**: Unique ID for this call."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**errinfo**: Context information for an error used for diagnostics.",
              {
                "$ul": [
                  "**api_call_docker_id**: ID of docker user.",
                  "**api_call_name**: Name of the API operation called.",
                  "**api_call_start**: Date/time of call start.",
                  "**api_call_txnid**: Unique ID for this call."
                ]
              }
            ],
            "**message**: The error message.",
            "**txnid**: Unique ID for this call."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
