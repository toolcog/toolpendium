{
  "name": "GetNamespacesRepositoriesImagesTags",
  "description": "Gets current and historical tags for an image.",
  "parameters": {
    "type": "object",
    "properties": {
      "namespace": {
        "type": "string",
        "description": "Namespace of the repository."
      },
      "repository": {
        "type": "string",
        "description": "Name of the repository."
      },
      "digest": {
        "type": "string",
        "description": "Digest of the image."
      },
      "page": {
        "type": "integer",
        "description": "Page number to get. Defaults to 1."
      },
      "page_size": {
        "type": "integer",
        "description": "Number of images to get per page. Defaults to 10. Max of 100."
      }
    },
    "required": [
      "namespace",
      "repository",
      "digest"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://hub.docker.com//v2/namespaces/{namespace}/repositories/{repository}/images/{digest}/tags{?page,page_size}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Paginated list of tags for this repository.",
        "**Key properties:**",
        {
          "$ul": [
            "**count**: Total count of tags for this image.",
            "**next**: Link to the next page if there are more tags.",
            "**previous**: Link to the previous page if not on first page.",
            "**results**: The current and historical tags for this image."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**errinfo**: Context information for an error used for diagnostics.",
              {
                "$ul": [
                  "**api_call_docker_id**: ID of docker user.",
                  "**api_call_name**: Name of the API operation called.",
                  "**api_call_start**: Date/time of call start.",
                  "**api_call_txnid**: Unique ID for this call."
                ]
              }
            ],
            "**message**: The error message.",
            "**txnid**: Unique ID for this call."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**errinfo**: Context information for an error used for diagnostics.",
              {
                "$ul": [
                  "**api_call_docker_id**: ID of docker user.",
                  "**api_call_name**: Name of the API operation called.",
                  "**api_call_start**: Date/time of call start.",
                  "**api_call_txnid**: Unique ID for this call."
                ]
              }
            ],
            "**message**: The error message.",
            "**txnid**: Unique ID for this call."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
