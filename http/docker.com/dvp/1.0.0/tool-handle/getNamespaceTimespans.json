{
  "name": "getNamespaceTimespans",
  "description": "Gets a list of timespans of the given type that have data for the given namespace and year",
  "parameters": {
    "type": "object",
    "properties": {
      "namespace": {
        "type": "string",
        "description": "Namespace to fetch data for"
      },
      "year": {
        "type": "integer",
        "description": "Year to fetch data for"
      },
      "timespantype": {
        "$ref": "#/components/schemas/TimespanType",
        "description": "Type of timespan to fetch data for"
      }
    },
    "required": [
      "namespace",
      "year",
      "timespantype"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://hub.docker.com/api/publisher/analytics/v1/namespaces/{namespace}/pulls/exports/years/{year}/{timespantype}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
