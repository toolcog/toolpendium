{
  "name": "NetworkInspect",
  "description": "Inspect a network",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Network ID or name"
      },
      "verbose": {
        "default": false,
        "type": "boolean",
        "description": "Detailed inspect output for troubleshooting"
      },
      "scope": {
        "type": "string",
        "description": "Filter the network by scope (swarm, global, or local)"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v1.33/networks/{id}{?verbose,scope}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Attachable**",
            "**Containers**",
            "**Created**",
            "**Driver**",
            "**EnableIPv6**",
            [
              "**IPAM**",
              {
                "$ul": [
                  "**Config**: List of IPAM configuration options, specified as a map: `{\"Subnet\": <CIDR>, \"IPRange\": <CIDR>, \"Gateway\": <IP address>, \"AuxAddress\": <device_name:IP address>}`",
                  "**Driver**: Name of the IPAM driver to use. (default: default)",
                  "**Options**: Driver-specific options, specified as a map."
                ]
              }
            ],
            "**Id**",
            "**Ingress**",
            "**Internal**",
            "**Labels**",
            "**Name**",
            "**Options**",
            "**Scope**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            "**message**: The error message."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            "**message**: The error message."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
