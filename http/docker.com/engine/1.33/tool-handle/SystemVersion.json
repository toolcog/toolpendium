  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v1.33/version"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ApiVersion**",
            "**Arch**",
            "**BuildTime**",
            "**Experimental**",
            "**GitCommit**",
            "**GoVersion**",
            "**KernelVersion**",
            "**MinAPIVersion**",
            "**Os**",
            "**Version**"
          ]
        },
        {
          "$lang": "json",
