{
  "name": "ImageCommit",
  "description": "Create a new image from a container",
  "parameters": {
    "type": "object",
    "properties": {
      "container": {
        "type": "string",
        "description": "The ID or name of the container to commit"
      },
      "repo": {
        "type": "string",
        "description": "Repository name for the created image"
      },
      "tag": {
        "type": "string",
        "description": "Tag name for the create image"
      },
      "comment": {
        "type": "string",
        "description": "Commit message"
      },
      "author": {
        "type": "string",
        "description": "Author of the image (e.g., `John Hannibal Smith <hannibal@a-team.com>`)"
      },
      "pause": {
        "default": true,
        "type": "boolean",
        "description": "Whether to pause the container before committing"
      },
      "changes": {
        "type": "string",
        "description": "`Dockerfile` instructions to apply while committing"
      },
      "body": {
        "$ref": "#/components/schemas/ContainerConfig",
        "description": "The container configuration"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/v1.33/commit{?container,repo,tag,comment,author,pause,changes}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Response to an API call that returns just an Id",
        "**Key properties:**",
        {
          "$ul": [
            "**Id**: The id of the newly created object."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            "**message**: The error message."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            "**message**: The error message."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
