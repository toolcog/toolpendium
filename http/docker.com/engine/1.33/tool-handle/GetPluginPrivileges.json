{
  "name": "GetPluginPrivileges",
  "description": "Get plugin privileges",
  "parameters": {
    "type": "object",
    "properties": {
      "remote": {
        "type": "string",
        "description": "The name of the plugin. The `:latest` tag is optional, and is the default if omitted."
      }
    },
    "required": [
      "remote"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v1.33/plugins/privileges{?remote}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Describes a permission the user has to accept upon installing the plugin.",
        "**Key properties:**",
        {
          "$ul": [
            "**Description**",
            "**Name**",
            "**Value**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an error.",
        "**Key properties:**",
        {
          "$ul": [
            "**message**: The error message."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
