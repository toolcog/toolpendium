{
  "name": "DeleteFlow",
  "description": "Delete a specific Flow.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^FW[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Flow resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://studio.twilio.com/v1/Flows/{Sid}"
    }
  },
  "responses": {}
}
