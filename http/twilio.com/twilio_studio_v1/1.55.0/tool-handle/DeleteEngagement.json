{
  "name": "DeleteEngagement",
  "description": "Delete this Engagement and all Steps relating to it.",
  "parameters": {
    "type": "object",
    "properties": {
      "FlowSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^FW[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Flow to delete Engagements from."
      },
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^FN[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Engagement resource to delete."
      }
    },
    "required": [
      "FlowSid",
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://studio.twilio.com/v1/Flows/{FlowSid}/Engagements/{Sid}"
    }
  },
  "responses": {}
}
