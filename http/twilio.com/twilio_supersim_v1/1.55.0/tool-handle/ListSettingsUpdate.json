{
  "name": "ListSettingsUpdate",
  "description": "Retrieve a list of Settings Updates.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sim": {
        "type": "string",
        "description": "Filter the Settings Updates by a Super SIM's SID or UniqueName."
      },
      "Status": {
        "$ref": "#/components/schemas/settings_update_enum_status",
        "type": "string",
        "description": "Filter the Settings Updates by status. Can be `scheduled`, `in-progress`, `successful`, or `failed`."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://supersim.twilio.com/v1/SettingsUpdates{?Sim,Status,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListSettingsUpdateResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**settings_updates**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
