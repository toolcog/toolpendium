{
  "name": "DeleteRole",
  "description": "Remove a user role from your account's default service",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^RL[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Role resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://conversations.twilio.com/v1/Roles/{Sid}"
    }
  },
  "responses": {}
}
