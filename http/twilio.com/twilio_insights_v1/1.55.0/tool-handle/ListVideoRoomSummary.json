{
  "name": "ListVideoRoomSummary",
  "description": "Get a list of Programmable Video Rooms.",
  "parameters": {
    "type": "object",
    "properties": {
      "RoomType": {
        "items": {
          "$ref": "#/$defs/video_room_summary_enum_room_type"
        },
        "type": "array",
        "description": "Type of room. Can be `go`, `peer_to_peer`, `group`, or `group_small`."
      },
      "Codec": {
        "items": {
          "$ref": "#/$defs/video_room_summary_enum_codec"
        },
        "type": "array",
        "description": "Codecs used by participants in the room. Can be `VP8`, `H264`, or `VP9`."
      },
      "RoomName": {
        "type": "string",
        "description": "Room friendly name."
      },
      "CreatedAfter": {
        "format": "date-time",
        "type": "string",
        "description": "Only read rooms that started on or after this ISO 8601 timestamp."
      },
      "CreatedBefore": {
        "format": "date-time",
        "type": "string",
        "description": "Only read rooms that started before this ISO 8601 timestamp."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "$defs": {
      "video_room_summary_enum_room_type": {
        "enum": [
          "go",
          "peer_to_peer",
          "group",
          "group_small"
        ],
        "type": "string"
      },
      "video_room_summary_enum_codec": {
        "enum": [
          "VP8",
          "H264",
          "VP9"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://insights.twilio.com/v1/Video/Rooms{?RoomType,Codec,RoomName,CreatedAfter,CreatedBefore,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListVideoRoomSummaryResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**rooms**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
