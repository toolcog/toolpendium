{
  "name": "FetchAccountSettings",
  "description": "Get the Voice Insights Settings.",
  "parameters": {
    "type": "object",
    "properties": {
      "SubaccountSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^AC[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique SID identifier of the Subaccount."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://insights.twilio.com/v1/Voice/Settings{?SubaccountSid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**account_sid**: The unique SID identifier of the Account.",
            "**advanced_features**: A boolean flag indicating whether Advanced Features for Voice Insights are enabled.",
            "**url**: The URL of this resource.",
            "**voice_trace**: A boolean flag indicating whether Voice Trace is enabled."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
