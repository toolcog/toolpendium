{
  "name": "ListConferenceParticipant",
  "description": "Get a list of Conference Participants Summaries for a Conference.",
  "parameters": {
    "type": "object",
    "properties": {
      "ConferenceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^CF[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique SID identifier of the Conference."
      },
      "ParticipantSid": {
        "type": "string",
        "description": "The unique SID identifier of the Participant."
      },
      "Label": {
        "type": "string",
        "description": "User-specified label for a participant."
      },
      "Events": {
        "type": "string",
        "description": "Conference events generated by application or participant activity; e.g. `hold`, `mute`, etc."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "ConferenceSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://insights.twilio.com/v1/Conferences/{ConferenceSid}/Participants{?ParticipantSid,Label,Events,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListConferenceParticipantResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**participants**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
