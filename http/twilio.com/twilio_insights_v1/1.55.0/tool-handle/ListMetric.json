{
  "name": "ListMetric",
  "description": "Get a list of Call Metrics for a Call.",
  "parameters": {
    "type": "object",
    "properties": {
      "CallSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^CA[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique SID identifier of the Call."
      },
      "Edge": {
        "$ref": "#/components/schemas/metric_enum_twilio_edge",
        "type": "string",
        "description": "The Edge of this Metric. One of `unknown_edge`, `carrier_edge`, `sip_edge`, `sdk_edge` or `client_edge`."
      },
      "Direction": {
        "$ref": "#/components/schemas/metric_enum_stream_direction",
        "type": "string",
        "description": "The Direction of this Metric. One of `unknown`, `inbound`, `outbound` or `both`."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "CallSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://insights.twilio.com/v1/Voice/{CallSid}/Metrics{?Edge,Direction,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListMetricResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**metrics**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
