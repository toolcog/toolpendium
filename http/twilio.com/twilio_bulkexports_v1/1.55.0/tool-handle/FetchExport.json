{
  "name": "FetchExport",
  "description": "Fetch a specific Export.",
  "parameters": {
    "type": "object",
    "properties": {
      "ResourceType": {
        "type": "string",
        "description": "The type of communication – Messages, Calls, Conferences, and Participants"
      }
    },
    "required": [
      "ResourceType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://bulkexports.twilio.com/v1/Exports/{ResourceType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**links**: Contains a dictionary of URL links to nested resources of this Export.",
            "**resource_type**: The type of communication – Messages, Calls, Conferences, and Participants",
            "**url**: The URL of this resource."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
