{
  "name": "CreateInsightsSession",
  "description": "To obtain session details for fetching reports and dashboards",
  "parameters": {
    "type": "object",
    "properties": {
      "Authorization": {
        "type": "string",
        "description": "The Authorization HTTP request header"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://flex-api.twilio.com/v1/Insights/Session"
    },
    "headers": {
      "Authorization": {
        "$": "Authorization"
      }
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**base_url**: The base URL to fetch reports and dashboards",
            "**session_expiry**: The session expiry date and time, given in ISO 8601 format.",
            "**session_id**: The unique ID for the session",
            "**url**: The URL of this resource.",
            "**workspace_id**: Unique ID to identify the user's workspace"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
