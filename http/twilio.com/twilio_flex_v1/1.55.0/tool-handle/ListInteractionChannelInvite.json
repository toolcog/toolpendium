{
  "name": "ListInteractionChannelInvite",
  "description": "List all Invites for a Channel.",
  "parameters": {
    "type": "object",
    "properties": {
      "InteractionSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^KD[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The Interaction SID for this Channel."
      },
      "ChannelSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^UO[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The Channel SID for this Participant."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "InteractionSid",
      "ChannelSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://flex-api.twilio.com/v1/Interactions/{InteractionSid}/Channels/{ChannelSid}/Invites{?PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListInteractionChannelInviteResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**invites**",
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
