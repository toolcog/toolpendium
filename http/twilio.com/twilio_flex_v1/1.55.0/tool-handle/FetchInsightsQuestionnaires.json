{
  "name": "FetchInsightsQuestionnaires",
  "description": "To get the Questionnaire Detail",
  "parameters": {
    "type": "object",
    "properties": {
      "QuestionnaireSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^GM[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the questionnaire"
      },
      "Authorization": {
        "type": "string",
        "description": "The Authorization HTTP request header"
      }
    },
    "required": [
      "QuestionnaireSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://flex-api.twilio.com/v1/Insights/QualityManagement/Questionnaires/{QuestionnaireSid}"
    },
    "headers": {
      "Authorization": {
        "$": "Authorization"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**account_sid**: The SID of the [Account](https://www.twilio.com/docs/iam/api/account) that created the Flex Insights resource and owns this resource.",
            "**active**: The flag to enable or disable questionnaire",
            "**description**: The description of this questionnaire",
            "**name**: The name of this category.",
            "**questionnaire_sid**: The sid of this questionnaire",
            "**questions**: The list of questions with category for a questionnaire",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
