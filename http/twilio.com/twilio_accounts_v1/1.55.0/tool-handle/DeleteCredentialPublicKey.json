{
  "name": "DeleteCredentialPublicKey",
  "description": "Delete a Credential from your account",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^CR[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The Twilio-provided string that uniquely identifies the PublicKey resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://accounts.twilio.com/v1/Credentials/PublicKeys/{Sid}"
    }
  },
  "responses": {}
}
