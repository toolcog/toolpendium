{
  "name": "FetchSafelist",
  "description": "Check if a phone number exists in SafeList.",
  "parameters": {
    "type": "object",
    "properties": {
      "PhoneNumber": {
        "type": "string",
        "description": "The phone number to be fetched from SafeList. Phone numbers must be in [E.164 format](https://www.twilio.com/docs/glossary/what-e164)."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://accounts.twilio.com/v1/SafeList/Numbers{?PhoneNumber}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**phone_number**: The phone number in SafeList.",
            "**sid**: The unique string that we created to identify the SafeList resource."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
