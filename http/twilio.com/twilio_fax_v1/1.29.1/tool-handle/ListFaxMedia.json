{
  "name": "ListFaxMedia",
  "description": "Retrieve a list of all fax media instances for the specified fax.",
  "parameters": {
    "type": "object",
    "properties": {
      "FaxSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^FX[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the fax with the FaxMedia resources to read."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      }
    },
    "required": [
      "FaxSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://fax.twilio.com/v1/Faxes/{FaxSid}/Media{?PageSize}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListFaxMediaResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**media**",
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
