{
  "name": "DeleteFax",
  "description": "Delete a specific fax and its associated media.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^FX[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The Twilio-provided string that uniquely identifies the Fax resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://fax.twilio.com/v1/Faxes/{Sid}"
    }
  },
  "responses": {}
}
