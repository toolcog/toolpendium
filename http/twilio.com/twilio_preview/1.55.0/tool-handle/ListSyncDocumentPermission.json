{
  "name": "ListSyncDocumentPermission",
  "description": "Retrieve a list of all Permissions applying to a Sync Document.",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^IS[0-9a-fA-F]{32}$",
        "type": "string",
        "description": ""
      },
      "DocumentSid": {
        "type": "string",
        "description": "Identifier of the Sync Document. Either a SID or a unique name."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "ServiceSid",
      "DocumentSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://preview.twilio.com/Sync/Services/{ServiceSid}/Documents/{DocumentSid}/Permissions{?PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListSyncDocumentPermissionResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**permissions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
