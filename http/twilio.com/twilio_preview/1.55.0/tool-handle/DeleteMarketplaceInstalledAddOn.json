{
  "name": "DeleteMarketplaceInstalledAddOn",
  "description": "Remove an Add-on installation from your account",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^XE[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the InstalledAddOn resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://preview.twilio.com/marketplace/InstalledAddOns/{Sid}"
    }
  },
  "responses": {}
}
