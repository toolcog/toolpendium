{
  "name": "ListMediaRecording",
  "description": "Returns a list of MediaRecordings.",
  "parameters": {
    "type": "object",
    "properties": {
      "Order": {
        "$ref": "#/components/schemas/media_recording_enum_order",
        "type": "string",
        "description": "The sort order of the list by `date_created`. Can be: `asc` (ascending) or `desc` (descending) with `desc` as the default."
      },
      "Status": {
        "$ref": "#/components/schemas/media_recording_enum_status",
        "type": "string",
        "description": "Status to filter by, with possible values `processing`, `completed`, `deleted`, or `failed`."
      },
      "ProcessorSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^ZX[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "SID of a MediaProcessor to filter by."
      },
      "SourceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^[a-zA-Z]{2}[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "SID of a MediaRecording source to filter by."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://media.twilio.com/v1/MediaRecordings{?Order,Status,ProcessorSid,SourceSid,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListMediaRecordingResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**media_recordings**",
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
