{
  "name": "DeleteMediaRecording",
  "description": "Deletes a MediaRecording resource identified by a SID.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^KV[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the MediaRecording resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://media.twilio.com/v1/MediaRecordings/{Sid}"
    }
  },
  "responses": {}
}
