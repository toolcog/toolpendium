{
  "name": "FetchUsAppToPersonUsecase",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "MessagingServiceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^MG[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the [Messaging Service](https://www.twilio.com/docs/messaging/api/service-resource) to fetch the resource from."
      },
      "BrandRegistrationSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^BN[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique string to identify the A2P brand."
      }
    },
    "required": [
      "MessagingServiceSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://messaging.twilio.com/v1/Services/{MessagingServiceSid}/Compliance/Usa2p/Usecases{?BrandRegistrationSid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**us_app_to_person_usecases**: Human readable name, code, description and post_approval_required (indicates whether or not post approval is required for this Use Case) of A2P Campaign Use Cases."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
