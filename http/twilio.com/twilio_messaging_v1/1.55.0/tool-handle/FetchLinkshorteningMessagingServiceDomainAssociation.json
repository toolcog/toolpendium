{
  "name": "FetchLinkshorteningMessagingServiceDomainAssociation",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "MessagingServiceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^MG[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "Unique string used to identify the Messaging service that this domain should be associated with."
      }
    },
    "required": [
