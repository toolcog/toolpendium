    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://messaging.twilio.com/v1/LinkShortening/MessagingServices/{MessagingServiceSid}/Domain"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**domain_sid**: The unique string that we created to identify the Domain resource.",
            "**messaging_service_sid**: The unique string that identifies the messaging service",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
