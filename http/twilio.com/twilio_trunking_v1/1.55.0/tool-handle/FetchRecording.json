{
  "name": "FetchRecording",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "TrunkSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^TK[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Trunk from which to fetch the recording settings."
      }
    },
    "required": [
      "TrunkSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://trunking.twilio.com/v1/Trunks/{TrunkSid}/Recording"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**mode**",
            "**trim**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
