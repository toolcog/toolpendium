{
  "name": "ListDocumentPermission",
  "description": "Retrieve a list of all Permissions applying to a Sync Document.",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "type": "string",
        "description": "The SID of the [Sync Service](https://www.twilio.com/docs/sync/api/service) with the Document Permission resources to read."
      },
      "DocumentSid": {
        "type": "string",
        "description": "The SID of the Sync Document with the Document Permission resources to read. Can be the Document resource's `sid` or its `unique_name`."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "ServiceSid",
      "DocumentSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://sync.twilio.com/v1/Services/{ServiceSid}/Documents/{DocumentSid}/Permissions{?PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListDocumentPermissionResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**permissions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
