{
  "name": "DeleteSyncStream",
  "description": "Delete a specific Stream.",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "type": "string",
        "description": "The SID of the [Sync Service](https://www.twilio.com/docs/sync/api/service) with the Sync Stream resource to delete."
      },
      "Sid": {
        "type": "string",
        "description": "The SID of the Stream resource to delete."
      }
    },
    "required": [
      "ServiceSid",
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://sync.twilio.com/v1/Services/{ServiceSid}/Streams/{Sid}"
    }
  },
  "responses": {}
}
