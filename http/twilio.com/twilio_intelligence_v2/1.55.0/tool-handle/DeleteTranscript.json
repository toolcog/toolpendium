{
  "name": "DeleteTranscript",
  "description": "Delete a specific Transcript.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^GT[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "A 34 character string that uniquely identifies this Transcript."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://intelligence.twilio.com/v2/Transcripts/{Sid}"
    }
  },
  "responses": {}
}
