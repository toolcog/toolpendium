{
  "name": "ListTranscript",
  "description": "Retrieve a list of Transcripts for a given service.",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^GA[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique SID identifier of the Service."
      },
      "BeforeStartTime": {
        "type": "string",
        "description": "Filter by before StartTime."
      },
      "AfterStartTime": {
        "type": "string",
        "description": "Filter by after StartTime."
      },
      "BeforeDateCreated": {
        "type": "string",
        "description": "Filter by before DateCreated."
      },
      "AfterDateCreated": {
        "type": "string",
        "description": "Filter by after DateCreated."
      },
      "Status": {
        "type": "string",
        "description": "Filter by status."
      },
      "LanguageCode": {
        "type": "string",
        "description": "Filter by Language Code."
      },
      "SourceSid": {
        "type": "string",
        "description": "Filter by SourceSid."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://intelligence.twilio.com/v2/Transcripts{?ServiceSid,BeforeStartTime,AfterStartTime,BeforeDateCreated,AfterDateCreated,Status,LanguageCode,SourceSid,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListTranscriptResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**transcripts**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
