{
  "name": "ListOperatorResult",
  "description": "Retrieve a list of Operator Results for the given Transcript.",
  "parameters": {
    "type": "object",
    "properties": {
      "TranscriptSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^GT[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "A 34 character string that uniquely identifies this Transcript."
      },
      "Redacted": {
        "type": "boolean",
        "description": "Grant access to PII redacted/unredacted Language Understanding operator. If redaction is enabled, the default is True."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "TranscriptSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://intelligence.twilio.com/v2/Transcripts/{TranscriptSid}/OperatorResults{?Redacted,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListOperatorResultResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**operator_results**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
