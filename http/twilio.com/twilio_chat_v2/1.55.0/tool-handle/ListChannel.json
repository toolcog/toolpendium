{
  "name": "ListChannel",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^IS[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the [Service](https://www.twilio.com/docs/chat/rest/service-resource) to read the Channel resources from."
      },
      "Type": {
        "items": {
          "$ref": "#/$defs/channel_enum_channel_type"
        },
        "type": "array",
        "description": "The visibility of the Channels to read. Can be: `public` or `private` and defaults to `public`."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "ServiceSid"
    ],
    "$defs": {
      "channel_enum_channel_type": {
        "enum": [
          "public",
          "private"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://chat.twilio.com/v2/Services/{ServiceSid}/Channels{?Type,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListChannelResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**channels**",
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
