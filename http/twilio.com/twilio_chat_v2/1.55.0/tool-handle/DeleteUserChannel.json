{
  "name": "DeleteUserChannel",
  "description": "Removes User from selected Channel.",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^IS[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the [Service](https://www.twilio.com/docs/api/chat/rest/services) to read the resources from."
      },
      "UserSid": {
        "type": "string",
        "description": "The SID of the [User](https://www.twilio.com/docs/api/chat/rest/users) to read the User Channel resources from."
      },
      "ChannelSid": {
        "type": "string",
        "description": "The SID of the [Channel](https://www.twilio.com/docs/api/chat/rest/channels) the resource belongs to."
      },
      "X-Twilio-Webhook-Enabled": {
        "$ref": "#/components/schemas/user_channel_enum_webhook_enabled_type",
        "type": "string",
        "description": "The X-Twilio-Webhook-Enabled HTTP request header"
      }
    },
    "required": [
      "ServiceSid",
      "UserSid",
      "ChannelSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://chat.twilio.com/v2/Services/{ServiceSid}/Users/{UserSid}/Channels/{ChannelSid}"
    },
    "headers": {
      "X-Twilio-Webhook-Enabled": {
        "$": "X-Twilio-Webhook-Enabled"
      }
    }
  },
  "responses": {}
}
