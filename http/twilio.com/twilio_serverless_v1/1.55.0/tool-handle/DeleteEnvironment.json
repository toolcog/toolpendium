{
  "name": "DeleteEnvironment",
  "description": "Delete a specific environment.",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "type": "string",
        "description": "The SID of the Service to delete the Environment resource from."
      },
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^ZE[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Environment resource to delete."
      }
    },
    "required": [
      "ServiceSid",
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://serverless.twilio.com/v1/Services/{ServiceSid}/Environments/{Sid}"
    }
  },
  "responses": {}
}
