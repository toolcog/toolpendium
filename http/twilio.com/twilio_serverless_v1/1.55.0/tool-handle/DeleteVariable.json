{
  "name": "DeleteVariable",
  "description": "Delete a specific Variable.",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "type": "string",
        "description": "The SID of the Service to delete the Variable resource from."
      },
      "EnvironmentSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^ZE[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Environment with the Variables to delete."
      },
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^ZV[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Variable resource to delete."
      }
    },
    "required": [
      "ServiceSid",
      "EnvironmentSid",
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://serverless.twilio.com/v1/Services/{ServiceSid}/Environments/{EnvironmentSid}/Variables/{Sid}"
    }
  },
  "responses": {}
}
