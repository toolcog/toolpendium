{
  "name": "DeleteFunction",
  "description": "Delete a Function resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "ServiceSid": {
        "type": "string",
        "description": "The SID of the Service to delete the Function resource from."
      },
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^ZH[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Function resource to delete."
      }
    },
    "required": [
      "ServiceSid",
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://serverless.twilio.com/v1/Services/{ServiceSid}/Functions/{Sid}"
    }
  },
  "responses": {}
}
