{
  "name": "FetchSchemaVersion",
  "description": "Fetch a specific schema and version.",
  "parameters": {
    "type": "object",
    "properties": {
      "Id": {
        "type": "string",
        "description": "The unique identifier of the schema. Each schema can have multiple versions, that share the same id."
      },
      "SchemaVersion": {
        "type": "integer",
        "description": "The version of the schema"
      }
    },
    "required": [
      "Id",
      "SchemaVersion"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://events.twilio.com/v1/Schemas/{Id}/Versions/{SchemaVersion}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**date_created**: The date the schema version was created, given in ISO 8601 format.",
            "**id**: The unique identifier of the schema. Each schema can have multiple versions, that share the same id.",
            "**raw**",
            "**schema_version**: The version of this schema.",
            "**url**: The URL of this resource."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
