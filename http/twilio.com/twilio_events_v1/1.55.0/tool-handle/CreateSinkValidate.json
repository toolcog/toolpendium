{
  "name": "CreateSinkValidate",
  "description": "Validate that a test event for a Sink was received.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^DG[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "A 34 character string that uniquely identifies the Sink being validated."
      },
      "body": {
        "properties": {
          "TestId": {
            "description": "A 34 character string that uniquely identifies the test event for a Sink being validated.",
            "type": "string"
          }
        },
        "required": [
          "TestId"
        ],
        "title": "CreateSinkValidateRequest",
        "type": "object"
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://events.twilio.com/v1/Sinks/{Sid}/Validate"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**result**: Feedback indicating whether the given Sink was validated."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
