{
  "name": "CreateSinkTest",
  "description": "Create a new Sink Test Event for the given Sink.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^DG[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "A 34 character string that uniquely identifies the Sink to be Tested."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://events.twilio.com/v1/Sinks/{Sid}/Test"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**result**: Feedback indicating whether the test event was generated."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
