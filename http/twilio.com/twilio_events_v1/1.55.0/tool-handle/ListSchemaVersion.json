{
  "name": "ListSchemaVersion",
  "description": "Retrieve a paginated list of versions of the schema.",
  "parameters": {
    "type": "object",
    "properties": {
      "Id": {
        "type": "string",
        "description": "The unique identifier of the schema. Each schema can have multiple versions, that share the same id."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "Id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://events.twilio.com/v1/Schemas/{Id}/Versions{?PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListSchemaVersionResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**schema_versions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
