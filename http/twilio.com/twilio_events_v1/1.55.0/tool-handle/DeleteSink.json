{
  "name": "DeleteSink",
  "description": "Delete a specific Sink.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^DG[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "A 34 character string that uniquely identifies this Sink."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://events.twilio.com/v1/Sinks/{Sid}"
    }
  },
  "responses": {}
}
