{
  "name": "ListComposition",
  "description": "List of all Recording compositions.",
  "parameters": {
    "type": "object",
    "properties": {
      "Status": {
        "$ref": "#/components/schemas/composition_enum_status",
        "type": "string",
        "description": "Read only Composition resources with this status. Can be: `enqueued`, `processing`, `completed`, `deleted`, or `failed`."
      },
      "DateCreatedAfter": {
        "format": "date-time",
        "type": "string",
        "description": "Read only Composition resources created on or after this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) date-time with time zone."
      },
      "DateCreatedBefore": {
        "format": "date-time",
        "type": "string",
        "description": "Read only Composition resources created before this ISO 8601 date-time with time zone."
      },
      "RoomSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^RM[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "Read only Composition resources with this Room SID."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://video.twilio.com/v1/Compositions{?Status,DateCreatedAfter,DateCreatedBefore,RoomSid,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListCompositionResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**compositions**",
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
