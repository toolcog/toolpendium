{
  "name": "DeleteComposition",
  "description": "Delete a Recording Composition resource identified by a Composition SID.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^CJ[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Composition resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://video.twilio.com/v1/Compositions/{Sid}"
    }
  },
  "responses": {}
}
