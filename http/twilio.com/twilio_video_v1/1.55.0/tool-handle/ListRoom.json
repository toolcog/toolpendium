{
  "name": "ListRoom",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "Status": {
        "$ref": "#/components/schemas/room_enum_room_status",
        "type": "string",
        "description": "Read only the rooms with this status. Can be: `in-progress` (default) or `completed`"
      },
      "UniqueName": {
        "type": "string",
        "description": "Read only rooms with the this `unique_name`."
      },
      "DateCreatedAfter": {
        "format": "date-time",
        "type": "string",
        "description": "Read only rooms that started on or after this date, given as `YYYY-MM-DD`."
      },
      "DateCreatedBefore": {
        "format": "date-time",
        "type": "string",
        "description": "Read only rooms that started before this date, given as `YYYY-MM-DD`."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://video.twilio.com/v1/Rooms{?Status,UniqueName,DateCreatedAfter,DateCreatedBefore,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListRoomResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**rooms**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
