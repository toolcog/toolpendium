{
  "name": "ListRoomRecording",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "RoomSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^RM[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the room with the RoomRecording resources to read."
      },
      "Status": {
        "$ref": "#/components/schemas/room_recording_enum_status",
        "type": "string",
        "description": "Read only the recordings with this status. Can be: `processing`, `completed`, or `deleted`."
      },
      "SourceSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^[a-zA-Z]{2}[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "Read only the recordings that have this `source_sid`."
      },
      "DateCreatedAfter": {
        "format": "date-time",
        "type": "string",
        "description": "Read only recordings that started on or after this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) datetime with time zone."
      },
      "DateCreatedBefore": {
        "format": "date-time",
        "type": "string",
        "description": "Read only Recordings that started before this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) datetime with time zone."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    },
    "required": [
      "RoomSid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://video.twilio.com/v1/Rooms/{RoomSid}/Recordings{?Status,SourceSid,DateCreatedAfter,DateCreatedBefore,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListRoomRecordingResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**recordings**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
