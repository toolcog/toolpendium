{
  "name": "DeleteRecording",
  "description": "Delete a Recording resource identified by a Recording SID.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^RT[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Recording resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://video.twilio.com/v1/Recordings/{Sid}"
    }
  },
  "responses": {}
}
