{
  "name": "DeleteSipCredential",
  "description": "Delete a credential resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "AccountSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^AC[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique id of the Account that is responsible for this resource."
      },
      "CredentialListSid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^CL[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique id that identifies the credential list that contains the desired credentials."
      },
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^CR[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique id that identifies the resource to delete."
      }
    },
    "required": [
      "AccountSid",
      "CredentialListSid",
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.twilio.com/2010-04-01/Accounts/{AccountSid}/SIP/CredentialLists/{CredentialListSid}/Credentials/{Sid}.json"
    }
  },
  "responses": {}
}
