{
  "name": "ListCustomerProfile",
  "description": "Retrieve a list of all Customer-Profiles for an account.",
  "parameters": {
    "type": "object",
    "properties": {
      "Status": {
        "$ref": "#/components/schemas/customer_profile_enum_status",
        "type": "string",
        "description": "The verification status of the Customer-Profile resource."
      },
      "FriendlyName": {
        "type": "string",
        "description": "The string that you assigned to describe the resource."
      },
      "PolicySid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^RN[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique string of a policy that is associated to the Customer-Profile resource."
      },
      "PageSize": {
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "How many resources to return in each list page. The default is 50, and the maximum is 1000."
      },
      "Page": {
        "minimum": 0,
        "type": "integer",
        "description": "The page index. This value is simply for client state."
      },
      "PageToken": {
        "type": "string",
        "description": "The page token. This is provided by the API."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://trusthub.twilio.com/v1/CustomerProfiles{?Status,FriendlyName,PolicySid,PageSize,Page,PageToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ListCustomerProfileResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**first_page_url**",
                  "**key**",
                  "**next_page_url**",
                  "**page**",
                  "**page_size**",
                  "**previous_page_url**",
                  "**url**"
                ]
              }
            ],
            "**results**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
