{
  "name": "DeleteCustomerProfile",
  "description": "Delete a specific Customer-Profile.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^BU[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The unique string that we created to identify the Customer-Profile resource."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://trusthub.twilio.com/v1/CustomerProfiles/{Sid}"
    }
  },
  "responses": {}
}
