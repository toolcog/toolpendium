{
  "name": "UpdateFlowValidate",
  "description": "Validate flow JSON definition",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "CommitMessage": {
            "description": "Description of change made in the revision.",
            "type": "string"
          },
          "Definition": {
            "description": "JSON representation of flow definition."
          },
          "FriendlyName": {
            "description": "The string that you assigned to describe the Flow.",
            "type": "string"
          },
          "Status": {
            "$ref": "#/$defs/flow_validate_enum_status"
          }
        },
        "required": [
          "FriendlyName",
          "Status",
          "Definition"
        ],
        "title": "UpdateFlowValidateRequest",
        "type": "object"
      }
    },
    "$defs": {
      "flow_validate_enum_status": {
        "enum": [
          "draft",
          "published"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://studio.twilio.com/v2/Flows/Validate"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**valid**: Boolean if the flow definition is valid."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
