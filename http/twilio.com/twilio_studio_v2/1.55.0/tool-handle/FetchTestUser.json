{
  "name": "FetchTestUser",
  "description": "Fetch flow test users",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^FW[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "Unique identifier of the flow."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://studio.twilio.com/v2/Flows/{Sid}/TestUsers"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**sid**: Unique identifier of the flow.",
            "**test_users**: List of test user identities that can test draft versions of the flow.",
            "**url**: The URL of this resource."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
