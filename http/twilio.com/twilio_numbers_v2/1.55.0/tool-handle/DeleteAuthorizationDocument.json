{
  "name": "DeleteAuthorizationDocument",
  "description": "Cancel the AuthorizationDocument request.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^PX[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "A 34 character string that uniquely identifies this AuthorizationDocument."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://numbers.twilio.com/v2/HostedNumber/AuthorizationDocuments/{Sid}"
    }
  },
  "responses": {}
}
