{
  "name": "DeleteCommand",
  "description": "Delete a Command instance from your account.",
  "parameters": {
    "type": "object",
    "properties": {
      "Sid": {
        "maxLength": 34,
        "minLength": 34,
        "pattern": "^DC[0-9a-fA-F]{32}$",
        "type": "string",
        "description": "The SID of the Command resource to delete."
      }
    },
    "required": [
      "Sid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://wireless.twilio.com/v1/Commands/{Sid}"
    }
  },
  "responses": {}
}
