{
  "name": "getMapsUsingGET",
  "description": "Return a list assembly maps for a species",
  "parameters": {
    "type": "object",
    "properties": {
      "speciesTypeKey": {
        "format": "int32",
        "type": "integer",
        "description": "RGD species type key. A full list of keys is available throught the lookup service.  1=human, 2=mouse, 3=rat,ect"
      }
    },
    "required": [
      "speciesTypeKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/lookup/maps/{speciesTypeKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Map list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**dbsnpVersion**",
            "**description**",
            "**key**",
            "**methodKey**",
            "**name**",
            "**notes**",
            "**primaryRefAssembly**",
            "**rank**",
            "**refSeqAssemblyAcc**",
            "**refSeqAssemblyName**",
            "**rgdId**",
            "**source**",
            "**speciesTypeKey**",
            "**ucscAssemblyId**",
            "**unit**",
            "**version**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
