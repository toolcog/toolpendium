{
  "name": "getAnnotationsUsingPOST",
  "description": "Return a list of genes annotated to an ontology term",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/AnnotationRequest",
        "description": "data"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/annotations/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Annotation list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**annotatedObjectRgdId**",
            "**annotationExtension**",
            "**aspect**",
            "**createdBy**",
            "**createdDate**",
            "**dataSrc**",
            "**evidence**",
            "**geneProductFormId**",
            "**key**",
            "**lastModifiedBy**",
            "**lastModifiedDate**",
            "**notes**",
            "**objectName**",
            "**objectSymbol**",
            "**originalCreatedDate**",
            "**qualifier**",
            "**refRgdId**",
            "**relativeTo**",
            "**rgdObjectKey**",
            "**speciesTypeKey**",
            "**term**",
            "**termAcc**",
            "**withInfo**",
            "**xrefSource**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
