{
  "name": "searchPathwaysUsingGET",
  "description": "Return a list of pathways based on search term",
  "parameters": {
    "type": "object",
    "properties": {
      "searchString": {
        "type": "string",
        "description": "Free text search string"
      }
    },
    "required": [
      "searchString"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/pathways/diagrams/search/{searchString}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Pathway list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**description**",
            "**hasAlteredPath**",
            "**id**",
            "**name**",
            "**objectList**",
            "**pathwayCategories**",
            "**referenceList**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
