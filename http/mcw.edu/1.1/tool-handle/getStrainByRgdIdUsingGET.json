{
  "name": "getStrainByRgdIdUsingGET",
  "description": "Return a strain by RGD ID",
  "parameters": {
    "type": "object",
    "properties": {
      "rgdId": {
        "format": "int32",
        "type": "integer",
        "description": "RGD ID of the strain"
      }
    },
    "required": [
      "rgdId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/strains/{rgdId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Strain"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**backgroundStrainRgdId**",
            "**chrAltered**",
            "**color**",
            "**geneticStatus**",
            "**genetics**",
            "**imageUrl**",
            "**inbredGen**",
            "**key**",
            "**lastStatus**",
            [
              "**lastStatusObject**",
              {
                "$ul": [
                  "**cryopreservedEmbryo**",
                  "**cryopreservedSperm**",
                  "**cryorecovery**",
                  "**key**",
                  "**liveAnimals**",
                  "**statusDate**",
                  "**strainRgdId**"
                ]
              }
            ],
            "**modificationMethod**",
            "**name**",
            "**notes**",
            "**origin**",
            "**researchUse**",
            "**rgdId**",
            "**source**",
            "**speciesTypeKey**",
            "**statusLog**",
            "**strain**",
            "**strainTypeName**",
            "**substrain**",
            "**symbol**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
