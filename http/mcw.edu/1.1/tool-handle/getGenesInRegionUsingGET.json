{
  "name": "getGenesInRegionUsingGET",
  "description": "Return a list of genes in region",
  "parameters": {
    "type": "object",
    "properties": {
      "chr": {
        "type": "string",
        "description": "Chromosome"
      },
      "start": {
        "format": "int64",
        "type": "integer",
        "description": "Start Position"
      },
      "stop": {
        "format": "int64",
        "type": "integer",
        "description": "Stop Position"
      },
      "mapKey": {
        "format": "int32",
        "type": "integer",
        "description": "A list of RGD assembly map keys can be found in the lookup service"
      }
    },
    "required": [
      "chr",
      "start",
      "stop",
      "mapKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/genes/region/{chr}/{start}/{stop}/{mapKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "MappedGenePosition list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**chromosome**",
            "**mapKey**",
            "**rgdId**",
            "**start**",
            "**stop**",
            "**strand**",
            "**symbol**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
