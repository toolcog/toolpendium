{
  "name": "getTermAccIdsUsingGET",
  "description": "Returns a list ontology term accession IDs annotated to an rgd object",
  "parameters": {
    "type": "object",
    "properties": {
      "rgdId": {
        "format": "int32",
        "type": "integer",
        "description": "RGD ID"
      }
    },
    "required": [
      "rgdId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/annotations/accId/{rgdId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "MapPair list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**keyValue**",
            "**stringValue**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
