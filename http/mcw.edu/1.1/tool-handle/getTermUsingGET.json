{
  "name": "getTermUsingGET",
  "description": "Returns term for Accession ID",
  "parameters": {
    "type": "object",
    "properties": {
      "accId": {
        "type": "string",
        "description": "Term Accession ID"
      }
    },
    "required": [
      "accId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/ontology/term/{accId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Term"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accId**",
            "**comment**",
            "**createdBy**",
            "**creationDate**",
            "**definition**",
            "**modificationDate**",
            "**obsolete**",
            "**ontologyId**",
            "**term**",
            "**xrefs**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
