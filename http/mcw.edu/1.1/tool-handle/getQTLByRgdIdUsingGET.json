{
  "name": "getQTLByRgdIdUsingGET",
  "description": "Return a QTL for provided RGD ID",
  "parameters": {
    "type": "object",
    "properties": {
      "rgdId": {
        "format": "int32",
        "type": "integer",
        "description": "RGD ID"
      }
    },
    "required": [
      "rgdId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/qtls/{rgdId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "QTL"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**chromosome**",
            "**flank1RgdId**",
            "**flank2RgdId**",
            "**inheritanceType**",
            "**key**",
            "**linkageImage**",
            "**lod**",
            "**lodImage**",
            "**mostSignificantCmoTerm**",
            "**name**",
            "**notes**",
            "**peakOffset**",
            "**peakRgdId**",
            "**pvalue**",
            "**rgdId**",
            "**sourceUrl**",
            "**speciesTypeKey**",
            "**symbol**",
            "**variance**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
