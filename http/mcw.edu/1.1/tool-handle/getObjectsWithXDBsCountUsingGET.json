{
  "name": "getObjectsWithXDBsCountUsingGET",
  "description": "Count of objects with external database ids, by database id, for specified species, object type and date",
  "parameters": {
    "type": "object",
    "properties": {
      "speciesTypeKey": {
        "format": "int32",
        "type": "integer",
        "description": "speciesTypeKey"
      },
      "objectKey": {
        "format": "int32",
        "type": "integer",
        "description": "objectKey"
      },
      "dateYYYYMMDD": {
        "type": "string",
        "description": "dateYYYYMMDD"
      }
    },
    "required": [
      "speciesTypeKey",
      "objectKey",
      "dateYYYYMMDD"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/stats/count/objectWithXdb/{speciesTypeKey}/{objectKey}/{dateYYYYMMDD}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
