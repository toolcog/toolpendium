{
  "name": "getChromosomesByAssemblyUsingGET",
  "description": "Return a list of chromosomes",
  "parameters": {
    "type": "object",
    "properties": {
      "mapKey": {
        "format": "int32",
        "type": "integer",
        "description": "mapKey"
      }
    },
    "required": [
      "mapKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/maps/chr/{mapKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
