{
  "name": "getMappedSSLPByPositionUsingGET",
  "description": "Returns a list SSLP for given position and assembly map",
  "parameters": {
    "type": "object",
    "properties": {
      "chr": {
        "type": "string",
        "description": "Chromosome"
      },
      "start": {
        "format": "int64",
        "type": "integer",
        "description": "Start Position"
      },
      "stop": {
        "format": "int64",
        "type": "integer",
        "description": "Stop Position"
      },
      "mapKey": {
        "format": "int32",
        "type": "integer",
        "description": "A list of assembly map keys can be found using the lookup service"
      }
    },
    "required": [
      "chr",
      "start",
      "stop",
      "mapKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/sslps/mapped/{chr}/{start}/{stop}/{mapKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "MappedSSLP list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**chromosome**",
            "**mapKey**",
            [
              "**sslp**",
              {
                "$ul": [
                  "**expectedSize**",
                  "**forwardSeq**",
                  "**key**",
                  "**name**",
                  "**notes**",
                  "**reverseSeq**",
                  "**rgdId**",
                  "**speciesTypeKey**",
                  "**sslpType**",
                  "**templateSeq**"
                ]
              }
            ],
            "**start**",
            "**stop**",
            "**strand**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
