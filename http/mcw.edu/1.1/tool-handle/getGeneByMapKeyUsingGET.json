{
  "name": "getGeneByMapKeyUsingGET",
  "description": "Return a list of all genes with position information for an assembly",
  "parameters": {
    "type": "object",
    "properties": {
      "mapKey": {
        "format": "int32",
        "type": "integer",
        "description": "A list of RGD assembly map keys can be found in the lookup service"
      }
    },
    "required": [
      "mapKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/genes/map/{mapKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "MappedGene list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**chromosome**",
            [
              "**gene**",
              {
                "$ul": [
                  "**agrDescription**",
                  "**description**",
                  "**ensemblFullName**",
                  "**ensemblGeneSymbol**",
                  "**ensemblGeneType**",
                  "**geneSource**",
                  "**key**",
                  "**mergedDescription**",
                  "**name**",
                  "**ncbiAnnotStatus**",
                  "**nomenReviewDate**",
                  "**nomenSource**",
                  "**notes**",
                  "**refSeqStatus**",
                  "**rgdId**",
                  "**soAccId**",
                  "**speciesTypeKey**",
                  "**symbol**",
                  "**type**",
                  "**variant**"
                ]
              }
            ],
            "**mapKey**",
            "**start**",
            "**stop**",
            "**strand**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
