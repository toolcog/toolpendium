{
  "name": "getAffectedGenomicModelsUsingGET",
  "description": "Get affected genomic models (rat strains with gene alleles) submitted by RGD to AGR by taxonId",
  "parameters": {
    "type": "object",
    "properties": {
      "taxonId": {
        "type": "string",
        "description": "The taxon ID for species"
      }
    },
    "required": [
      "taxonId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/agr/affectedGenomicModels/{taxonId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
