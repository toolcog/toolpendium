{
  "name": "getGTEXMappingUsingGET",
  "description": "Translate an RGD ID to an GTEx ID",
  "parameters": {
    "type": "object",
    "properties": {
      "rgdId": {
        "format": "int32",
        "type": "integer",
        "description": "RGD ID"
      }
    },
    "required": [
      "rgdId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/lookup/id/map/GTEx/{rgdId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
