{
  "name": "getObjectsWithRefSeqDiffUsingGET",
  "description": "Count difference of objects with reference sequence(s), by object type, for specified species and date range",
  "parameters": {
    "type": "object",
    "properties": {
      "speciesTypeKey": {
        "format": "int32",
        "type": "integer",
        "description": "speciesTypeKey"
      },
      "dateFromYYYYMMDD": {
        "type": "string",
        "description": "dateFromYYYYMMDD"
      },
      "dateToYYYYMMDD": {
        "type": "string",
        "description": "dateToYYYYMMDD"
      }
    },
    "required": [
      "speciesTypeKey",
      "dateFromYYYYMMDD",
      "dateToYYYYMMDD"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/stats/diff/objectWithRefSeq/{speciesTypeKey}/{dateFromYYYYMMDD}/{dateToYYYYMMDD}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
