{
  "name": "getAnnotationsUsingGET",
  "description": "Returns a list annotations for an ontology term or a term and it's children",
  "parameters": {
    "type": "object",
    "properties": {
      "accId": {
        "type": "string",
        "description": "Ontology term accession ID"
      },
      "speciesTypeKey": {
        "format": "int32",
        "type": "integer",
        "description": "A list of species type keys can be found using the lookup service"
      },
      "includeChildren": {
        "type": "boolean",
        "description": "true: return annotations for the term and children, false: return annotations for the term only "
      }
    },
    "required": [
      "accId",
      "speciesTypeKey",
      "includeChildren"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/annotations/{accId}/{speciesTypeKey}/{includeChildren}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Annotation list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**annotatedObjectRgdId**",
            "**annotationExtension**",
            "**aspect**",
            "**createdBy**",
            "**createdDate**",
            "**dataSrc**",
            "**evidence**",
            "**geneProductFormId**",
            "**key**",
            "**lastModifiedBy**",
            "**lastModifiedDate**",
            "**notes**",
            "**objectName**",
            "**objectSymbol**",
            "**originalCreatedDate**",
            "**qualifier**",
            "**refRgdId**",
            "**relativeTo**",
            "**rgdObjectKey**",
            "**speciesTypeKey**",
            "**term**",
            "**termAcc**",
            "**withInfo**",
            "**xrefSource**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
