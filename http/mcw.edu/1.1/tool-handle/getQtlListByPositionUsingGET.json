{
  "name": "getQtlListByPositionUsingGET",
  "description": "Returns a list QTL for given position and assembly map",
  "parameters": {
    "type": "object",
    "properties": {
      "chr": {
        "type": "string",
        "description": "Chromosome"
      },
      "start": {
        "format": "int64",
        "type": "integer",
        "description": "Start Position"
      },
      "stop": {
        "format": "int64",
        "type": "integer",
        "description": "Stop Position"
      },
      "mapKey": {
        "format": "int32",
        "type": "integer",
        "description": "A list of assembly map keys can be found using the lookup service"
      }
    },
    "required": [
      "chr",
      "start",
      "stop",
      "mapKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/qtls/{chr}/{start}/{stop}/{mapKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "QTL list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**chromosome**",
            "**flank1RgdId**",
            "**flank2RgdId**",
            "**inheritanceType**",
            "**key**",
            "**linkageImage**",
            "**lod**",
            "**lodImage**",
            "**mostSignificantCmoTerm**",
            "**name**",
            "**notes**",
            "**peakOffset**",
            "**peakRgdId**",
            "**pvalue**",
            "**rgdId**",
            "**sourceUrl**",
            "**speciesTypeKey**",
            "**symbol**",
            "**variance**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
