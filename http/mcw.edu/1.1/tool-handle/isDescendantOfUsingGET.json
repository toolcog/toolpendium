{
  "name": "isDescendantOfUsingGET",
  "description": "Returns true or false for terms",
  "parameters": {
    "type": "object",
    "properties": {
      "accId1": {
        "type": "string",
        "description": "Child Term Accession ID"
      },
      "accId2": {
        "type": "string",
        "description": "Parent Term Accession ID"
      }
    },
    "required": [
      "accId1",
      "accId2"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/ontology/term/{accId1}/{accId2}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
