{
  "name": "getStrainsByPositionUsingGET",
  "description": "Return all active strains by position",
  "parameters": {
    "type": "object",
    "properties": {
      "chr": {
        "type": "string",
        "description": "Chromosome"
      },
      "start": {
        "format": "int64",
        "type": "integer",
        "description": "Start Position"
      },
      "stop": {
        "format": "int64",
        "type": "integer",
        "description": "Stop Position"
      },
      "mapKey": {
        "format": "int32",
        "type": "integer",
        "description": "RGD Map Key (available through lookup service)"
      }
    },
    "required": [
      "chr",
      "start",
      "stop",
      "mapKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/strains/{chr}/{start}/{stop}/{mapKey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Strain list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**backgroundStrainRgdId**",
            "**chrAltered**",
            "**color**",
            "**geneticStatus**",
            "**genetics**",
            "**imageUrl**",
            "**inbredGen**",
            "**key**",
            "**lastStatus**",
            [
              "**lastStatusObject**",
              {
                "$ul": [
                  "**cryopreservedEmbryo**",
                  "**cryopreservedSperm**",
                  "**cryorecovery**",
                  "**key**",
                  "**liveAnimals**",
                  "**statusDate**",
                  "**strainRgdId**"
                ]
              }
            ],
            "**modificationMethod**",
            "**name**",
            "**notes**",
            "**origin**",
            "**researchUse**",
            "**rgdId**",
            "**source**",
            "**speciesTypeKey**",
            "**statusLog**",
            "**strain**",
            "**strainTypeName**",
            "**substrain**",
            "**symbol**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
