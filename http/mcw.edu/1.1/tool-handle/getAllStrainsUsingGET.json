{
  "name": "getAllStrainsUsingGET",
  "description": "Return all active strains in RGD",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//rest.rgd.mcw.edu/rgdws/strains/all"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Strain list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**backgroundStrainRgdId**",
            "**chrAltered**",
            "**color**",
            "**geneticStatus**",
            "**genetics**",
            "**imageUrl**",
            "**inbredGen**",
            "**key**",
            "**lastStatus**",
            [
              "**lastStatusObject**",
              {
                "$ul": [
                  "**cryopreservedEmbryo**",
                  "**cryopreservedSperm**",
                  "**cryorecovery**",
                  "**key**",
                  "**liveAnimals**",
                  "**statusDate**",
                  "**strainRgdId**"
                ]
              }
            ],
            "**modificationMethod**",
            "**name**",
            "**notes**",
            "**origin**",
            "**researchUse**",
            "**rgdId**",
            "**source**",
            "**speciesTypeKey**",
            "**statusLog**",
            "**strain**",
            "**strainTypeName**",
            "**substrain**",
            "**symbol**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
