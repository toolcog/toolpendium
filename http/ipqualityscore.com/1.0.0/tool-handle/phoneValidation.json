{
  "name": "phoneValidation",
  "description": "Phone Validation",
  "parameters": {
    "type": "object",
    "properties": {
      "country": {
        "example": "UK",
        "type": "string",
        "description": "country"
      },
      "YOUR_API_KEY_HERE": {
        "example": "asd24#sdfs322#",
        "type": "string",
        "description": "(Required) YOUR_API_KEY_HERE"
      },
      "USER_PHONE_HERE": {
        "example": "18007132618",
        "type": "string",
        "description": "(Required) USER_PHONE_HERE"
      }
    },
    "required": [
      "YOUR_API_KEY_HERE",
      "USER_PHONE_HERE"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ipqualityscore.com/api/json/phone/{YOUR_API_KEY_HERE}/{USER_PHONE_HERE}{?country}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**VOIP**",
            "**active**",
            "**active_status**",
            [
              "**associated_email_addresses**",
              {
                "$ul": [
                  "**emails**",
                  "**status**"
                ]
              }
            ],
            "**carrier**",
            "**city**",
            "**country**",
            "**dialing_code**",
            "**do_not_call**",
            "**formatted**",
            "**fraud_score**",
            "**leaked**",
            "**line_type**",
            "**local_format**",
            "**mcc**",
            "**message**",
            "**mnc**",
            "**name**",
            "**prepaid**",
            "**recent_abuse**",
            "**region**",
            "**request_id**",
            "**risky**",
            "**sms_domain**",
            "**sms_email**",
            "**spammer**",
            "**success**",
            "**timezone**",
            "**user_activity**",
            "**valid**",
            "**zip_code**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**request_id**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**request_id**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
