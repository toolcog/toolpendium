            "**category**",
            "**content_type**",
            "**dns_valid**",
            "**domain**",
            [
              "**domain_age**",
              {
                "$ul": [
                  "**human**",
                  "**iso**",
                  "**timestamp**"
                ]
              }
            ],
            "**domain_rank**",
            "**ip_address**",
            "**malware**",
            "**message**",
            "**page_size**",
            "**parking**",
            "**phishing**",
            "**request_id**",
            "**risk_score**",
            "**server**",
            "**spamming**",
            "**status_code**",
            "**success**",
            "**suspicious**",
            "**unsafe**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
