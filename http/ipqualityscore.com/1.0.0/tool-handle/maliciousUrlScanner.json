{
  "name": "maliciousUrlScanner",
  "description": "Malicious URL Scanner",
  "parameters": {
    "type": "object",
    "properties": {
      "YOUR_API_KEY_HERE": {
        "example": "asd24#sdfs322#",
        "type": "string",
        "description": "(Required) YOUR_API_KEY_HERE"
      },
      "URL_HERE": {
        "example": "https%3A%2F%2Fgoogle.com",
        "type": "string",
        "description": "(Required) URL_HERE"
      }
    },
    "required": [
      "YOUR_API_KEY_HERE",
      "URL_HERE"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ipqualityscore.com/api/json/url/{YOUR_API_KEY_HERE}/{URL_HERE}"
    }
  },
