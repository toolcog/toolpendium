{
  "name": "predictMultipart",
  "description": "Returns a prediction from an image or multi-label model for the specified image.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/ImageClassificationRequest"
      }
    },
    "$defs": {
      "ImageClassificationRequest": {
        "properties": {
          "modelId": {
            "description": "ID of the model that makes the prediction.",
            "example": "WJH4YCA7YX4PCWVNCYNWYHBMY4",
            "type": "string"
          },
          "numResults": {
            "description": "Number of probabilities to return.",
            "example": 3,
            "format": "int32",
            "minimum": 1,
            "type": "integer"
          },
          "sampleBase64Content": {
            "description": "The image contained in a base64 string.",
            "example": "SomeBase64EncodedImage",
            "type": "string"
          },
          "sampleId": {
            "description": "String that you can pass in to tag the prediction. Optional. Can be any value, and is returned in the response.",
            "type": "string"
          },
          "sampleLocation": {
            "description": "URL of the image file.",
            "type": "string"
          }
        },
        "required": [
          "modelId"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://salesforce.local/v2/vision/predict"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**object**",
            "**probabilities**",
            "**sampleId**: Value passed in when the prediction call was made. Returned only if the sampleId request parameter is provided."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
