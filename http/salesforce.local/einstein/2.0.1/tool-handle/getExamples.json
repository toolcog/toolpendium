{
  "name": "getExamples",
  "description": "Returns all the examples for the specified dataset,",
  "parameters": {
    "type": "object",
    "properties": {
      "datasetId": {
        "type": "string",
        "description": "Dataset Id"
      },
      "offset": {
        "default": "0",
        "type": "string",
        "description": "Index of the example from which you want to start paging."
      },
      "count": {
        "default": "100",
        "type": "string",
        "description": "Number of examples to return."
      },
      "source": {
        "enum": [
          "all",
          "feedback",
          "upload"
        ],
        "type": "string",
        "description": "return examples that were created in the dataset as feedback"
      }
    },
    "required": [
      "datasetId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://salesforce.local/v2/language/datasets/{datasetId}/examples{?offset,count,source}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**object**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
