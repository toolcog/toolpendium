{
  "name": "getTrainedModelMetrics",
  "description": "Returns the metrics for a model",
  "parameters": {
    "type": "object",
    "properties": {
      "modelId": {
        "type": "string",
        "description": "Model Id"
      }
    },
    "required": [
      "modelId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://salesforce.local/v2/language/models/{modelId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**algorithm**",
            "**createdAt**: Date and time that the model was created.",
            "**id**: Model Id",
            "**language**",
            "**metricsData**: Model metrics values.",
            "**object**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
