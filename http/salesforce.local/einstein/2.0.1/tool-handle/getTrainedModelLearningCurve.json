{
  "name": "getTrainedModelLearningCurve",
  "description": "Returns the metrics for each epoch in a model.",
  "parameters": {
    "type": "object",
    "properties": {
      "modelId": {
        "type": "string",
        "description": "Model Id"
      },
      "offset": {
        "default": "0",
        "type": "string",
        "description": "Index of the epoch from which you want to start paging"
      },
      "count": {
        "default": "25",
        "type": "string",
        "description": "Number of epoch to return. Maximum valid value is 25."
      }
    },
    "required": [
      "modelId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://salesforce.local/v2/language/models/{modelId}/lc{?offset,count}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**object**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
