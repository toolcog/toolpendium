{
  "name": "WorkTypeApi_Search",
  "description": "Return all work types for the account that match the query param",
  "parameters": {
    "type": "object",
    "properties": {
      "queryOptions.query": {
        "type": "string"
      },
      "queryOptions.orderBy": {
        "type": "string"
      },
      "queryOptions.order": {
        "enum": [
          "None",
          "Asc",
          "Desc"
        ],
        "type": "string"
      },
      "queryOptions.page": {
        "format": "int32",
        "type": "integer"
      },
      "queryOptions.pageSize": {
        "format": "int32",
        "type": "integer"
      },
      "x-auth-key": {
        "default": "[authentication key]",
        "type": "string",
        "description": ""
      },
      "x-auth-secret": {
        "default": "[authentication secret]",
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "x-auth-key",
      "x-auth-secret"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.envoice.in/api/worktype/search{?queryOptions.query,queryOptions.orderBy,queryOptions.order,queryOptions.page,queryOptions.pageSize}"
    },
    "headers": {
      "x-auth-key": {
        "$": "x-auth-key"
      },
      "x-auth-secret": {
        "$": "x-auth-secret"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**CreatedOn**: Date of creation",
            "**Id**: Entity id",
            "**Title**: Indicates the title of of the work type (Logo design, development...)"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
