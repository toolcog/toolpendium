{
  "name": "InvoiceApi_Uri",
  "description": "Return the unique url to the client's invoice",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int32",
        "type": "integer",
        "description": ""
      },
      "x-auth-key": {
        "default": "[authentication key]",
        "type": "string",
        "description": ""
      },
      "x-auth-secret": {
        "default": "[authentication secret]",
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "id",
      "x-auth-key",
      "x-auth-secret"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.envoice.in/api/invoice/uri{?id}"
    },
    "headers": {
      "x-auth-key": {
        "$": "x-auth-key"
      },
      "x-auth-secret": {
        "$": "x-auth-secret"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Link**: Unique link to the invoice"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
