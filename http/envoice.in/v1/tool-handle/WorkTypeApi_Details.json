{
  "name": "WorkTypeApi_Details",
  "description": "Return work type details",
  "parameters": {
    "type": "object",
    "properties": {
      "workTypeId": {
        "format": "int32",
        "type": "integer"
      },
      "x-auth-key": {
        "default": "[authentication key]",
        "type": "string",
        "description": ""
      },
      "x-auth-secret": {
        "default": "[authentication secret]",
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "workTypeId",
      "x-auth-key",
      "x-auth-secret"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.envoice.in/api/worktype/details{?workTypeId}"
    },
    "headers": {
      "x-auth-key": {
        "$": "x-auth-key"
      },
      "x-auth-secret": {
        "$": "x-auth-secret"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**CreatedOn**: Date of creation",
            "**Id**: Entity id",
            "**Title**: Indicates the title of of the work type (Logo design, development...)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
