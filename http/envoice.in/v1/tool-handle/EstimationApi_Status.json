{
  "name": "EstimationApi_Status",
  "description": "Retrieve the status of the estimation",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int32",
        "type": "integer"
      },
      "x-auth-key": {
        "default": "[authentication key]",
        "type": "string",
        "description": ""
      },
      "x-auth-secret": {
        "default": "[authentication secret]",
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "id",
      "x-auth-key",
      "x-auth-secret"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.envoice.in/api/estimation/status{?id}"
    },
    "headers": {
      "x-auth-key": {
        "$": "x-auth-key"
      },
      "x-auth-secret": {
        "$": "x-auth-secret"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
