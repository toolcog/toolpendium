{
  "name": "PaymentLinkApi_All",
  "description": "Create a payment link",
  "parameters": {
    "type": "object",
    "properties": {
      "queryOptions.page": {
        "format": "int32",
        "type": "integer"
      },
      "queryOptions.pageSize": {
        "format": "int32",
        "type": "integer"
      },
      "x-auth-key": {
        "default": "[authentication key]",
        "type": "string",
        "description": ""
      },
      "x-auth-secret": {
        "default": "[authentication secret]",
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "x-auth-key",
      "x-auth-secret"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.envoice.in/api/paymentlink/all{?queryOptions.page,queryOptions.pageSize}"
    },
    "headers": {
      "x-auth-key": {
        "$": "x-auth-key"
      },
      "x-auth-secret": {
        "$": "x-auth-secret"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Count**",
            "**ErrorMessages**",
            "**IsFaulted**",
            "**Result**",
            "**TotalCount**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
