{
  "name": "TaxApi_Update",
  "description": "Update an existing tax",
  "parameters": {
    "type": "object",
    "properties": {
      "x-auth-key": {
        "default": "[authentication key]",
        "type": "string",
        "description": ""
      },
      "x-auth-secret": {
        "default": "[authentication secret]",
        "type": "string",
        "description": ""
      },
      "body": {
        "$ref": "#/$defs/TaxUpdateApiModel"
      }
    },
    "required": [
      "x-auth-key",
      "x-auth-secret",
      "body"
    ],
    "$defs": {
      "TaxUpdateApiModel": {
        "properties": {
          "Id": {
            "description": "Entity id",
            "format": "int32",
            "type": "integer"
          },
          "Name": {
            "description": "Name of the task",
            "type": "string"
          },
          "Percentage": {
            "description": "Task percentage. Ex: 18%",
            "format": "double",
            "type": "number"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://www.envoice.in/api/tax/update"
    },
    "headers": {
      "x-auth-key": {
        "$": "x-auth-key"
      },
      "x-auth-secret": {
        "$": "x-auth-secret"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
