{
  "name": "GeneralApi_DateFormats",
  "description": "Return all of the platform supported Date Formats",
  "parameters": {
    "type": "object",
    "properties": {
      "x-auth-key": {
        "default": "[authentication key]",
        "type": "string",
        "description": ""
      },
      "x-auth-secret": {
        "default": "[authentication secret]",
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "x-auth-key",
      "x-auth-secret"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.envoice.in/api/general/dateformats"
    },
    "headers": {
      "x-auth-key": {
        "$": "x-auth-key"
      },
      "x-auth-secret": {
        "$": "x-auth-secret"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
