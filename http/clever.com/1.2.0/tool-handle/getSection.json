{
  "name": "getSection",
  "description": "Returns a specific section",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.clever.com/v1.2/sections/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**course_description**",
                  "**course_name**",
                  "**course_number**",
                  "**created**",
                  "**district**",
                  "**grade**",
                  "**id**",
                  "**last_modified**",
                  "**name**",
                  "**period**",
                  "**school**",
                  "**section_number**",
                  "**sis_id**",
                  "**students**",
                  "**subject**",
                  "**teacher**",
                  "**teachers**",
                  [
                    "**term**",
                    {
                      "$ul": [
                        "**end_date**",
                        "**name**",
                        "**start_date**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
