{
  "name": "getPopularCars",
  "description": "Get make model wise top 50 popular cars on national, state, city level",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "The API Authentication Key. Mandatory with all API calls."
      },
      "state": {
        "format": "string",
        "type": "string",
        "description": "State level sales count"
      },
      "city_state": {
        "format": "string",
        "type": "string",
        "description": "City level sales count, pipe seperated like city_state=jacksonville|FL"
      },
      "car_type": {
        "enum": [
          "new",
          "used"
        ],
        "format": "string",
        "type": "string",
        "description": "Inventory type for which popular count is to be searched"
      },
      "country": {
        "default": "us",
        "enum": [
          "us",
          "ca",
          "US",
          "CA"
        ],
        "type": "string",
        "description": "Country for which the popular cars are to be searched"
      }
    },
    "required": [
      "car_type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://marketcheck-prod.apigee.net/v2/popular/cars{?api_key,state,city_state,car_type,country}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Top 50 popular cars",
        "**Key properties:**",
        {
          "$ul": [
            "**new_top50**: Array of all new top 50 cars",
            "**used_top50**: Array of all used top 50 cars"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**message**: Error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
