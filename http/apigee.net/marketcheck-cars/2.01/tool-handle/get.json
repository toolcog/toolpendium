{
  "name": "get",
  "description": "get client filters",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "The API Authentication Key. Mandatory with all API calls."
      },
      "country": {
        "enum": [
          "US",
          "CA",
          "UK"
        ],
        "type": "string",
        "description": "To filter listing on Country in which they are listed"
      }
    },
    "required": [
      "country"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://marketcheck-prod.apigee.net/v2/client/configure/get{?api_key,country}"
    }
  },
  "responses": {}
}
