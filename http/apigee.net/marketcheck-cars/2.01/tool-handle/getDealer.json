{
  "name": "getDealer",
  "description": "Get a particular dealer's information by its id",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "The API Authentication Key. Mandatory with all API calls."
      },
      "id": {
        "format": "string",
        "type": "string",
        "description": "Dealer id to get all the dealer info attributes"
      },
      "provider": {
        "default": false,
        "type": "boolean",
        "description": "boolean param to include site providers name in response"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://marketcheck-prod.apigee.net/v2/dealer/car/{id}{?api_key,provider}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**city**: City of the dealer",
            "**country**: country of the dealer",
            "**data_source**: Datasource of the dealer",
            "**dealer_type**: Type of dealer (franchise/independent)",
            "**dealership_group_name**: Dealership group name of dealer",
            "**distance**: Distance of dealer from given location",
            "**id**: The unique id associated with the dealer in the Marketcheck database",
            "**inventory_url**: Website of the dealer",
            "**latitude**: Latutide for the dealer location",
            "**listing_count**: Listing count of the dealer",
            "**location_ll**: location_ll for the dealer location",
            "**longitude**: Longitude for the dealer location",
            "**seller_email**: Contact email of the dealer",
            "**seller_name**: Name of the dealer",
            "**seller_phone**: Contact no of the dealer",
            "**state**: State of the dealer",
            "**status**: Status of the dealer",
            "**street**: Street of the dealer",
            "**zip**: Zip of the dealer"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**message**: Error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
