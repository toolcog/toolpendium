{
  "name": "rankCar",
  "description": "Computer rank for car listings based on inputs provided.Weights for ranking parameters can also be provided.",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "The API Authentication Key. Mandatory with all API calls."
      },
      "append_api_key": {
        "default": true,
        "type": "boolean",
        "description": "Flag on whether to include api_key in response API urls (if any)"
      },
      "body": {
        "$ref": "#/components/schemas/CarRankRequest",
        "description": "Inputs needed for ranking a group of car listings"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://marketcheck-prod.apigee.net/v2/search/car/active/rank/listings{?api_key,append_api_key}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Ranking query response",
        "**Key properties:**",
        {
          "$ul": [
            "**num_ranked**: The number of listings ranked",
            "**ranked_listings**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**message**: Error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
