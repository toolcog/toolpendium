{
  "name": "predictCarPrice",
  "description": "Predict car price based on it's specifications",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "The API Authentication Key. Mandatory with all API calls."
      },
      "vin": {
        "type": "string",
        "description": "Predict price for a VIN"
      },
      "car_type": {
        "enum": [
          "used",
          "new"
        ],
        "type": "string",
        "description": "Car condition"
      },
      "year": {
        "type": "integer",
        "description": "Car manufacturing year"
      },
      "make": {
        "type": "string",
        "description": "Car's make"
      },
      "model": {
        "type": "string",
        "description": "Car's model"
      },
      "trim": {
        "type": "string",
        "description": "Car's trim"
      },
      "is_certified": {
        "type": "boolean",
        "description": "Boolean to indicate car is certified or not"
      },
      "carfax_1_owner": {
        "type": "boolean",
        "description": "Boolean to indicate car is carfax one owner or not"
      },
      "carfax_clean_title": {
        "type": "boolean",
        "description": "Boolean to indicate car has clean title or not"
      },
      "base_exterior_color": {
        "type": "string",
        "description": "Base exterior color of the car"
      },
      "base_interior_color": {
        "type": "string",
        "description": "Base interior color of the car"
      },
      "transmission": {
        "enum": [
          "Manual",
          "Automatic"
        ],
        "type": "string",
        "description": "Transmission on the car"
      },
      "drivetrain": {
        "enum": [
          "4WD",
          "AWD",
          "FWD",
          "RWD"
        ],
        "type": "string",
        "description": "Drivetrain on the car"
      },
      "engine_size": {
        "type": "number",
        "description": "Engine Size of the car"
      },
      "engine_block": {
        "enum": [
          "I",
          "V",
          "H"
        ],
        "type": "string",
        "description": "Engine Block of the car"
      },
      "cylinders": {
        "type": "integer",
        "description": "Number of cylinders in the vehicle"
      },
      "doors": {
        "type": "integer",
        "description": "Number of doors in the vehicle"
      },
      "highway_mpg": {
        "type": "integer",
        "description": "Highway mileage"
      },
      "city_mpg": {
        "type": "integer",
        "description": "City mileage of the car"
      },
      "latitude": {
        "type": "number",
        "description": "Latitude component of the location"
      },
      "longitude": {
        "type": "number",
        "description": "Longitude component of the location"
      },
      "miles": {
        "type": "integer",
        "description": "miles vehicle has driven in total"
      },
      "zip": {
        "type": "string",
        "description": "Location zip"
      },
      "country": {
        "default": "us",
        "enum": [
          "us",
          "ca"
        ],
        "type": "string",
        "description": "Country for which car price will be predicted"
      }
    },
    "required": [
      "car_type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://marketcheck-prod.apigee.net/v2/predict/car/price{?api_key,vin,car_type,year,make,model,trim,is_certified,carfax_1_owner,carfax_clean_title,base_exterior_color,base_interior_color,transmission,drivetrain,engine_size,engine_block,cylinders,doors,highway_mpg,city_mpg,latitude,longitude,miles,zip,country}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Predicted price of a car",
        "**Key properties:**",
        {
          "$ul": [
            "**predicted_price**: Actual predicted price of a car",
            [
              "**price_range**: Price range around the predicted price, estimating the error in predicted price",
              {
                "$ul": [
                  "**lower_bound**: Lower bound",
                  "**upper_bound**: Upper bound"
                ]
              }
            ],
            [
              "**specs**: Specs of predicted vehicle",
              {
                "$ul": [
                  "**base_exterior_color**: Base exterior color of vehicle",
                  "**base_interior_color**: base interior color of vehicle",
                  "**carfax_1_owner**: Indicates whether car has had only one owner or not",
                  "**carfax_clean_title**: Indicates whether car has clean ownership records",
                  "**city_mpg**: City mileage",
                  "**cylinders**: Number of cylinders",
                  "**doors**: Number of doors",
                  "**drivetrain**: Drivetrain of the vehicle",
                  "**engine_block**: Engine block of the vehicle",
                  "**engine_size**: Engine size",
                  "**highway_mpg**: Highway mileage",
                  "**is_certified**: Certified vehicle",
                  "**latitude**: Latutide for the vehicle location",
                  "**longitude**: Longitude for the vehicle location",
                  "**make**: make",
                  "**miles**: Miles",
                  "**model**: model",
                  "**transmission**: transmission",
                  "**trim**: trim",
                  "**year**: year"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**message**: Error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
