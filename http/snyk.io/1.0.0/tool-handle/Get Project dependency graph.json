{
  "name": "Get_Project_dependency_graph",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "orgId": {
        "type": "string",
        "description": "The organization ID. The `API_KEY` must have access to this organization."
      },
      "projectId": {
        "type": "string",
        "description": "The project ID to return issues for."
      }
    },
    "required": [
      "orgId",
      "projectId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.snyk.io/v1/org/{orgId}/project/{projectId}/dep-graph"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**depGraph**: The dependency-graph object",
              {
                "$ul": [
                  [
                    "**graph**: A directional graph of the packages in the project",
                    {
                      "$ul": [
                        "**nodes**: A list of the first-level packages",
                        "**rootNodeId**: The internal id of the root node"
                      ]
                    }
                  ],
                  [
                    "**pkgManager**: The package manager of the project",
                    {
                      "$ul": [
                        "**name**: The name of the package manager",
                        "**repositories**",
                        "**version**: The version of the package manager"
                      ]
                    }
                  ],
                  "**pkgs**: A list of dependencies in the project",
                  "**schemaVersion**: The scheme version of the depGraph object"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
