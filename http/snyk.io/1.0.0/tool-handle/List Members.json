{
  "name": "List_Members",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "orgId": {
        "type": "string",
        "description": "The organization ID."
      },
      "includeGroupAdmins": {
        "default": "false",
        "type": "boolean",
        "description": "Include group administrators who also have access to this organization."
      }
    },
    "required": [
      "orgId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.snyk.io/v1/org/{orgId}/members{?includeGroupAdmins}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
