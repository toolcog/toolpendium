{
  "name": "Retrieve_ignore",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "orgId": {
        "type": "string",
        "description": "The organization ID to modify ignores for. The `API_KEY` must have access to this organization."
      },
      "projectId": {
        "type": "string",
        "description": "The project ID to modify ignores for."
      },
      "issueId": {
        "type": "string",
        "description": "The issue ID to modify ignores for. Can be a vulnerability or a license Issue."
      }
    },
    "required": [
      "orgId",
      "projectId",
      "issueId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.snyk.io/v1/org/{orgId}/project/{projectId}/ignore/{issueId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
