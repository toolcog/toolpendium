{
  "name": "Provision_a_user_to_the_organization",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "orgId": {
        "type": "string",
        "description": "The organization ID. The `API_KEY` must not exceed the permissions being granted to the provisioned user."
      },
      "body": {
        "example": {
          "email": "",
          "role": "",
          "rolePublicId": ""
        },
        "properties": {
          "email": {
            "description": "The email of the user.",
            "type": "string"
          },
          "role": {
            "description": "Deprecated. Name of the role to grant this user. Must be one of `ADMIN`, `COLLABORATOR`, or `RESTRICTED_COLLABORATOR`. This field is invalid if `rolePublicId` is supplied with the request.",
            "type": "string"
          },
          "rolePublicId": {
            "description": "ID of the role to grant this user.",
            "type": "string"
          }
        },
        "required": [
          "email"
        ],
        "type": "object"
      }
    },
    "required": [
      "orgId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.snyk.io/v1/org/{orgId}/provision"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**: Timestamp of when this provision record was created.",
            "**email**: The email of the user.",
            "**role**: Name of the role granted for this user.",
            "**rolePublicId**: ID of the role to granted for this user."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
