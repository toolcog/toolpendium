{
  "name": "getPositionsForOrganization",
  "description": "Get billing positions for a given organization",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string",
        "description": "The organization to compute the billing information for"
      },
      "fromDate": {
        "format": "date-time",
        "type": "string",
        "description": "Billing start date"
      },
      "toDate": {
        "format": "date-time",
        "type": "string",
        "description": "Billing end date"
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//backend.id4i.de/api/v1/billing/{organizationId}/positions{?fromDate,toDate}"
    }
  },
