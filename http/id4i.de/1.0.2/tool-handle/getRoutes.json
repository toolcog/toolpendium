{
  "name": "getRoutes",
  "description": "Retrieve all public routes for a GUID",
  "parameters": {
    "type": "object",
    "properties": {
      "id4n": {
        "type": "string",
        "description": "id4n"
      },
      "type": {
        "default": "web",
        "type": "string",
        "description": "type"
      },
      "interpolate": {
        "default": true,
        "type": "boolean",
        "description": "interpolate"
      }
    },
    "required": [
      "id4n",
      "type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//backend.id4i.de/api/v1/public/routes/{id4n}{?type,interpolate}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Route list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**params**",
            "**priority**",
            "**public**",
            "**type**",
            "**validUntil**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ApiError"
        },
        "**Key properties:**",
        {
          "$ul": [
