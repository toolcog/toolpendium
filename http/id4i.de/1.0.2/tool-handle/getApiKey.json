{
  "name": "getApiKey",
  "description": "Showing the details of an API key.",
  "parameters": {
    "type": "object",
    "properties": {
      "key": {
        "type": "string",
        "description": "The API key to show."
      }
    },
    "required": [
      "key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//backend.id4i.de/api/v1/apikeys/{key}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ApiKeyPresentation"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**: Whether this API key is active",
            "**createdAt**: The UTC unix timestamp of when this api key has been created",
            "**createdBy**",
            "**key**: The api key identifier",
            "**label**: The label / name of the api key",
            "**organizationId**: The organization namespace this api key belongs to"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
