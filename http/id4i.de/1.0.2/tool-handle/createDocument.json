{
  "name": "createDocument",
  "description": "The documents' mime type is suggested on octet-stream data. Otherwise the specified content mime type is used.",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "type": "string",
        "description": "organizationId"
      },
      "id4n": {
