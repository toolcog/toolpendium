{
  "name": "extensionConfiguration",
  "description": "Configuration Resource",
  "parameters": {
    "type": "object",
    "properties": {
      "x-applecloudextension-session-id": {
        "maxLength": 128,
        "minLength": 1,
        "type": "string"
      },
      "x-applecloudextension-retry-count": {
        "format": "uint32",
        "minimum": 1,
        "type": "number"
      },
      "Request-Timeout": {
        "format": "uint32",
        "minimum": 1,
        "type": "number"
      },
      "User-Agent": {
        "maxLength": 250,
        "pattern": "AppleCloudExtension/([0-9]+\\.[0-9]+\\.[0-9]+) *.*",
        "type": "string"
      },
      "Accept-Language": {
        "type": "string"
      },
      "If-None-Match": {
        "$ref": "#/$defs/ExtensionConfigTag"
      },
      "Cache-Control": {
        "type": "string"
      }
    },
    "required": [
      "x-applecloudextension-session-id",
      "Request-Timeout",
      "User-Agent",
      "Accept-Language",
      "Cache-Control"
    ],
    "$defs": {
      "ExtensionConfigTag": {
        "maxLength": 1002,
        "pattern": "[\"][ -~]{1000}[\"]",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://cloudextension-testservice.local/api/configuration"
    },
    "headers": {
      "x-applecloudextension-session-id": {
        "$": "x-applecloudextension-session-id"
      },
      "x-applecloudextension-retry-count": {
        "$": "x-applecloudextension-retry-count"
      },
      "Request-Timeout": {
        "$": "Request-Timeout"
      },
      "User-Agent": {
        "$": "User-Agent"
      },
      "Accept-Language": {
        "$": "Accept-Language"
      },
      "If-None-Match": {
        "$": "If-None-Match"
      },
      "Cache-Control": {
        "$": "Cache-Control"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**hdr**",
            [
              "**intent**",
              {
                "$ul": [
                  [
                    "**addMedia**",
                    {
                      "$ul": [
                        "**hdr**",
                        "**url**"
                      ]
                    }
                  ],
                  "**hdr**",
                  [
                    "**playMedia**",
                    {
                      "$ul": [
                        "**hdr**",
                        "**url**"
                      ]
                    }
                  ],
                  [
                    "**updateMediaAffinity**",
                    {
                      "$ul": [
                        "**hdr**",
                        "**url**"
                      ]
                    }
                  ]
                ]
              }
            ],
            [
              "**media**",
              {
                "$ul": [
                  [
                    "**queues**",
                    {
                      "$ul": [
                        "**hdr**",
                        [
                          "**playMedia**",
                          {
                            "$ul": [
                              "**hdr**",
                              "**url** (default: /queues/playMedia)"
                            ]
                          }
                        ],
                        [
                          "**updateActivity**",
                          {
                            "$ul": [
                              "**hdr**",
                              "**url** (default: /queues/updateActivity)"
                            ]
                          }
                        ]
                      ]
                    }
                  ]
                ]
              }
            ],
            "**url**",
            "**version**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
