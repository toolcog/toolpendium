{
  "name": "appCategories-get_collection",
  "parameters": {
    "type": "object",
    "properties": {
      "filter[platforms]": {
        "items": {
          "enum": [
            "IOS",
            "MAC_OS",
            "TV_OS"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'platforms'"
      },
      "exists[parent]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by existence or non-existence of related 'parent'"
      },
      "fields[appCategories]": {
        "items": {
          "enum": [
            "parent",
            "platforms",
            "subcategories"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "the fields to include for returned resources of type appCategories"
      },
      "limit": {
        "maximum": 200,
        "type": "integer",
        "description": "maximum resources per page"
      },
      "include": {
        "items": {
          "enum": [
            "parent",
            "subcategories"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "comma-separated list of relationships to include"
      },
      "limit[subcategories]": {
        "maximum": 50,
        "type": "integer",
        "description": "maximum number of related subcategories returned (when they are included)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appstoreconnect.apple.com//v1/appCategories{?filter[platforms],exists[parent],fields[appCategories],limit,include,limit[subcategories]}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "AppCategoriesResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**included**",
            [
              "**links**",
              {
                "$ul": [
                  "**first**",
                  "**next**",
                  "**self**"
                ]
              }
            ],
            [
              "**meta**",
              {
                "$ul": [
                  [
                    "**paging**",
                    {
                      "$ul": [
                        "**limit**",
                        "**total**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
