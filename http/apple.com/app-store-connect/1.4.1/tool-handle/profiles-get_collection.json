{
  "name": "profiles-get_collection",
  "parameters": {
    "type": "object",
    "properties": {
      "filter[name]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'name'"
      },
      "filter[profileState]": {
        "items": {
          "enum": [
            "ACTIVE",
            "INVALID"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'profileState'"
      },
      "filter[profileType]": {
        "items": {
          "enum": [
            "IOS_APP_DEVELOPMENT",
            "IOS_APP_STORE",
            "IOS_APP_ADHOC",
            "IOS_APP_INHOUSE",
            "MAC_APP_DEVELOPMENT",
            "MAC_APP_STORE",
            "MAC_APP_DIRECT",
            "TVOS_APP_DEVELOPMENT",
            "TVOS_APP_STORE",
            "TVOS_APP_ADHOC",
            "TVOS_APP_INHOUSE",
            "MAC_CATALYST_APP_DEVELOPMENT",
            "MAC_CATALYST_APP_STORE",
            "MAC_CATALYST_APP_DIRECT"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'profileType'"
      },
      "filter[id]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by id(s)"
      },
      "sort": {
        "items": {
          "enum": [
            "id",
            "-id",
            "name",
            "-name",
            "profileState",
            "-profileState",
            "profileType",
            "-profileType"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "comma-separated list of sort expressions; resources will be sorted as specified"
      },
      "fields[profiles]": {
        "items": {
          "enum": [
            "bundleId",
            "certificates",
            "createdDate",
            "devices",
            "expirationDate",
            "name",
            "platform",
            "profileContent",
            "profileState",
            "profileType",
            "uuid"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "the fields to include for returned resources of type profiles"
      },
      "limit": {
        "maximum": 200,
        "type": "integer",
        "description": "maximum resources per page"
      },
      "include": {
        "items": {
          "enum": [
            "bundleId",
            "certificates",
            "devices"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "comma-separated list of relationships to include"
      },
      "fields[certificates]": {
        "items": {
          "enum": [
            "certificateContent",
            "certificateType",
            "csrContent",
            "displayName",
            "expirationDate",
            "name",
            "platform",
            "serialNumber"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "the fields to include for returned resources of type certificates"
      },
      "fields[devices]": {
        "items": {
          "enum": [
            "addedDate",
            "deviceClass",
            "model",
            "name",
            "platform",
            "status",
            "udid"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "the fields to include for returned resources of type devices"
      },
      "fields[bundleIds]": {
        "items": {
          "enum": [
            "app",
            "bundleIdCapabilities",
            "identifier",
            "name",
            "platform",
            "profiles",
            "seedId"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "the fields to include for returned resources of type bundleIds"
      },
      "limit[certificates]": {
        "maximum": 50,
        "type": "integer",
        "description": "maximum number of related certificates returned (when they are included)"
      },
      "limit[devices]": {
        "maximum": 50,
        "type": "integer",
        "description": "maximum number of related devices returned (when they are included)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appstoreconnect.apple.com//v1/profiles{?filter[name],filter[profileState],filter[profileType],filter[id],sort,fields[profiles],limit,include,fields[certificates],fields[devices],fields[bundleIds],limit[certificates],limit[devices]}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ProfilesResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**included**",
            [
              "**links**",
              {
                "$ul": [
                  "**first**",
                  "**next**",
                  "**self**"
                ]
              }
            ],
            [
              "**meta**",
              {
                "$ul": [
                  [
                    "**paging**",
                    {
                      "$ul": [
                        "**limit**",
                        "**total**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
