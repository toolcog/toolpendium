{
  "name": "devices-get_collection",
  "parameters": {
    "type": "object",
    "properties": {
      "filter[name]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'name'"
      },
      "filter[platform]": {
        "items": {
          "enum": [
            "IOS",
            "MAC_OS"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'platform'"
      },
      "filter[status]": {
        "items": {
          "enum": [
            "ENABLED",
            "DISABLED"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'status'"
      },
      "filter[udid]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'udid'"
      },
      "filter[id]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by id(s)"
      },
      "sort": {
        "items": {
          "enum": [
            "id",
            "-id",
            "name",
            "-name",
            "platform",
            "-platform",
            "status",
            "-status",
            "udid",
            "-udid"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "comma-separated list of sort expressions; resources will be sorted as specified"
      },
      "fields[devices]": {
        "items": {
          "enum": [
            "addedDate",
            "deviceClass",
            "model",
            "name",
            "platform",
            "status",
            "udid"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "the fields to include for returned resources of type devices"
      },
      "limit": {
        "maximum": 200,
        "type": "integer",
        "description": "maximum resources per page"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appstoreconnect.apple.com//v1/devices{?filter[name],filter[platform],filter[status],filter[udid],filter[id],sort,fields[devices],limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "DevicesResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**links**",
              {
                "$ul": [
                  "**first**",
                  "**next**",
                  "**self**"
                ]
              }
            ],
            [
              "**meta**",
              {
                "$ul": [
                  [
                    "**paging**",
                    {
                      "$ul": [
                        "**limit**",
                        "**total**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
