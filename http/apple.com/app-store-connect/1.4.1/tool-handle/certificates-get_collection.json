{
  "name": "certificates-get_collection",
  "parameters": {
    "type": "object",
    "properties": {
      "filter[certificateType]": {
        "items": {
          "enum": [
            "IOS_DEVELOPMENT",
            "IOS_DISTRIBUTION",
            "MAC_APP_DISTRIBUTION",
            "MAC_INSTALLER_DISTRIBUTION",
            "MAC_APP_DEVELOPMENT",
            "DEVELOPER_ID_KEXT",
            "DEVELOPER_ID_APPLICATION",
            "DEVELOPMENT",
            "DISTRIBUTION"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'certificateType'"
      },
      "filter[displayName]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'displayName'"
      },
      "filter[serialNumber]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by attribute 'serialNumber'"
      },
      "filter[id]": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "filter by id(s)"
      },
      "sort": {
        "items": {
          "enum": [
            "certificateType",
            "-certificateType",
            "displayName",
            "-displayName",
            "id",
            "-id",
            "serialNumber",
            "-serialNumber"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "comma-separated list of sort expressions; resources will be sorted as specified"
      },
      "fields[certificates]": {
        "items": {
          "enum": [
            "certificateContent",
            "certificateType",
            "csrContent",
            "displayName",
            "expirationDate",
            "name",
            "platform",
            "serialNumber"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "the fields to include for returned resources of type certificates"
      },
      "limit": {
        "maximum": 200,
        "type": "integer",
        "description": "maximum resources per page"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appstoreconnect.apple.com//v1/certificates{?filter[certificateType],filter[displayName],filter[serialNumber],filter[id],sort,fields[certificates],limit}"
    }
  },
