{
  "name": "cities_installers_list",
  "description": "returns a list of cities with a qualified, registered operator (driller or installer)",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://apps.nrs.gov.bc.ca/gwells/api/v1//cities/installers/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**organization**",
              {
                "$ul": [
                  "**city**",
                  "**email**",
                  "**fax_tel**",
                  "**main_tel**",
                  "**name**",
                  "**org_guid**",
                  "**org_verbose_name**",
                  "**postal_code**",
                  "**province_state**",
                  "**street_address**",
                  "**website_url**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
