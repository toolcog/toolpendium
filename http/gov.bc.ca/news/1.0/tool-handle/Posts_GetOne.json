{
  "name": "Posts_GetOne",
  "description": "Get the post associated with the key",
  "parameters": {
    "type": "object",
    "properties": {
      "key": {
        "type": "string",
        "description": ""
      },
      "api-version": {
        "default": "1.0",
        "type": "string",
        "description": "The requested API version"
      }
    },
    "required": [
      "key",
      "api-version"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://news.api.gov.bc.ca//api/Posts/{key}{?api-version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**key**",
            "**timestamp**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
