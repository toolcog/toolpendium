{
  "name": "Posts_Get",
  "description": "Get the posts associated with the keys in the list passed in.",
  "parameters": {
    "type": "object",
    "properties": {
      "postKeys": {
        "default": "",
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": ""
      },
      "api-version": {
        "default": "1.0",
        "type": "string",
        "description": "The requested API version"
      }
    },
    "required": [
      "api-version"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://news.api.gov.bc.ca//api/Posts{?postKeys,api-version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**key**",
            "**timestamp**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
