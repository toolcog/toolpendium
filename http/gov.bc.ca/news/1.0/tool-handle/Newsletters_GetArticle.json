{
  "name": "Newsletters_GetArticle",
  "description": "Get an article belonging to a Newsletter edition",
  "parameters": {
    "type": "object",
    "properties": {
      "newsletterKey": {
        "type": "string",
        "description": ""
      },
      "editionKey": {
        "type": "string",
        "description": ""
      },
      "articleKey": {
        "type": "string",
        "description": ""
      },
      "api-version": {
        "default": "1.0",
        "type": "string",
        "description": "The requested API version"
      }
    },
    "required": [
      "newsletterKey",
      "editionKey",
      "articleKey",
      "api-version"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://news.api.gov.bc.ca//api/Newsletters/{newsletterKey}/Editions/{editionKey}/Articles/{articleKey}{?api-version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**key**",
            "**timestamp**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
