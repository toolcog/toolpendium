{
  "name": "updateManagedCredentialUsernamePassword",
  "description": "Updates the username and password for a Managed Credential.\n\nThis command can only be accessed by the unrestricted users of an account.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "credentialId": {
        "type": "integer",
        "description": "The ID of the Credential to update."
      },
      "body": {
        "properties": {
          "password": {
            "description": "The password to use when accessing the Managed Service.\n",
            "example": "s3cur3P@ssw0rd",
            "type": "string"
          },
          "username": {
            "description": "The username to use when accessing the Managed Service.\n",
            "example": "johndoe",
            "maxLength": 5000,
            "minLength": 0,
            "type": "string"
          }
        },
        "required": [
          "password"
        ],
        "description": "The new username and password to assign to the Managed Credential.\n"
      }
    },
    "required": [
      "credentialId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.linode.com/v4/managed/credentials/{credentialId}/update"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
