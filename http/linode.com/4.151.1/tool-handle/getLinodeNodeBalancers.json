{
  "name": "getLinodeNodeBalancers",
  "description": "Returns a list of NodeBalancers that are assigned to this Linode and readable by the requesting User.\n\nRead permission to a NodeBalancer can be given to a User by accessing the User's Grants Update\n([PUT /account/users/{username}/grants](/docs/api/account/#users-grants-update)) endpoint.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "linodeId": {
        "type": "integer",
        "description": "ID of the Linode to look up"
      }
    },
    "required": [
      "linodeId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.linode.com/v4/linode/instances/{linodeId}/nodebalancers"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**page**: The current [page](/docs/api/#pagination).",
            "**pages**: The total number of [pages](/docs/api/#pagination).",
            "**results**: The total number of results."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
