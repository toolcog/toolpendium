{
  "name": "deleteNodeBalancerConfigNode",
  "description": "Deletes a Node from this Config. This backend will no longer receive traffic for the configured port of this NodeBalancer.\n\nThis does not change or remove the Linode whose address was used in the creation of this Node.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "nodeBalancerId": {
        "type": "integer",
        "description": "The ID of the NodeBalancer to access."
      },
      "configId": {
        "type": "integer",
        "description": "The ID of the Config to access"
      },
      "nodeId": {
        "type": "integer",
        "description": "The ID of the Node to access"
      }
    },
    "required": [
      "nodeBalancerId",
      "configId",
      "nodeId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.linode.com/v4/nodebalancers/{nodeBalancerId}/configs/{configId}/nodes/{nodeId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
