{
  "name": "getStackScripts",
  "description": "If the request is not authenticated, only public StackScripts are returned.\n\nFor more information on StackScripts, please read our [StackScripts documentation](/docs/products/tools/stackscripts/).\n",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "The page of a collection to return."
      },
      "page_size": {
        "default": 100,
        "maximum": 500,
        "minimum": 25,
        "type": "integer",
        "description": "The number of items to return per page."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.linode.com/v4/linode/stackscripts{?page,page_size}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**page**: The current [page](/docs/api/#pagination).",
            "**pages**: The total number of [pages](/docs/api/#pagination).",
            "**results**: The total number of results."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
