{
  "name": "deleteDatabaseMongoDBInstanceBackup",
  "description": "Delete a single backup for an accessible Managed MongoDB Database.\n\nRequires `read_write` access to the Database.\n\nThe Database must not be provisioning to perform this command.\n\n**Note**: New MongoDB Databases cannot currently be created.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "instanceId": {
        "type": "integer",
        "description": "The ID of the Managed MongoDB Database."
      },
      "backupId": {
        "type": "integer",
        "description": "The ID of the Managed MongoDB Database backup."
      }
    },
    "required": [
      "instanceId",
      "backupId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.linode.com/v4/databases/mongodb/instances/{instanceId}/backups/{backupId}"
    }
  },
  "responses": {
