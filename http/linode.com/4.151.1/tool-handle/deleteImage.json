{
  "name": "deleteImage",
  "description": "Deletes a private Image you have permission to `read_write`.\n\n\n**Deleting an Image is a destructive action and cannot be undone.**\n",
  "parameters": {
    "type": "object",
    "properties": {
      "imageId": {
        "type": "string",
        "description": "ID of the Image to look up."
      }
    },
    "required": [
      "imageId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.linode.com/v4/images/{imageId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
