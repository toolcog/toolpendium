{
  "name": "getObjectStorageSSL",
  "description": "Returns a boolean value indicating if this bucket has a corresponding TLS/SSL certificate that was\nuploaded by an Account user.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "clusterId": {
        "type": "string",
        "description": "The ID of the cluster this bucket exists in."
      },
      "bucket": {
        "type": "string",
        "description": "The bucket name."
      }
    },
    "required": [
      "clusterId",
      "bucket"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.linode.com/v4/object-storage/buckets/{clusterId}/{bucket}/ssl"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "If this Object Storage bucket has a corresponding TLS/SSL Certificate.\n",
        "**Key properties:**",
        {
          "$ul": [
            "**ssl**: A boolean indicating if this Bucket has a corresponding TLS/SSL certificate that was uploaded by an Account user."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
