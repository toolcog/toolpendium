{
  "name": "getFirewallDevices",
  "description": "Returns a paginated list of a Firewall's Devices. A Firewall Device assigns a\nFirewall to a Linode service (referred to as the Device's `entity`). Currently,\nonly Devices with an entity of type `linode` are accepted.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "The page of a collection to return."
      },
      "page_size": {
        "default": 100,
        "maximum": 500,
        "minimum": 25,
        "type": "integer",
        "description": "The number of items to return per page."
      },
      "firewallId": {
        "type": "integer",
        "description": "ID of the Firewall to access.\n"
      }
    },
    "required": [
      "firewallId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.linode.com/v4/networking/firewalls/{firewallId}/devices{?page,page_size}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**page**: The current [page](/docs/api/#pagination).",
            "**pages**: The total number of [pages](/docs/api/#pagination).",
            "**results**: The total number of results."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
