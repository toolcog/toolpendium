  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.linode.com/v4/account/settings/managed-enable"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
