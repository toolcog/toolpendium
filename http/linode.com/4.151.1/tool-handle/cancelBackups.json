{
  "name": "cancelBackups",
  "description": "Cancels the Backup service on the given Linode. Deletes all of this Linode's existing backups forever.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "linodeId": {
        "type": "integer",
        "description": "The ID of the Linode to cancel backup service for."
      }
    },
    "required": [
      "linodeId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.linode.com/v4/linode/instances/{linodeId}/backups/cancel"
    }
  },
