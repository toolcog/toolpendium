{
  "name": "deleteStackScript",
  "description": "Deletes a private StackScript you have permission to `read_write`. You cannot delete a public StackScript.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "stackscriptId": {
        "type": "string",
        "description": "The ID of the StackScript to look up."
      }
    },
    "required": [
      "stackscriptId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.linode.com/v4/linode/stackscripts/{stackscriptId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
