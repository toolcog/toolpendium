{
  "name": "getDatabasesMySQLInstanceCredentials",
  "description": "Display the root username and password for an accessible Managed MySQL Database.\n\nThe Database must have an `active` status to perform this command.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "instanceId": {
        "type": "integer",
        "description": "The ID of the Managed MySQL Database."
      }
    },
    "required": [
      "instanceId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.linode.com/v4/databases/mysql/instances/{instanceId}/credentials"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Managed Database object for database credentials.",
        "**Key properties:**",
        {
          "$ul": [
            "**password**: The randomly-generated root password for the Managed Database instance.",
            "**username**: The root username for the Managed Database instance."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
