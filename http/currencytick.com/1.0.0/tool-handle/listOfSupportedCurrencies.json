{
  "name": "listOfSupportedCurrencies",
  "description": "Get the list of supported currencies, currently 220 currencies are supported.",
  "parameters": {
    "type": "object",
    "properties": {
      "apikey": {
        "description": "Authentication key.",
        "example": "YOUR_API_KEY",
        "type": "string"
      }
    },
    "required": [
      "apikey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.currencytick.com/supported_currencies{?apikey}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
