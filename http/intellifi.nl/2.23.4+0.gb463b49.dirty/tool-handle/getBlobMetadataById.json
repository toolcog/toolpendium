{
  "name": "getBlobMetadataById",
  "description": "Get binary large object (blob)",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Unique identifier"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://brain.intellifi.cloud/api/blobs/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**blob_key**: Unique key to the blob",
            "**content_type**: Media type of the resource. Automatically detected when not given in a POST.",
            "**download_url**: Url to the individual resource.",
            "**filename**: Filename of the blob",
            "**hash**: Hash of the blob",
            "**id**: Unique identifier for resource.",
            "**time_created**: [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) formatted string for when this resource was created.",
            "**time_last_accessed**: [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) formatted string for when this resource was last accessed.",
            "**time_updated**: [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) formatted string for when this resource was updated.",
            "**upload_url**: Url to the individual resource.",
            "**url**: Url to the individual resource."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
