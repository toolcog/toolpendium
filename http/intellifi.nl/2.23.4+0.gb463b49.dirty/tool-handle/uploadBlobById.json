{
  "name": "uploadBlobById",
  "description": "Create binary large object (blob)",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Unique identifier"
      },
      "body": {
        "properties": {
          "file": {
            "format": "binary",
            "type": "string"
          }
        }
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://brain.intellifi.cloud/api/blobs/{id}/upload"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {}
}
