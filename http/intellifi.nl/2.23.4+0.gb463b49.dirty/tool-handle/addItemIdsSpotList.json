{
  "name": "addItemIdsSpotList",
  "description": "Add spots to an existing list",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Unique identifier"
      },
      "body": {
        "$ref": "#/components/schemas/ListOfItemIds",
        "description": "A JSON array containing spot ids"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://brain.intellifi.cloud/api/sets/spotlists/{id}/ids"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status**: Status code",
            "**total**: Total items in the list"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
