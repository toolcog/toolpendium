{
  "name": "tilejson_v2_locations_tiles_tiles_json_get",
  "description": "Tilejson",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://openaq.local/v2/locations/tiles/tiles.json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "TileJSON"
        },
        "TileJSON model.\nBased on https://github.com/mapbox/tilejson-spec/tree/master/2.2.0",
        "**Key properties:**",
        {
          "$ul": [
            "**attribution**",
            "**bounds** (default: -180,-90,180,90)",
            "**data** (default: )",
            "**description**",
            "**grids** (default: )",
            "**legend**",
            "**maxzoom** (default: 30)",
            "**minzoom** (default: 0)",
            "**name**",
            "**scheme** (default: xyz)",
            "**template**",
            "**tilejson** (default: 2.2.0)",
            "**tiles**",
            "**version** (default: 1.0.0)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
