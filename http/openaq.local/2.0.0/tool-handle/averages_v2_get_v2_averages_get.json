{
  "name": "averages_v2_get_v2_averages_get",
  "description": "Averages V2 Get",
  "parameters": {
    "type": "object",
    "properties": {
      "date_from": {
        "anyOf": [
          {
            "format": "date-time",
            "type": "string"
          },
          {
            "format": "date",
            "type": "string"
          }
        ],
        "default": "2000-01-01T00:00:00+00:00",
        "title": "Date From"
      },
      "date_to": {
        "anyOf": [
          {
            "format": "date-time",
            "type": "string"
          },
          {
            "format": "date",
            "type": "string"
          }
        ],
        "default": "2021-08-23T09:48:00+00:00",
        "title": "Date To"
      },
      "parameter_id": {
        "title": "Parameter Id",
        "type": "integer"
      },
      "parameter": {
        "items": {
          "anyOf": [
            {
              "exclusiveMinimum": true,
              "maximum": 2147483647,
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "string"
            }
          ]
        },
        "title": "Parameter",
        "type": "array"
      },
      "unit": {
        "items": {
          "type": "string"
        },
        "title": "Unit",
        "type": "array"
      },
      "project_id": {
        "title": "Project Id",
        "type": "integer"
      },
      "project": {
        "items": {
          "anyOf": [
            {
              "exclusiveMinimum": true,
              "maximum": 2147483647,
              "minimum": 0,
              "type": "integer"
            },
            {
              "type": "string"
            }
          ]
        },
        "title": "Project",
        "type": "array"
      },
      "country_id": {
        "description": "\n        Limit results by a certain country using two letter country code.\n        (ex. /US)\n        ",
        "maxLength": 2,
        "minLength": 2,
        "pattern": "[a-zA-Z][a-zA-Z]",
        "title": "Country Id",
        "type": "string"
      },
      "country": {
        "description": "\n        Limit results by a certain country using two letter country code.\n        (ex. ?country=US or ?country=US&country=MX)\n        ",
        "items": {
          "maxLength": 2,
          "minLength": 2,
          "pattern": "[a-zA-Z][a-zA-Z]",
          "type": "string"
        },
        "title": "Country",
        "type": "array"
      },
      "limit": {
        "default": 100,
        "description": "Change the number of results returned.",
        "exclusiveMinimum": true,
        "maximum": 100000,
        "minimum": 0,
        "title": "Limit",
        "type": "integer"
      },
      "page": {
        "default": 1,
        "description": "Paginate through results.",
        "exclusiveMinimum": true,
        "maximum": 6000,
        "minimum": 0,
        "title": "Page",
        "type": "integer"
      },
      "offset": {
        "default": 0,
        "maximum": 10000,
        "minimum": 0,
        "title": "Offset",
        "type": "integer"
      },
      "sort": {
        "allOf": [
          {
            "$ref": "#/$defs/Sort"
          }
        ],
        "default": "desc",
        "description": "Define sort order."
      },
      "spatial": {
        "$ref": "#/$defs/Spatial"
      },
      "temporal": {
        "$ref": "#/$defs/Temporal"
      },
      "location": {
        "items": {
          "type": "string"
        },
        "title": "Location",
        "type": "array"
      },
      "group": {
        "default": false,
        "title": "Group",
        "type": "boolean"
      }
    },
    "required": [
      "spatial",
      "temporal"
    ],
    "$defs": {
      "Sort": {
        "description": "An enumeration.",
        "enum": [
          "asc",
          "desc"
        ],
        "title": "Sort",
        "type": "string"
      },
      "Spatial": {
        "description": "An enumeration.",
        "enum": [
          "country",
          "location",
          "project",
          "total"
        ],
        "title": "Spatial",
        "type": "string"
      },
      "Temporal": {
        "description": "An enumeration.",
        "enum": [
          "day",
          "month",
          "year",
          "moy",
          "dow",
          "hour",
          "hod"
        ],
        "title": "Temporal",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://openaq.local/v2/averages{?date_from,date_to,parameter_id,parameter,unit,project_id,project,country_id,country,limit,page,offset,sort,spatial,temporal,location,group}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "OpenAQResult"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**found** (default: 0)",
                  "**license** (default: CC BY 4.0d)",
                  "**limit** (default: 100)",
                  "**name** (default: openaq-api)",
                  "**page** (default: 1)",
                  "**website** (default: /)"
                ]
              }
            ],
            "**results** (default: )"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "HTTPValidationError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
