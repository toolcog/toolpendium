{
  "name": "summary_get_v2_summary_get",
  "description": "Summary Get",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://openaq.local/v2/summary"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "OpenAQResult"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  "**found** (default: 0)",
                  "**license** (default: CC BY 4.0d)",
                  "**limit** (default: 100)",
                  "**name** (default: openaq-api)",
                  "**page** (default: 1)",
                  "**website** (default: /)"
                ]
              }
            ],
            "**results** (default: )"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
