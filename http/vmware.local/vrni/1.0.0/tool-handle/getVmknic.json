{
  "name": "getVmknic",
  "description": "Show vmknic details",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "entity id"
      },
      "time": {
        "format": "int64",
        "type": "integer",
        "description": "time in epoch seconds"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://vmware.local/entities/vmknics/{id}{?time}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entity_id**",
            "**entity_type**",
            "**name**",
            [
              "**host**",
              {
                "$ul": [
                  "**entity_id**",
                  "**entity_type**"
                ]
              }
            ],
            "**ip_addresses**",
            [
              "**layer2_network**",
              {
                "$ul": [
                  "**entity_id**",
                  "**entity_type**"
                ]
              }
            ],
            [
              "**vlan**",
              {
                "$ul": [
                  "**begin**",
                  "**end**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**details**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
