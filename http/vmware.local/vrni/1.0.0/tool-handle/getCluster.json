{
  "name": "getCluster",
  "description": "Show cluster details",
  "parameters": {
    "type": "object",
    "properties": {
      "time": {
        "format": "int64",
        "type": "integer",
        "description": "time in epoch seconds"
      },
      "id": {
        "type": "string",
        "description": "entity id"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://vmware.local/entities/clusters/{id}{?time}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entity_id**",
            "**entity_type**",
            "**name**",
            [
              "**nsx_manager**",
              {
                "$ul": [
                  "**entity_id**",
                  "**entity_type**"
                ]
              }
            ],
            "**num_cpu_cores**",
            "**num_datastores**",
            "**num_hosts**",
            "**total_cpus**",
            "**total_memory**",
            [
              "**vcenter_manager**",
              {
                "$ul": [
                  "**entity_id**",
                  "**entity_type**"
                ]
              }
            ],
            "**vendor_id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**details**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
