{
  "name": "getBrocadeSwitchSnmpConfig",
  "description": "Show snmp config for brocade switch data source",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "entity id"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://vmware.local/data-sources/brocade-switches/{id}/snmp-config"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**config_snmp_2c**",
              {
                "$ul": [
                  "**community_string**"
                ]
              }
            ],
            [
              "**config_snmp_3**",
              {
                "$ul": [
                  "**authentication_password**",
                  "**authentication_type**",
                  "**context_name**",
                  "**privacy_password**",
                  "**privacy_type**",
                  "**username**"
                ]
              }
            ],
            "**snmp_enabled** (default: false)",
            "**snmp_version**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
