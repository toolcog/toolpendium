{
  "name": "listDatastores",
  "description": "List datastores",
  "parameters": {
    "type": "object",
    "properties": {
      "size": {
        "default": 10,
        "type": "number",
        "description": "page size of results"
      },
      "cursor": {
        "type": "string",
        "description": "cursor from previous response"
      },
      "start_time": {
        "type": "number",
        "description": "start time for query in epoch seconds"
      },
      "end_time": {
        "type": "number",
        "description": "end time for query in epoch seconds"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://vmware.local/entities/datastores{?size,cursor,start_time,end_time}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**cursor**",
            "**end_time**",
            "**results**",
            "**start_time**",
            "**total_count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**details**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
