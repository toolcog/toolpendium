{
  "name": "getTier",
  "description": "Show tier details",
  "parameters": {
    "type": "object",
    "properties": {
      "tier-id": {
        "type": "string"
      },
      "Authorization": {
        "format": "NetworkInsight <token>",
        "type": "string",
        "description": "Authorization Header"
      }
    },
    "required": [
      "tier-id",
      "Authorization"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://vmware.local/groups/tiers/{tier-id}"
    },
    "headers": {
      "Authorization": {
        "$": "Authorization"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entity_id**",
            "**entity_type**",
            "**name**",
            [
              "**application**",
              {
                "$ul": [
                  "**entity_id**",
                  "**entity_type**"
                ]
              }
            ],
            "**group_membership_criteria**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
