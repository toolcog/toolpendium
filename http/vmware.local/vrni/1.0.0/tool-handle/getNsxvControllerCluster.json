{
  "name": "getNsxvControllerCluster",
  "description": "Show nsx controller-cluster details",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "entity id"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://vmware.local/data-sources/nsxv-managers/{id}/controller-cluster"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**controller_password**",
            "**enabled** (default: false)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
