{
  "name": "getLicenseByID",
  "description": "Fetches a list of available photo licenses for Flickr",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      }
    },
    "required": [
      "api_key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.photos.licenses.getInfo{&api_key}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**licenses**",
              {
                "$ul": [
                  "**license**"
                ]
              }
            ],
            "**stat**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
