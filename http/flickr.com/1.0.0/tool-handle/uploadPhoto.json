{
  "name": "uploadPhoto",
  "description": "Uploads a new photo to Flickr",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "api_key": {
            "type": "string"
          },
          "content_type": {
            "enum": [
              "1",
              "2",
              "3"
            ],
            "type": "string"
          },
          "description": {
            "type": "string"
          },
          "hidden": {
            "enum": [
              "1",
              "2"
            ],
            "type": "string"
          },
          "is_family": {
            "enum": [
              "0",
              "1"
            ],
            "type": "string"
          },
          "is_friend": {
            "enum": [
              "0",
              "1"
            ],
            "type": "string"
          },
          "is_public": {
            "enum": [
              "0",
              "1"
            ],
            "type": "string"
          },
          "photo": {
            "format": "binary",
            "type": "string"
          },
          "safety_level": {
            "enum": [
              "1",
              "2",
              "3"
            ],
            "type": "string"
          },
          "tags": {
            "type": "string"
          },
          "title": {
            "type": "string"
          }
        },
        "required": [
          "api_key",
          "photo"
        ],
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.flickr.com/services/upload"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
