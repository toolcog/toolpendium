{
  "name": "getPhotoSizesByID",
  "description": "Returns photo sizes",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      },
      "photo_id": {
        "pattern": "^[0-9]+$",
        "type": "string"
      }
    },
    "required": [
      "api_key",
      "photo_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.photos.getSizes{&api_key,photo_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**sizes**",
              {
                "$ul": [
                  "**canblog**",
                  "**candownload**",
                  "**canprint**",
                  "**sizes**"
                ]
              }
            ],
            "**stat**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
