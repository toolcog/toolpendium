{
  "name": "getPhotostreamContextByID",
  "description": "Returns next and previous photos for a photo in a photostream",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      },
      "photo_id": {
        "pattern": "^[0-9]+$",
        "type": "string"
      }
    },
    "required": [
      "api_key",
      "photo_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.photos.getContext{&api_key,photo_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**count**",
              {
                "$ul": [
                  "**_content**"
                ]
              }
            ],
            [
              "**nextphoto**",
              {
                "$ul": [
                  "**farm**",
                  "**id**",
                  "**is_faved**",
                  "**license**",
                  "**media**",
                  "**owner**",
                  "**safe**",
                  "**secret**",
                  "**server**",
                  "**thumb**",
                  "**title**",
                  "**url**"
                ]
              }
            ],
            [
              "**prevphoto**",
              {
                "$ul": [
                  "**farm**",
                  "**id**",
                  "**is_faved**",
                  "**license**",
                  "**media**",
                  "**owner**",
                  "**safe**",
                  "**secret**",
                  "**server**",
                  "**thumb**",
                  "**title**",
                  "**url**"
                ]
              }
            ],
            "**stat**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
