{
  "name": "getFavoritesByPersonID",
  "description": "Returns a list of the user's favorite photos. Only photos which the calling user has permission to see are returned.",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      },
      "user_id": {
        "type": "string"
      },
      "min_fave_date": {
        "type": "number"
      },
      "max_fave_date": {
        "type": "number"
      },
      "page": {
        "type": "number"
      },
      "per_page": {
        "type": "number"
      }
    },
    "required": [
      "api_key",
      "user_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.favorites.getList{&api_key,user_id,min_fave_date,max_fave_date,page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**page**",
            "**pages**",
            "**perpage**",
            "**photos**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
