{
  "name": "echo",
  "description": "Echos the input parameters back in the response",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      },
      "echo": {
        "type": "string"
      }
    },
    "required": [
      "api_key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.test.echo{&api_key,echo}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**echo**",
              {
                "$ul": [
                  "**_content**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
