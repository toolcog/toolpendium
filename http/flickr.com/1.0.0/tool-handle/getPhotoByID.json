{
  "name": "getPhotoByID",
  "description": "Returns a photo",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      },
      "photo_id": {
        "pattern": "^[0-9]+$",
        "type": "string"
      }
    },
    "required": [
      "api_key",
      "photo_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.photos.getInfo{&api_key,photo_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**photo**",
              {
                "$ul": [
                  [
                    "**comments**",
                    {
                      "$ul": [
                        "**_content**"
                      ]
                    }
                  ],
                  [
                    "**dates**",
                    {
                      "$ul": [
                        "**lastupdate**",
                        "**posted**",
                        "**taken**",
                        "**takengranularity**",
                        "**takenunknown**"
                      ]
                    }
                  ],
                  "**dateuploaded**",
                  [
                    "**description**",
                    {
                      "$ul": [
                        "**_content**"
                      ]
                    }
                  ],
                  [
                    "**editability**",
                    {
                      "$ul": [
                        "**canaddmeta**",
                        "**cancomment**"
                      ]
                    }
                  ],
                  "**farm**",
                  "**id**",
                  "**isfavorite**",
                  "**license**",
                  "**media**",
                  [
                    "**notes**",
                    {
                      "$ul": [
                        "**note**"
                      ]
                    }
                  ],
                  "**originalsecret**",
                  [
                    "**owner**",
                    {
                      "$ul": [
                        "**iconfarm**",
                        "**iconserver**",
                        "**is_ad_free**",
                        "**ispro**",
                        "**location**",
                        "**noindexfollow**",
                        "**nsid**",
                        "**path_alias**",
                        "**realname**",
                        "**username**"
                      ]
                    }
                  ],
                  [
                    "**people**",
                    {
                      "$ul": [
                        "**haspeople**"
                      ]
                    }
                  ],
                  [
                    "**permissions**",
                    {
                      "$ul": [
                        "**permaddmeta**",
                        "**permcomment**"
                      ]
                    }
                  ],
                  [
                    "**publiceditability**",
                    {
                      "$ul": [
                        "**canaddmeta**",
                        "**cancomment**"
                      ]
                    }
                  ],
                  "**rotation**",
                  "**safe**",
                  "**safety_level**",
                  "**secret**",
                  "**server**",
                  [
                    "**tags**",
                    {
                      "$ul": [
                        "**tag**"
                      ]
                    }
                  ],
                  [
                    "**title**",
                    {
                      "$ul": [
                        "**_content**"
                      ]
                    }
                  ],
                  [
                    "**urls**",
                    {
                      "$ul": [
                        "**url**"
                      ]
                    }
                  ],
                  [
                    "**usage**",
                    {
                      "$ul": [
                        "**canblog**",
                        "**candownload**",
                        "**canprint**",
                        "**canshare**"
                      ]
                    }
                  ],
