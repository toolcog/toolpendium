{
  "name": "getGroupDiscussionsByID",
  "description": "Get a list of discussion topics in a group.",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      },
      "group_id": {
        "pattern": "^([0-9]+@N[0-9]+)|([0-9a-zA-Z-_]+)$",
        "type": "string"
      },
      "page": {
        "type": "number"
      },
      "per_page": {
        "type": "number"
      }
    },
    "required": [
      "api_key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.groups.discuss.topics.getList{&api_key,group_id,page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**iconfarm**",
            "**iconserver**",
            "**ispoolmoderated**",
            "**lang**",
            "**members**",
            "**name**",
            "**page**",
            "**pages**",
            "**per_page**",
            "**privacy**",
            "**topics**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
