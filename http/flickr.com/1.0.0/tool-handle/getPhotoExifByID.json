{
  "name": "getPhotoExifByID",
  "description": "Retrieves a list of EXIF/TIFF/GPS tags for a given photo. The calling user must have permission to view the photo.",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      },
      "photo_id": {
        "pattern": "^[0-9]+$",
        "type": "string"
      },
      "secret": {
        "type": "string"
      }
    },
    "required": [
      "api_key",
      "photo_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.photos.getExif{&api_key,photo_id,secret}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**photo**",
              {
                "$ul": [
                  "**camera**",
                  "**exif**",
                  "**farm**",
                  "**id**",
                  "**secret**",
                  "**server**"
                ]
              }
            ],
            "**stat**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
