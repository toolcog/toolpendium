{
  "name": "getMediaByPersonID",
  "description": "Return photos from the given user's photostream",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string"
      },
      "user_id": {
        "type": "string"
      },
      "safe_search": {
        "type": "number"
      },
      "min_upload_date": {
        "type": "number"
      },
      "max_upload_date": {
        "type": "number"
      },
      "min_taken_date": {
        "type": "number"
      },
      "max_taken_date": {
        "type": "number"
      },
      "content_type": {
        "type": "number"
      },
      "privacy_filter": {
        "type": "number"
      },
      "page": {
        "type": "number"
      },
      "per_page": {
        "type": "number"
      }
    },
    "required": [
      "api_key",
      "user_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/rest?method=flickr.people.getPhotos{&api_key,user_id,safe_search,min_upload_date,max_upload_date,min_taken_date,max_taken_date,content_type,privacy_filter,page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**page**",
            "**pages**",
            "**perpage**",
            "**photos**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
