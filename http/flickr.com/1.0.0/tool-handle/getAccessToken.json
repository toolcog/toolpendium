{
  "name": "getAccessToken",
  "description": "Returns an access token",
  "parameters": {
    "type": "object",
    "properties": {
      "oauth_consumer_key": {
        "type": "string"
      },
      "oauth_nonce": {
        "type": "string"
      },
      "oauth_timestamp": {
        "pattern": "^[0-9]+$",
        "type": "string"
      },
      "oauth_signature_method": {
        "type": "string"
      },
      "oauth_version": {
        "type": "string"
      },
      "oauth_signature": {
        "type": "string"
      },
      "oauth_verifier": {
        "type": "string"
      },
      "oauth_token": {
        "type": "string"
      }
    },
    "required": [
      "oauth_consumer_key",
      "oauth_nonce",
      "oauth_timestamp",
      "oauth_signature_method",
      "oauth_version",
      "oauth_signature",
      "oauth_verifier",
      "oauth_token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flickr.com/services/oauth/access_token{?oauth_consumer_key,oauth_nonce,oauth_timestamp,oauth_signature_method,oauth_version,oauth_signature,oauth_verifier,oauth_token}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
