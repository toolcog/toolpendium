{
  "name": "list_account_numbers",
  "description": "List Account Numbers",
  "parameters": {
    "type": "object",
    "properties": {
      "cursor": {
        "description": "Return the page of entries after this one.",
        "type": "string"
      },
      "limit": {
        "description": "Limit the size of the list that is returned. The default (and maximum) is 100 objects.",
        "minimum": 0,
        "type": "integer"
      },
      "status": {
        "description": "The status to retrieve Account Numbers for.",
        "enum": [
          "active",
          "disabled",
          "canceled"
        ],
        "type": "string",
        "x-enum-descriptions": [
          "The account number is active.",
          "The account number is temporarily disabled.",
          "The account number is permanently disabled."
        ]
      },
      "account_id": {
        "description": "Filter Account Numbers to those belonging to the specified Account.",
        "type": "string"
      },
      "created_at.after": {
        "description": "Return results after this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) timestamp.",
        "format": "date-time",
        "type": "string"
      },
      "created_at.before": {
        "description": "Return results before this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) timestamp.",
        "format": "date-time",
        "type": "string"
      },
      "created_at.on_or_after": {
        "description": "Return results on or after this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) timestamp.",
        "format": "date-time",
        "type": "string"
      },
      "created_at.on_or_before": {
        "description": "Return results on or before this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) timestamp.",
        "format": "date-time",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.increase.com/account_numbers{?cursor,limit,status,account_id,created_at.after,created_at.before,created_at.on_or_after,created_at.on_or_before}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Account Number List"
        },
        "A list of Account Number objects",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: The contents of the list.",
            "**next_cursor**: A pointer to a place in the list."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
