{
  "name": "list_account_statements",
  "description": "List Account Statements",
  "parameters": {
    "type": "object",
    "properties": {
      "cursor": {
        "description": "Return the page of entries after this one.",
        "type": "string"
      },
      "limit": {
        "description": "Limit the size of the list that is returned. The default (and maximum) is 100 objects.",
        "minimum": 0,
        "type": "integer"
      },
      "account_id": {
        "description": "Filter Account Statements to those belonging to the specified Account.",
        "type": "string"
      },
      "statement_period_start.after": {
        "description": "Return results after this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) timestamp.",
        "format": "date-time",
        "type": "string"
      },
      "statement_period_start.before": {
        "description": "Return results before this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) timestamp.",
        "format": "date-time",
        "type": "string"
      },
      "statement_period_start.on_or_after": {
        "description": "Return results on or after this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) timestamp.",
        "format": "date-time",
        "type": "string"
      },
      "statement_period_start.on_or_before": {
        "description": "Return results on or before this [ISO 8601](https://en.wikipedia.org/wiki/ISO_8601) timestamp.",
        "format": "date-time",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.increase.com/account_statements{?cursor,limit,account_id,statement_period_start.after,statement_period_start.before,statement_period_start.on_or_after,statement_period_start.on_or_before}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Account Statement List"
        },
        "A list of Account Statement objects",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: The contents of the list.",
            "**next_cursor**: A pointer to a place in the list."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
