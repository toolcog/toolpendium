{
  "name": "list_external_accounts",
  "description": "List External Accounts",
  "parameters": {
    "type": "object",
    "properties": {
      "cursor": {
        "description": "Return the page of entries after this one.",
        "type": "string"
      },
      "limit": {
        "description": "Limit the size of the list that is returned. The default (and maximum) is 100 objects.",
        "minimum": 0,
        "type": "integer"
      },
      "status.in": {
        "description": "Filter External Accounts for those with the specified status or statuses. For GET requests, this should be encoded as a comma-delimited string, such as `?in=one,two,three`.",
        "items": {
          "enum": [
            "active",
            "archived"
          ],
          "type": "string",
          "x-enum-descriptions": [
            "The External Acccount is active.",
            "The External Account is archived and won't appear in the dashboard."
          ]
        },
        "type": "array"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.increase.com/external_accounts{?cursor,limit,status.in}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "External Account List"
        },
        "A list of External Account objects",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: The contents of the list.",
            "**next_cursor**: A pointer to a place in the list."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
