{
  "name": "calls_info",
  "description": "Returns information about a Call.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `calls:read`"
      },
      "id": {
        "type": "string",
        "description": "`id` of the Call returned by the [`calls.add`](/methods/calls.add) method."
      }
    },
    "required": [
      "token",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/calls.info{?id}"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Default success template"
        },
        "This method either only returns a brief _OK_ response or a verbose schema is not available for this method.",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Default error template"
        },
        "This method either only returns a brief _not OK_ response or a verbose schema is not available for this method.",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
