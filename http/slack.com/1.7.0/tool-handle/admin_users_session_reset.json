{
  "name": "admin_users_session_reset",
  "description": "Wipes all valid sessions on all devices for a given user",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `admin.users:write`"
      },
      "body": {
        "properties": {
          "mobile_only": {
            "description": "Only expire mobile sessions (default: false)",
            "type": "boolean"
          },
          "user_id": {
            "description": "The ID of the user to wipe sessions for",
            "type": "string"
          },
          "web_only": {
            "description": "Only expire web sessions (default: false)",
            "type": "boolean"
          }
        },
        "required": [
          "user_id"
        ],
        "type": "object"
      }
    },
    "required": [
      "token",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://slack.com/api/admin.users.session.reset"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
