{
  "name": "dnd_endSnooze",
  "description": "Ends the current user's snooze mode immediately.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `dnd:write`"
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://slack.com/api/dnd.endSnooze"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "dnd.endSnooze schema"
        },
        "Schema for successful response from dnd.endSnooze method",
        "**Key properties:**",
        {
          "$ul": [
            "**dnd_enabled**",
            "**next_dnd_end_ts**",
            "**next_dnd_start_ts**",
            "**ok**",
            "**snooze_enabled**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "dnd.endSnooze error schema"
        },
        "Schema for error response from dnd.endSnooze method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
