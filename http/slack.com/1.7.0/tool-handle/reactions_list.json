{
  "name": "reactions_list",
  "description": "Lists reactions made by a user.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `reactions:read`"
      },
      "user": {
        "type": "string",
        "description": "Show reactions made by this user. Defaults to the authed user."
      },
      "full": {
        "type": "boolean",
        "description": "If true always return the complete reaction list."
      },
      "count": {
        "type": "integer"
      },
      "page": {
        "type": "integer"
      },
      "cursor": {
        "type": "string",
        "description": "Parameter for pagination. Set `cursor` equal to the `next_cursor` attribute returned by the previous request's `response_metadata`. This parameter is optional, but pagination is mandatory: the default value simply fetches the first \"page\" of the collection. See [pagination](/docs/pagination) for more details."
      },
      "limit": {
        "type": "integer",
        "description": "The maximum number of items to return. Fewer than the requested number of items may be returned, even if the end of the list hasn't been reached."
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/reactions.list{?token,user,full,count,page,cursor,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "reactions.list schema"
        },
        "Schema for successful response from reactions.list method",
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**ok**",
            [
              "**paging**",
              {
                "$ul": [
                  "**count**",
                  "**page**",
                  "**pages**",
                  "**per_page**",
                  "**spill**",
                  "**total**"
                ]
              }
            ],
            "**response_metadata**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "reactions.list error schema"
        },
        "Schema for error response from reactions.list method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
