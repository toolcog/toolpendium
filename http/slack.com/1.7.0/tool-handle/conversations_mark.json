{
  "name": "conversations_mark",
  "description": "Sets the read cursor in a channel.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `conversations:write`"
      },
      "body": {
        "properties": {
          "channel": {
            "description": "Channel or conversation to set the read cursor for.",
            "type": "string"
          },
          "ts": {
            "description": "Unique identifier of message you want marked as most recently seen in this conversation.",
            "type": "number"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://slack.com/api/conversations.mark"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "conversations.mark success schema"
        },
        "Schema for successful response conversations.mark method",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "conversations.mark error schema"
        },
        "Schema for error response from conversations.mark method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**needed**",
            "**ok**",
            "**provided**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
