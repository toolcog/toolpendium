{
  "name": "dialog_open",
  "description": "Open a dialog with a user",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `none`"
      },
      "dialog": {
        "type": "string",
        "description": "The dialog definition. This must be a JSON-encoded string."
      },
      "trigger_id": {
        "type": "string",
        "description": "Exchange a trigger to post to the user."
      }
    },
    "required": [
      "token",
      "dialog",
      "trigger_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/dialog.open{?dialog,trigger_id}"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "dialog.open schema"
        },
        "Schema for successful response from dialog.open method",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "dialog.open error schema"
        },
        "Schema for error response from dialog.open method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
