{
  "name": "migration_exchange",
  "description": "For Enterprise Grid workspaces, map local user IDs to global user IDs",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `tokens.basic`"
      },
      "users": {
        "type": "string",
        "description": "A comma-separated list of user ids, up to 400 per request"
      },
      "team_id": {
        "type": "string",
        "description": "Specify team_id starts with `T` in case of Org Token"
      },
      "to_old": {
        "type": "boolean",
        "description": "Specify `true` to convert `W` global user IDs to workspace-specific `U` IDs. Defaults to `false`."
      }
    },
    "required": [
      "token",
      "users"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/migration.exchange{?token,users,team_id,to_old}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "migration.exchange success schema"
        },
        "Schema for successful response from migration.exchange method",
        "**Key properties:**",
        {
          "$ul": [
            "**enterprise_id**",
            "**invalid_user_ids**",
            "**ok**",
            "**team_id**",
            "**user_id_map**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "migration.exchange error schema"
        },
        "Schema for error response from migration.exchange method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
