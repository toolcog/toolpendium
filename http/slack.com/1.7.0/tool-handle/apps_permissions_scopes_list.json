{
  "name": "apps_permissions_scopes_list",
  "description": "Returns list of scopes this app has on a team.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `none`"
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/apps.permissions.scopes.list{?token}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "api.permissions.scopes.list success schema"
        },
        "Schema for successful response api.permissions.scopes.list method",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**",
            [
              "**scopes**",
              {
                "$ul": [
                  "**app_home**",
                  "**channel**",
                  "**group**",
                  "**im**",
                  "**mpim**",
                  "**team**",
                  "**user**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "apps.permissions.scopes.list error schema"
        },
        "Schema for error response from apps.permissions.scopes.list method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
