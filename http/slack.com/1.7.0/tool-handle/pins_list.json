{
  "name": "pins_list",
  "description": "Lists items pinned to a channel.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `pins:read`"
      },
      "channel": {
        "type": "string",
        "description": "Channel to get pinned items for."
      }
    },
    "required": [
      "token",
      "channel"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/pins.list{?token,channel}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "pins.list success schema"
        },
        "Schema for successful response from pins.list method",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "pins.list error schema"
        },
        "Schema for error response from pins.list method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
