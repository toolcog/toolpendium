{
  "name": "apps_event_authorizations_list",
  "description": "Get a list of authorizations for the given event context. Each authorization represents an app installation that the event is visible to.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `authorizations:read`"
      },
      "event_context": {
        "type": "string"
      },
      "cursor": {
        "type": "string"
      },
      "limit": {
        "type": "integer"
      }
    },
    "required": [
      "token",
      "event_context"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/apps.event.authorizations.list{?event_context,cursor,limit}"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Default success template"
        },
