{
  "name": "views_publish",
  "description": "Publish a static view for a User.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `none`"
      },
      "user_id": {
        "type": "string",
        "description": "`id` of the user you want publish a view to."
      },
      "view": {
        "type": "string",
        "description": "A [view payload](/reference/surfaces/views). This must be a JSON-encoded string."
      },
      "hash": {
        "type": "string",
        "description": "A string that represents view state to protect against possible race conditions."
      }
    },
    "required": [
      "token",
      "user_id",
      "view"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/views.publish{?user_id,view,hash}"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Default success template"
        },
        "This method either only returns a brief _OK_ response or a verbose schema is not available for this method.",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Default error template"
        },
        "This method either only returns a brief _not OK_ response or a verbose schema is not available for this method.",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
