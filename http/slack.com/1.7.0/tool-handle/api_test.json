{
  "name": "api_test",
  "description": "Checks API calling code.",
  "parameters": {
    "type": "object",
    "properties": {
      "error": {
        "type": "string",
        "description": "Error response to return"
      },
      "foo": {
        "type": "string",
        "description": "example property to return"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/api.test{?error,foo}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "api.test success schema"
        },
        "Schema for successful response api.test method",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "api.test error schema"
        },
        "Schema for error response api.test method",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
