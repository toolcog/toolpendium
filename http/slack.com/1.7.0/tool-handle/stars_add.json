{
  "name": "stars_add",
  "description": "Adds a star to an item.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `stars:write`"
      },
      "body": {
        "properties": {
          "channel": {
            "description": "Channel to add star to, or channel where the message to add star to was posted (used with `timestamp`).",
            "type": "string"
          },
          "file": {
            "description": "File to add star to.",
            "type": "string"
          },
          "file_comment": {
            "description": "File comment to add star to.",
            "type": "string"
          },
          "timestamp": {
            "description": "Timestamp of the message to add star to.",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://slack.com/api/stars.add"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "stars.add schema"
        },
        "Schema for successful response from stars.add method",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "stars.add error schema"
        },
        "Schema for error response from stars.add method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
