{
  "name": "auth_revoke",
  "description": "Revokes a token.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `none`"
      },
      "test": {
        "type": "boolean",
        "description": "Setting this parameter to `1` triggers a _testing mode_ where the specified token will not actually be revoked."
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/auth.revoke{?token,test}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "auth.revoke schema"
        },
        "Schema for successful response from auth.revoke method",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**",
            "**revoked**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "auth.revoke error schema"
        },
        "Schema for error response from auth.revoke method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
