{
  "name": "conversations_join",
  "description": "Joins an existing conversation.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `channels:write`"
      },
      "body": {
        "properties": {
          "channel": {
            "description": "ID of conversation to join",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://slack.com/api/conversations.join"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "conversations.join success schema"
        },
        "Schema for successful response from conversations.join method",
        "**Key properties:**",
        {
          "$ul": [
            "**channel**",
            "**ok**",
            [
              "**response_metadata**",
              {
                "$ul": [
                  "**warnings**"
                ]
              }
            ],
            "**warning**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "conversations.join error schema"
        },
        "Schema for error response from conversations.join method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**needed**",
            "**ok**",
            "**provided**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
