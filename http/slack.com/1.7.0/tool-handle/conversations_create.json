{
  "name": "conversations_create",
  "description": "Initiates a public or private channel-based conversation",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `conversations:write`"
      },
      "body": {
        "properties": {
          "is_private": {
            "description": "Create a private channel instead of a public one",
            "type": "boolean"
          },
          "name": {
            "description": "Name of the public or private channel to create",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://slack.com/api/conversations.create"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "conversations.create success schema"
        },
        "Schema for successful response conversations.create method",
        "**Key properties:**",
        {
          "$ul": [
            "**channel**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "conversations.create error schema"
        },
        "Schema for error response from conversations.create method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**detail**",
            "**error**",
            "**needed**",
            "**ok**",
            "**provided**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
