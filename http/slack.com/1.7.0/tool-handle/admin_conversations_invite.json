{
  "name": "admin_conversations_invite",
  "description": "Invite a user to a public or private channel.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `admin.conversations:write`"
      },
      "body": {
        "properties": {
          "channel_id": {
            "description": "The channel that the users will be invited to.",
            "type": "string"
          },
          "user_ids": {
            "description": "The users to invite.",
            "type": "string"
          }
        },
        "required": [
          "user_ids",
          "channel_id"
        ],
        "type": "object"
      }
    },
    "required": [
      "token",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://slack.com/api/admin.conversations.invite"
    },
    "headers": {
      "token": {
        "$": "token"
      }
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "admin.conversations.invite schema"
        },
        "Schema for successful response of admin.conversations.invite",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "admin.conversations.invite error schema"
        },
        "Schema for error response from admin.conversations.invite",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
