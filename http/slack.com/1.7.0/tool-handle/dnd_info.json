{
  "name": "dnd_info",
  "description": "Retrieves a user's current Do Not Disturb status.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `dnd:read`"
      },
      "user": {
        "type": "string",
        "description": "User to fetch status for (defaults to current user)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/dnd.info{?token,user}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "dnd.info schema"
        },
        "Schema for successful response from dnd.info method",
        "**Key properties:**",
        {
          "$ul": [
            "**dnd_enabled**",
            "**next_dnd_end_ts**",
            "**next_dnd_start_ts**",
            "**ok**",
            "**snooze_enabled**",
            "**snooze_endtime**",
            "**snooze_remaining**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "dnd.info error schema"
        },
        "Schema for error response from dnd.info method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
