{
  "name": "users_identity",
  "description": "Get a user's identity.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `identity.basic`"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/users.identity{?token}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "users.identity schema"
        },
        "Schema for successful response from users.identity method",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "users.identity error schema"
        },
        "Schema for error response from users.identity method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
