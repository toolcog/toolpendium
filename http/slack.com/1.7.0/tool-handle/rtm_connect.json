{
  "name": "rtm_connect",
  "description": "Starts a Real Time Messaging session.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `rtm:stream`"
      },
      "batch_presence_aware": {
        "type": "boolean",
        "description": "Batch presence deliveries via subscription. Enabling changes the shape of `presence_change` events. See [batch presence](/docs/presence-and-status#batching)."
      },
      "presence_sub": {
        "type": "boolean",
        "description": "Only deliver presence events when requested by subscription. See [presence subscriptions](/docs/presence-and-status#subscriptions)."
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/rtm.connect{?token,batch_presence_aware,presence_sub}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "rtm.connect schema"
        },
        "Schema for successful response from rtm.connect method",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**",
            [
              "**self**",
              {
                "$ul": [
                  "**id**",
                  "**name**"
                ]
              }
            ],
            [
              "**team**",
              {
                "$ul": [
                  "**domain**",
                  "**id**",
                  "**name**"
                ]
              }
            ],
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "rtm.connect error schema"
        },
        "Schema for error response from rtm.connect method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
