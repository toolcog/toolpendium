{
  "name": "team_info",
  "description": "Gets information about the current team.",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `team:read`"
      },
      "team": {
        "type": "string",
        "description": "Team to get info on, if omitted, will return information about the current team. Will only return team that the authenticated token is allowed to see through external shared channels"
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/team.info{?token,team}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "team.info schema"
        },
        "Schema for successful response from team.info method",
        "**Key properties:**",
        {
          "$ul": [
            "**ok**",
            [
              "**team**",
              {
                "$ul": [
                  "**archived**",
                  "**avatar_base_url**",
                  "**created**",
                  "**date_create**",
                  "**deleted**",
                  "**description**",
                  "**discoverable**",
                  "**domain**",
                  "**email_domain**",
                  "**enterprise_id**",
                  "**enterprise_name**",
                  [
                    "**external_org_migrations**",
                    {
                      "$ul": [
                        "**current**",
                        "**date_updated**"
                      ]
                    }
                  ],
                  "**has_compliance_export**",
                  [
                    "**icon**",
                    {
                      "$ul": [
                        "**image_102**",
                        "**image_132**",
                        "**image_230**",
                        "**image_34**",
                        "**image_44**",
                        "**image_68**",
                        "**image_88**",
                        "**image_default**"
                      ]
                    }
                  ],
                  "**id**",
                  "**is_assigned**",
                  "**is_enterprise**",
                  "**is_over_storage_limit**",
                  "**limit_ts**",
                  "**locale**",
                  "**messages_count**",
                  "**msg_edit_window_mins**",
                  "**name**",
                  "**over_integrations_limit**",
                  "**over_storage_limit**",
                  "**pay_prod_cur**",
                  "**plan**",
                  [
                    "**primary_owner**",
                    {
                      "$ul": [
                        "**email**",
                        "**id**"
                      ]
                    }
                  ],
                  [
                    "**sso_provider**",
                    {
                      "$ul": [
                        "**label**",
                        "**name**",
                        "**type**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "team.info error schema"
        },
        "Schema for error response from team.info method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
