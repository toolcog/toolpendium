{
  "name": "chat_getPermalink",
  "description": "Retrieve a permalink URL for a specific extant message",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string",
        "description": "Authentication token. Requires scope: `none`"
      },
      "channel": {
        "type": "string",
        "description": "The ID of the conversation or channel containing the message"
      },
      "message_ts": {
        "type": "string",
        "description": "A message's `ts` value, uniquely identifying it within a channel"
      }
    },
    "required": [
      "token",
      "channel",
      "message_ts"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://slack.com/api/chat.getPermalink{?token,channel,message_ts}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "chat.getPermalink success schema"
        },
        "Schema for successful response chat.getPermalink",
        "**Key properties:**",
        {
          "$ul": [
            "**channel**",
            "**ok**",
            "**permalink**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "chat.getPermalink error schema"
        },
        "Schema for error response from chat.getPermalink method",
        "**Key properties:**",
        {
          "$ul": [
            "**callstack**: Note: PHP callstack is only visible in dev/qa",
            "**error**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
