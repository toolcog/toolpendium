{
  "name": "app_payments_sepacredittransfer_download",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "appId": {
        "type": "integer",
        "description": "The identifier of the Sinao application you are using. Each application corresponds to a company and an accounting. A user can have several Sinao applications. To retrieve the identifiers of its applications, you must call the endpoint `/apps`."
      },
      "ids": {
        "items": {
          "type": "integer"
        },
        "type": "array"
      },
      "amounts": {
        "items": {
          "type": "integer"
        },
        "type": "array",
        "description": "Optional array with amounts (keys must to correspond to ids)"
      },
      "debtor_name": {
        "type": "string"
      },
      "debtor_iban": {
        "type": "string"
      },
      "debtor_bic": {
        "type": "string"
      },
      "btchBookg": {
        "type": "integer"
      }
    },
    "required": [
      "appId",
      "ids"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.sinao.app/v1/apps/{appId}/sepamandates/credittransfer{?ids,amounts,debtor_name,debtor_iban,debtor_bic,btchBookg}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
