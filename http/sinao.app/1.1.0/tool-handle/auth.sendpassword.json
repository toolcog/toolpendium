{
  "name": "auth_sendpassword",
  "description": "Send a forgotten_password_token by email to recover password",
  "parameters": {
    "type": "object",
    "properties": {
      "email": {
        "format": "email",
        "type": "string",
        "description": "Email for login"
      }
    },
    "required": [
      "email"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.sinao.app/v1/sendpassword{?email}"
    }
  },
  "responses": {}
}
