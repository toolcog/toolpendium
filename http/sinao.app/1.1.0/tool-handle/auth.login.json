{
  "name": "auth_login",
  "description": "User login by email and password. Used only on Sinao API. CloudAccounting API needs a different API key",
  "parameters": {
    "type": "object",
    "properties": {
      "email": {
        "format": "email",
        "type": "string",
        "description": "Email for login"
      },
      "password": {
        "type": "string",
        "description": "Password for login in clear text"
      }
    },
    "required": [
      "email",
      "password"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.sinao.app/v1/login{?email,password}"
    }
  },
  "responses": {}
}
