{
  "name": "app_policies_invitations_list",
  "description": "Get all sent invitations",
  "parameters": {
    "type": "object",
    "properties": {
      "appId": {
        "type": "integer",
        "description": "The identifier of the Sinao application you are using. Each application corresponds to a company and an accounting. A user can have several Sinao applications. To retrieve the identifiers of its applications, you must call the endpoint `/apps`."
      },
      "limit": {
        "default": 10,
        "maximum": 10000,
        "minimum": 1,
        "type": "integer",
        "description": "The numbers of items to return."
      },
      "search": {
        "type": "string",
        "description": "A string to search for in objects."
      },
      "filters": {
        "items": {
          "properties": {
            "comparator": {
              "enum": [
                "=",
                "<",
                ">",
                "<=",
                ">=",
                "!=",
                "LIKE",
                "IN",
                "REGEX"
              ],
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "value": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array",
        "description": "List of filters to apply to the query."
      },
      "order": {
        "items": {
          "properties": {
            "direction": {
              "enum": [
                "asc",
                "desc"
              ],
              "type": "string"
            },
            "name": {
              "type": "string"
            }
          },
          "type": "object"
        },
        "type": "array",
        "description": "List in order of priority of the variables by which to order the result."
      }
    },
    "required": [
      "appId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.sinao.app/v1/apps/{appId}/access/invite{?limit,search,filters,order}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**app**",
              {
                "$ul": [
                  [
                    "**admin**",
                    {
                      "$ul": [
                        "**civility**: Civility is the lastname prefix",
                        "**establishments**",
                        "**firstname**",
                        "**id**",
                        "**image**",
                        "**lastname**",
                        "**metadata**",
                        "**created_at**",
                        "**email**: Email used as login",
                        "**last_access_at**",
                        "**password**",
                        "**password_is_undefined**",
                        "**updated_at**"
                      ]
                    }
                  ],
                  "**config**",
                  "**hostname_alias**: Specific hostname for this app. To use default URI, null this param",
                  "**id**",
                  "**last_access_at**",
                  [
                    "**last_user**",
                    {
                      "$ul": [
                        "**civility**: Civility is the lastname prefix",
                        "**establishments**",
                        "**firstname**",
                        "**id**",
                        "**image**",
                        "**lastname**",
                        "**metadata**",
                        "**created_at**",
                        "**email**: Email used as login",
                        "**last_access_at**",
                        "**password**",
                        "**password_is_undefined**",
                        "**updated_at**"
                      ]
                    }
                  ],
                  [
                    "**organization**",
                    {
                      "$ul": [
                        "**billing_name**: Company name for billing",
                        "**capital**: Capital in cents",
                        "**closeaccounting_period**",
                        "**code_naf**: French NAF Code",
                        "**country_iso2**",
                        "**dissolution_date**",
                        "**establishments**: List of contact establishments. Some establishments come from the public database (INSEE), they cannot be edited and have no ID",
                        "**founding_date**",
                        "**founding_location**",
                        "**greffe**: French. Tribunal de commerce",
                        "**id**",
                        "**legal_form**",
                        "**logo**",
                        "**name**: Company name visible on the app",
                        "**national_id**: Unique National Id, format by country. **In France : [0-9]{9} with last number as security key.**",
                        "**number_of_employees**",
                        "**rcs**: French. Registre du Commerce et des Sociétés",
                        "**slogan**: Free variable",
                        "**tax_id**: International Tax Id",
                        "**vat_id**: European VAT Id. **In France : FR [0-9]{2} [0-9]{9}**",
                        "**vat_system**",
                        "**app**"
                      ]
                    }
                  ],
                  "**policies**",
                  [
                    "**subscription**",
                    {
                      "$ul": [
                        "**access_level**",
                        "**id**",
                        "**payment_card**",
                        "**payment_failed_count**",
                        "**period_ending_date**",
                        "**period_remaining_days**",
                        "**period_starting_date**",
                        "**plan_color**",
                        "**plan_name**",
                        "**status**",
                        "**stripe_customer_id**",
                        "**stripe_plan_id**",
                        "**stripe_subscription_id**"
                      ]
                    }
                  ],
                  "**url**: URI to this app. Can be overwrited by hostname_alias param"
                ]
              }
            ],
            "**id**",
            [
              "**profile**",
              {
                "$ul": [
                  "**description**",
                  "**homepage**",
                  "**name**",
                  "**restricted**",
                  "**rights**: Routes list",
                  "**visible**"
                ]
              }
            ],
            [
              "**recipient_user**",
              {
                "$ul": [
                  "**civility**: Civility is the lastname prefix",
                  "**establishments**",
                  "**firstname**",
                  "**id**",
                  "**image**",
                  "**lastname**",
                  "**metadata**",
                  "**created_at**",
                  "**email**: Email used as login",
                  "**last_access_at**",
                  "**password**",
                  "**password_is_undefined**",
                  "**updated_at**"
                ]
              }
            ],
            [
              "**sender_user**",
              {
                "$ul": [
                  "**civility**: Civility is the lastname prefix",
                  "**establishments**",
                  "**firstname**",
                  "**id**",
                  "**image**",
                  "**lastname**",
                  "**metadata**",
                  "**created_at**",
                  "**email**: Email used as login",
                  "**last_access_at**",
                  "**password**",
                  "**password_is_undefined**",
                  "**updated_at**"
                ]
              }
            ],
            "**used_at**",
            "**validity**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
