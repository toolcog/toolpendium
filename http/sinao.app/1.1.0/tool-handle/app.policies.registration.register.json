{
  "name": "app_policies_registration_register",
  "description": "Registration for an invited user",
  "parameters": {
    "type": "object",
    "properties": {
      "accessToken": {
        "type": "string"
      },
      "password": {
        "minimum": 8,
        "type": "string",
        "description": "Password for account"
      },
      "firstname": {
        "type": "string",
        "description": "First name of user before account creation / account link"
      },
      "lastname": {
        "type": "string",
        "description": "Last name of user before account creation / account link"
      },
      "cgu": {
        "type": "boolean",
        "description": "User has valided CGU ?"
      }
    },
    "required": [
      "accessToken",
      "password",
      "cgu"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.sinao.app/v1/apps/access/invite/{accessToken}/register{?password,firstname,lastname,cgu}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**civility**: Civility is the lastname prefix",
            "**establishments**",
            "**firstname**",
            "**id**",
            "**image**",
            "**lastname**",
            "**metadata**",
            "**created_at**",
            "**email**: Email used as login",
            "**last_access_at**",
            "**password**",
            "**password_is_undefined**",
            "**updated_at**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
