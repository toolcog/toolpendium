{
  "name": "auth_changepassword",
  "description": "Change password by email forgotten_password_token",
  "parameters": {
    "type": "object",
    "properties": {
      "forgotten_password_token": {
        "type": "string"
      },
      "password": {
        "minimum": 8,
        "type": "string",
        "description": "Password for account"
      }
    },
    "required": [
      "forgotten_password_token",
      "password"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.sinao.app/v1/changepassword{?forgotten_password_token,password}"
    }
  },
  "responses": {}
}
