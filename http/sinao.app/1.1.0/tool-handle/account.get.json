{
  "name": "account_get",
  "description": "Get current logged user, as `User` object",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.sinao.app/v1/me"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**civility**: Civility is the lastname prefix",
            "**establishments**",
            "**firstname**",
            "**id**",
            "**image**",
            "**lastname**",
            "**metadata**",
            "**created_at**",
            "**email**: Email used as login",
            "**last_access_at**",
            "**password**",
            "**password_is_undefined**",
            "**updated_at**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
