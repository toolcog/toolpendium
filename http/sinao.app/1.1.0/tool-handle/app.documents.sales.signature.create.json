{
  "name": "app_documents_sales_signature_create",
  "description": "Create a new signature",
  "parameters": {
    "type": "object",
    "properties": {
      "appId": {
        "type": "integer",
        "description": "The identifier of the Sinao application you are using. Each application corresponds to a company and an accounting. A user can have several Sinao applications. To retrieve the identifiers of its applications, you must call the endpoint `/apps`."
      },
      "documentId": {
        "type": "integer"
      },
      "email": {
        "type": "string"
      },
      "firstname": {
        "type": "string"
      },
      "lastname": {
        "type": "string"
      },
      "phone": {
        "type": "string"
      }
    },
    "required": [
      "appId",
      "documentId",
      "email",
      "firstname",
      "lastname",
      "phone"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.sinao.app/v1/apps/{appId}/signature{?documentId,email,firstname,lastname,phone}"
    }
  },
  "responses": {}
}
