      },
      "id": {
        "type": "integer",
        "description": "Invoice id to finalize"
      },
      "status": {
        "enum": [
          "waiting",
          "goodforagreement",
          "refused",
          "transformed",
          "deleted"
        ],
        "type": "string",
        "description": "Status to update"
      }
    },
    "required": [
      "appId",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.sinao.app/v1/apps/{appId}/quotes/{id}/updatestatus{?status}"
    }
  },
  "responses": {}
