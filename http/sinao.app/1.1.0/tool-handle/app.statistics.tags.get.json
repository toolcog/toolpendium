{
  "name": "app_statistics_tags_get",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "appId": {
        "type": "integer",
        "description": "The identifier of the Sinao application you are using. Each application corresponds to a company and an accounting. A user can have several Sinao applications. To retrieve the identifiers of its applications, you must call the endpoint `/apps`."
      },
      "object": {
        "enum": [
          "invoice",
          "quote",
          "purchase",
          "product"
        ],
        "type": "string"
      }
    },
    "required": [
      "appId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.sinao.app/v1/apps/{appId}/tags{?object}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
