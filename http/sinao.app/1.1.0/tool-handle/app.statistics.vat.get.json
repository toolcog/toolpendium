{
  "name": "app_statistics_vat_get",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "appId": {
        "type": "integer",
        "description": "The identifier of the Sinao application you are using. Each application corresponds to a company and an accounting. A user can have several Sinao applications. To retrieve the identifiers of its applications, you must call the endpoint `/apps`."
      },
      "detailed": {
        "default": false,
        "type": "boolean"
      },
      "start_at": {
        "default": "now",
        "format": "date-time",
        "type": "string"
      },
      "period": {
        "enum": [
          "monthly",
          "quarterly",
          "half-yearly",
          "yearly"
        ],
        "type": "string"
      },
      "end_at": {
        "format": "date-time",
        "type": "string"
      }
    },
    "required": [
      "appId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.sinao.app/v1/apps/{appId}/statistics/vat{?detailed,start_at,period,end_at}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
