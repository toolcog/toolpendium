{
  "name": "Get_User_store_recommendations_",
  "description": "Get user store recommendations",
  "parameters": {
    "type": "object",
    "properties": {
      "identity_cookie": {
        "format": "double",
        "type": "number",
        "description": "The BBC-id cookie value"
      }
    },
    "required": [
      "identity_cookie"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ibl.api.bbci.co.uk/ibl/v1/user/recommendations{?identity_cookie}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
