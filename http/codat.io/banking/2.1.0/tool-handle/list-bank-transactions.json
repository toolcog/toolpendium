{
  "name": "list-bank-transactions",
  "description": "Gets a list of transactions incurred by a company across all bank accounts.",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": 1,
        "example": 1,
        "format": "int32",
        "minimum": 0,
        "type": "integer",
        "description": "Page number. [Read more](https://docs.codat.io/using-the-api/paging)."
      },
      "pageSize": {
        "default": 100,
        "example": 100,
        "format": "int32",
        "maximum": 5000,
        "minimum": 1,
        "type": "integer",
        "description": "Number of records to return in a page. [Read more](https://docs.codat.io/using-the-api/paging)."
      },
      "query": {
        "type": "string",
        "description": "Codat query string. [Read more](https://docs.codat.io/using-the-api/querying)."
      },
      "orderBy": {
        "example": "-modifiedDate",
        "type": "string",
        "description": "Field to order results by. [Read more](https://docs.codat.io/using-the-api/ordering-results)."
      },
      "companyId": {
        "description": "Unique identifier for your SMB in Codat.",
        "example": "8a210b68-6988-11ed-a1eb-0242ac120002",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "page",
      "companyId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.codat.io/companies/{companyId}/data/banking-transactions{?page,pageSize,query,orderBy}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**results**",
            [
              "**_links**",
              {
                "$ul": [
                  [
                    "**current**",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**next**",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**previous**",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ],
                  [
                    "**self**",
                    {
                      "$ul": [
                        "**href**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**pageNumber**",
            "**pageSize**",
            "**totalResults**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
