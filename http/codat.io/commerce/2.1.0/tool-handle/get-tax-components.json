{
  "name": "get-tax-components",
  "description": "This endpoint returns a lits of tax rates from the commerce platform, including tax rate names and values. This supports the mapping of tax rates from the commerce platform to the accounting platform.",
  "parameters": {
    "type": "object",
    "properties": {
      "companyId": {
        "description": "Unique identifier for your SMB in Codat.",
        "example": "8a210b68-6988-11ed-a1eb-0242ac120002",
        "format": "uuid",
        "type": "string"
      },
      "connectionId": {
        "description": "Unique identifier for a company's data connection.",
        "example": "2e9d2c44-f675-40ba-8049-353bfcb5e171",
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "companyId",
      "connectionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.codat.io/companies/{companyId}/connections/{connectionId}/data/commerce-taxComponents"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**taxComponents**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
