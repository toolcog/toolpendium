{
  "name": "get-create-bank-account-model",
  "description": "Gets the options of pushing bank account transactions.",
  "parameters": {
    "type": "object",
    "properties": {
      "companyId": {
        "description": "Unique identifier for your SMB in Codat.",
        "example": "8a210b68-6988-11ed-a1eb-0242ac120002",
        "format": "uuid",
        "type": "string"
      },
      "connectionId": {
        "description": "Unique identifier for a company's data connection.",
        "example": "2e9d2c44-f675-40ba-8049-353bfcb5e171",
        "format": "uuid",
        "type": "string"
      },
      "accountId": {
        "example": "8a210b68-6988-11ed-a1eb-0242ac120002",
        "format": "uuid",
        "type": "string",
        "description": "Unique identifier for an account"
      }
    },
    "required": [
      "companyId",
      "connectionId",
      "accountId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.codat.io/companies/{companyId}/connections/{connectionId}/options/bankAccounts/{accountId}/bankTransactions"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Push Option"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**description**",
            "**displayName**",
            "**options**",
            "**properties**",
            "**required**",
            "**type**",
            [
              "**validation**",
              {
                "$ul": [
                  "**information**",
                  "**warnings**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
