{
  "name": "Lookup",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "description": "Allowed values are \"cnam\", \"format\", \"hlr\" and \"mnp\"."
      },
      "number": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "The phone number to look up."
      },
      "json": {
        "type": "string",
        "description": "Determines whether the response shall be returned in JSON format. Does not work with type \"format\"."
      }
    },
    "required": [
      "type",
      "number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://gateway.seven.io/api/lookup{?type,number,json}"
    }
  },
  "responses": {}
}
