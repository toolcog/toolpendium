{
  "name": "Analytics",
  "parameters": {
    "type": "object",
    "properties": {
      "start": {
        "type": "string",
        "description": "Start date of the statistics in the format YYYY-MM-DD. By default, the date of 30 days ago is set."
      },
      "end": {
        "type": "string",
        "description": "End date of the statistics in the format YYYY-MM-DD. By default, the current day."
      },
      "label": {
        "type": "string",
        "description": "Shows only data of a specific label."
      },
      "subaccounts": {
        "type": "string",
        "description": "Receive the data only for the main account, all your (sub-)accounts or only for specific subaccounts."
      },
      "group_by": {
        "enum": [
          "date",
          "label",
          "subaccount",
          "country"
        ],
        "type": "string",
        "description": "Defines the grouping of the data."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://gateway.seven.io/api/analytics{?start,end,label,subaccounts,group_by}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**date**",
            "**direct**",
            "**economy**",
            "**hlr**",
            "**inbound**",
            "**mnp**",
            "**usage_eur**",
            "**voice**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
