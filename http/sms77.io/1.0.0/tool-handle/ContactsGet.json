{
  "name": "ContactsGet",
  "parameters": {
    "type": "object",
    "properties": {
      "action": {
        "enum": [
          "read"
        ],
        "type": "string",
        "description": "Determines the action to execute."
      },
      "json": {
        "default": 0,
        "enum": [
          0,
          1
        ],
        "type": "number",
        "description": "Defines whether to return the response as JSON or CSV separated by semicolon."
      }
    },
    "required": [
      "action"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://gateway.seven.io/api/contacts{?action,json}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
