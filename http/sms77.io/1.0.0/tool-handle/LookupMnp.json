{
  "name": "LookupMnp",
  "parameters": {
    "type": "object",
    "properties": {
      "number": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "The phone number to look up."
      },
      "json": {
        "type": "string",
        "description": "Determines whether the response shall be returned in JSON format."
      }
    },
    "required": [
      "number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://gateway.seven.io/api/lookup/mnp{?number,json}"
    }
  },
  "responses": {}
}
