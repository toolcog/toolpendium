{
  "name": "getLicensee",
  "description": "Return a Licensee by 'licenseeNumber'",
  "parameters": {
    "type": "object",
    "properties": {
      "licenseeNumber": {
        "type": "string",
        "description": "Unique number (across all Products of a Vendor) that identifies the Licensee. Vendor can assign this number when creating a Licensee or let NetLicensing generate one. Read-only after creation of the first License for the Licensee."
      }
    },
    "required": [
      "licenseeNumber"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://go.netlicensing.io/core/v2/rest/licensee/{licenseeNumber}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**infos**",
            "**items**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
