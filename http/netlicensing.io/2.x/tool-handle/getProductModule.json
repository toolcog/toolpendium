{
  "name": "getProductModule",
  "description": "Return a Product Module by 'productModuleNumber'",
  "parameters": {
    "type": "object",
    "properties": {
      "productModuleNumber": {
        "type": "string",
        "description": "Unique number (across all Products of a Vendor) that identifies the Product Module. Vendor can assign this number when creating a Product Module or let NetLicensing generate one. Read-only after creation of the first Licensee for the Product."
      }
    },
    "required": [
      "productModuleNumber"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://go.netlicensing.io/core/v2/rest/productmodule/{productModuleNumber}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**infos**",
            "**items**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
