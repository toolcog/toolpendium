{
  "name": "transferLicenses",
  "description": "Licenses transfer between Licensees",
  "parameters": {
    "type": "object",
    "properties": {
      "licenseeNumber": {
        "type": "string",
        "description": "Licensee number with a maximum length of 1000 characters"
      },
      "body": {
        "properties": {
          "sourceLicenseeNumber": {
            "description": "Licensee number which Licenses to be transferred",
            "type": "string"
          }
        },
        "required": [
          "sourceLicenseeNumber"
        ],
        "type": "object"
      }
    },
    "required": [
      "licenseeNumber",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://go.netlicensing.io/core/v2/rest/licensee/{licenseeNumber}/transfer"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**infos**",
            "**items**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
