{
  "name": "deleteProduct",
  "description": "Delete a Product by 'number'",
  "parameters": {
    "type": "object",
    "properties": {
      "productNumber": {
        "type": "string",
        "description": "Unique number that identifies the Product."
      },
      "forceCascade": {
        "type": "boolean",
        "description": "Force object deletion and all descendants."
      }
    },
    "required": [
      "productNumber"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://go.netlicensing.io/core/v2/rest/product/{productNumber}{?forceCascade}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**infos**",
            "**items**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
