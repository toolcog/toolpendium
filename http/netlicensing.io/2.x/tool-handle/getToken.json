  "name": "getToken",
  "description": "Return a token by 'tokenNumber'",
  "parameters": {
    "type": "object",
    "properties": {
      "tokenNumber": {
        "type": "string",
        "description": "Token number"
      }
    },
    "required": [
      "tokenNumber"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://go.netlicensing.io/core/v2/rest/token/{tokenNumber}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**infos**",
            "**items**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
