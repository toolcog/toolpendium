{
  "name": "updatePaymentMethod",
  "description": "Sets the provided properties to a Payment Method. Return an updated Payment Method",
  "parameters": {
    "type": "object",
    "properties": {
      "paymentMethodNumber": {
        "type": "string",
        "description": "Payment method number"
      },
      "body": {
        "properties": {
          "active": {
            "description": "If set to 'false', the Payment Method is disabled.",
            "type": "boolean"
          },
          "paypal.subject": {
            "description": "The e-mail address of the PayPal account for which you are making the API calls.",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "paymentMethodNumber"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://go.netlicensing.io/core/v2/rest/paymentmethod/{paymentMethodNumber}"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**infos**",
            "**items**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
