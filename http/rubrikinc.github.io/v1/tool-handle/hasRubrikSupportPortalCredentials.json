{
  "name": "hasRubrikSupportPortalCredentials",
  "description": "Check whether the specified Rubrik cluster has an existing set of credentials for the Rubrik support portal.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "default": "me",
        "type": "string",
        "description": "The ID of a Rubrik cluster, or use *me* for the Rubrik cluster that is hosting the current session.\n"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/cluster/{id}/rubrik_support_portal_credentials"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**value**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
