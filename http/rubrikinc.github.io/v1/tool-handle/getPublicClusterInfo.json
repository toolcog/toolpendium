{
  "name": "getPublicClusterInfo",
  "description": "Retrieve public information about the Rubrik cluster.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "default": "me",
        "type": "string",
        "description": "ID of the Rubrik cluster or *me* for self."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/cluster/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**acceptedEulaVersion**: Version of the EULA accepted by admin.",
            "**apiVersion**: REST API version.",
            [
              "**geolocation**",
              {
                "$ul": [
                  "**address**: Address information for mapping the location of the Rubrik cluster. This"
                ]
              }
            ],
            "**id**: ID of the Rubrik cluster.",
            "**latestEulaVersion**: Latest version of the EULA that must be accepted by admin.",
            "**name**: Name of the cluster.",
            [
              "**timezone**",
              {
                "$ul": [
                  "**timezone**: Cluster time zone."
                ]
              }
            ],
            "**version**: Rubrik cluster software version."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
