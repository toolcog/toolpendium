{
  "name": "countMssqlDbV1",
  "description": "Returns a count of Microsoft SQL databases.",
  "parameters": {
    "type": "object",
    "properties": {
      "root_id": {
        "type": "string",
        "description": "Include only instances that belong to this root."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/mssql/db/count{?root_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**numDoNotProtect**",
            "**numNoSla**",
            "**numProtected**",
            "**numTotal**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
