{
  "name": "queryPasswordRequirements",
  "description": "Query user password requirements for a cluster.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "default": "me",
        "type": "string",
        "description": "ID of the Rubrik cluster or *me* for self."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/cluster/{id}/security/password_requirements"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**blockPreviousPasswords**",
            "**clearApiTokens**",
            "**clearWebSessions**",
            "**maxLength**",
            "**minLength**",
            "**minLowerCase**",
            "**minNumerics**",
            "**minSpecial**",
            "**minUpperCase**",
            "**useZxcvbn**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
