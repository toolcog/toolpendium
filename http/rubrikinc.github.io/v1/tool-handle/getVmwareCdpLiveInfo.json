{
  "name": "getVmwareCdpLiveInfo",
  "description": "Returns the live CDP info for a set of CDP-enabled virtual machines",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "items": {
          "description": "ID of the CDP-enabled virtual machine.",
          "type": "string"
        },
        "type": "array",
        "description": "The ID of each CDP-enabled virtual machine for which live info is being retrieved."
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/vmware/vm/cdp"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**responses**: The live CDP info for the virtual machines."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
