{
  "name": "manualDiscoverIpv4",
  "description": "Manually specifies discovery data. This endpoint output is identical to the output of the 'discover' endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "default": "me",
        "type": "string",
        "description": "ID of the Rubrik cluster, or *me* for the current cluster."
      },
      "body": {
        "$ref": "#/components/schemas/ManualDiscoveryNodeIpv4Info",
        "description": "Manual discovery input data."
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/cluster/{id}/manual_discover_ipv4"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**nodeInfo**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
