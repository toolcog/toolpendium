{
  "name": "queryMssqlInstance",
  "description": "Returns a list of summary information for Microsoft SQL instances.",
  "parameters": {
    "type": "object",
    "properties": {
      "root_id": {
        "type": "string",
        "description": "Include only instances that belong to this root."
      },
      "primary_cluster_id": {
        "type": "string",
        "description": "Limits the instances returned within one cluster specified by primary_cluster_id."
      },
      "snappable_status": {
        "enum": [
          "Protectable"
        ],
        "type": "string",
        "description": "Determines whether SQL Server instances are fetched with additional privilege checks."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/mssql/instance{?root_id,primary_cluster_id,snappable_status}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**: List of matching objects.",
            "**hasMore**: If there is more.",
            "**total**: Total list responses."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
