{
  "name": "queryMssqlHostConfig",
  "description": "Returns a list of customized SQL Server host configurations.",
  "parameters": {
    "type": "object",
    "properties": {
      "host_id": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "IDs of the hosts."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/mssql/host/configuration{?host_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**: List of matching objects.",
            "**hasMore**: If there is more.",
            "**total**: Total list responses."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
