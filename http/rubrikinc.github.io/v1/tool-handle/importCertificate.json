{
  "name": "importCertificate",
  "description": "Import a certificate.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/CertificateImportRequest",
        "description": "Request to import a certificate."
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/certificate"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**certId**: ID of the certificate.",
            "**description**: User-friendly description for the certificate.",
            "**expiration**: The expiration date for the certificate.",
            "**hasKey**: A Boolean value that specifies whether or not the certificate is associated with a stored private key. When this value is 'true,' the private key for the certificate is stored. When this value is 'false,' the private key for the certificate is not stored.",
            "**name**: Display name for the certificate.",
            "**pemFile**: The certificates, in PEM format.",
            "**usedBy**: A list of components using the certificate."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
