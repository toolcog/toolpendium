{
  "name": "getTotpGlobalSetting",
  "description": "Returns TOTP global setting, including whether TOTP is enforced or not.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "default": "me",
        "type": "string",
        "description": "ID assigned to a Rubrik cluster. Use *me* to refer to the Rubrik cluster that is hosting the current API session."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/cluster/{id}/security/totp/setting"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**isEnforced**: Indicates whether the TOTP is enforced globally or not."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
