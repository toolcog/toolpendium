{
  "name": "markAgentSecondaryCertificate",
  "description": "Mark a secondary cluster certificate to be asynchronously synced to all Rubrik Backup Service instances for which this cluster is the primary.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "type": "string",
        "description": "ID of certificate to add."
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/certificate/agent"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**certId**: ID of the certificate.",
            "**clusterUuid**: Parsed cluster ID from the certificate.",
            "**isAgentEnabled**: Whether this certificate has been marked for use by agents.",
            "**name**: Display name for the certificate."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
