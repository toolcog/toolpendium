{
  "name": "unsetTruststoreCertificate",
  "description": "Remove certificate associated with specified truststore.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "default": "me",
        "type": "string",
        "description": "ID of the Rubrik cluster or *me* for self."
      },
      "truststores": {
        "items": {
          "enum": [
            "System",
            "Gcp"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "Comma separated list of truststore types."
      }
    },
    "required": [
      "id",
      "truststores"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/cluster/{id}/security/truststore{?truststores}"
    }
  },
  "responses": {}
}
