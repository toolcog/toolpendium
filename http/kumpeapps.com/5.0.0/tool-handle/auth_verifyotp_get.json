{
  "name": "auth_verifyotp_get",
  "description": "Verifies YubiKey OTP for authenticated user",
  "parameters": {
    "type": "object",
    "properties": {
      "otp": {
        "type": "string",
        "description": "YubiKey OTP code"
      }
    },
    "required": [
      "otp"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/authentication/verifyotp{?otp}"
    }
  },
  "responses": {
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Access Denied",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
