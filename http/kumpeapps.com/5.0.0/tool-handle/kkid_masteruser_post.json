{
  "name": "kkid_masteruser_post",
  "description": "By passing in the appropriate variables this method creates a new user with master account access. (The use of this method is restricted to Superusers ONLY)\n",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "username of user to create"
      },
      "password": {
        "format": "password",
        "type": "string",
        "description": "password of user to create"
      },
      "email": {
        "type": "string",
        "description": "email address of user to create"
      },
      "firstName": {
        "type": "string",
        "description": "First Name of user to create"
      },
      "lastName": {
        "type": "string",
        "description": "Last Name of user to create"
      }
    },
    "required": [
      "username",
      "password",
      "email",
      "firstName",
      "lastName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/kkid/masteruser{?username,password,email,firstName,lastName}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "add user",
        "**Key properties:**",
        {
          "$ul": [
            "**added**",
            "**aff_added**",
            "**aff_id**",
            "**aff_payout_type**",
            "**avatar**",
            "**city**",
            "**comment**",
            "**country**",
            "**disable_lock_until**",
            "**email**",
            "**i_agree**",
            "**is_affiliate**",
            "**is_locked**",
            "**lang**",
            "**last_login**",
            "**login**",
            "**name_f**",
            "**name_l**",
            "**pass**",
            "**pass_dattm**",
            "**phone**",
            "**pin**",
            "**plain_password**",
            "**remember_key**",
            "**remote_addr**",
            "**require_consent**",
            "**reseller_id**",
            "**saved_form_id**",
            "**state**",
            "**status**",
            "**street**",
            "**street2**",
            "**subusers_parent_id**",
            "**tax_id**",
            "**unsubscribed**",
            "**user_agent**",
            "**user_id**",
            "**zip**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "405": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "API Access Denied",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
