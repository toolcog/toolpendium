{
  "name": "auth_authkey_patch",
  "description": "Mark user auth key as compromised",
  "parameters": {
    "type": "object",
    "properties": {
      "auth_key": {
        "format": "password",
        "type": "string",
        "description": "auth key to mark as compromised"
      },
      "comments": {
        "type": "string",
        "description": "Comments (like how was this compromised)"
      }
    },
    "required": [
      "auth_key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/authentication/authkey{?auth_key,comments}"
    }
  },
  "responses": {
    "202": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
