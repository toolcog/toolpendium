{
  "name": "kkid_userlist_delete",
  "description": "By passing in the appropriate variables this method deletes the specified user. (This function is restricted to Superusers ONLY)\n",
  "parameters": {
    "type": "object",
    "properties": {
      "userID": {
        "type": "integer",
        "description": "userID of the user you wish to delete"
      }
    },
    "required": [
      "userID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/kkid/userlist{?userID}"
    }
  },
  "responses": {
    "405": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "API Access Denied",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "412": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "API Key invalid",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
