{
  "name": "appkey_post",
  "description": "Request a new app key by passing username and password for app account",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "Username assigned to your app"
      },
      "password": {
        "format": "password",
        "type": "string",
        "description": "Password assigned to your app"
      },
      "supportsYubikey": {
        "type": "boolean",
        "description": "App supports YubiKey OTP"
      }
    },
    "required": [
      "username",
      "password",
      "supportsYubikey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/appkey{?username,password,supportsYubikey}"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**app_key**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
