{
  "name": "kkid_wishlist_get",
  "description": "Get list of wishlist items",
  "parameters": {
    "type": "object",
    "properties": {
      "kidUserId": {
        "type": "integer",
        "description": "userID of the kid"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/kkid/wishlist{?kidUserId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Allowance",
        "**Key properties:**",
        {
          "$ul": [
            "**success**",
            "**wish**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
