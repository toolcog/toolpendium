{
  "name": "kkid_userlist_put",
  "description": "By passing in the appropriate variables this method updates the user's profile\n",
  "parameters": {
    "type": "object",
    "properties": {
      "userID": {
        "type": "integer",
        "description": "userID of the user you wish to update"
      },
      "username": {
        "type": "string",
        "description": "username of user to create"
      },
      "email": {
        "type": "string",
        "description": "email address of user to create"
      },
      "firstName": {
        "type": "string",
        "description": "First Name of user to create"
      },
      "lastName": {
        "type": "string",
        "description": "Last Name of user to create"
      },
      "emoji": {
        "type": "string",
        "description": "emoji character for user"
      },
      "tmdbKey": {
        "type": "string",
        "description": "User's TMdB Session Key"
      },
      "enableWishList": {
        "type": "boolean",
        "description": "set status of Wish List module enabled"
      },
      "enableChores": {
        "type": "boolean",
        "description": "set status of chores module enabled"
      },
      "enableAllowance": {
        "type": "boolean",
        "description": "set status of allowance module enabled"
      },
      "enableAdmin": {
        "type": "boolean",
        "description": "set status of isAdmin"
      },
      "enableTmdb": {
        "type": "boolean",
        "description": "set status of enableTmdb (movie and tv search)"
      },
      "enableObjectDetection": {
        "type": "boolean",
        "description": "set status of enableObjectDetection"
      }
    },
    "required": [
      "userID",
      "username",
      "email",
      "firstName",
      "lastName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/kkid/userlist{?userID,username,email,firstName,lastName,emoji,tmdbKey,enableWishList,enableChores,enableAllowance,enableAdmin,enableTmdb,enableObjectDetection}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "add user",
        "**Key properties:**",
        {
          "$ul": [
            "**added**",
            "**aff_added**",
            "**aff_id**",
            "**aff_payout_type**",
            "**avatar**",
            "**city**",
            "**comment**",
            "**country**",
            "**disable_lock_until**",
            "**email**",
            "**i_agree**",
            "**is_affiliate**",
            "**is_locked**",
            "**lang**",
            "**last_login**",
            "**login**",
            "**name_f**",
            "**name_l**",
            "**pass**",
            "**pass_dattm**",
            "**phone**",
            "**pin**",
            "**plain_password**",
            "**remember_key**",
            "**remote_addr**",
            "**require_consent**",
            "**reseller_id**",
            "**saved_form_id**",
            "**state**",
            "**status**",
            "**street**",
            "**street2**",
            "**subusers_parent_id**",
            "**tax_id**",
            "**unsubscribed**",
            "**user_agent**",
            "**user_id**",
            "**zip**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "405": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "API Access Denied",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "412": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "API Key invalid",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
