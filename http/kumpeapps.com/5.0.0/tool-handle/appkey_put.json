{
  "name": "appkey_put",
  "description": "Pass your app key to deactivate the key",
  "parameters": {
    "type": "object",
    "properties": {
      "app_key": {
        "type": "string",
        "description": "app key to deactivate"
      }
    },
    "required": [
      "app_key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/appkey{?app_key}"
    }
  },
  "responses": {
    "202": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
