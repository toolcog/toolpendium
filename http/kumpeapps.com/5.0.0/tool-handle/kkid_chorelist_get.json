{
  "name": "kkid_chorelist_get",
  "description": "By passing in the appropriate options, you can search for\nchores assigned to a given user within the authenticated user's master account\n",
  "parameters": {
    "type": "object",
    "properties": {
      "kidUsername": {
        "type": "string",
        "description": "Username of kid you wish to search"
      },
      "day": {
        "enum": [
          "Sunday",
          "Monday",
          "Tuesday",
          "Wednesday",
          "Thursday",
          "Friday",
          "Saturday",
          "Weekly"
        ],
        "type": "string",
        "description": "Day of week for chores (Weekly for weekly chores)"
      },
      "status": {
        "type": "string",
        "description": "Status of Chore to search"
      },
      "blockDash": {
        "type": "boolean",
        "description": "Filter results by blockDash parameter"
      },
      "optional": {
        "type": "boolean",
        "description": "Filter results by optional parameter"
      },
      "canSteal": {
        "type": "boolean",
        "description": "Filter results by canSteal parameter"
      },
      "includeCalendar": {
        "type": "boolean",
        "description": "include calendar notations (default is false)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/kkid/chorelist{?kidUsername,day,status,blockDash,optional,canSteal,includeCalendar}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "chorelist",
        "**Key properties:**",
        {
          "$ul": [
            "**chore**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "206": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "nodata",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "405": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "API Access Denied",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "412": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "API Key invalid",
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
