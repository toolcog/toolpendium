{
  "name": "kkid_share_get",
  "description": "Create share link",
  "parameters": {
    "type": "object",
    "properties": {
      "linkUserId": {
        "type": "string",
        "description": "User ID that the link should be authenticated to"
      },
      "link": {
        "enum": [
          "https://khome.kumpeapps.com/portal/wish-list.php",
          "https://khome.kumpeapps.com/portal/chores-today.php"
        ],
        "type": "string",
        "description": "Link to share"
      },
      "scope": {
        "enum": [
          "WishList",
          "WishListAdmin",
          "Chores",
          "ChoresAdmin"
        ],
        "type": "string",
        "description": "Authentication scope for link"
      },
      "scope2": {
        "enum": [
          "WishList",
          "WishListAdmin",
          "Chores",
          "ChoresAdmin"
        ],
        "type": "string",
        "description": "Authentication scope for link"
      },
      "scope3": {
        "enum": [
          "WishList",
          "WishListAdmin",
          "Chores",
          "ChoresAdmin"
        ],
        "type": "string",
        "description": "Authentication scope for link"
      },
      "scope4": {
        "enum": [
          "WishList",
          "WishListAdmin",
          "Chores",
          "ChoresAdmin"
        ],
        "type": "string",
        "description": "Authentication scope for link"
      }
    },
    "required": [
      "linkUserId",
      "link",
      "scope"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://restapi.kumpeapps.com/v5/kkid/share{?linkUserId,link,scope,scope2,scope3,scope4}"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**auth_link**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
