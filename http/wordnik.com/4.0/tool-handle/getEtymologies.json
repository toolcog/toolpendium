{
  "name": "getEtymologies",
  "description": "Fetches etymology data",
  "parameters": {
    "type": "object",
    "properties": {
      "word": {
        "type": "string",
        "description": "Word to return"
      },
      "useCanonical": {
        "default": "false",
        "enum": [
          "false",
          "true"
        ],
        "type": "string",
        "description": "If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested."
      }
    },
    "required": [
      "word"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.wordnik.com/v4/word.json/{word}/etymologies{?useCanonical}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
