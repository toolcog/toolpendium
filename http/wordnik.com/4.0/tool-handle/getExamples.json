{
  "name": "getExamples",
  "description": "Returns examples for a word",
  "parameters": {
    "type": "object",
    "properties": {
      "word": {
        "type": "string",
        "description": "Word to return examples for"
      },
      "includeDuplicates": {
        "default": "false",
        "enum": [
          "false",
          "true"
        ],
        "type": "string",
        "description": "Show duplicate examples from different sources"
      },
      "useCanonical": {
        "default": "false",
        "enum": [
          "false",
          "true"
        ],
        "type": "string",
        "description": "If true will try to return the correct word root ('cats' -> 'cat'). If false returns exactly what was requested."
      },
      "skip": {
        "default": 0,
        "format": "int32",
        "type": "integer",
        "description": "Results to skip"
      },
      "limit": {
        "default": 5,
        "format": "int32",
        "type": "integer",
        "description": "Maximum number of results to return"
      }
    },
    "required": [
      "word"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.wordnik.com/v4/word.json/{word}/examples{?includeDuplicates,useCanonical,skip,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**examples**",
            "**facets**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
