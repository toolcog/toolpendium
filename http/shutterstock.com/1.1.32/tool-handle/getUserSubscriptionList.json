{
  "name": "getUserSubscriptionList",
  "description": "List user subscriptions",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/user/subscriptions"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of subscriptions",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Subscriptions retrieved from this user",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Optional error message",
            "**page**: Current page that is being queried",
            "**per_page**: Amount of subscriptions to show per page",
            "**total_count**: Total number of subscriptions for this user"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
