{
  "name": "licenseEditorialImage",
  "description": "Deprecated; use `POST /v2/editorial/images/licenses` instead to get licenses for one or more editorial images. You must specify the country and one or more editorial images to license. The download links in the response are valid for 8 hours.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/LicenseEditorialContentRequest",
        "description": "License editorial content"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/editorial/licenses"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of editorial license results",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: License results",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Optional error message",
            "**page**: Current page of the response",
            "**per_page**: Number of results per page",
            "**total_count**: Total count of all results"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
