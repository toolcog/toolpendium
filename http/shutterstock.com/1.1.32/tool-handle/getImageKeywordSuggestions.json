{
  "name": "getImageKeywordSuggestions",
  "description": "This endpoint returns up to 10 important keywords from a block of plain text.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/SearchEntitiesRequest",
        "description": "Plain text to extract keywords from"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/images/search/suggestions"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The response to a request for keyword analysis",
        "**Key properties:**",
        {
          "$ul": [
            "**keywords**: The top keywords from the submitted text"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
