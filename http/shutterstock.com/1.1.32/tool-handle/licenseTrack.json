{
  "name": "licenseTrack",
  "description": "This endpoint gets licenses for one or more tracks. The download links in the response are valid for 8 hours.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "enum": [
          "audio_platform",
          "premier_music_basic",
          "premier_music_extended",
          "premier_music_pro",
          "premier_music_comp",
          "asset_all_music"
        ],
        "type": "string",
        "description": "License type"
      },
      "search_id": {
        "type": "string",
        "description": "The ID of the search that led to licensing this track"
      },
      "body": {
        "$ref": "#/components/schemas/LicenseAudioRequest",
        "description": "Tracks to license"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/audio/licenses{?license,search_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of audio license results",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: License results",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Server-generated message, if any",
            "**page**: Current page that is returned",
            "**per_page**: Number of results per page",
            "**total_count**: Total count of all results across all pages"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
