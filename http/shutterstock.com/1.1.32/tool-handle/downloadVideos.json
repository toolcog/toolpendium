{
  "name": "downloadVideos",
  "description": "This endpoint redownloads videos that you have already received a license for.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "The license ID of the item to (re)download. The download links in the response are valid for 8 hours."
      },
      "body": {
        "$ref": "#/components/schemas/RedownloadVideo",
        "description": "Information about the videos to redownload"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/videos/licenses/{id}/downloads"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "URL object",
        "**Key properties:**",
        {
          "$ul": [
            "**url**: URL that can be used to download the unwatermarked, licensed asset"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
