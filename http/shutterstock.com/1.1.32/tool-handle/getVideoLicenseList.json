{
  "name": "getVideoLicenseList",
  "description": "This endpoint lists existing licenses.",
  "parameters": {
    "type": "object",
    "properties": {
      "video_id": {
        "pattern": "^[1-9]\\d*$",
        "type": "string",
        "x-pattern-description": "A number that does not start with 0",
        "description": "Show licenses for the specified video ID"
      },
      "license": {
        "pattern": "^.+$",
        "type": "string",
        "x-pattern-description": "Minimum 1 character",
        "description": "Show videos that are available with the specified license, such as `standard` or `enhanced`; prepending a `-` sign excludes results from that license"
      },
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "Page number"
      },
      "per_page": {
        "default": 20,
        "maximum": 200,
        "minimum": 1,
        "type": "integer",
        "description": "Number of results per page"
      },
      "sort": {
        "default": "newest",
        "enum": [
          "newest",
          "oldest"
        ],
        "type": "string",
        "description": "Sort by oldest or newest videos first"
      },
      "username": {
        "type": "string",
        "description": "Filter licenses by username of licensee"
      },
      "start_date": {
        "format": "date-time",
        "type": "string",
        "description": "Show licenses created on or after the specified date"
      },
      "end_date": {
        "format": "date-time",
        "type": "string",
        "description": "Show licenses created before the specified date"
      },
      "download_availability": {
        "default": "all",
        "enum": [
          "all",
          "downloadable",
          "non_downloadable"
        ],
        "type": "string",
        "description": "Filter licenses by download availability"
      },
      "team_history": {
        "default": false,
        "type": "boolean",
        "description": "Set to true to see license history for all members of your team."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/videos/licenses{?video_id,license,page,per_page,sort,username,start_date,end_date,download_availability,team_history}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of download events",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Download events",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Server-generated message, if any",
            "**page**: The current page of results",
            "**per_page**: The number of results per page",
            "**total_count**: The total number of results across all pages"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
