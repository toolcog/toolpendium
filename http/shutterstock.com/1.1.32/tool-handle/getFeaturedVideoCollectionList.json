{
  "name": "getFeaturedVideoCollectionList",
  "description": "This endpoint lists featured video collections and a name and cover video for each collection.",
  "parameters": {
    "type": "object",
    "properties": {
      "embed": {
        "enum": [
          "share_url"
        ],
        "type": "string",
        "description": "What information to include in the response, such as a URL to the collection"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/videos/collections/featured{?embed}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of featured collections",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Featured collections",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Server-generated message, if any",
            "**page**: Current page that is returned",
            "**per_page**: Number of results per page",
            "**total_count**: Total count of all results across all pages"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
