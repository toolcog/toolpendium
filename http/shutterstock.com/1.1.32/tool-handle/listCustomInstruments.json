{
  "name": "listCustomInstruments",
  "description": "This endpoint lists the instruments that you can include in computer audio. If you specify more than one search parameter, the API uses an AND condition.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Show instruments with the specified ID"
      },
      "per_page": {
        "default": 20,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Number of results per page"
      },
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "Page number"
      },
      "name": {
        "type": "string",
        "description": "Show instruments with the specified name (case-sensitive)"
      },
      "tag": {
        "type": "string",
        "description": "Show instruments with the specified tag, such as Percussion or Strings (case-sensitive)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/ai/audio/instruments{?id,per_page,page,name,tag}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Image search results",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: List of instrumnets",
            "**page**: Current page that is returned",
            "**per_page**: Number of results per page"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
