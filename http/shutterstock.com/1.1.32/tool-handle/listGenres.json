{
  "name": "listGenres",
  "description": "This endpoint returns a list of all audio genres.",
  "parameters": {
    "type": "object",
    "properties": {
      "language": {
        "type": "string",
        "description": "Which language the genres will be returned"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/audio/genres{?language}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of audio genres",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: List of genres"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
