{
  "name": "deleteImageCollectionItems",
  "description": "This endpoint removes one or more images from a collection.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Collection ID"
      },
      "item_id": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "One or more image IDs to remove from the collection"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/images/collections/{id}/items{?item_id}"
    }
  },
  "responses": {}
}
