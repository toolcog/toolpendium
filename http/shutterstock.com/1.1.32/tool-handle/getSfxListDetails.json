{
  "name": "getSfxListDetails",
  "description": "This endpoint shows information about sound effects.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "items": {
          "format": "asset-id",
          "type": "string"
        },
        "maxItems": 500,
        "type": "array",
        "description": "One or more sound effect IDs"
      },
      "view": {
        "default": "minimal",
        "enum": [
          "minimal",
          "full"
        ],
        "type": "string",
        "description": "Amount of detail to render in the response"
      },
      "language": {
        "$ref": "#/components/schemas/Language",
        "description": "Language for the keywords and categories in the response"
      },
      "library": {
        "enum": [
          "shutterstock",
          "premier",
          "premiumbeat"
        ],
        "type": "string",
        "description": "Which library to fetch from"
      },
      "search_id": {
        "type": "string",
        "description": "The ID of the search that is related to this request"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/sfx{?id,view,language,library,search_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of sound effects",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Sound Effects"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
