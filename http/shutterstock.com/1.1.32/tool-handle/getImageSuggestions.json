{
  "name": "getImageSuggestions",
  "description": "This endpoint provides autocomplete suggestions for partial search terms.",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "type": "string",
        "description": "Search term for which you want keyword suggestions"
      },
      "limit": {
        "default": 10,
        "maximum": 25,
        "minimum": 1,
        "type": "integer",
        "description": "Limit the number of suggestions"
      }
    },
    "required": [
      "query"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/images/search/suggestions{?query,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of search suggestions",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Search suggestions"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
