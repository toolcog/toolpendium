{
  "name": "validate",
  "description": "Validate input",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "Integer ID"
      },
      "tag": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "List of tags"
      },
      "user-agent": {
        "type": "string",
        "description": "User agent"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/test/validate{?id,tag}"
    },
    "headers": {
      "user-agent": {
        "$": "user-agent"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Validation results",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**header**: Validation results",
              {
                "$ul": [
                  "**user-agent**: User agent to expect in the response"
                ]
              }
            ],
            [
              "**query**: Validation results",
              {
                "$ul": [
                  "**id**: Integer ID that was passed in the request",
                  "**tag**: List of tags that were passed in the request"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
