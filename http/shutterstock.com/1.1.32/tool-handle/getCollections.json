{
  "name": "getCollections",
  "description": "This endpoint returns a list of catalog collections.",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "Page number"
      },
      "per_page": {
        "default": 20,
        "maximum": 50,
        "minimum": 2,
        "type": "integer",
        "description": "Number of results per page"
      },
      "sort": {
        "default": "newest",
        "enum": [
          "newest",
          "oldest"
        ],
        "type": "string",
        "description": "Sort by"
      },
      "shared": {
        "default": false,
        "type": "boolean",
        "description": "Set to true to omit collections that you own and return only collections  that are shared with you"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/catalog/collections{?page,per_page,sort,shared}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of catalog collections",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: List of catalog collections",
            "**page**",
            "**per_page**",
            "**total_count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
