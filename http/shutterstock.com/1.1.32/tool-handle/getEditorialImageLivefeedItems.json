{
  "name": "getEditorialImageLivefeedItems",
  "description": "Get editorial livefeed items",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Editorial livefeed ID; must be an URI encoded string"
      },
      "country": {
        "format": "country-code-3",
        "type": "string",
        "description": "Returns only if the livefeed items are available for distribution in a certain country"
      }
    },
    "required": [
      "id",
      "country"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/editorial/images/livefeeds/{id}/items{?country}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of editorial items",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Editorial items",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Optional error message",
            "**page**: Current page of the response",
            "**per_page**: Number of results per page",
            "**total_count**: Total count of all results"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
