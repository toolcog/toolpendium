{
  "name": "getImageCollectionList",
  "description": "This endpoint lists your collections of images and their basic attributes.",
  "parameters": {
    "type": "object",
    "properties": {
      "embed": {
        "items": {
          "enum": [
            "share_code",
            "share_url"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "Which sharing information to include in the response, such as a URL to the collection"
      },
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "Page number"
      },
      "per_page": {
        "default": 100,
        "maximum": 150,
        "minimum": 1,
        "type": "integer",
        "description": "Number of results per page"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/images/collections{?embed,page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of collections",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Collections",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Server-generated message, if any",
            "**page**: The current page of results",
            "**per_page**: The number of results per page",
            "**total_count**: The total number of results across all pages"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
