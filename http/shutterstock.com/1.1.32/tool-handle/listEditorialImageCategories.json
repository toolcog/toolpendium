{
  "name": "listEditorialImageCategories",
  "description": "This endpoint lists the categories that editorial images can belong to, which are separate from the categories that other types of assets can belong to.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/editorial/images/categories"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of editorial categories",
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
