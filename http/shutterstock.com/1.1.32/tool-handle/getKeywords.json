{
  "name": "getKeywords",
  "description": "This endpoint returns a list of suggested keywords for a media item that you specify or upload.",
  "parameters": {
    "type": "object",
    "properties": {
      "asset_id": {
        "oneOf": [
          {
            "format": "asset-upload-id",
            "type": "string"
          },
          {
            "format": "asset-id",
            "type": "string"
          }
        ],
        "description": "The asset ID or upload ID to suggest keywords for"
      }
    },
    "required": [
      "asset_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/cv/keywords{?asset_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of keywords",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Keywords",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Server-generated message, if any"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
