{
  "name": "getContributorList",
  "description": "This endpoint lists information about one or more contributors, including contributor type, equipment they use and other attributes.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "One or more contributor IDs"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/contributors{?id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of contributor profiles",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Conributor profiles",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Error message",
            "**page**: Page of response",
            "**per_page**: Number of contributors per page",
            "**total_count**: Total count of contributors for this request"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
