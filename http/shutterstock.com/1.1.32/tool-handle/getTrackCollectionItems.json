{
  "name": "getTrackCollectionItems",
  "description": "This endpoint lists the IDs of tracks in a collection and the date that each was added.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Collection ID"
      },
      "page": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "Page number"
      },
      "per_page": {
        "default": 100,
        "maximum": 150,
        "minimum": 1,
        "type": "integer",
        "description": "Number of results per page"
      },
      "share_code": {
        "type": "string",
        "description": "Code to retrieve the contents of a shared collection"
      },
      "sort": {
        "default": "oldest",
        "enum": [
          "newest",
          "oldest"
        ],
        "type": "string",
        "description": "Sort order"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/audio/collections/{id}/items{?page,per_page,share_code,sort}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of items in a collection",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Assets in the collection",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Server-generated message, if any",
            "**page**: The current page of results",
            "**per_page**: The number of results per page",
            "**total_count**: The total number of results across all pages"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
