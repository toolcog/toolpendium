{
  "name": "getFeaturedImageCollectionList",
  "description": "This endpoint lists featured collections of specific types and a name and cover image for each collection.",
  "parameters": {
    "type": "object",
    "properties": {
      "embed": {
        "enum": [
          "share_url"
        ],
        "type": "string",
        "description": "Which sharing information to include in the response, such as a URL to the collection"
      },
      "type": {
        "items": {
          "enum": [
            "photo",
            "editorial",
            "vector"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "The types of collections to return"
      },
      "asset_hint": {
        "default": "1x",
        "enum": [
          "1x",
          "2x"
        ],
        "type": "string",
        "description": "Cover image size"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/images/collections/featured{?embed,type,asset_hint}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List of featured collections",
        "**Key properties:**",
        {
          "$ul": [
            "**data**: Featured collections",
            "**errors**: Error list; appears only if there was an error",
            "**message**: Server-generated message, if any",
            "**page**: Current page that is returned",
            "**per_page**: Number of results per page",
            "**total_count**: Total count of all results across all pages"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
