{
  "name": "fetchRenders",
  "description": "This endpoint shows the status of one or more audio renders, including download links for completed audio.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "One or more render IDs"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/ai/audio/renders{?id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Audio render data",
        "**Key properties:**",
        {
          "$ul": [
            "**audio_renders**: Audio render results"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
