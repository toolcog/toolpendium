{
  "name": "echo",
  "description": "Echo text",
  "parameters": {
    "type": "object",
    "properties": {
      "text": {
        "default": "ok",
        "type": "string",
        "description": "Text to echo"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/test{?text}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Text to echo in the response",
        "**Key properties:**",
        {
          "$ul": [
            "**text**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
