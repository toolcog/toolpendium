{
  "name": "downloadTracks",
  "description": "This endpoint redownloads tracks that you have already received a license for. The download links in the response are valid for 8 hours.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "License ID"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.shutterstock.com/v2/audio/licenses/{id}/downloads"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Audio License URL object",
        "**Key properties:**",
        {
          "$ul": [
            "**shorts_loops_stems**: URL that can be used to download the .zip file containing shorts, loops, and stems",
            "**url**: URL that can be used to download the unwatermarked, licensed asset"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
