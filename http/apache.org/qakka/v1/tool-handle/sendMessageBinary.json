{
  "name": "sendMessageBinary",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "queueName": {
        "type": "string",
        "description": "Name of Queue"
      },
      "regions": {
        "type": "string",
        "description": "Regions to which message is to be sent"
      },
      "delay": {
        "type": "string"
      },
      "expiration": {
        "type": "string"
      },
      "contentType": {
        "type": "string",
        "description": "Content type of the data to be sent with Queue Message"
      }
    },
    "required": [
      "queueName",
      "contentType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://apache.local/queues/{queueName}/messages{?regions,delay,expiration,contentType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Response returned by most Queue API calls.",
        "**Key properties:**",
        {
          "$ul": [
            "**count**: Count of Queues or QueueMessages returned by the call.",
            "**message**: Informative message intended for client.",
            "**queueMessages**: Queues Messages returned by the call, or empty if not applicable.",
            "**queues**: Queues returned but the call, or empty if not applicable."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
