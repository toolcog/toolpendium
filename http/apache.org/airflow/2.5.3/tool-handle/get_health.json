{
  "name": "get_health",
  "description": "Get the status of Airflow's metadatabase and scheduler. It includes info about\nmetadatabase and last heartbeat of scheduler.\n",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/health"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Instance status information.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**metadatabase**: The status of the metadatabase.",
              {
                "$ul": [
                  "**status**: Health status"
                ]
              }
            ],
            [
              "**scheduler**: The status and the latest scheduler heartbeat.",
              {
                "$ul": [
                  "**latest_scheduler_heartbeat**: The time the scheduler last do a heartbeat.",
                  "**status**: Health status"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
