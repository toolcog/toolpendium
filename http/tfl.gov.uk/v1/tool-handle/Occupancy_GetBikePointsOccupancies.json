{
  "name": "Occupancy_GetBikePointsOccupancies",
  "description": "Get the occupancy for bike points.",
  "parameters": {
    "type": "object",
    "properties": {
      "ids": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": ""
      }
    },
    "required": [
      "ids"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.digital.tfl.gov.uk/Occupancy/BikePoints/{ids}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Bike point occupancy",
        "**Key properties:**",
        {
          "$ul": [
            "**bikesCount**: Total bike counts",
            "**eBikesCount**: Total ebikes count",
            "**emptyDocks**: Empty docks",
            "**id**: Id of the bike point such as BikePoints_1",
            "**name**: Name / Common name of the bike point",
            "**standardBikesCount**: Total standard bikes count",
            "**totalDocks**: Total docks available"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
