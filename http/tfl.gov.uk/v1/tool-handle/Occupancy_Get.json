{
  "name": "Occupancy_Get",
  "description": "Gets the occupancy for a car park with a given id",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.digital.tfl.gov.uk/Occupancy/CarPark/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**bays**",
            "**carParkDetailsUrl**",
            "**id**",
            "**name**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
