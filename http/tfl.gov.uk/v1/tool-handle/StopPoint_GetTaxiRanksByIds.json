{
  "name": "StopPoint_GetTaxiRanksByIds",
  "description": "Gets a list of taxi ranks corresponding to the given stop point id.",
  "parameters": {
    "type": "object",
    "properties": {
      "stopPointId": {
        "type": "string",
        "description": "stopPointId is required to get the taxi ranks."
      }
    },
    "required": [
      "stopPointId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.digital.tfl.gov.uk/StopPoint/{stopPointId}/TaxiRanks"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**additionalProperties**: A bag of additional key/value pairs with extra information about this place.",
            "**children**",
            "**childrenUrls**",
            "**commonName**: A human readable name.",
            "**distance**: The distance of the place from its search point, if this is the result\r",
            "**id**: A unique identifier.",
            "**lat**: WGS84 latitude of the location.",
            "**lon**: WGS84 longitude of the location.",
            "**placeType**: The type of Place. See /Place/Meta/placeTypes for possible values.",
            "**url**: The unique location of this resource."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
