{
  "name": "Line_Timetable",
  "description": "Gets the timetable for a specified station on the give line",
  "parameters": {
    "type": "object",
    "properties": {
      "fromStopPointId": {
        "type": "string",
        "description": "The originating station's stop point id (station naptan code e.g. 940GZZLUASL, you can use /StopPoint/Search/{query} endpoint to find a stop point id from a station name)"
      },
      "id": {
        "type": "string",
        "description": "A single line id e.g. victoria"
      }
    },
    "required": [
      "fromStopPointId",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.digital.tfl.gov.uk/Line/{id}/Timetable/{fromStopPointId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**direction**",
            [
              "**disambiguation**",
              {
                "$ul": [
                  "**disambiguationOptions**"
                ]
              }
            ],
            "**lineId**",
            "**lineName**",
            "**pdfUrl**",
            "**stations**",
            "**statusErrorMessage**",
            "**stops**",
            [
              "**timetable**",
              {
                "$ul": [
                  "**departureStopId**",
                  "**routes**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
