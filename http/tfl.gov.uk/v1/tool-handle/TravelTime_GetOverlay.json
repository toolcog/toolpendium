{
  "name": "TravelTime_GetOverlay",
  "description": "Gets the TravelTime overlay.",
  "parameters": {
    "type": "object",
    "properties": {
      "z": {
        "format": "int32",
        "type": "integer",
        "description": "The zoom level."
      },
      "pinLat": {
        "format": "double",
        "type": "number",
        "description": "The latitude of the pin."
      },
      "pinLon": {
        "format": "double",
        "type": "number",
        "description": "The longitude of the pin."
      },
      "mapCenterLat": {
        "format": "double",
        "type": "number",
        "description": "The map center latitude."
      },
      "mapCenterLon": {
        "format": "double",
        "type": "number",
        "description": "The map center longitude."
      },
      "scenarioTitle": {
        "type": "string",
        "description": "The title of the scenario."
      },
      "timeOfDayId": {
        "type": "string",
        "description": "The id for the time of day (AM/INTER/PM)"
      },
      "modeId": {
        "type": "string",
        "description": "The id of the mode."
      },
      "width": {
        "format": "int32",
        "type": "integer",
        "description": "The width of the requested overlay."
      },
      "height": {
        "format": "int32",
        "type": "integer",
        "description": "The height of the requested overlay."
      },
      "direction": {
        "enum": [
          "Average",
          "From",
          "To"
        ],
        "type": "string",
        "description": "The direction of travel."
      },
      "travelTimeInterval": {
        "format": "int32",
        "type": "integer",
        "description": "The total minutes between the travel time bands"
      }
    },
    "required": [
      "z",
      "pinLat",
      "pinLon",
      "mapCenterLat",
      "mapCenterLon",
      "scenarioTitle",
      "timeOfDayId",
      "modeId",
      "width",
      "height",
      "direction",
      "travelTimeInterval"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.digital.tfl.gov.uk/TravelTimes/overlay/{z}/mapcenter/{mapCenterLat}/{mapCenterLon}/pinlocation/{pinLat}/{pinLon}/dimensions/{width}/{height}{?scenarioTitle,timeOfDayId,modeId,direction,travelTimeInterval}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
