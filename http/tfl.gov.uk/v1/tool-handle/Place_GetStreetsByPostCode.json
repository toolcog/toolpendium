{
  "name": "Place_GetStreetsByPostCode",
  "description": "Gets the set of streets associated with a post code.",
  "parameters": {
    "type": "object",
    "properties": {
      "postcode": {
        "type": "string"
      },
      "postcodeInput.postcode": {
        "pattern": "^(([gG][iI][rR] {0,}0[aA]{2})|((([a-pr-uwyzA-PR-UWYZ][a-hk-yA-HK-Y]?[0-9][0-9]?)|(([a-pr-uwyzA-PR-UWYZ][0-9][a-hjkstuwA-HJKSTUW])|([a-pr-uwyzA-PR-UWYZ][a-hk-yA-HK-Y][0-9][abehmnprv-yABEHMNPRV-Y]))) {0,}[0-9][abd-hjlnp-uw-zABD-HJLNP-UW-Z]{2}))$",
        "type": "string"
      },
      "Postcode": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "postcode",
      "Postcode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.digital.tfl.gov.uk/Place/Address/Streets/{Postcode}{?postcode,postcodeInput.postcode}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
