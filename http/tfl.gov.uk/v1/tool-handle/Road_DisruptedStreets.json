{
  "name": "Road_DisruptedStreets",
  "description": "Gets a list of disrupted streets. If no date filters are provided, current disruptions are returned.",
  "parameters": {
    "type": "object",
    "properties": {
      "startDate": {
        "format": "date-time",
        "type": "string",
        "description": "Optional, the start time to filter on."
      },
      "endDate": {
        "format": "date-time",
        "type": "string",
        "description": "Optional, The end time to filter on."
      }
    },
    "required": [
      "startDate",
      "endDate"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.digital.tfl.gov.uk/Road/all/Street/Disruption{?startDate,endDate}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
