{
  "name": "getemailinfo",
  "description": "Get email validation information and other demographics for an email address.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "email": {
        "type": "string",
        "description": "Email address to retrieve validation information"
      }
    },
    "required": [
      "license",
      "email"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getemailinfo{?license,email}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Code**",
            "**Credits**",
            "**Email**",
            "**Geolocation**",
            "**Info**",
            "**IsDisposable**",
            "**IsEducational**",
            "**IsGeneric**",
            "**IsGovernment**",
            "**IsOrganizational**",
            "**IsVulgar**",
            "**Response**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
