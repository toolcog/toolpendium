{
  "name": "getweather",
  "description": "Use city and state to retrieve current US weather information.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "city": {
        "type": "string",
        "description": "City for weather information"
      },
      "state": {
        "type": "string",
        "description": "State for weather information"
      }
    },
    "required": [
      "license",
      "city",
      "state"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getweather{?license,city,state}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**City**",
            "**Code**",
            "**Credits**",
            "**RelativeHumidity**",
            "**State**",
            "**TempC**",
            "**TempF**",
            "**VisibilityMiles**",
            "**Weather**",
            "**WindDir**",
            "**WindMPH**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
