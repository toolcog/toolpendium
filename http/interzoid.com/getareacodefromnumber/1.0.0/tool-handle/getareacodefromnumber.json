{
  "name": "getareacodefromnumber",
  "description": "The area code will be parsed out of a telephone number and used to retrieve information about the area code.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "number": {
        "type": "string",
        "description": "Telephone number to retrieve area code information"
      }
    },
    "required": [
      "license",
      "number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getareacodefromnumber{?license,number}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Abbreviation**",
            "**AreaCode**",
            "**Code**",
            "**Credits**",
            "**Locale**",
            "**PrimaryCity**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
