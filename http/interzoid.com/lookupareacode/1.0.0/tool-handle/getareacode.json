{
  "name": "getareacode",
  "description": "Gets telephone area code information for a given area code.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "areacode": {
        "type": "string",
        "description": "Telephone area code to retrieve area code information"
      }
    },
    "required": [
      "license",
      "areacode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getareacode{?license,areacode}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Abbreviation**",
            "**AreaCode**",
            "**Code**",
            "**Credits**",
            "**Locale**",
            "**PrimaryCity**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
