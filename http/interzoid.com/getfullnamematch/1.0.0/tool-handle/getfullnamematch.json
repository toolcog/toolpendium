{
  "name": "getfullnamematch",
  "description": "Gets a similarity key for matching purposes for full name data, where first and last name are part of the same field.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "fullname": {
        "type": "string",
        "description": "Full name from which to generate similarity key"
      }
    },
    "required": [
      "license",
      "fullname"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getfullnamematch{?license,fullname}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Code**",
            "**Credits**",
            "**Simkey**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
