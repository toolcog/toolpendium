{
  "name": "globalpageload",
  "description": "Gets page load performance from a specified geography\n",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "origin": {
        "type": "string",
        "description": "Geographic location to perform the measurement from (Paris, Hong Kong, Seoul, Mumbai, Sao Paolo, London, etc. see API home page for full list)"
      },
      "url": {
        "type": "string",
        "description": "specific URL to perform load test time"
      }
    },
    "required": [
      "license",
      "origin",
      "url"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/globalpageload{?license,origin,url}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Code**",
            "**Contents**",
            "**Credits**",
            "**Origin**",
            "**PageResponseStatus**",
            "**Seconds**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
