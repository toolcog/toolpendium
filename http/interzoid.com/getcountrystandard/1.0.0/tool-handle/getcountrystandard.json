{
  "name": "getcountrystandard",
  "description": "Gets a pre-selected country name standard for various permutations of a given country name.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "country": {
        "type": "string",
        "description": "Country name from which to retrieve the standardized version"
      }
    },
    "required": [
      "license",
      "country"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getcountrystandard{?license,country}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Code**",
            "**CountryStandard**",
            "**Credits**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
