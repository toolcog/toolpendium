{
  "name": "getstateabbreviation",
  "description": "Gets a two-letter abbreviation for a state or province given a permutation of state or province data.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "state": {
        "type": "string",
        "description": "State (or province) name from which to retrieve the two letter abbreviation."
      }
    },
    "required": [
      "license",
      "state"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getstateabbreviation{?license,state}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Abbreviation**",
            "**Code**",
            "**Credits**",
            "**State**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
