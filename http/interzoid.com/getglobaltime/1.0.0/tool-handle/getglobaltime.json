{
  "name": "getglobaltime",
  "description": "Gets the current time for a global locale (city, state, region, or country such as Chicago, London, Paris, Seoul, Spain, Buenos Aires, Hawaii, Moscow, Tokyo, Hanoi, etc.)",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "locale": {
        "type": "string",
        "description": "Geographic locale to get the current time for"
      }
    },
    "required": [
      "license",
      "locale"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getglobaltime{?license,locale}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ClockTime**",
            "**Code**",
            "**Credits**",
            "**CurrentDate**",
            "**CurrentTime**",
            "**Day**",
            "**Hour**",
            "**Locale**",
            "**Minute**",
            "**Month**",
            "**MonthNumber**",
            "**Second**",
            "**TimeStamp**",
            "**Weekday**",
            "**Year**",
            "**Zone**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
