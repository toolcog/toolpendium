{
  "name": "getzipcodeinfo",
  "description": "For a given zip code, detailed information is returned, including city, state, latitude, longitude, area size, and various population demographics, including income, age, and presence of farming data.",
  "parameters": {
    "type": "object",
    "properties": {
      "license": {
        "type": "string",
        "description": "Your Interzoid license API key. Register at www.interzoid.com/register"
      },
      "zip": {
        "type": "string",
        "description": "Zip code to retrieve detailed information"
      }
    },
    "required": [
      "license",
      "zip"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.interzoid.com/getzipcodeinfo{?license,zip}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**AreaSquareMiles**",
            "**City**",
            "**Code**",
            "**Credits**",
            "**ElderlyHouseholdPercent**",
            "**FarmerHouseholdPercent**",
            "**IncomePercent100k**",
            "**IncomePercent200k**",
            "**IncomePercent50k**",
            "**Latitude**",
            "**Longitude**",
            "**MarriedHouseholdPercent**",
            "**Population**",
            "**State**",
            "**ZipCode**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
