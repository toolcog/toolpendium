{
  "name": "getChargeStationConnectors",
  "description": "List connectors for a chargestation",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "chargeStation id"
      },
      "include_evse": {
        "type": "boolean",
        "description": "Populate evse"
      },
      "include_organization": {
        "type": "boolean",
        "description": "Populate organization"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
