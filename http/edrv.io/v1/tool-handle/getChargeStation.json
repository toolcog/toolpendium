{
  "name": "getChargeStation",
  "description": "Get a single charge station's data",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "The charge station id that needs to be fetched"
      },
      "include_location": {
        "type": "boolean",
        "description": "Populate location"
      },
      "include_evses": {
        "type": "boolean",
        "description": "Populate evses"
      },
      "include_organization": {
        "type": "boolean",
        "description": "Populate organization"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//api.edrv.io/v1/chargestations/{id}{?include_location,include_evses,include_organization}"
    }
  },
  "responses": {}
}
