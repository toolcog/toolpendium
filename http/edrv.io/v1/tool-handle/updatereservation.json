{
  "name": "updatereservation",
  "description": "Use to request a update an existing reservation. The request will wait for the charge station to process the command. It will timeout after 60 seconds.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID of the reservation that needs to be fetched"
      },
      "body": {
        "properties": {
          "connector": {
            "minimum": 1,
            "type": "integer"
          },
          "driver": {
            "type": "string"
          },
          "endDate": {
            "type": "string"
          },
          "evse": {
            "minimum": 1,
            "type": "integer"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//api.edrv.io/v1/reservations/{id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**ok**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
