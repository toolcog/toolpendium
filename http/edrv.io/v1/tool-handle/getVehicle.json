{
  "name": "getVehicle",
  "description": "Get a vehicle's data",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "The vehicule id that needs to be fetched"
      },
      "include_driver": {
        "type": "boolean",
        "description": "Populate driver"
      },
      "include_token": {
        "type": "boolean",
        "description": "Populate token"
      },
      "include_organization": {
        "type": "boolean",
        "description": "Populate organization"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//api.edrv.io/v1/vehicles/{id}{?include_driver,include_token,include_organization}"
    }
  },
  "responses": {}
}
