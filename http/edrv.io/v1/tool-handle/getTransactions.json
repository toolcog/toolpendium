{
  "name": "getTransactions",
  "description": "Get a list of transactions",
  "parameters": {
    "type": "object",
    "properties": {
      "status": {
        "enum": [
          "Started",
          "Ended"
        ],
        "type": "string",
        "description": "Started to get only active transactions"
      },
      "paginate_limit": {
        "default": 20,
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "Number of results per page"
      },
      "paginate_page": {
        "type": "string",
        "description": "The queried page index"
      },
      "paginate_enabled": {
        "default": true,
        "type": "boolean",
        "description": "Enable pagination"
      },
      "sort_by": {
        "default": "createdAt",
        "type": "string",
        "description": "Sort data by this key"
      },
      "sort_order": {
        "default": "desc",
        "enum": [
          "desc",
          "asc"
        ],
        "type": "string",
        "description": "asc to sort ascending (default is desc - descending)"
      },
      "createdAt[$gte]": {
        "format": "date-time",
        "type": "string",
        "description": "Date as ISO String"
      },
      "createdAt[$lte]": {
        "format": "date-time",
        "type": "string",
        "description": "Date as ISO String"
      },
      "updatedAt[$gte]": {
        "format": "date-time",
        "type": "string",
        "description": "Date as ISO String"
      },
      "updatedAt[$lte]": {
        "format": "date-time",
        "type": "string",
        "description": "Date as ISO String"
      },
      "include_chargestation": {
        "type": "boolean",
        "description": "Populate chargestation"
      },
      "include_evse": {
        "type": "boolean",
        "description": "Populate evse"
      },
      "include_connector": {
        "type": "boolean",
        "description": "Populate connector"
      },
      "include_driver": {
        "type": "boolean",
        "description": "Populate driver"
      },
      "include_token": {
        "type": "boolean",
        "description": "Populate token"
      },
      "include_reservation": {
        "type": "boolean",
        "description": "Populate reservation"
      },
      "include_organization": {
        "type": "boolean",
        "description": "Populate organization"
      },
      "include_rate": {
        "type": "boolean",
        "description": "Populate rate"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//api.edrv.io/v1/transactions{?status,paginate_limit,paginate_page,paginate_enabled,sort_by,sort_order,createdAt[$gte],createdAt[$lte],updatedAt[$gte],updatedAt[$lte],include_chargestation,include_evse,include_connector,include_driver,include_token,include_reservation,include_organization,include_rate}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**hasNext**",
            "**hasPrevious**",
            "**message**",
            "**ok**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
