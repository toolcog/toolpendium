{
  "name": "patchToken",
  "description": "Update a token",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID of token that needs to be updated"
      },
      "body": {
        "properties": {
          "active": {
            "default": true,
            "type": "boolean"
          },
          "channel": {
            "enum": [
              "physical",
              "slack",
              "telegram",
              "sms"
            ],
            "type": "string"
          },
          "driver": {
            "type": "string"
          },
          "physicalId": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object",
        "description": "Include token properties to create here"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//api.edrv.io/v1/tokens/{id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**ok**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
