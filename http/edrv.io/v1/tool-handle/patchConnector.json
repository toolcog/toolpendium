{
  "name": "patchConnector",
  "description": "Update a connector's data",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID of connector that needs to be updated"
      },
      "body": {
        "properties": {
          "chargestation": {
            "type": "string"
          },
          "format": {
            "type": "string"
          },
          "power": {
            "type": "integer"
          },
          "power_type": {
            "type": "string"
          },
          "rate": {
            "type": "string"
          },
          "type": {
            "type": "string"
          }
        },
        "type": "object",
        "description": "Include connector properties to update here"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//api.edrv.io/v1/connectors/{id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**ok**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
