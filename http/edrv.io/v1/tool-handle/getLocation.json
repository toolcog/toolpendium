{
  "name": "getLocation",
  "description": "Get a location's data",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "The location id that needs to be fetched"
      },
      "include_chargestations": {
        "type": "boolean",
        "description": "Populate chargestations"
      },
      "include_organization": {
        "type": "boolean",
        "description": "Populate organization"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//api.edrv.io/v1/location/{id}{?include_chargestations,include_organization}"
    }
  },
  "responses": {}
}
