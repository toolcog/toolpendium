{
  "name": "getConnector",
  "description": "Get a connector",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID of connector that needs to be fetched"
      },
      "include_evse": {
        "type": "boolean",
        "description": "Populate evse"
      },
      "include_organization": {
        "type": "boolean",
        "description": "Populate organization"
      },
      "include_rate": {
        "type": "boolean",
        "description": "Populate rate"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//api.edrv.io/v1/connectors/{id}{?include_evse,include_organization,include_rate}"
    }
  },
  "responses": {}
}
