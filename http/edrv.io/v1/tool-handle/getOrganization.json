{
  "name": "getOrganization",
  "description": "Get one organization's data by id",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID of organization that needs to be fetched"
      },
      "include_locations": {
        "type": "boolean",
        "description": "Populate locations"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//api.edrv.io/v1/organizations/{id}{?include_locations}"
    }
  },
  "responses": {}
}
