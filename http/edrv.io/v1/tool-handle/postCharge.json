{
  "name": "postCharge",
  "description": "Change charge",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "The vehicle id that needs to be fetched"
      },
      "body": {
        "properties": {
          "action": {
            "enum": [
              "START",
              "STOP"
            ],
            "type": "string"
          }
        },
        "required": [
          "action"
        ],
        "type": "object",
        "description": "Include command properties to send here"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//api.edrv.io/v1/vehicles/{id}/charge"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**ok**",
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
