{
  "name": "getReservation",
  "description": "Get one reservation data",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID of the reservation that needs to be fetched"
      },
      "include_chargestation": {
        "type": "boolean",
        "description": "Populate chargestation"
      },
      "include_organization": {
        "type": "boolean",
        "description": "Populate organization"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//api.edrv.io/v1/reservations/{id}{?include_chargestation,include_organization}"
    }
  },
  "responses": {}
}
