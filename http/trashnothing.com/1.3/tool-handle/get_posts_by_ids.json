{
  "name": "get_posts_by_ids",
  "description": "Retrieve multiple posts",
  "parameters": {
    "type": "object",
    "properties": {
      "post_ids": {
        "type": "string",
        "description": "A comma separated list of the post IDs. If more than 10 post IDs are passed, only the first 10 posts will be returned.\n"
      }
    },
    "required": [
      "post_ids"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://trashnothing.com/api/v1.3/posts/multiple{?post_ids}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**forbidden**: The IDs of posts that are forbidden for the current user.",
            "**not_found**: The IDs of posts that weren't found (may have been deleted or never existed).",
            "**posts**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
