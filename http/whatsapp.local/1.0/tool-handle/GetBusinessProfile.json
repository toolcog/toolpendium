{
  "name": "GetBusinessProfile",
  "description": "Get-Business-Profile",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://whatsapp.local/settings/business/profile"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Response"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**errors**: Only returned with a failed request. Contains an array of error objects that are present when there is an error. ",
            [
              "**meta**: Contains generic information such as WhatsApp Business API Client version.",
              {
                "$ul": [
                  "**api_status**",
                  "**version**"
                ]
              }
            ],
            [
              "**settings**: ",
              {
                "$ul": [
                  [
                    "**business**: ",
                    {
                      "$ul": [
                        [
                          "**profile**",
                          {
                            "$ul": [
                              "**address**: Address of the business",
                              "**description**: Description of the business",
                              "**email**: Email address to contact the business",
                              "**vertical**: Industry of the business",
                              "**websites**: URLs associated with business (e.g., website, Facebook page, Instagram)"
                            ]
                          }
                        ]
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
