{
  "name": "Customers_GetQuotes",
  "description": "Returns a list of Customer's quotes.",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "format": "int64",
        "type": "integer",
        "description": "Id of Customer to return quotes."
      }
    },
    "required": [
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.bigredcloud.com/api/v1/customers/{itemId}/quotes"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**acCode**",
            "**closedDate**",
            "**comments**",
            "**companyId**",
            "**customFields**",
            "**customerOwnerId**",
            "**customerOwnerName**",
            "**ddNumber**",
            "**deliveryList**",
            "**deliveryTo**",
            "**entryDate**",
            "**id**",
            "**layoutType**",
            "**note**",
            "**poNumber**",
            "**procDate**",
            "**productTrans**",
            "**reference**",
            "**saleInvoiceId**",
            "**saleRepCode**",
            "**saleRepId**",
            "**timeStamp**",
            "**total**",
            "**totalNet**",
            "**totalVat**",
            "**vatTypeId**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
