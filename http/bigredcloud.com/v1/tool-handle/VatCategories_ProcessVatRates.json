{
  "name": "VatCategories_ProcessVatRates",
  "description": "Process Vat Rates",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "items": {
          "$ref": "#/$defs/VatRatesByVatCategoryDto"
        },
        "type": "array",
        "description": "Array of Vat Rates."
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "VatRatesByVatCategoryDto": {
        "example": {
          "vatCategoryId": 1,
          "vatRates": [
            {
              "id": 0,
              "isActive": true,
              "isDefault": false,
              "orderIndex": 0,
              "percentage": 30,
              "vatCategoryId": 1
            }
          ]
        },
        "properties": {
          "vatCategoryId": {
            "format": "int64",
            "type": "integer"
          },
          "vatRates": {
            "items": {
              "$ref": "#/$defs/VatRateDto"
            },
            "type": "array"
          }
        },
        "type": "object"
      },
      "VatRateDto": {
        "properties": {
          "id": {
            "format": "int64",
            "type": "integer"
          },
          "isActive": {
            "type": "boolean"
          },
          "isDefault": {
            "type": "boolean"
          },
          "orderIndex": {
            "format": "int32",
            "type": "integer"
          },
          "percentage": {
            "format": "double",
            "type": "number"
          },
          "timestamp": {
            "format": "byte",
            "type": "string"
          },
          "vatCategoryId": {
            "format": "int64",
            "type": "integer"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://app.bigredcloud.com/api/v1/vatCategories/vatRates"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
