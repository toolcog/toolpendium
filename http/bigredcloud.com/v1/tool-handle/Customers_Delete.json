{
  "name": "Customers_Delete",
  "description": "Removes an existing Customer.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "Id of Customer to remove."
      },
      "timestamp": {
        "type": "string",
        "description": "Timestamp of Customer to remove. Should be encoded in Base64."
      }
    },
    "required": [
      "id",
      "timestamp"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://app.bigredcloud.com/api/v1/customers/{id}{?timestamp}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
