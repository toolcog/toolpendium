{
  "name": "Unflag",
  "description": "Removes previously created user or message flag\n\nRequired permissions:\n- FlagMessage\n- FlagUser\n",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/FlagRequest"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "FlagRequest": {
        "nullable": true,
        "properties": {
          "target_message_id": {
            "description": "ID of the message when reporting a message",
            "title": "Target Message ID",
            "type": "string",
            "x-stream-index": "001"
          },
          "target_user_id": {
            "description": "ID of the user when reporting a user",
            "title": "Target User ID",
            "type": "string",
            "x-stream-index": "002"
          },
          "user": {
            "$ref": "#/$defs/UserObjectRequest"
          },
          "user_id": {
            "type": "string",
            "x-stream-index": "003.001"
          }
        },
        "type": "object"
      },
      "UserObjectRequest": {
        "additionalProperties": true,
        "description": "Represents chat user",
        "properties": {
          "ban_expires": {
            "description": "Expiration date of the ban",
            "format": "date-time",
            "title": "Ban expires",
            "type": "string",
            "x-stream-index": "015"
          },
          "banned": {
            "description": "Whether a user is banned or not",
            "title": "Banned",
            "type": "boolean",
            "x-stream-index": "014"
          },
          "id": {
            "description": "Unique user identifier",
            "title": "ID",
            "type": "string",
            "x-stream-index": "003"
          },
          "invisible": {
            "type": "boolean",
            "x-stream-index": "021"
          },
          "language": {
            "description": "Preferred language of a user",
            "title": "Language",
            "type": "string",
            "x-stream-index": "030"
          },
          "push_notifications": {
            "$ref": "#/$defs/PushNotificationSettingsRequest"
          },
          "revoke_tokens_issued_before": {
            "description": "Revocation date for tokens",
            "format": "date-time",
            "title": "Revoke tokens issued before",
            "type": "string",
            "x-stream-index": "037"
          },
          "role": {
            "description": "Determines the set of user permissions",
            "title": "Role",
            "type": "string",
            "x-stream-index": "004"
          },
          "teams": {
            "description": "List of teams user is a part of",
            "items": {
              "type": "string"
            },
            "title": "Teams",
            "type": "array",
            "x-stream-index": "031"
          }
        },
        "required": [
          "id"
        ],
        "title": "User object",
        "type": "object"
      },
      "PushNotificationSettingsRequest": {
        "properties": {
          "disabled": {
            "type": "boolean",
            "x-stream-index": "001"
          },
          "disabled_until": {
            "format": "date-time",
            "type": "string",
            "x-stream-index": "002"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://chat.stream-io-api.com/moderation/unflag"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**duration**: Duration of the request in human-readable format",
            [
              "**flag**: Contains information about flagged user or message",
              {
                "$ul": [
                  "**approved_at**: Date of the approval",
                  "**created_at**: Date/time of creation",
                  "**created_by_automod**",
                  [
                    "**details**",
                    {
                      "$ul": [
                        [
                          "**automod**",
                          {
                            "$ul": [
                              "**action**",
                              "**image_labels**",
                              [
                                "**message_details**",
                                {
                                  "$ul": [
                                    "**pin_changed**",
                                    "**should_enrich**",
                                    "**skip_push**",
                                    "**updated_by_id**"
                                  ]
                                }
                              ],
                              "**original_message_type**",
                              [
                                "**result**",
                                {
                                  "$ul": [
                                    "**action**",
                                    [
                                      "**ai_moderation_response**",
                                      {
                                        "$ul": [
                                          "**action**",
                                          "**automod_response**",
                                          "**explicit**",
                                          "**spam**",
                                          "**toxic**"
                                        ]
                                      }
                                    ],
                                    "**blocked_word**",
                                    "**blocklist_name**",
                                    "**created_at**",
                                    "**message_id**",
                                    "**moderated_by**",
                                    [
                                      "**moderation_thresholds**: Sets thresholds for AI moderation",
                                      {
                                        "$ul": [
                                          [
                                            "**explicit**",
                                            {
                                              "$ul": [
                                                "**block**: Threshold for automatic message block",
                                                "**flag**: Threshold for automatic message flag"
                                              ]
                                            }
                                          ],
                                          [
                                            "**spam**",
                                            {
                                              "$ul": [
                                                "**block**: Threshold for automatic message block",
                                                "**flag**: Threshold for automatic message flag"
                                              ]
                                            }
                                          ],
                                          [
                                            "**toxic**",
                                            {
                                              "$ul": [
                                                "**block**: Threshold for automatic message block",
                                                "**flag**: Threshold for automatic message flag"
                                              ]
                                            }
                                          ]
                                        ]
                                      }
                                    ],
                                    "**updated_at**",
                                    "**user_bad_karma**",
                                    "**user_karma**"
                                  ]
                                }
                              ]
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  "**rejected_at**: Date of the rejection",
                  "**reviewed_at**: Date of the review",
                  [
                    "**target_message**: Represents any chat message",
                    {
                      "$ul": [
                        "**attachments**: Array of message attachments",
                        "**before_message_send_failed**: Whether `before_message_send webhook` failed or not. Field is only accessible in push webhook",
                        "**cid**: Channel unique identifier in <type>:<id> format",
                        "**command**: Contains provided slash command",
                        "**created_at**: Date/time of creation",
                        "**deleted_at**: Date/time of deletion",
                        "**html**: Contains HTML markup of the message. Can only be set when using server-side API",
                        "**i18n**: Object with translations. Key `language` contains the original language key. Other keys contain translations",
                        "**id**: Message ID is unique string identifier of the message",
                        "**image_labels**: Contains image moderation information",
                        "**latest_reactions**: List of 10 latest reactions to this message",
                        "**mentioned_users**: List of mentioned users",
                        "**mml**: Should be empty if `text` is provided. Can only be set when using server-side API",
                        "**own_reactions**: List of 10 latest reactions of authenticated user to this message",
                        "**parent_id**: ID of parent message (thread)",
                        "**pin_expires**: Date when pinned message expires",
                        "**pinned**: Whether message is pinned or not",
                        "**pinned_at**: Date when message got pinned",
                        [
                          "**pinned_by**: Represents chat user",
                          {
                            "$ul": [
                              "**ban_expires**: Expiration date of the ban",
                              "**banned**: Whether a user is banned or not",
                              "**created_at**: Date/time of creation",
                              "**deactivated_at**: Date of deactivation",
                              "**deleted_at**: Date/time of deletion",
                              "**id**: Unique user identifier",
                              "**invisible**",
                              "**language**: Preferred language of a user",
                              "**last_active**: Date of last activity",
                              "**online**: Whether a user online or not",
                              [
                                "**push_notifications**",
                                {
                                  "$ul": [
                                    "**disabled**",
                                    "**disabled_until**"
                                  ]
                                }
                              ],
                              "**revoke_tokens_issued_before**: Revocation date for tokens",
                              "**role**: Determines the set of user permissions",
                              "**teams**: List of teams user is a part of",
                              "**updated_at**: Date/time of the last update"
                            ]
                          }
                        ],
                        "**quoted_message**",
                        "**quoted_message_id**",
                        "**reaction_counts**: An object containing number of reactions of each type. Key: reaction type (string), value: number of reactions (int)",
                        "**reaction_scores**: An object containing scores of reactions of each type. Key: reaction type (string), value: total score of reactions (int)",
                        "**reply_count**: Number of replies to this message",
                        "**shadowed**: Whether the message was shadowed or not",
                        "**show_in_channel**: Whether thread reply should be shown in the channel as well",
                        "**silent**: Whether message is silent or not",
                        "**text**: Text of the message. Should be empty if `mml` is provided",
                        "**thread_participants**: List of users who participate in thread",
                        "**type**: Contains type of the message",
                        "**updated_at**: Date/time of the last update",
                        [
                          "**user**: Represents chat user",
                          {
                            "$ul": [
                              "**ban_expires**: Expiration date of the ban",
                              "**banned**: Whether a user is banned or not",
                              "**created_at**: Date/time of creation",
                              "**deactivated_at**: Date of deactivation",
                              "**deleted_at**: Date/time of deletion",
                              "**id**: Unique user identifier",
                              "**invisible**",
                              "**language**: Preferred language of a user",
                              "**last_active**: Date of last activity",
                              "**online**: Whether a user online or not",
                              [
                                "**push_notifications**",
                                {
                                  "$ul": [
                                    "**disabled**",
                                    "**disabled_until**"
                                  ]
                                }
                              ],
                              "**revoke_tokens_issued_before**: Revocation date for tokens",
                              "**role**: Determines the set of user permissions",
                              "**teams**: List of teams user is a part of",
                              "**updated_at**: Date/time of the last update"
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  "**target_message_id**: ID of flagged message",
                  [
                    "**target_user**: Represents chat user",
                    {
                      "$ul": [
                        "**ban_expires**: Expiration date of the ban",
                        "**banned**: Whether a user is banned or not",
                        "**created_at**: Date/time of creation",
                        "**deactivated_at**: Date of deactivation",
                        "**deleted_at**: Date/time of deletion",
                        "**id**: Unique user identifier",
                        "**invisible**",
                        "**language**: Preferred language of a user",
                        "**last_active**: Date of last activity",
                        "**online**: Whether a user online or not",
                        [
                          "**push_notifications**",
                          {
                            "$ul": [
                              "**disabled**",
                              "**disabled_until**"
                            ]
                          }
                        ],
                        "**revoke_tokens_issued_before**: Revocation date for tokens",
                        "**role**: Determines the set of user permissions",
                        "**teams**: List of teams user is a part of",
                        "**updated_at**: Date/time of the last update"
                      ]
                    }
                  ],
                  "**updated_at**: Date/time of the last update",
                  [
                    "**user**: Represents chat user",
                    {
                      "$ul": [
                        "**ban_expires**: Expiration date of the ban",
                        "**banned**: Whether a user is banned or not",
                        "**created_at**: Date/time of creation",
                        "**deactivated_at**: Date of deactivation",
                        "**deleted_at**: Date/time of deletion",
                        "**id**: Unique user identifier",
                        "**invisible**",
                        "**language**: Preferred language of a user",
                        "**last_active**: Date of last activity",
                        "**online**: Whether a user online or not",
                        [
                          "**push_notifications**",
                          {
                            "$ul": [
                              "**disabled**",
                              "**disabled_until**"
                            ]
                          }
                        ],
                        "**revoke_tokens_issued_before**: Revocation date for tokens",
                        "**role**: Determines the set of user permissions",
                        "**teams**: List of teams user is a part of",
                        "**updated_at**: Date/time of the last update"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Error response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**StatusCode**: Response HTTP status code",
            "**code**: API error code",
            "**details**: Additional error-specific information",
            "**duration**: Request duration",
            "**exception_fields**: Additional error info",
            "**message**: Message describing an error",
            "**more_info**: URL with additional information"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "429": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Error response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**StatusCode**: Response HTTP status code",
            "**code**: API error code",
            "**details**: Additional error-specific information",
            "**duration**: Request duration",
            "**exception_fields**: Additional error info",
            "**message**: Message describing an error",
            "**more_info**: URL with additional information"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
