{
  "name": "dcim_devices_napalm",
  "description": "Execute a NAPALM method on a Device",
  "parameters": {
    "type": "object",
    "properties": {
      "method": {
        "type": "string"
      },
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this device."
      }
    },
    "required": [
      "method",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://demo.netbox.dev/api/dcim/devices/{id}/napalm/{?method}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**method**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
