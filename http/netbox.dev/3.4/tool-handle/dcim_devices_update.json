            "**custom_fields** (default: [object Object])",
            "**description**",
            [
              "**device_role**",
              {
                "$ul": [
                  "**device_count**",
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**",
                  "**virtualmachine_count**"
                ]
              }
