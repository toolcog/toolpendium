                      ]
                    }
                  ],
                  "**model**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**display**",
            [
              "**face**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**id**",
            "**last_updated**",
            "**local_context_data**",
            [
              "**location**",
              {
                "$ul": [
                  "**_depth**",
                  "**display**",
                  "**id**",
                  "**name**",
                  "**rack_count**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**name**",
            [
              "**parent_device**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            [
              "**platform**",
              {
                "$ul": [
                  "**device_count**",
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**",
                  "**virtualmachine_count**"
                ]
              }
            ],
            "**position**",
            [
              "**primary_ip**",
              {
                "$ul": [
                  "**address**: IPv4 or IPv6 address (with mask)",
                  "**display**",
                  "**family**",
                  "**id**",
                  "**url**"
                ]
              }
            ],
            [
              "**primary_ip4**",
              {
                "$ul": [
                  "**address**: IPv4 or IPv6 address (with mask)",
                  "**display**",
                  "**family**",
                  "**id**",
                  "**url**"
                ]
              }
            ],
            [
              "**primary_ip6**",
              {
                "$ul": [
                  "**address**: IPv4 or IPv6 address (with mask)",
                  "**display**",
                  "**family**",
                  "**id**",
                  "**url**"
                ]
              }
            ],
            [
              "**rack**",
              {
                "$ul": [
                  "**device_count**",
                  "**display**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**serial**",
            [
              "**site**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            [
              "**status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            [
              "**tenant**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**url**",
            "**vc_position**",
            "**vc_priority**",
            [
              "**virtual_chassis**",
              {
