{
  "name": "extras_export-templates_bulk_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/ExportTemplate"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "ExportTemplate": {
        "properties": {
          "as_attachment": {
            "description": "Download file as attachment",
            "title": "As attachment",
            "type": "boolean"
          },
          "content_types": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "uniqueItems": true
          },
          "created": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "file_extension": {
            "description": "Extension to append to the rendered filename",
            "maxLength": 15,
            "title": "File extension",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "last_updated": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "mime_type": {
            "description": "Defaults to <code>text/plain</code>",
            "maxLength": 50,
            "title": "MIME type",
            "type": "string"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "template_code": {
            "description": "Jinja2 template code. The list of objects being exported is passed as a context variable named <code>queryset</code>.",
            "minLength": 1,
            "title": "Template code",
            "type": "string"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "content_types",
          "name",
          "template_code"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://demo.netbox.dev/api/extras/export-templates/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**as_attachment**: Download file as attachment",
            "**content_types**",
            "**created**",
            "**description**",
            "**display**",
            "**file_extension**: Extension to append to the rendered filename",
            "**id**",
            "**last_updated**",
            "**mime_type**: Defaults to <code>text/plain</code>",
            "**name**",
            "**template_code**: Jinja2 template code. The list of objects being exported is passed as a context variable named <code>queryset</code>.",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
