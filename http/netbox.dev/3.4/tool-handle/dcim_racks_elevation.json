{
  "name": "dcim_racks_elevation",
  "description": "Rack elevation representing the list of rack units. Also supports rendering the elevation as an SVG.",
  "parameters": {
    "type": "object",
    "properties": {
      "q": {
        "minLength": 1,
        "type": "string"
      },
      "face": {
        "default": "front",
        "enum": [
          "front",
          "rear"
        ],
        "type": "string"
      },
      "render": {
        "default": "json",
        "enum": [
          "json",
          "svg"
        ],
        "type": "string"
      },
      "unit_width": {
        "default": 220,
        "type": "integer"
      },
      "unit_height": {
        "default": 22,
        "type": "integer"
      },
      "legend_width": {
        "default": 30,
        "type": "integer"
      },
      "margin_width": {
        "default": 15,
        "type": "integer"
      },
      "exclude": {
        "type": "integer"
      },
      "expand_devices": {
        "default": true,
        "type": "boolean"
      },
      "include_images": {
        "default": true,
        "type": "boolean"
      },
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this rack."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://demo.netbox.dev/api/dcim/racks/{id}/elevation/{?q,face,render,unit_width,unit_height,legend_width,margin_width,exclude,expand_devices,include_images}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**device**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**display**",
            [
              "**face**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**id**",
            "**name**",
            "**occupied**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
