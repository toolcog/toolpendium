{
  "name": "wireless_wireless-lans_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this Wireless LAN."
      },
      "body": {
        "$ref": "#/$defs/WritableWirelessLAN"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritableWirelessLAN": {
        "properties": {
          "auth_cipher": {
            "enum": [
              "auto",
              "tkip",
              "aes"
            ],
            "title": "Auth cipher",
            "type": "string"
          },
          "auth_psk": {
            "maxLength": 64,
            "title": "Pre-shared key",
            "type": "string"
          },
          "auth_type": {
            "enum": [
              "open",
              "wep",
              "wpa-personal",
              "wpa-enterprise"
            ],
            "title": "Auth Type",
            "type": "string"
          },
          "comments": {
            "title": "Comments",
            "type": "string"
          },
          "created": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "custom_fields": {
            "default": {},
            "title": "Custom fields",
            "type": "object"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "group": {
            "nullable": true,
            "title": "Group",
            "type": "integer"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "last_updated": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "ssid": {
            "maxLength": 32,
            "minLength": 1,
            "title": "SSID",
            "type": "string"
          },
          "status": {
            "enum": [
              "active",
              "reserved",
              "disabled",
              "deprecated"
            ],
            "title": "Status",
            "type": "string"
          },
          "tags": {
            "items": {
              "$ref": "#/$defs/NestedTag"
            },
            "type": "array"
          },
          "tenant": {
            "nullable": true,
            "title": "Tenant",
            "type": "integer"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          },
          "vlan": {
            "nullable": true,
            "title": "VLAN",
            "type": "integer"
          }
        },
        "required": [
          "ssid"
        ],
        "type": "object"
      },
      "NestedTag": {
        "properties": {
          "color": {
            "maxLength": 6,
            "minLength": 1,
            "pattern": "^[0-9a-f]{6}$",
            "title": "Color",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "Id",
            "type": "integer"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "slug": {
            "format": "slug",
            "maxLength": 100,
            "minLength": 1,
            "title": "Slug",
            "type": "string"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "name",
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://demo.netbox.dev/api/wireless/wireless-lans/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**auth_cipher**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**auth_psk**",
            [
              "**auth_type**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**comments**",
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**description**",
            "**display**",
            [
              "**group**",
              {
                "$ul": [
                  "**_depth**",
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**",
                  "**wirelesslan_count**"
                ]
              }
            ],
            "**id**",
            "**last_updated**",
            "**ssid**",
            [
              "**status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            [
              "**tenant**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**url**",
            [
              "**vlan**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**url**",
                  "**vid**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
