{
  "name": "extras_tags_create",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/Tag"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "Tag": {
        "properties": {
          "color": {
            "maxLength": 6,
            "minLength": 1,
            "pattern": "^[0-9a-f]{6}$",
            "title": "Color",
            "type": "string"
          },
          "created": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "Id",
            "type": "integer"
          },
          "last_updated": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "slug": {
            "format": "slug",
            "maxLength": 100,
            "minLength": 1,
            "title": "Slug",
            "type": "string"
          },
          "tagged_items": {
            "readOnly": true,
            "title": "Tagged items",
            "type": "integer"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "name",
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://demo.netbox.dev/api/extras/tags/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**color**",
            "**created**",
            "**description**",
            "**display**",
            "**id**",
            "**last_updated**",
            "**name**",
            "**slug**",
            "**tagged_items**",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
