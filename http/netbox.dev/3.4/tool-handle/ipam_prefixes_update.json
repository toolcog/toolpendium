{
  "name": "ipam_prefixes_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this prefix."
      },
      "body": {
        "$ref": "#/$defs/WritablePrefix"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritablePrefix": {
        "properties": {
          "_depth": {
            "readOnly": true,
            "title": "depth",
            "type": "integer"
          },
          "children": {
            "readOnly": true,
            "title": "Children",
            "type": "integer"
          },
          "comments": {
            "title": "Comments",
            "type": "string"
          },
          "created": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "custom_fields": {
            "default": {},
            "title": "Custom fields",
            "type": "object"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "family": {
            "readOnly": true,
            "title": "Family",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "is_pool": {
            "description": "All IP addresses within this prefix are considered usable",
            "title": "Is a pool",
            "type": "boolean"
          },
          "last_updated": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "mark_utilized": {
            "description": "Treat as 100% utilized",
            "title": "Mark utilized",
            "type": "boolean"
          },
          "prefix": {
            "description": "IPv4 or IPv6 network with mask",
            "title": "Prefix",
            "type": "string"
          },
          "role": {
            "description": "The primary function of this prefix",
            "nullable": true,
            "title": "Role",
            "type": "integer"
          },
          "site": {
            "nullable": true,
            "title": "Site",
            "type": "integer"
          },
          "status": {
            "description": "Operational status of this prefix",
            "enum": [
              "container",
              "active",
              "reserved",
              "deprecated"
            ],
            "title": "Status",
            "type": "string"
          },
          "tags": {
            "items": {
              "$ref": "#/$defs/NestedTag"
            },
            "type": "array"
          },
          "tenant": {
            "nullable": true,
            "title": "Tenant",
            "type": "integer"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          },
          "vlan": {
            "nullable": true,
            "title": "VLAN",
            "type": "integer"
          },
          "vrf": {
            "nullable": true,
            "title": "VRF",
            "type": "integer"
          }
        },
        "required": [
          "prefix"
        ],
        "type": "object"
      },
      "NestedTag": {
        "properties": {
          "color": {
            "maxLength": 6,
            "minLength": 1,
            "pattern": "^[0-9a-f]{6}$",
            "title": "Color",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "Id",
            "type": "integer"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "slug": {
            "format": "slug",
            "maxLength": 100,
            "minLength": 1,
            "title": "Slug",
            "type": "string"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "name",
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://demo.netbox.dev/api/ipam/prefixes/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**_depth**",
            "**children**",
            "**comments**",
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**description**",
            "**display**",
            [
              "**family**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**id**",
            "**is_pool**: All IP addresses within this prefix are considered usable",
            "**last_updated**",
            "**mark_utilized**: Treat as 100% utilized",
            "**prefix**: IPv4 or IPv6 network with mask",
            [
              "**role**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**prefix_count**",
                  "**slug**",
                  "**url**",
                  "**vlan_count**"
                ]
              }
            ],
            [
              "**site**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            [
              "**status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            [
              "**tenant**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**url**",
            [
              "**vlan**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**url**",
                  "**vid**"
                ]
              }
            ],
            [
              "**vrf**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**prefix_count**",
                  "**rd**: Unique route distinguisher (as defined in RFC 4364)",
                  "**url**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
