{
  "name": "dcim_front-ports_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this front port."
      },
      "body": {
        "$ref": "#/$defs/WritableFrontPort"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritableFrontPort": {
        "properties": {
          "_occupied": {
            "readOnly": true,
            "title": "occupied",
            "type": "boolean"
          },
          "cable": {
            "$ref": "#/$defs/NestedCable"
          },
          "cable_end": {
            "minLength": 1,
            "readOnly": true,
            "title": "Cable end",
            "type": "string"
          },
          "color": {
            "maxLength": 6,
            "pattern": "^[0-9a-f]{6}$",
            "title": "Color",
            "type": "string"
          },
          "created": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "custom_fields": {
            "default": {},
            "title": "Custom fields",
            "type": "object"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "device": {
            "title": "Device",
            "type": "integer"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "label": {
            "description": "Physical label",
            "maxLength": 64,
            "title": "Label",
            "type": "string"
          },
          "last_updated": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "link_peers": {
            "description": "\nReturn the appropriate serializer for the link termination model.\n",
            "items": {
              "nullable": true,
              "type": "string"
            },
            "readOnly": true,
            "type": "array"
          },
          "link_peers_type": {
            "readOnly": true,
            "title": "Link peers type",
            "type": "string"
          },
          "mark_connected": {
            "description": "Treat as if a cable is connected",
            "title": "Mark connected",
            "type": "boolean"
          },
          "module": {
            "nullable": true,
            "title": "Module",
            "type": "integer"
          },
          "name": {
            "maxLength": 64,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "rear_port": {
            "title": "Rear port",
            "type": "integer"
          },
          "rear_port_position": {
            "maximum": 1024,
            "minimum": 1,
            "title": "Rear port position",
            "type": "integer"
          },
          "tags": {
            "items": {
              "$ref": "#/$defs/NestedTag"
            },
            "type": "array"
          },
          "type": {
            "enum": [
              "8p8c",
              "8p6c",
              "8p4c",
              "8p2c",
              "6p6c",
              "6p4c",
              "6p2c",
              "4p4c",
              "4p2c",
              "gg45",
              "tera-4p",
              "tera-2p",
              "tera-1p",
              "110-punch",
              "bnc",
              "f",
              "n",
              "mrj21",
              "fc",
              "lc",
              "lc-pc",
              "lc-upc",
              "lc-apc",
              "lsh",
              "lsh-pc",
              "lsh-upc",
              "lsh-apc",
              "mpo",
              "mtrj",
              "sc",
              "sc-pc",
              "sc-upc",
              "sc-apc",
              "st",
              "cs",
              "sn",
              "sma-905",
              "sma-906",
              "urm-p2",
              "urm-p4",
              "urm-p8",
              "splice",
              "other"
            ],
            "title": "Type",
            "type": "string"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "device",
          "name",
          "type",
          "rear_port"
        ],
        "type": "object"
      },
      "NestedCable": {
        "properties": {
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "label": {
            "maxLength": 100,
            "title": "Label",
            "type": "string"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "type": "object"
      },
      "NestedTag": {
        "properties": {
          "color": {
            "maxLength": 6,
            "minLength": 1,
            "pattern": "^[0-9a-f]{6}$",
            "title": "Color",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "Id",
            "type": "integer"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "slug": {
            "format": "slug",
            "maxLength": 100,
            "minLength": 1,
            "title": "Slug",
            "type": "string"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "name",
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://demo.netbox.dev/api/dcim/front-ports/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**_occupied**",
            [
              "**cable**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**label**",
                  "**url**"
                ]
              }
            ],
            "**cable_end**",
            "**color**",
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**description**",
            [
              "**device**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**display**",
            "**id**",
            "**label**: Physical label",
            "**last_updated**",
            "**link_peers**: ",
            "**link_peers_type**",
            "**mark_connected**: Treat as if a cable is connected",
            [
              "**module**",
              {
                "$ul": [
                  "**device**",
                  "**display**",
                  "**id**",
                  [
                    "**module_bay**",
                    {
                      "$ul": [
                        "**display**",
                        "**id**",
                        "**name**",
                        "**url**"
                      ]
                    }
                  ],
                  "**url**"
                ]
              }
            ],
            "**name**",
            [
              "**rear_port**",
              {
                "$ul": [
                  "**description**",
                  "**display**",
                  "**id**",
                  "**label**: Physical label",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**rear_port_position**",
            "**tags**",
            [
              "**type**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
