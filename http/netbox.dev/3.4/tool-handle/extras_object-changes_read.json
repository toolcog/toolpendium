{
  "name": "extras_object-changes_read",
  "description": "Retrieve a list of recent changes.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this object change."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://demo.netbox.dev/api/extras/object-changes/{id}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**action**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**changed_object**: ",
            "**changed_object_id**",
            "**changed_object_type**",
            "**display**",
            "**id**",
            "**postchange_data**",
            "**prechange_data**",
            "**request_id**",
            "**time**",
            "**url**",
            [
              "**user**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**url**",
                  "**username**: Required. 150 characters or fewer. Letters, digits and @/./+/-/_ only."
                ]
              }
            ],
            "**user_name**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
