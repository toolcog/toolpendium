{
  "name": "dcim_racks_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this rack."
      },
      "body": {
        "$ref": "#/$defs/WritableRack"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritableRack": {
        "properties": {
          "asset_tag": {
            "description": "A unique tag used to identify this rack",
            "maxLength": 50,
            "nullable": true,
            "title": "Asset tag",
            "type": "string"
          },
          "comments": {
            "title": "Comments",
            "type": "string"
          },
          "created": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "custom_fields": {
            "default": {},
            "title": "Custom fields",
            "type": "object"
          },
          "desc_units": {
            "description": "Units are numbered top-to-bottom",
            "title": "Descending units",
            "type": "boolean"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "device_count": {
            "readOnly": true,
            "title": "Device count",
            "type": "integer"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "facility_id": {
            "maxLength": 50,
            "nullable": true,
            "title": "Facility ID",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "last_updated": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "location": {
            "nullable": true,
            "title": "Location",
            "type": "integer"
          },
          "max_weight": {
            "description": "Maximum load capacity for the rack",
            "maximum": 2147483647,
            "minimum": 0,
            "nullable": true,
            "title": "Max weight",
            "type": "integer"
          },
          "mounting_depth": {
            "description": "Maximum depth of a mounted device, in millimeters. For four-post racks, this is the distance between the front and rear rails.",
            "maximum": 32767,
            "minimum": 0,
            "nullable": true,
            "title": "Mounting depth",
            "type": "integer"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "outer_depth": {
            "description": "Outer dimension of rack (depth)",
            "maximum": 32767,
            "minimum": 0,
            "nullable": true,
            "title": "Outer depth",
            "type": "integer"
          },
          "outer_unit": {
            "enum": [
              "mm",
              "in"
            ],
            "title": "Outer unit",
            "type": "string"
          },
          "outer_width": {
            "description": "Outer dimension of rack (width)",
            "maximum": 32767,
            "minimum": 0,
            "nullable": true,
            "title": "Outer width",
            "type": "integer"
          },
          "powerfeed_count": {
            "readOnly": true,
            "title": "Powerfeed count",
            "type": "integer"
          },
          "role": {
            "description": "Functional role",
            "nullable": true,
            "title": "Role",
            "type": "integer"
          },
          "serial": {
            "maxLength": 50,
            "title": "Serial number",
            "type": "string"
          },
          "site": {
            "title": "Site",
            "type": "integer"
          },
          "status": {
            "enum": [
              "reserved",
              "available",
              "planned",
              "active",
              "deprecated"
            ],
            "title": "Status",
            "type": "string"
          },
          "tags": {
            "items": {
              "$ref": "#/$defs/NestedTag"
            },
            "type": "array"
          },
          "tenant": {
            "nullable": true,
            "title": "Tenant",
            "type": "integer"
          },
          "type": {
            "enum": [
              "2-post-frame",
              "4-post-frame",
              "4-post-cabinet",
              "wall-frame",
              "wall-frame-vertical",
              "wall-cabinet",
              "wall-cabinet-vertical"
            ],
            "title": "Type",
            "type": "string"
          },
          "u_height": {
            "description": "Height in rack units",
            "maximum": 100,
            "minimum": 1,
            "title": "Height (U)",
            "type": "integer"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          },
          "weight": {
            "format": "decimal",
            "nullable": true,
            "title": "Weight",
            "type": "number"
          },
          "weight_unit": {
            "enum": [
              "kg",
              "g",
              "lb",
              "oz"
            ],
            "title": "Weight unit",
            "type": "string"
          },
          "width": {
            "description": "Rail-to-rail width",
            "enum": [
              10,
              19,
              21,
              23
            ],
            "title": "Width",
            "type": "integer"
          }
        },
        "required": [
          "name",
          "site"
        ],
        "type": "object"
      },
      "NestedTag": {
        "properties": {
          "color": {
            "maxLength": 6,
            "minLength": 1,
            "pattern": "^[0-9a-f]{6}$",
            "title": "Color",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "Id",
            "type": "integer"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "slug": {
            "format": "slug",
            "maxLength": 100,
            "minLength": 1,
            "title": "Slug",
            "type": "string"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "name",
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://demo.netbox.dev/api/dcim/racks/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**asset_tag**: A unique tag used to identify this rack",
            "**comments**",
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**desc_units**: Units are numbered top-to-bottom",
            "**description**",
            "**device_count**",
            "**display**",
            "**facility_id**",
            "**id**",
            "**last_updated**",
            [
              "**location**",
              {
                "$ul": [
                  "**_depth**",
                  "**display**",
                  "**id**",
                  "**name**",
                  "**rack_count**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**max_weight**: Maximum load capacity for the rack",
            "**mounting_depth**: Maximum depth of a mounted device, in millimeters. For four-post racks, this is the distance between the front and rear rails.",
            "**name**",
            "**outer_depth**: Outer dimension of rack (depth)",
            [
              "**outer_unit**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**outer_width**: Outer dimension of rack (width)",
            "**powerfeed_count**",
            [
              "**role**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**rack_count**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**serial**",
            [
              "**site**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            [
              "**status**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**tags**",
            [
              "**tenant**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            [
              "**type**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            "**u_height**: Height in rack units",
            "**url**",
            "**weight**",
            [
              "**weight_unit**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ],
            [
              "**width**",
              {
                "$ul": [
                  "**label**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
