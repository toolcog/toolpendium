{
  "name": "dcim_inventory-item-templates_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this inventory item template."
      },
      "body": {
        "$ref": "#/$defs/WritableInventoryItemTemplate"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritableInventoryItemTemplate": {
        "properties": {
          "_depth": {
            "readOnly": true,
            "title": "depth",
            "type": "integer"
          },
          "component": {
            "readOnly": true,
            "title": "Component",
            "type": "object"
          },
          "component_id": {
            "maximum": 9223372036854776000,
            "minimum": 0,
            "nullable": true,
            "title": "Component id",
            "type": "integer"
          },
          "component_type": {
            "nullable": true,
            "title": "Component type",
            "type": "string"
          },
          "created": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "device_type": {
            "title": "Device type",
            "type": "integer"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "label": {
            "description": "Physical label",
            "maxLength": 64,
            "title": "Label",
            "type": "string"
          },
          "last_updated": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "manufacturer": {
            "nullable": true,
            "title": "Manufacturer",
            "type": "integer"
          },
          "name": {
            "description": "\n{module} is accepted as a substitution for the module bay position when attached to a module type.\n",
            "maxLength": 64,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "parent": {
            "nullable": true,
            "title": "Parent",
            "type": "integer"
          },
          "part_id": {
            "description": "Manufacturer-assigned part identifier",
            "maxLength": 50,
            "title": "Part ID",
            "type": "string"
          },
          "role": {
            "nullable": true,
            "title": "Role",
            "type": "integer"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "device_type",
          "name"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://demo.netbox.dev/api/dcim/inventory-item-templates/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**_depth**",
            "**component**",
            "**component_id**",
            "**component_type**",
            "**created**",
            "**description**",
            [
              "**device_type**",
              {
                "$ul": [
                  "**device_count**",
                  "**display**",
                  "**id**",
                  [
                    "**manufacturer**",
                    {
                      "$ul": [
                        "**devicetype_count**",
                        "**display**",
                        "**id**",
                        "**name**",
                        "**slug**",
                        "**url**"
                      ]
                    }
                  ],
                  "**model**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**display**",
            "**id**",
            "**label**: Physical label",
            "**last_updated**",
            [
              "**manufacturer**",
              {
                "$ul": [
                  "**devicetype_count**",
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**name**: ",
            "**parent**",
            "**part_id**: Manufacturer-assigned part identifier",
            [
              "**role**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**inventoryitem_count**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
