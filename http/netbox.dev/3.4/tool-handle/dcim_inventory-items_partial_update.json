{
  "name": "dcim_inventory-items_partial_update",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "A unique integer value identifying this inventory item."
      },
      "body": {
        "$ref": "#/$defs/WritableInventoryItem"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "WritableInventoryItem": {
        "properties": {
          "_depth": {
            "readOnly": true,
            "title": "depth",
            "type": "integer"
          },
          "asset_tag": {
            "description": "A unique tag used to identify this item",
            "maxLength": 50,
            "nullable": true,
            "title": "Asset tag",
            "type": "string"
          },
          "component": {
            "readOnly": true,
            "title": "Component",
            "type": "object"
          },
          "component_id": {
            "maximum": 9223372036854776000,
            "minimum": 0,
            "nullable": true,
            "title": "Component id",
            "type": "integer"
          },
          "component_type": {
            "nullable": true,
            "title": "Component type",
            "type": "string"
          },
          "created": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Created",
            "type": "string"
          },
          "custom_fields": {
            "default": {},
            "title": "Custom fields",
            "type": "object"
          },
          "description": {
            "maxLength": 200,
            "title": "Description",
            "type": "string"
          },
          "device": {
            "title": "Device",
            "type": "integer"
          },
          "discovered": {
            "description": "This item was automatically discovered",
            "title": "Discovered",
            "type": "boolean"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "ID",
            "type": "integer"
          },
          "label": {
            "description": "Physical label",
            "maxLength": 64,
            "title": "Label",
            "type": "string"
          },
          "last_updated": {
            "format": "date-time",
            "nullable": true,
            "readOnly": true,
            "title": "Last updated",
            "type": "string"
          },
          "manufacturer": {
            "nullable": true,
            "title": "Manufacturer",
            "type": "integer"
          },
          "name": {
            "maxLength": 64,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "parent": {
            "nullable": true,
            "title": "Parent",
            "type": "integer"
          },
          "part_id": {
            "description": "Manufacturer-assigned part identifier",
            "maxLength": 50,
            "title": "Part ID",
            "type": "string"
          },
          "role": {
            "nullable": true,
            "title": "Role",
            "type": "integer"
          },
          "serial": {
            "maxLength": 50,
            "title": "Serial number",
            "type": "string"
          },
          "tags": {
            "items": {
              "$ref": "#/$defs/NestedTag"
            },
            "type": "array"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "device",
          "name"
        ],
        "type": "object"
      },
      "NestedTag": {
        "properties": {
          "color": {
            "maxLength": 6,
            "minLength": 1,
            "pattern": "^[0-9a-f]{6}$",
            "title": "Color",
            "type": "string"
          },
          "display": {
            "readOnly": true,
            "title": "Display",
            "type": "string"
          },
          "id": {
            "readOnly": true,
            "title": "Id",
            "type": "integer"
          },
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "slug": {
            "format": "slug",
            "maxLength": 100,
            "minLength": 1,
            "title": "Slug",
            "type": "string"
          },
          "url": {
            "format": "uri",
            "readOnly": true,
            "title": "Url",
            "type": "string"
          }
        },
        "required": [
          "name",
          "slug"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://demo.netbox.dev/api/dcim/inventory-items/{id}/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**_depth**",
            "**asset_tag**: A unique tag used to identify this item",
            "**component**",
            "**component_id**",
            "**component_type**",
            "**created**",
            "**custom_fields** (default: [object Object])",
            "**description**",
            [
              "**device**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**name**",
                  "**url**"
                ]
              }
            ],
            "**discovered**: This item was automatically discovered",
            "**display**",
            "**id**",
            "**label**: Physical label",
            "**last_updated**",
            [
              "**manufacturer**",
              {
                "$ul": [
                  "**devicetype_count**",
                  "**display**",
                  "**id**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**name**",
            "**parent**",
            "**part_id**: Manufacturer-assigned part identifier",
            [
              "**role**",
              {
                "$ul": [
                  "**display**",
                  "**id**",
                  "**inventoryitem_count**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**serial**",
            "**tags**",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
