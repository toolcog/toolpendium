{
  "name": "getFileContent",
  "description": "By passing in the appropriate options, retrieve data for specific file",
  "parameters": {
    "type": "object",
    "properties": {
      "FileId": {
        "example": "4ff1e5cc-9835-40d5-bb18-09fdb118db9c",
        "format": "uuid",
        "type": "string",
        "description": "File id for single object"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "FileId",
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/files.xro/1.0//Files/{FileId}/Content"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
