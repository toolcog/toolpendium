{
  "name": "getInbox",
  "description": "Search for the user inbox",
  "parameters": {
    "type": "object",
    "properties": {
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/files.xro/1.0//Inbox"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Email**: The email address used to email files to the inbox. Only the inbox will have this element.",
            "**FileCount**: The number of files in the folder",
            "**Id**: Xero unique identifier for a folder  Files",
            "**IsInbox**: to indicate if the folder is the Inbox. The Inbox cannot be renamed or deleted.",
            "**Name**: The name of the folder"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
