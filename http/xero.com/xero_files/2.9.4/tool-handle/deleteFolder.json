{
  "name": "deleteFolder",
  "description": "By passing in the appropriate ID, you can delete a folder",
  "parameters": {
    "type": "object",
    "properties": {
      "FolderId": {
        "example": "4ff1e5cc-9835-40d5-bb18-09fdb118db9c",
        "format": "uuid",
        "type": "string",
        "description": "Folder id for single object"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "FolderId",
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.xero.com/files.xro/1.0//Folders/{FolderId}"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {}
}
