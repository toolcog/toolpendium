{
  "name": "uploadFile",
  "description": "Uploads a File",
  "parameters": {
    "type": "object",
    "properties": {
      "FolderId": {
        "example": "4ff1e5cc-9835-40d5-bb18-09fdb118db9c",
        "format": "uuid",
        "type": "string",
        "description": "pass an optional folder id to save file to specific folder"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      },
      "body": {
        "properties": {
          "body": {
            "format": "byte",
            "type": "string"
          },
          "filename": {
            "type": "string"
          },
          "mimeType": {
            "type": "string"
          },
          "name": {
            "description": "exact name of the file you are uploading",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.xero.com/files.xro/1.0//Files{?FolderId}"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**CreatedDateUtc**: Created date in UTC",
            "**FolderId**: Folder relation object's UUID",
            "**Id**: File object's UUID",
            "**MimeType**: MimeType of the file (image/png, image/jpeg, application/pdf, etc..)",
            "**Name**: File Name",
            "**Size**: Numeric value in bytes",
            "**UpdatedDateUtc**: Updated date in UTC",
            [
              "**User**",
              {
                "$ul": [
                  "**FirstName**: First name of user",
                  "**FullName**: Last name of user",
                  "**Id**: Xero identifier",
                  "**LastName**: Last name of user",
                  "**Name**: Key is Name, but returns Email address of user who created the file"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
