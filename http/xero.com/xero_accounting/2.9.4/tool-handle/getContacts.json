{
  "name": "getContacts",
  "description": "Retrieves all contacts in a Xero organisation",
  "parameters": {
    "type": "object",
    "properties": {
      "If-Modified-Since": {
        "format": "date-time",
        "type": "string",
        "description": "Only records created or modified since this timestamp will be returned"
      },
      "where": {
        "type": "string",
        "description": "Filter by an any element"
      },
      "order": {
        "type": "string",
        "description": "Order by an any element"
      },
      "IDs": {
        "items": {
          "format": "uuid",
          "type": "string"
        },
        "type": "array",
        "description": "Filter by a comma separated list of ContactIDs. Allows you to retrieve a specific set of contacts in a single call."
      },
      "page": {
        "type": "integer",
        "description": "e.g. page=1 - Up to 100 contacts will be returned in a single API call."
      },
      "includeArchived": {
        "type": "boolean",
        "description": "e.g. includeArchived=true - Contacts with a status of ARCHIVED will be included in the response"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/Contacts{?where,order,IDs,page,includeArchived}"
    },
    "headers": {
      "If-Modified-Since": {
        "$": "If-Modified-Since"
      },
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Contacts**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
