{
  "name": "getQuotes",
  "description": "Retrieves sales quotes",
  "parameters": {
    "type": "object",
    "properties": {
      "If-Modified-Since": {
        "format": "date-time",
        "type": "string",
        "description": "Only records created or modified since this timestamp will be returned"
      },
      "DateFrom": {
        "format": "date",
        "type": "string",
        "description": "Filter for quotes after a particular date"
      },
      "DateTo": {
        "format": "date",
        "type": "string",
        "description": "Filter for quotes before a particular date"
      },
      "ExpiryDateFrom": {
        "format": "date",
        "type": "string",
        "description": "Filter for quotes expiring after a particular date"
      },
      "ExpiryDateTo": {
        "format": "date",
        "type": "string",
        "description": "Filter for quotes before a particular date"
      },
      "ContactID": {
        "format": "uuid",
        "type": "string",
        "description": "Filter for quotes belonging to a particular contact"
      },
      "Status": {
        "type": "string",
        "description": "Filter for quotes of a particular Status"
      },
      "page": {
        "type": "integer",
        "description": "e.g. page=1 â€“ Up to 100 Quotes will be returned in a single API call with line items shown for each quote"
      },
      "order": {
        "type": "string",
        "description": "Order by an any element"
      },
      "QuoteNumber": {
        "type": "string",
        "description": "Filter by quote number (e.g. GET https://.../Quotes?QuoteNumber=QU-0001)"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/Quotes{?DateFrom,DateTo,ExpiryDateFrom,ExpiryDateTo,ContactID,Status,page,order,QuoteNumber}"
    },
    "headers": {
      "If-Modified-Since": {
        "$": "If-Modified-Since"
      },
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Quotes**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
