{
  "name": "deleteContactGroupContact",
  "description": "Deletes a specific contact from a contact group using a unique contact Id",
  "parameters": {
    "type": "object",
    "properties": {
      "ContactGroupID": {
        "format": "uuid",
        "type": "string",
        "description": "Unique identifier for a Contact Group"
      },
      "ContactID": {
        "format": "uuid",
        "type": "string",
        "description": "Unique identifier for a Contact"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "ContactGroupID",
      "ContactID",
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/ContactGroups/{ContactGroupID}/Contacts/{ContactID}"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Elements**: Array of Elements of validation Errors",
            "**ErrorNumber**: Exception number",
            "**Message**: Exception message",
            "**Type**: Exception type"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
