{
  "name": "getContactByContactNumber",
  "description": "Retrieves a specific contact by contact number in a Xero organisation",
  "parameters": {
    "type": "object",
    "properties": {
      "ContactNumber": {
        "type": "string",
        "description": "This field is read only on the Xero contact screen, used to identify contacts in external systems (max length = 50)."
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "ContactNumber",
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/Contacts/{ContactNumber}"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Contacts**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
