{
  "name": "getPurchaseOrderAsPdf",
  "description": "Retrieves specific purchase order as PDF files using a unique purchase order Id",
  "parameters": {
    "type": "object",
    "properties": {
      "PurchaseOrderID": {
        "format": "uuid",
        "type": "string",
        "description": "Unique identifier for an Purchase Order"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "PurchaseOrderID",
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/PurchaseOrders/{PurchaseOrderID}/pdf"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
