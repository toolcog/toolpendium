{
  "name": "deleteContactGroupContacts",
  "description": "Deletes all contacts from a specific contact group",
  "parameters": {
    "type": "object",
    "properties": {
      "ContactGroupID": {
        "format": "uuid",
        "type": "string",
        "description": "Unique identifier for a Contact Group"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "ContactGroupID",
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/ContactGroups/{ContactGroupID}/Contacts"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {}
}
