{
  "name": "getReportBankSummary",
  "description": "Retrieves report for bank summary",
  "parameters": {
    "type": "object",
    "properties": {
      "fromDate": {
        "format": "date",
        "type": "string",
        "description": "The from date for the Bank Summary report e.g. 2018-03-31"
      },
      "toDate": {
        "format": "date",
        "type": "string",
        "description": "The to date for the Bank Summary report e.g. 2018-03-31"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/Reports/BankSummary{?fromDate,toDate}"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Reports**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
