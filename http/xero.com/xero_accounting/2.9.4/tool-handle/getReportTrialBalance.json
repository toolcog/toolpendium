{
  "name": "getReportTrialBalance",
  "description": "Retrieves report for trial balance",
  "parameters": {
    "type": "object",
    "properties": {
      "date": {
        "format": "date",
        "type": "string",
        "description": "The date for the Trial Balance report e.g. 2018-03-31"
      },
      "paymentsOnly": {
        "type": "boolean",
        "description": "Return cash only basis for the Trial Balance report"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/Reports/TrialBalance{?date,paymentsOnly}"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Reports**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
