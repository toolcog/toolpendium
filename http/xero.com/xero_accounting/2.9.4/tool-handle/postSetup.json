{
  "name": "postSetup",
  "description": "Sets the chart of accounts, the conversion date and conversion balances",
  "parameters": {
    "type": "object",
    "properties": {
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      },
      "body": {
        "$ref": "#/components/schemas/Setup",
        "description": "Object including an accounts array, a conversion balances array and a conversion date object in body of request"
      }
    },
    "required": [
      "xero-tenant-id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/Setup"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**ImportSummary**: A summary of the import from setup endpoint",
              {
                "$ul": [
                  [
                    "**Accounts**: A summary of the accounts changes",
                    {
                      "$ul": [
                        "**Deleted**: The number of accounts deleted",
                        "**Errored**: The number of accounts that had an error",
                        "**Locked**: The number of locked accounts",
                        "**New**: The number of new accounts created",
                        "**NewOrUpdated**: The number of new or updated accounts",
                        "**Present**",
                        "**System**: The number of system accounts",
                        "**Total**: The total number of accounts in the org",
                        "**Updated**: The number of accounts updated"
                      ]
                    }
                  ],
                  [
                    "**Organisation**",
                    {
                      "$ul": [
                        "**Present**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
