{
  "name": "deleteLinkedTransaction",
  "description": "Deletes a specific linked transactions (billable expenses)",
  "parameters": {
    "type": "object",
    "properties": {
      "LinkedTransactionID": {
        "format": "uuid",
        "type": "string",
        "description": "Unique identifier for a LinkedTransaction"
      },
      "xero-tenant-id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "LinkedTransactionID",
      "xero-tenant-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.xero.com/api.xro/2.0/LinkedTransactions/{LinkedTransactionID}"
    },
    "headers": {
      "xero-tenant-id": {
        "$": "xero-tenant-id"
      }
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Elements**: Array of Elements of validation Errors",
            "**ErrorNumber**: Exception number",
            "**Message**: Exception message",
            "**Type**: Exception type"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
