            "type": "string"
          },
          "Reimbursements": {
            "description": "The Reimbursements for the Payslip",
            "example": 0,
            "format": "double",
            "type": "number",
            "x-is-money": true
          },
          "Super": {
            "description": "The Super for the Payslip",
            "example": 75.6,
            "format": "double",
            "type": "number",
            "x-is-money": true
          },
          "Tax": {
            "description": "The Tax for the Payslip",
            "example": 198,
            "format": "double",
            "type": "number",
            "x-is-money": true
          },
          "UpdatedDateUTC": {
            "description": "Last modified timestamp",
            "example": "/Date(1583967733054+0000)/",
            "readOnly": true,
            "type": "string",
            "x-is-msdate-time": true
          },
          "Wages": {
            "description": "The Wages for the Payslip",
            "example": 1060.5,
            "format": "double",
            "type": "number",
            "x-is-money": true
          }
        },
        "type": "object"
      },
      "ValidationError": {
        "externalDocs": {
          "url": "https://developer.xero.com/documentation/api/http-response-codes"
        },
        "properties": {
          "Message": {
            "description": "Validation error message",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
