          "NetPay": {
            "description": "The NetPay for the Payslip",
            "example": 862.5,
            "format": "double",
            "type": "number",
            "x-is-money": true
          },
          "PayslipID": {
            "description": "Xero identifier for the payslip",
            "example": "f3c0874d-7cdd-459a-a95c-d90d51decc42",
