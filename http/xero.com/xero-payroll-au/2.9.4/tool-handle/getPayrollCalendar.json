      "PayrollCalendarID": {
        "example": "4ff1e5cc-9835-40d5-bb18-09fdb118db9c",
        "format": "uuid",
        "type": "string",
        "description": "Payroll Calendar id for single object"
      },
      "Xero-Tenant-Id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "PayrollCalendarID",
      "Xero-Tenant-Id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/payroll.xro/1.0/PayrollCalendars/{PayrollCalendarID}"
    },
    "headers": {
