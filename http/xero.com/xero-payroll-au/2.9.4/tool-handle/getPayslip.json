{
  "name": "getPayslip",
  "description": "Retrieves for a payslip by a unique payslip id",
  "parameters": {
    "type": "object",
    "properties": {
      "PayslipID": {
        "example": "4ff1e5cc-9835-40d5-bb18-09fdb118db9c",
        "format": "uuid",
        "type": "string",
        "description": "Payslip id for single object"
      },
      "Xero-Tenant-Id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "PayslipID",
      "Xero-Tenant-Id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/payroll.xro/1.0/Payslip/{PayslipID}"
    },
    "headers": {
      "Xero-Tenant-Id": {
        "$": "Xero-Tenant-Id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**Payslip**",
              {
                "$ul": [
                  "**DeductionLines**",
                  "**Deductions**: The Deductions for the Payslip",
                  "**EarningsLines**",
                  "**EmployeeID**: The Xero identifier for an employee",
                  "**FirstName**: First name of employee",
                  "**LastName**: Last name of employee",
                  "**LeaveAccrualLines**",
                  "**LeaveEarningsLines**",
                  "**NetPay**: The NetPay for the Payslip",
                  "**PayslipID**: Xero identifier for the payslip",
                  "**ReimbursementLines**",
                  "**Reimbursements**: The Reimbursements for the Payslip",
                  "**Super**: The Super for the Payslip",
                  "**SuperannuationLines**",
                  "**Tax**: The Tax for the Payslip",
                  "**TaxLines**",
                  "**TimesheetEarningsLines**",
                  "**UpdatedDateUTC**: Last modified timestamp",
                  "**Wages**: The Wages for the Payslip"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
