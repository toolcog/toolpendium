{
  "name": "getLeaveApplication",
  "description": "Retrieves a leave application by a unique leave application id",
  "parameters": {
    "type": "object",
    "properties": {
      "LeaveApplicationID": {
        "example": "4ff1e5cc-9835-40d5-bb18-09fdb118db9c",
        "format": "uuid",
        "type": "string",
        "description": "Leave Application id for single object"
      },
      "Xero-Tenant-Id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "LeaveApplicationID",
      "Xero-Tenant-Id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/payroll.xro/1.0/LeaveApplications/{LeaveApplicationID}"
    },
    "headers": {
      "Xero-Tenant-Id": {
        "$": "Xero-Tenant-Id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**LeaveApplications**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
