{
  "name": "getTimesheet",
  "description": "Retrieves a timesheet by using a unique timesheet id",
  "parameters": {
    "type": "object",
    "properties": {
      "TimesheetID": {
        "example": "4ff1e5cc-9835-40d5-bb18-09fdb118db9c",
        "format": "uuid",
        "type": "string",
        "description": "Timesheet id for single object"
      },
      "Xero-Tenant-Id": {
        "type": "string",
        "description": "Xero identifier for Tenant"
      }
    },
    "required": [
      "TimesheetID",
      "Xero-Tenant-Id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.xero.com/payroll.xro/1.0/Timesheets/{TimesheetID}"
    },
    "headers": {
      "Xero-Tenant-Id": {
        "$": "Xero-Tenant-Id"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**Timesheet**",
              {
                "$ul": [
                  "**EmployeeID**: The Xero identifier for an employee",
                  "**EndDate**: Period end date (YYYY-MM-DD)",
                  "**Hours**: Timesheet total hours",
                  "**StartDate**: Period start date (YYYY-MM-DD)",
                  "**Status**",
                  "**TimesheetID**: The Xero identifier for a Payroll Timesheet",
                  "**TimesheetLines**",
                  "**UpdatedDateUTC**: Last modified timestamp",
                  "**ValidationErrors**: Displays array of validation error messages from the API"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
