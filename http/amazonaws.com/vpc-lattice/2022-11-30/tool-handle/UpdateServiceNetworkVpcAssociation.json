        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "securityGroupIds"
        ],
        "properties": {
          "securityGroupIds": {
            "description": "The IDs of the security groups. ",
