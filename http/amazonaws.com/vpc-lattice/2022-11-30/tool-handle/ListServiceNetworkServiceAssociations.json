      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://vpc-lattice.us-east-1.amazonaws.com/servicenetworkserviceassociations{?maxResults,nextToken,serviceIdentifier,serviceNetworkIdentifier}"
    },
    "headers": {
