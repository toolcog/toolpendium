{
  "name": "ListWorkflows",
  "description": "List the migration workflows.",
  "parameters": {
    "type": "object",
    "properties": {
      "maxResults": {
        "type": "integer",
        "minimum": 0,
        "maximum": 100,
        "description": "The maximum number of results that can be returned."
      },
      "nextToken": {
        "type": "string",
        "pattern": ".*\\S.*",
        "minLength": 0,
        "maxLength": 2048,
        "description": "The pagination token."
      },
      "templateId": {
        "type": "string",
        "pattern": "[-a-zA-Z0-9_.+]+[-a-zA-Z0-9_.+ ]*",
        "minLength": 1,
        "maxLength": 100,
        "description": "The ID of the template."
      },
      "adsApplicationConfigurationName": {
        "type": "string",
        "pattern": "[-a-zA-Z0-9_.+]+[-a-zA-Z0-9_.+ ]*",
        "minLength": 1,
        "maxLength": 100,
        "description": "The name of the application configured in Application Discovery Service."
      },
      "status": {
        "type": "string",
        "enum": [
          "CREATING",
          "NOT_STARTED",
          "CREATION_FAILED",
          "STARTING",
          "IN_PROGRESS",
          "WORKFLOW_FAILED",
          "PAUSED",
          "PAUSING",
          "PAUSING_FAILED",
          "USER_ATTENTION_REQUIRED",
          "DELETING",
          "DELETION_FAILED",
          "DELETED",
          "COMPLETED"
        ],
        "description": "The status of the migration workflow."
      },
      "name": {
        "type": "string",
        "description": "The name of the migration workflow."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://migrationhub-orchestrator.us-east-1.amazonaws.com/migrationworkflows{?maxResults,nextToken,templateId,adsApplicationConfigurationName,status,name}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
