      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "stepGroupId",
      "workflowId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://migrationhub-orchestrator.us-east-1.amazonaws.com/workflowstep/{id}#stepGroupId&workflowId{?stepGroupId,workflowId}"
    },
