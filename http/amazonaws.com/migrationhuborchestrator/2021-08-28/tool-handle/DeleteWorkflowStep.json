{
  "name": "DeleteWorkflowStep",
  "description": "Delete a step in a migration workflow. Pause the workflow to delete a running step.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "[a-zA-Z0-9-]+",
        "minLength": 1,
        "maxLength": 100,
        "description": "The ID of the step you want to delete."
      },
      "stepGroupId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9-]+",
        "minLength": 1,
        "maxLength": 100,
        "description": "The ID of the step group that contains the step you want to delete."
      },
      "workflowId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9-]+",
        "minLength": 1,
        "maxLength": 100,
        "description": "The ID of the migration workflow."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
