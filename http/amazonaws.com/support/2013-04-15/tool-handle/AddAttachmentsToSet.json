        }
      },
      "Attachment": {
        "type": "object",
        "properties": {
          "fileName": {
            "allOf": [
              {
                "$ref": "#/$defs/FileName"
              },
              {
                "description": "The name of the attachment file."
              }
            ]
          },
          "data": {
            "allOf": [
