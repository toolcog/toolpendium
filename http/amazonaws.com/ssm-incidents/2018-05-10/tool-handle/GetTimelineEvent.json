      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "eventId",
      "incidentRecordArn"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://ssm-incidents.us-east-1.amazonaws.com/getTimelineEvent#eventId&incidentRecordArn{?eventId,incidentRecordArn}"
    },
    "headers": {
