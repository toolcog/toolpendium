{
  "name": "ListStreams",
  "description": "<p>Lists all of the streams in your Amazon Web Services account.</p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">ListStreams</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 250,
        "description": "The maximum number of results to return at a time."
      },
      "nextToken": {
        "type": "string",
        "description": "A token used to get the next set of results."
      },
      "isAscendingOrder": {
        "type": "boolean",
        "description": "Set to true to return the list of streams in ascending order."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
