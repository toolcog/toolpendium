{
  "name": "ListCertificatesByCA",
  "description": "<p>List the device certificates signed by the specified CA certificate.</p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">ListCertificatesByCA</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "caCertificateId": {
        "type": "string",
        "pattern": "(0x)?[a-fA-F0-9]+",
        "minLength": 64,
        "maxLength": 64,
        "description": "The ID of the CA certificate. This operation will list all registered device certificate that were signed by this CA certificate."
      },
      "pageSize": {
        "type": "integer",
        "minimum": 1,
        "maximum": 250,
        "description": "The result page size."
      },
      "marker": {
        "type": "string",
        "pattern": "[A-Za-z0-9+/]+={0,2}",
        "maxLength": 1024,
        "description": "The marker for the next set of results."
      },
      "isAscendingOrder": {
        "type": "boolean",
        "description": "Specifies the order for results. If True, the results are returned in ascending order, based on the creation date."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "caCertificateId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://iot.us-east-1.amazonaws.com/certificates-by-ca/{caCertificateId}{?pageSize,marker,isAscendingOrder}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The output of the ListCertificatesByCA operation.",
        "**Key properties:**",
        {
          "$ul": [
            "**certificates**",
            "**nextMarker**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
