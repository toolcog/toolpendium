{
  "name": "ListAuditMitigationActionsTasks",
  "description": "<p>Gets a list of audit mitigation action tasks that match the specified filters.</p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">ListAuditMitigationActionsTasks</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "auditTaskId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9\\-]+",
        "minLength": 1,
        "maxLength": 40,
        "description": "Specify this filter to limit results to tasks that were applied to results for a specific audit."
      },
      "findingId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_-]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "Specify this filter to limit results to tasks that were applied to a specific audit finding."
      },
      "taskStatus": {
        "type": "string",
        "enum": [
          "IN_PROGRESS",
          "COMPLETED",
          "FAILED",
          "CANCELED"
        ],
        "description": "Specify this filter to limit results to tasks that are in a specific state."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 250,
        "description": "The maximum number of results to return at one time. The default is 25."
      },
      "nextToken": {
        "type": "string",
        "description": "The token for the next set of results."
      },
      "startTime": {
        "type": "string",
        "format": "date-time",
        "description": "Specify this filter to limit results to tasks that began on or after a specific date and time."
      },
      "endTime": {
        "type": "string",
        "format": "date-time",
        "description": "Specify this filter to limit results to tasks that were completed or canceled on or before a specific date and time."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "startTime",
      "endTime"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://iot.us-east-1.amazonaws.com/audit/mitigationactions/tasks#startTime&endTime{?auditTaskId,findingId,taskStatus,maxResults,nextToken,startTime,endTime}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**tasks**",
            "**nextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
