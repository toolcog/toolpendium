{
  "name": "ListDetectMitigationActionsExecutions",
  "description": "<p> Lists mitigation actions executions for a Device Defender ML Detect Security Profile. </p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">ListDetectMitigationActionsExecutions</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "taskId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_-]+",
        "minLength": 1,
        "maxLength": 128,
        "description": " The unique identifier of the task. "
      },
      "violationId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9\\-]+",
        "minLength": 1,
        "maxLength": 128,
        "description": " The unique identifier of the violation. "
      },
      "thingName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128,
        "description": " The name of the thing whose mitigation actions are listed. "
      },
      "startTime": {
        "type": "string",
        "format": "date-time",
        "description": " A filter to limit results to those found after the specified time. You must specify either the startTime and endTime or the taskId, but not both. "
      },
      "endTime": {
        "type": "string",
        "format": "date-time",
        "description": " The end of the time period for which ML Detect mitigation actions executions are returned. "
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 250,
        "description": " The maximum number of results to return at one time. The default is 25. "
      },
      "nextToken": {
        "type": "string",
        "description": " The token for the next set of results. "
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://iot.us-east-1.amazonaws.com/detect/mitigationactions/executions{?taskId,violationId,thingName,startTime,endTime,maxResults,nextToken}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**actionsExecutions**",
