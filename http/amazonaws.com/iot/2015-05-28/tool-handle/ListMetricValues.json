{
  "name": "ListMetricValues",
  "description": "Lists the values reported for an IoT Device Defender metric (device-side metric, cloud-side metric, or custom metric) by the given thing during the specified time period.",
  "parameters": {
    "type": "object",
    "properties": {
      "thingName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128,
        "description": "The name of the thing for which security profile metric values are returned."
      },
      "metricName": {
        "type": "string",
        "description": "The name of the security profile metric for which values are returned."
      },
      "dimensionName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9:_-]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "The dimension name."
      },
      "dimensionValueOperator": {
        "type": "string",
        "enum": [
          "IN",
          "NOT_IN"
        ],
        "description": "The dimension value operator."
      },
      "startTime": {
        "type": "string",
        "format": "date-time",
        "description": "The start of the time period for which metric values are returned."
      },
      "endTime": {
        "type": "string",
        "format": "date-time",
        "description": "The end of the time period for which metric values are returned."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 250,
        "description": "The maximum number of results to return at one time."
      },
      "nextToken": {
        "type": "string",
        "description": "The token for the next set of results."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "thingName",
      "metricName",
      "startTime",
      "endTime"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://iot.us-east-1.amazonaws.com/metric-values#thingName&metricName&startTime&endTime{?thingName,metricName,dimensionName,dimensionValueOperator,startTime,endTime,maxResults,nextToken}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**metricDatumList**",
