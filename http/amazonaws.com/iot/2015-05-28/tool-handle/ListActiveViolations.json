{
  "name": "ListActiveViolations",
  "description": "<p>Lists the active violations for a given Device Defender security profile.</p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">ListActiveViolations</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "thingName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128,
        "description": "The name of the thing whose active violations are listed."
      },
      "securityProfileName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9:_-]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "The name of the Device Defender security profile for which violations are listed."
      },
      "behaviorCriteriaType": {
        "type": "string",
        "enum": [
          "STATIC",
          "STATISTICAL",
          "MACHINE_LEARNING"
        ],
        "description": " The criteria for a behavior. "
      },
      "listSuppressedAlerts": {
        "type": "boolean",
        "description": " A list of all suppressed alerts. "
      },
      "verificationState": {
        "type": "string",
        "enum": [
          "FALSE_POSITIVE",
          "BENIGN_POSITIVE",
          "TRUE_POSITIVE",
          "UNKNOWN"
        ],
        "description": "The verification state of the violation (detect alarm)."
      },
      "nextToken": {
        "type": "string",
        "description": "The token for the next set of results."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 250,
        "description": "The maximum number of results to return at one time."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://iot.us-east-1.amazonaws.com/active-violations{?thingName,securityProfileName,behaviorCriteriaType,listSuppressedAlerts,verificationState,nextToken,maxResults}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**activeViolations**",
            "**nextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
