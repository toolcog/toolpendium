      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**copyPrivateIp**",
            "**copyTags**",
            "**ec2LaunchTemplateID**",
            "**launchDisposition**",
            "**licensing**",
            "**name**",
            "**sourceServerID**",
            "**targetInstanceTypeRightSizingMethod**"
          ]
        },
        {
