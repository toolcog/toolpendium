{
  "name": "UntagResource",
  "description": "Deletes the specified set of tags from the specified set of Elastic Disaster Recovery resources.",
  "parameters": {
    "type": "object",
    "properties": {
      "resourceArn": {
        "type": "string",
        "pattern": "^arn:.{16,2044}$",
        "minLength": 20,
        "maxLength": 2048,
        "description": "ARN of the resource for which tags are to be removed."
      },
      "tagKeys": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TagKey"
        },
        "description": "Array of tags to be removed."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "resourceArn",
      "tagKeys"
    ],
    "$defs": {
      "TagKey": {
        "type": "string",
        "minLength": 0,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://drs.us-east-1.amazonaws.com/tags/{resourceArn}#tagKeys{?tagKeys}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
