            }
          },
          "vpcID": {
            "description": "Which VPC ID to protect.",
            "type": "string",
            "pattern": "^vpc-[0-9a-fA-F]{8,}$",
            "minLength": 12,
            "maxLength": 21
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
