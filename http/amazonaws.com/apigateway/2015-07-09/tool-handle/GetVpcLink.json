{
  "name": "GetVpcLink",
  "description": "Gets a specified VPC link under the caller's account in a region.",
  "parameters": {
    "type": "object",
    "properties": {
      "vpclink_id": {
        "type": "string",
        "description": "The identifier of the VpcLink. It is used in an Integration to reference this VpcLink."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
