      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "restapi_id",
      "response_type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apigateway.us-east-1.amazonaws.com/restapis/{restapi_id}/gatewayresponses/{response_type}"
    },
    "headers": {
