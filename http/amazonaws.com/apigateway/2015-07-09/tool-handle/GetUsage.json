{
  "name": "GetUsage",
  "description": "Gets the usage data of a usage plan in a specified time interval.",
  "parameters": {
    "type": "object",
    "properties": {
      "usageplanId": {
        "type": "string",
        "description": "The Id of the usage plan associated with the usage data."
      },
