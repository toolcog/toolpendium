      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "LensAlias"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://wellarchitected.us-east-1.amazonaws.com/lenses/{LensAlias}/export{?LensVersion}"
    },
    "headers": {
