      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "ProfileArn"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://wellarchitected.us-east-1.amazonaws.com/profiles/{ProfileArn}/shares{?SharedWithPrefix,NextToken,MaxResults,Status}"
    },
    "headers": {
