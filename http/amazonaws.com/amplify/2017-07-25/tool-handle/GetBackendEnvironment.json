{
  "name": "GetBackendEnvironment",
  "description": " Returns a backend environment for an Amplify app. ",
  "parameters": {
    "type": "object",
    "properties": {
      "appId": {
        "type": "string",
        "pattern": "d[a-z0-9]+",
        "minLength": 1,
        "maxLength": 20,
        "description": " The unique id for an Amplify app. "
      },
      "environmentName": {
        "type": "string",
        "pattern": "\\(\\?s\\).\\+",
        "minLength": 1,
        "maxLength": 255,
        "description": " The name for the backend environment. "
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
