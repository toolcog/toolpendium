{
  "name": "DeleteApp",
  "description": " Deletes an existing Amplify app specified by an app ID. ",
  "parameters": {
    "type": "object",
    "properties": {
      "appId": {
        "type": "string",
        "pattern": "d[a-z0-9]+",
