{
  "name": "UpdateCertificate",
  "description": "Updates the active and inactive dates for a certificate.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "TransferService.UpdateCertificate"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateCertificateRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateCertificateRequest": {
        "type": "object",
        "required": [
          "CertificateId"
        ],
        "title": "UpdateCertificateRequest",
        "properties": {
          "CertificateId": {
            "allOf": [
              {
                "$ref": "#/$defs/CertificateId"
              },
              {
                "description": "The identifier of the certificate object that you are updating."
              }
            ]
          },
          "ActiveDate": {
            "allOf": [
              {
                "$ref": "#/$defs/CertDate"
              },
              {
                "description": "An optional date that specifies when the certificate becomes active."
              }
            ]
          },
          "InactiveDate": {
            "allOf": [
              {
                "$ref": "#/$defs/CertDate"
              },
              {
                "description": "An optional date that specifies when the certificate becomes inactive."
              }
            ]
          },
          "Description": {
            "allOf": [
              {
                "$ref": "#/$defs/Description"
              },
              {
                "description": "A short description to help identify the certificate."
              }
            ]
          }
        }
      },
      "CertificateId": {
        "type": "string",
        "pattern": "^cert-([0-9a-f]{17})$",
        "minLength": 22,
        "maxLength": 22
      },
      "CertDate": {
        "type": "string",
        "format": "date-time"
      },
      "Description": {
        "type": "string",
        "pattern": "^[\\p{Graph}]+",
        "minLength": 1,
        "maxLength": 200
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://transfer.us-east-1.amazonaws.com/#X-Amz-Target=TransferService.UpdateCertificate"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
