        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/DescribeUserRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DescribeUserRequest": {
        "type": "object",
        "required": [
          "ServerId",
          "UserName"
        ],
        "title": "DescribeUserRequest",
        "properties": {
          "ServerId": {
            "allOf": [
              {
                "$ref": "#/$defs/ServerId"
              },
              {
                "description": "A system-assigned unique identifier for a server that has this user assigned."
              }
            ]
          },
          "UserName": {
            "allOf": [
              {
                "$ref": "#/$defs/UserName"
              },
              {
                "description": "The name of the user assigned to one or more servers. User names are part of the sign-in credentials to use the Transfer Family service and perform file transfer tasks."
              }
            ]
          }
        }
      },
      "ServerId": {
        "type": "string",
        "pattern": "^s-([0-9a-f]{17})$",
        "minLength": 19,
        "maxLength": 19
      },
      "UserName": {
        "type": "string",
        "pattern": "^[\\w][\\w@.-]{2,99}$",
        "minLength": 3,
        "maxLength": 100
      }
    }
  },
  "handler": "http",
