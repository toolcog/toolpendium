{
  "name": "DeleteAccess",
  "description": "Allows you to delete the access specified in the <code>ServerID</code> and <code>ExternalID</code> parameters.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
