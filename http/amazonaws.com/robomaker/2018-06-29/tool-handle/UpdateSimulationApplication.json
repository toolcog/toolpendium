{
  "name": "UpdateSimulationApplication",
  "description": "Updates a simulation application.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "application",
          "simulationSoftwareSuite",
          "robotSoftwareSuite"
        ],
        "properties": {
          "application": {
            "description": "The application information for the simulation application.",
            "type": "string",
            "pattern": "arn:.*",
            "minLength": 1,
            "maxLength": 1224
          },
          "sources": {
            "description": "The sources of the simulation application.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/SourceConfig"
            }
          },
          "simulationSoftwareSuite": {
            "description": "Information about a simulation software suite.",
            "type": "object",
            "properties": {
              "name": {
                "allOf": [
                  {
                    "$ref": "#/$defs/SimulationSoftwareSuiteType"
                  },
                  {
                    "description": "The name of the simulation software suite."
                  }
                ]
              },
              "version": {
                "allOf": [
                  {
                    "$ref": "#/$defs/SimulationSoftwareSuiteVersionType"
                  },
                  {
                    "description": "The version of the simulation software suite."
                  }
                ]
              }
            }
          },
          "robotSoftwareSuite": {
            "description": "Information about a robot software suite (ROS distribution).",
            "type": "object",
            "properties": {
              "name": {
                "allOf": [
                  {
                    "$ref": "#/$defs/RobotSoftwareSuiteType"
                  },
                  {
                    "description": "The name of the robot software suite (ROS distribution)."
                  }
                ]
              },
              "version": {
                "allOf": [
                  {
                    "$ref": "#/$defs/RobotSoftwareSuiteVersionType"
                  },
                  {
                    "description": "The version of the robot software suite (ROS distribution)."
                  }
                ]
              }
            }
          },
          "renderingEngine": {
            "description": "Information about a rendering engine.",
            "type": "object",
            "properties": {
              "name": {
                "allOf": [
                  {
                    "$ref": "#/$defs/RenderingEngineType"
                  },
                  {
                    "description": "The name of the rendering engine."
                  }
                ]
              },
              "version": {
                "allOf": [
                  {
                    "$ref": "#/$defs/RenderingEngineVersionType"
                  },
                  {
                    "description": "The version of the rendering engine."
                  }
                ]
              }
            }
          },
          "currentRevisionId": {
            "description": "The revision id for the robot application.",
            "type": "string",
            "pattern": "[a-zA-Z0-9_.\\-]*",
            "minLength": 1,
            "maxLength": 40
          },
          "environment": {
            "description": "The object that contains the Docker image URI for either your robot or simulation applications.",
            "type": "object",
            "properties": {
              "uri": {
                "allOf": [
                  {
                    "$ref": "#/$defs/RepositoryUrl"
                  },
                  {
                    "description": "The Docker image URI for either your robot or simulation applications."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "SourceConfig": {
        "type": "object",
        "properties": {
          "s3Bucket": {
            "allOf": [
              {
                "$ref": "#/$defs/S3Bucket"
              },
              {
                "description": "The Amazon S3 bucket name."
              }
            ]
          },
          "s3Key": {
            "allOf": [
              {
                "$ref": "#/$defs/S3Key"
              },
              {
                "description": "The s3 object key."
              }
            ]
          },
          "architecture": {
            "allOf": [
              {
                "$ref": "#/$defs/Architecture"
              },
              {
                "description": "The target processor architecture for the application."
              }
            ]
          }
        },
        "description": "Information about a source configuration."
      },
      "S3Bucket": {
        "type": "string",
        "pattern": "[a-z0-9][a-z0-9.\\-]*[a-z0-9]",
        "minLength": 3,
        "maxLength": 63
      },
      "S3Key": {
        "type": "string",
        "pattern": ".*",
        "minLength": 1,
        "maxLength": 1024
      },
      "Architecture": {
        "type": "string",
        "enum": [
          "X86_64",
          "ARM64",
          "ARMHF"
        ]
      },
      "SimulationSoftwareSuiteType": {
        "type": "string",
        "enum": [
          "Gazebo",
          "RosbagPlay",
          "SimulationRuntime"
        ]
      },
      "SimulationSoftwareSuiteVersionType": {
        "type": "string",
        "pattern": "7|9|11|Kinetic|Melodic|Dashing|Foxy",
        "minLength": 0,
        "maxLength": 1024
      },
      "RobotSoftwareSuiteType": {
        "type": "string",
        "enum": [
          "ROS",
          "ROS2",
          "General"
        ]
      },
      "RobotSoftwareSuiteVersionType": {
        "type": "string",
        "enum": [
          "Kinetic",
          "Melodic",
          "Dashing",
          "Foxy"
        ]
      },
      "RenderingEngineType": {
        "type": "string",
        "enum": [
          "OGRE"
        ]
      },
      "RenderingEngineVersionType": {
        "type": "string",
        "pattern": "1.x",
        "minLength": 1,
        "maxLength": 4
      },
      "RepositoryUrl": {
        "type": "string",
        "pattern": ".+",
        "minLength": 1,
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://robomaker.us-east-1.amazonaws.com/updateSimulationApplication"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**arn**",
            "**name**",
            "**version**",
            "**sources**",
            "**simulationSoftwareSuite**",
            "**robotSoftwareSuite**",
            "**renderingEngine**",
            "**lastUpdatedAt**",
            "**revisionId**",
            "**environment**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "482": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "483": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "484": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
