{
  "name": "StartMulticastGroupSession",
  "description": "Starts a multicast group session.",
  "parameters": {
    "type": "object",
    "properties": {
      "Id": {
        "type": "string",
        "description": "",
        "maxLength": 256
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
