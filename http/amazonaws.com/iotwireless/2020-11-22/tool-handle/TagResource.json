{
  "name": "TagResource",
  "description": "Adds a tag to a resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "resourceArn": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1011,
        "description": "The ARN of the resource to add tags to."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
