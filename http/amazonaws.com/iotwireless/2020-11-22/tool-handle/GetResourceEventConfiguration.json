      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "Identifier",
      "identifierType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.iotwireless.us-east-1.amazonaws.com/event-configurations/{Identifier}#identifierType{?identifierType,partnerType}"
