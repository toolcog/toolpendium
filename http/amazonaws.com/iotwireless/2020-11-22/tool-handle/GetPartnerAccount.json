{
  "name": "GetPartnerAccount",
  "description": "Gets information about a partner account. If <code>PartnerAccountId</code> and <code>PartnerType</code> are <code>null</code>, returns all partner accounts.",
  "parameters": {
    "type": "object",
    "properties": {
