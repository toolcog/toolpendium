{
  "name": "DeregisterWirelessDevice",
  "description": "Deregister a wireless device from AWS IoT Wireless.",
  "parameters": {
    "type": "object",
    "properties": {
      "Identifier": {
        "type": "string",
        "maxLength": 256,
        "description": "The identifier of the wireless device to deregister from AWS IoT Wireless."
      },
      "WirelessDeviceType": {
        "type": "string",
        "enum": [
          "Sidewalk",
          "LoRaWAN"
        ],
