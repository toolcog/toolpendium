        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "Identifier"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "http://api.iotwireless.us-east-1.amazonaws.com/wireless-devices/{Identifier}/deregister{?WirelessDeviceType}"
    },
