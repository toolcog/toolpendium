{
  "name": "ListDevices",
  "description": "Lists the 1-Click compatible devices associated with your AWS account.",
  "parameters": {
    "type": "object",
    "properties": {
      "deviceType": {
        "type": "string",
        "description": "The type of the device, such as \"button\"."
