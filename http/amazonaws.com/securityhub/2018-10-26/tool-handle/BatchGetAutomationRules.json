{
  "name": "BatchGetAutomationRules",
  "description": " Retrieves a list of details for automation rules based on rule Amazon Resource Names (ARNs). ",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
