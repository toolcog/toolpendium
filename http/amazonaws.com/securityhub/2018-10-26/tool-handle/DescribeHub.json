{
  "name": "DescribeHub",
  "description": "Returns details about the Hub resource in your account, including the <code>HubArn</code> and the time when you enabled Security Hub.",
  "parameters": {
    "type": "object",
    "properties": {
      "HubArn": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The ARN of the Hub resource to retrieve."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://securityhub.us-east-1.amazonaws.com/accounts{?HubArn}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
