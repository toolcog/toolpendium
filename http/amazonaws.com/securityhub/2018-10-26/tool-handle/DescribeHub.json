      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**HubArn**",
            "**SubscribedAt**",
            "**AutoEnableControls**",
            "**ControlFindingGenerator**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
