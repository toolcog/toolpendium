      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "FindingAggregatorArn"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://securityhub.us-east-1.amazonaws.com/findingAggregator/delete/{FindingAggregatorArn}"
    },
