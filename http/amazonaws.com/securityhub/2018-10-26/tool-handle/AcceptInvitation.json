      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "MasterId",
          "InvitationId"
        ],
        "properties": {
          "MasterId": {
            "description": "The account ID of the Security Hub administrator account that sent the invitation.",
            "type": "string",
            "pattern": ".*\\S.*"
          },
          "InvitationId": {
            "description": "The identifier of the invitation sent from the Security Hub administrator account.",
            "type": "string",
            "pattern": ".*\\S.*"
          }
        }
      }
    },
    "required": [
