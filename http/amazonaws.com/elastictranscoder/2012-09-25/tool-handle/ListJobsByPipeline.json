{
  "name": "ListJobsByPipeline",
  "description": "<p>The ListJobsByPipeline operation gets a list of the jobs currently in a pipeline.</p> <p>Elastic Transcoder returns all of the jobs currently in the specified pipeline. The response body contains one element for each job that satisfies the search criteria.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "PipelineId": {
        "type": "string",
        "pattern": "^\\d{13}-\\w{6}$",
        "description": "The ID of the pipeline for which you want to get job information."
      },
      "Ascending": {
        "type": "string",
        "pattern": "(^true$)|(^false$)",
        "description": " To list jobs in chronological order by the date and time that they were submitted, enter <code>true</code>. To list jobs in reverse chronological order, enter <code>false</code>. "
      },
      "PageToken": {
        "type": "string",
        "pattern": "^\\d{13}-\\w{6}$",
        "description": " When Elastic Transcoder returns more than one page of results, use <code>pageToken</code> in subsequent <code>GET</code> requests to get each successive page of results. "
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "PipelineId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://elastictranscoder.us-east-1.amazonaws.com/2012-09-25/jobsByPipeline/{PipelineId}{?Ascending,PageToken}"
    },
    "headers": {
