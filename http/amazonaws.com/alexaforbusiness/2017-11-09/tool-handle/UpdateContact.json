{
  "name": "UpdateContact",
  "description": "Updates the contact details by the contact ARN.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AlexaForBusiness.UpdateContact"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateContactRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateContactRequest": {
        "type": "object",
        "required": [
          "ContactArn"
        ],
        "title": "UpdateContactRequest",
        "properties": {
          "ContactArn": {
            "allOf": [
              {
                "$ref": "#/$defs/Arn"
              },
              {
                "description": "The ARN of the contact to update."
              }
            ]
          },
          "DisplayName": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactName"
              },
              {
                "description": "The updated display name of the contact."
              }
            ]
          },
          "FirstName": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactName"
              },
              {
                "description": "The updated first name of the contact."
              }
            ]
          },
          "LastName": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactName"
              },
              {
                "description": "The updated last name of the contact."
              }
            ]
          },
          "PhoneNumber": {
            "allOf": [
              {
                "$ref": "#/$defs/RawPhoneNumber"
              },
              {
                "description": "The updated phone number of the contact. The phone number type defaults to WORK. You can either specify PhoneNumber or PhoneNumbers. We recommend that you use PhoneNumbers, which lets you specify the phone number type and multiple numbers."
              }
            ]
          },
          "PhoneNumbers": {
            "allOf": [
              {
                "$ref": "#/$defs/PhoneNumberList"
              },
              {
                "description": "The list of phone numbers for the contact."
              }
            ]
          },
          "SipAddresses": {
            "allOf": [
              {
                "$ref": "#/$defs/SipAddressList"
              },
              {
                "description": "The list of SIP addresses for the contact."
              }
            ]
          }
        }
      },
      "Arn": {
        "type": "string",
        "pattern": "arn:[a-z0-9-\\.]{1,63}:[a-z0-9-\\.]{0,63}:[a-z0-9-\\.]{0,63}:[a-z0-9-\\.]{0,63}:[^/].{0,1023}"
      },
      "ContactName": {
        "type": "string",
        "pattern": "[\\u0009\\u000A\\u000D\\u0020-\\u007E\\u0085\\u00A0-\\uD7FF\\uE000-\\uFFFD\\u10000-\\u10FFFF]*",
        "minLength": 1,
        "maxLength": 100
      },
      "RawPhoneNumber": {
        "type": "string",
        "pattern": "^[\\+0-9\\#\\,\\(][\\+0-9\\-\\.\\/\\(\\)\\,\\#\\s]+$",
        "minLength": 0,
        "maxLength": 50,
        "format": "password"
      },
      "PhoneNumberList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/PhoneNumber"
        },
        "minItems": 0,
        "maxItems": 3
      },
      "PhoneNumber": {
        "type": "object",
        "required": [
          "Number",
          "Type"
        ],
        "properties": {
          "Number": {
            "allOf": [
              {
                "$ref": "#/$defs/RawPhoneNumber"
              },
              {
                "description": "The raw value of the phone number."
              }
            ]
          },
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/PhoneNumberType"
              },
              {
                "description": "The type of the phone number."
              }
            ]
          }
        },
        "description": "The phone number for the contact containing the raw number and phone number type."
      },
      "PhoneNumberType": {
        "type": "string",
        "enum": [
          "MOBILE",
          "WORK",
          "HOME"
        ],
        "format": "password"
      },
      "SipAddressList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/SipAddress"
        },
        "minItems": 0,
        "maxItems": 1
      },
      "SipAddress": {
        "type": "object",
        "required": [
          "Uri",
          "Type"
        ],
        "properties": {
          "Uri": {
            "allOf": [
              {
                "$ref": "#/$defs/SipUri"
              },
              {
                "description": "The URI for the SIP address."
              }
            ]
          },
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/SipType"
              },
              {
                "description": "The type of the SIP address."
              }
            ]
          }
        },
        "description": "The SIP address for the contact containing the URI and SIP address type."
      },
      "SipUri": {
        "type": "string",
        "pattern": "^sip[s]?:([^@:]+)\\@([^@]+)$",
        "minLength": 1,
        "maxLength": 256,
        "format": "password"
      },
      "SipType": {
        "type": "string",
        "enum": [
          "WORK"
        ],
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://a4b.us-east-1.amazonaws.com/#X-Amz-Target=AlexaForBusiness.UpdateContact"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
