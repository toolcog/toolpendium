{
  "name": "UpdateNetworkProfile",
  "description": "Updates a network profile by the network profile ARN.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AlexaForBusiness.UpdateNetworkProfile"
        ]
