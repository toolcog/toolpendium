{
  "name": "CreateContact",
  "description": "Creates a contact with the specified details.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AlexaForBusiness.CreateContact"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/CreateContactRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "CreateContactRequest": {
        "type": "object",
        "required": [
          "FirstName"
        ],
        "title": "CreateContactRequest",
        "properties": {
          "DisplayName": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactName"
              },
              {
                "description": "The name of the contact to display on the console."
              }
            ]
          },
          "FirstName": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactName"
              },
              {
                "description": "The first name of the contact that is used to call the contact on the device."
              }
            ]
          },
          "LastName": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactName"
              },
              {
                "description": "The last name of the contact that is used to call the contact on the device."
              }
            ]
          },
          "PhoneNumber": {
            "allOf": [
              {
                "$ref": "#/$defs/RawPhoneNumber"
              },
              {
                "description": "The phone number of the contact in E.164 format. The phone number type defaults to WORK. You can specify PhoneNumber or PhoneNumbers. We recommend that you use PhoneNumbers, which lets you specify the phone number type and multiple numbers."
              }
            ]
          },
          "PhoneNumbers": {
            "allOf": [
              {
                "$ref": "#/$defs/PhoneNumberList"
              },
              {
                "description": "The list of phone numbers for the contact."
              }
            ]
          },
          "SipAddresses": {
            "allOf": [
              {
                "$ref": "#/$defs/SipAddressList"
              },
              {
                "description": "The list of SIP addresses for the contact."
              }
            ]
          },
          "ClientRequestToken": {
            "allOf": [
              {
                "$ref": "#/$defs/ClientRequestToken"
              },
              {
                "description": "A unique, user-specified identifier for this request that ensures idempotency."
              }
            ]
          },
          "Tags": {
            "allOf": [
              {
                "$ref": "#/$defs/TagList"
              },
              {
                "description": "The tags to be added to the specified resource. Do not provide system tags."
              }
            ]
          }
        }
      },
      "ContactName": {
        "type": "string",
        "pattern": "[\\u0009\\u000A\\u000D\\u0020-\\u007E\\u0085\\u00A0-\\uD7FF\\uE000-\\uFFFD\\u10000-\\u10FFFF]*",
        "minLength": 1,
        "maxLength": 100
      },
      "RawPhoneNumber": {
        "type": "string",
        "pattern": "^[\\+0-9\\#\\,\\(][\\+0-9\\-\\.\\/\\(\\)\\,\\#\\s]+$",
        "minLength": 0,
        "maxLength": 50,
        "format": "password"
      },
      "PhoneNumberList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/PhoneNumber"
        },
        "minItems": 0,
        "maxItems": 3
      },
      "PhoneNumber": {
        "type": "object",
        "required": [
          "Number",
          "Type"
        ],
        "properties": {
          "Number": {
            "allOf": [
              {
                "$ref": "#/$defs/RawPhoneNumber"
              },
              {
                "description": "The raw value of the phone number."
              }
            ]
          },
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/PhoneNumberType"
              },
              {
                "description": "The type of the phone number."
              }
            ]
          }
        },
        "description": "The phone number for the contact containing the raw number and phone number type."
      },
      "PhoneNumberType": {
        "type": "string",
        "enum": [
          "MOBILE",
          "WORK",
          "HOME"
        ],
        "format": "password"
      },
      "SipAddressList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/SipAddress"
        },
        "minItems": 0,
        "maxItems": 1
      },
      "SipAddress": {
        "type": "object",
        "required": [
          "Uri",
          "Type"
        ],
        "properties": {
          "Uri": {
            "allOf": [
              {
                "$ref": "#/$defs/SipUri"
              },
              {
                "description": "The URI for the SIP address."
              }
            ]
          },
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/SipType"
              },
              {
                "description": "The type of the SIP address."
              }
            ]
          }
        },
        "description": "The SIP address for the contact containing the URI and SIP address type."
      },
      "SipUri": {
        "type": "string",
        "pattern": "^sip[s]?:([^@:]+)\\@([^@]+)$",
        "minLength": 1,
        "maxLength": 256,
        "format": "password"
      },
      "SipType": {
        "type": "string",
        "enum": [
          "WORK"
        ],
        "format": "password"
      },
      "ClientRequestToken": {
        "type": "string",
        "pattern": "[a-zA-Z0-9][a-zA-Z0-9_-]*",
        "description": "A unique, user-specified identifier for the request that ensures idempotency.",
        "minLength": 10,
        "maxLength": 150
      },
      "TagList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Tag"
        }
      },
      "Tag": {
        "type": "object",
        "required": [
          "Key",
          "Value"
        ],
        "properties": {
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "The key of a tag. Tag keys are case-sensitive. "
              }
            ]
          },
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/TagValue"
              },
              {
                "description": "The value of a tag. Tag values are case sensitive and can be null."
              }
            ]
          }
        },
        "description": "A key-value pair that can be associated with a resource. "
      },
      "TagKey": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 1,
        "maxLength": 128
      },
      "TagValue": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 0,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://a4b.us-east-1.amazonaws.com/#X-Amz-Target=AlexaForBusiness.CreateContact"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ContactArn**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
