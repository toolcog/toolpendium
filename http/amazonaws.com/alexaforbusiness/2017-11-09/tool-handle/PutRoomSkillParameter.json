              }
            ]
          },
          "RoomSkillParameter": {
            "allOf": [
              {
                "$ref": "#/$defs/RoomSkillParameter"
              },
              {
                "description": "The updated room skill parameter. Required."
              }
            ]
          }
        }
      },
      "Arn": {
        "type": "string",
        "pattern": "arn:[a-z0-9-\\.]{1,63}:[a-z0-9-\\.]{0,63}:[a-z0-9-\\.]{0,63}:[a-z0-9-\\.]{0,63}:[^/].{0,1023}"
