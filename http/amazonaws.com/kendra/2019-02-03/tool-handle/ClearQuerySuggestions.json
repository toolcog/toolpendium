        "$ref": "#/$defs/ClearQuerySuggestionsRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ClearQuerySuggestionsRequest": {
        "type": "object",
        "required": [
          "IndexId"
        ],
        "title": "ClearQuerySuggestionsRequest",
        "properties": {
