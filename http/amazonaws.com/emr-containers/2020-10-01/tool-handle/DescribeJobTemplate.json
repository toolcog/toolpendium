    "type": "object",
    "properties": {
      "templateId": {
        "type": "string",
        "pattern": "[0-9a-z]+",
        "minLength": 1,
        "maxLength": 64,
        "description": "The ID of the job template that will be described."
      },
      "X-Amz-Content-Sha256": {
