{
  "name": "ConfigureLogsForPlaybackConfiguration",
  "description": "Amazon CloudWatch log settings for a playback configuration.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
