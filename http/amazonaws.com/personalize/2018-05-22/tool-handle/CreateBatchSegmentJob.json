        "minLength": 1,
        "maxLength": 63
      },
      "Arn": {
        "type": "string",
        "pattern": "arn:([a-z\\d-]+):personalize:.*:.*:.+",
        "maxLength": 256
      },
      "NumBatchResults": {
        "type": "integer"
      },
