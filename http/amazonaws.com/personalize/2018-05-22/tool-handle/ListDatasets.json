        "description": "Pagination limit"
      },
      "nextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AmazonPersonalize.ListDatasets"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
