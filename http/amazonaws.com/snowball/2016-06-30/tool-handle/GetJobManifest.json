      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/GetJobManifestRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "GetJobManifestRequest": {
        "type": "object",
        "required": [
          "JobId"
        ],
        "title": "GetJobManifestRequest",
        "properties": {
