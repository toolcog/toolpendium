{
  "name": "CreateWorkspace",
  "description": "Creates a workplace.",
  "parameters": {
    "type": "object",
    "properties": {
      "workspaceId": {
        "type": "string",
        "pattern": "[a-zA-Z_0-9][a-zA-Z_\\-0-9]*[a-zA-Z0-9]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "The ID of the workspace."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "s3Location",
          "role"
        ],
        "properties": {
          "description": {
            "description": "The description of the workspace.",
            "type": "string",
            "pattern": ".*",
            "minLength": 0,
            "maxLength": 2048
          },
          "s3Location": {
            "description": "The ARN of the S3 bucket where resources associated with the workspace are stored.",
            "type": "string",
            "pattern": ".*(^arn:((aws)|(aws-cn)|(aws-us-gov)):s3:::)([a-zA-Z0-9_-]+$).*",
            "minLength": 0,
            "maxLength": 1024
          },
          "role": {
            "description": "The ARN of the execution role associated with the workspace.",
            "type": "string",
            "pattern": "arn:((aws)|(aws-cn)|(aws-us-gov)):iam::[0-9]{12}:role/.*",
            "minLength": 20,
            "maxLength": 2048
          },
          "tags": {
            "description": "Metadata that you can use to manage the workspace",
            "type": "object",
            "minProperties": 0,
            "maxProperties": 50,
            "additionalProperties": {
              "$ref": "#/$defs/TagValue"
            }
          }
        }
      }
    },
    "required": [
      "workspaceId",
      "body"
    ],
    "$defs": {
      "TagValue": {
        "type": "string",
        "pattern": ".*",
        "minLength": 1,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://iottwinmaker.us-east-1.amazonaws.com/workspaces/{workspaceId}"
    },
    "headers": {
