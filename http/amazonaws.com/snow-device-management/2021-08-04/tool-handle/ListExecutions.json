{
  "name": "ListExecutions",
  "description": "Returns the status of tasks for one or more target devices.",
  "parameters": {
    "type": "object",
    "properties": {
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100,
        "description": "The maximum number of tasks to list per page."
      },
      "nextToken": {
        "type": "string",
        "pattern": "[a-zA-Z0-9+/=]*",
        "minLength": 1,
        "maxLength": 1024,
        "description": "A pagination token to continue to the next page of tasks."
      },
      "state": {
        "type": "string",
        "enum": [
          "QUEUED",
          "IN_PROGRESS",
          "CANCELED",
          "FAILED",
          "SUCCEEDED",
          "REJECTED",
          "TIMED_OUT"
        ],
        "description": "A structure used to filter the tasks by their current state."
      },
      "taskId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 64,
        "description": "The ID of the task."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "taskId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://snow-device-management.us-east-1.amazonaws.com/executions#taskId{?maxResults,nextToken,state,taskId}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**executions**",
