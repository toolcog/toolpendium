{
  "name": "ListDeviceResources",
  "description": "Returns a list of the Amazon Web Services resources available for a device. Currently, Amazon EC2 instances are the only supported resource type.",
  "parameters": {
    "type": "object",
    "properties": {
      "managedDeviceId": {
