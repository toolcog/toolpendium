      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "AutoScalingGroupName",
      "Action",
      "Version"
    ],
    "$defs": {
      "XmlStringMaxLen255": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255,
        "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\r\\n\\t]*"
      }
    }
  },
  "handler": "http",
  "request": {
