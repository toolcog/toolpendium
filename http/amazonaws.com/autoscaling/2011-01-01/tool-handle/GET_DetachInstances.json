        "maxLength": 19,
        "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\r\\n\\t]*"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://autoscaling.us-east-1.amazonaws.com/#Action=DetachInstances{?InstanceIds,AutoScalingGroupName,ShouldDecrementDesiredCapacity,Action,Version}"
    },
