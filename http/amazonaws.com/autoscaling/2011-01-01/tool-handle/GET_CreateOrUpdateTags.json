                "$ref": "#/$defs/XmlString"
              },
              {
                "description": "The name of the Auto Scaling group."
              }
            ]
          },
          "ResourceType": {
            "allOf": [
              {
                "$ref": "#/$defs/XmlString"
              },
              {
                "description": "The type of resource. The only supported value is <code>auto-scaling-group</code>."
              }
            ]
          },
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "The tag key."
