{
  "name": "UpdateJobStatus",
  "description": "<p>Updates the status for the specified job. Use this action to confirm that you want to run a job or to cancel an existing job. For more information, see <a href=\"https://docs.aws.amazon.com/AmazonS3/latest/userguide/batch-ops.html\">S3 Batch Operations</a> in the <i>Amazon S3 User Guide</i>.</p> <p/> <p>Related actions include:</p> <ul> <li> <p> <a href=\"https://docs.aws.amazon.com/AmazonS3/latest/API/API_control_CreateJob.html\">CreateJob</a> </p> </li> <li> <p> <a href=\"https://docs.aws.amazon.com/AmazonS3/latest/API/API_control_ListJobs.html\">ListJobs</a> </p> </li> <li> <p> <a href=\"https://docs.aws.amazon.com/AmazonS3/latest/API/API_control_DescribeJob.html\">DescribeJob</a> </p> </li> <li> <p> <a href=\"https://docs.aws.amazon.com/AmazonS3/latest/API/API_control_UpdateJobStatus.html\">UpdateJobStatus</a> </p> </li> </ul>",
  "parameters": {
    "type": "object",
    "properties": {
      "x-amz-account-id": {
        "type": "string",
        "pattern": "^\\d{12}$",
        "maxLength": 64,
        "description": "The Amazon Web Services account ID associated with the S3 Batch Operations job."
      },
      "id": {
        "type": "string",
        "pattern": "[a-zA-Z0-9\\-\\_]+",
        "minLength": 5,
        "maxLength": 36,
        "description": "The ID of the job whose status you want to update."
      },
      "requestedJobStatus": {
        "type": "string",
        "enum": [
          "Cancelled",
          "Ready"
        ],
        "description": "The status that you want to move the specified job to."
      },
      "statusUpdateReason": {
        "type": "string",
        "minLength": 1,
        "maxLength": 256,
        "description": "A description of the reason why you want to change the specified job's status. This field can be any string up to the maximum length."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "x-amz-account-id",
      "id",
      "requestedJobStatus"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://s3-control.us-east-1.amazonaws.com/v20180820/jobs/{id}/status#x-amz-account-id&requestedJobStatus{?requestedJobStatus,statusUpdateReason}"
    },
    "headers": {
      "x-amz-account-id": {
        "$": "x-amz-account-id"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
