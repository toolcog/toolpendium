{
  "name": "DeleteSchedule",
  "description": "Deletes the specified schedule.",
  "parameters": {
    "type": "object",
    "properties": {
      "clientToken": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9-_]+$",
        "minLength": 1,
        "maxLength": 64,
        "description": " Unique, case-sensitive identifier you provide to ensure the idempotency of the request. If you do not specify a client token, EventBridge Scheduler uses a randomly generated token for the request to ensure idempotency. "
      },
      "groupName": {
        "type": "string",
        "pattern": "^[0-9a-zA-Z-_.]+$",
        "minLength": 1,
        "maxLength": 64,
        "description": "The name of the schedule group associated with this schedule. If you omit this, the default schedule group is used."
      },
      "Name": {
        "type": "string",
        "pattern": "^[0-9a-zA-Z-_.]+$",
        "minLength": 1,
        "maxLength": 64,
        "description": "The name of the schedule to delete."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "Name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://scheduler.us-east-1.amazonaws.com/schedules/{Name}{?clientToken,groupName}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
