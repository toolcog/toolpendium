{
  "name": "GET_ModifyUser",
  "description": "Changes user password(s) and/or access string.",
  "parameters": {
    "type": "object",
    "properties": {
      "UserId": {
        "type": "string",
        "pattern": "[a-zA-Z][a-zA-Z0-9\\-]*",
        "minLength": 1,
        "description": "The ID of the user."
      },
      "AccessString": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "Access permissions string used for this user."
      },
      "AppendAccessString": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "Adds additional user permissions to the access string."
      },
      "Passwords": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/String"
        },
        "minItems": 1,
        "description": "The passwords belonging to the user. You are allowed up to two."
      },
      "NoPasswordRequired": {
        "type": "boolean",
        "description": "Indicates no password is required for the user."
      },
      "AuthenticationMode": {
        "type": "object",
        "properties": {
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/InputAuthenticationType"
              },
              {
                "description": "Specifies the authentication type. Possible options are IAM authentication, password and no password."
              }
            ]
          },
          "Passwords": {
            "allOf": [
              {
                "$ref": "#/$defs/PasswordListInput"
              },
              {
                "description": "Specifies the passwords to use for authentication if <code>Type</code> is set to <code>password</code>."
              }
            ]
          }
        },
        "description": "Specifies how to authenticate the user."
      },
      "Action": {
        "type": "string",
        "enum": [
          "ModifyUser"
        ]
      },
      "Version": {
        "type": "string",
        "enum": [
          "2015-02-02"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "UserId",
      "Action",
      "Version"
    ],
    "$defs": {
      "String": {
        "type": "string"
      },
      "InputAuthenticationType": {
        "type": "string",
        "enum": [
          "password",
          "no-password-required",
          "iam"
        ]
      },
      "PasswordListInput": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/String"
        },
        "minItems": 1
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://elasticache.us-east-1.amazonaws.com/#Action=ModifyUser{?UserId,AccessString,AppendAccessString,Passwords,NoPasswordRequired,AuthenticationMode,Action,Version}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
