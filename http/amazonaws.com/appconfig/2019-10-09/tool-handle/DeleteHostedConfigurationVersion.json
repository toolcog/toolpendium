{
  "name": "DeleteHostedConfigurationVersion",
  "description": "Deletes a version of a configuration from the AppConfig hosted configuration store.",
  "parameters": {
    "type": "object",
    "properties": {
      "ApplicationId": {
        "type": "string",
        "pattern": "[a-z0-9]{4,7}",
        "description": "The application ID."
      },
      "ConfigurationProfileId": {
        "type": "string",
        "pattern": "[a-z0-9]{4,7}",
