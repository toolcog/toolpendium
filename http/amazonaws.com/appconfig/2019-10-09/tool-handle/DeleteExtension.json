{
  "name": "DeleteExtension",
  "description": "Deletes an AppConfig extension. You must delete all associations to an extension before you delete the extension.",
  "parameters": {
    "type": "object",
    "properties": {
      "ExtensionIdentifier": {
        "type": "string",
        "minLength": 1,
        "maxLength": 2048,
        "description": "The name, ID, or Amazon Resource Name (ARN) of the extension you want to delete."
      },
      "version": {
        "type": "integer",
        "description": "A specific version of an extension to delete. If omitted, the highest version is deleted."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "ExtensionIdentifier"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://appconfig.us-east-1.amazonaws.com/extensions/{ExtensionIdentifier}{?version}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
