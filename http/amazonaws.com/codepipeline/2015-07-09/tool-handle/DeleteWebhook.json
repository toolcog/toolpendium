        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/DeleteWebhookInput"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DeleteWebhookInput": {
        "type": "object",
        "required": [
          "name"
        ],
        "title": "DeleteWebhookInput",
        "properties": {
          "name": {
            "allOf": [
