                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "datastoreId",
      "sourceImageSetId",
      "body"
    ],
    "$defs": {
      "CopySourceImageSetInformation": {
        "type": "object",
        "required": [
          "latestVersionId"
        ],
        "properties": {
          "latestVersionId": {
            "allOf": [
              {
                "$ref": "#/$defs/ImageSetExternalVersionId"
              },
              {
                "description": "The latest version identifier for the source image set."
              }
            ]
          }
        },
        "description": "Copy source image set information."
      },
      "ImageSetExternalVersionId": {
        "type": "string",
        "pattern": "\\d+"
      },
      "CopyDestinationImageSet": {
        "type": "object",
        "required": [
          "imageSetId",
          "latestVersionId"
        ],
        "properties": {
          "imageSetId": {
            "allOf": [
