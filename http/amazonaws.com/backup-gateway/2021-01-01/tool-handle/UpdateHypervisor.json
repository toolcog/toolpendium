      },
      "Name": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9-]*$",
        "minLength": 1,
        "maxLength": 100
      },
      "Password": {
        "type": "string",
        "pattern": "^[ -~]+$",
        "minLength": 1,
        "maxLength": 100,
        "format": "password"
      },
      "Username": {
