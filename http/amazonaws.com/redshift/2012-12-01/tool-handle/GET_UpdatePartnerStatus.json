      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "AccountId",
      "ClusterIdentifier",
      "DatabaseName",
      "PartnerName",
      "Status",
      "Action",
      "Version"
    ]
  },
  "handler": "http",
  "request": {
