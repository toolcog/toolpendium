{
  "name": "GET_EnableLogging",
  "description": "Starts logging information, such as queries and connection attempts, for the specified Amazon Redshift cluster.",
  "parameters": {
    "type": "object",
    "properties": {
      "ClusterIdentifier": {
        "type": "string",
        "maxLength": 2147483647,
        "description": "<p>The identifier of the cluster on which logging is to be started.</p> <p>Example: <code>examplecluster</code> </p>"
      },
      "BucketName": {
        "type": "string",
        "maxLength": 2147483647,
        "description": "<p>The name of an existing S3 bucket where the log files are to be stored.</p> <p>Constraints:</p> <ul> <li> <p>Must be in the same region as the cluster</p> </li> <li> <p>The cluster must have read bucket and put object permissions</p> </li> </ul>"
      },
      "S3KeyPrefix": {
        "type": "string",
        "maxLength": 2147483647,
        "description": "<p>The prefix applied to the log file names.</p> <p>Constraints:</p> <ul> <li> <p>Cannot exceed 512 characters</p> </li> <li> <p>Cannot contain spaces( ), double quotes (\"), single quotes ('), a backslash (\\), or control characters. The hexadecimal codes for invalid characters are: </p> <ul> <li> <p>x00 to x20</p> </li> <li> <p>x22</p> </li> <li> <p>x27</p> </li> <li> <p>x5c</p> </li> <li> <p>x7f or larger</p> </li> </ul> </li> </ul>"
      },
      "LogDestinationType": {
        "type": "string",
        "enum": [
          "s3",
          "cloudwatch"
        ],
        "description": "The log destination type. An enum with possible values of <code>s3</code> and <code>cloudwatch</code>."
      },
      "LogExports": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/String"
        },
        "description": "The collection of exported log types. Possible values are <code>connectionlog</code>, <code>useractivitylog</code>, and <code>userlog</code>."
      },
      "Action": {
        "type": "string",
        "enum": [
          "EnableLogging"
        ]
      },
      "Version": {
        "type": "string",
        "enum": [
          "2012-12-01"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "ClusterIdentifier",
      "Action",
      "Version"
    ],
    "$defs": {
      "String": {
        "type": "string",
        "maxLength": 2147483647
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://redshift.us-east-1.amazonaws.com/#Action=EnableLogging{?ClusterIdentifier,BucketName,S3KeyPrefix,LogDestinationType,LogExports,Action,Version}"
