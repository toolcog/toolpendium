{
  "name": "ListReplays",
  "description": "Lists your replays. You can either list all the replays or you can provide a prefix to match to the replay names. Filter parameters are exclusive.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSEvents.ListReplays"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ListReplaysRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ListReplaysRequest": {
        "type": "object",
        "title": "ListReplaysRequest",
        "properties": {
          "NamePrefix": {
            "allOf": [
              {
                "$ref": "#/$defs/ReplayName"
              },
              {
                "description": "A name prefix to filter the replays returned. Only replays with name that match the prefix are returned."
              }
            ]
          },
          "State": {
            "allOf": [
              {
                "$ref": "#/$defs/ReplayState"
              },
              {
                "description": "The state of the replay."
              }
            ]
          },
          "EventSourceArn": {
            "allOf": [
              {
                "$ref": "#/$defs/Arn"
              },
              {
                "description": "The ARN of the archive from which the events are replayed."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "The token returned by a previous call to retrieve the next set of results."
              }
            ]
          },
          "Limit": {
            "allOf": [
              {
                "$ref": "#/$defs/LimitMax100"
              },
              {
                "description": "The maximum number of replays to retrieve."
              }
            ]
          }
        }
      },
      "ReplayName": {
        "type": "string",
        "pattern": "[\\.\\-_A-Za-z0-9]+",
        "minLength": 1,
        "maxLength": 64
      },
      "ReplayState": {
        "type": "string",
        "enum": [
          "STARTING",
          "RUNNING",
          "CANCELLING",
          "COMPLETED",
          "CANCELLED",
          "FAILED"
        ]
      },
      "Arn": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1600
      },
      "NextToken": {
        "type": "string",
        "minLength": 1,
        "maxLength": 2048
      },
      "LimitMax100": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://events.us-east-1.amazonaws.com/#X-Amz-Target=AWSEvents.ListReplays"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Replays**",
            "**NextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
