{
  "name": "CreateArchive",
  "description": "Creates an archive of events with the specified settings. When you create an archive, incoming events might not immediately start being sent to the archive. Allow a short period of time for changes to take effect. If you do not specify a pattern to filter events sent to the archive, all events are sent to the archive except replayed events. Replayed events are not sent to an archive.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSEvents.CreateArchive"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/CreateArchiveRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "CreateArchiveRequest": {
        "type": "object",
        "required": [
          "ArchiveName",
          "EventSourceArn"
        ],
        "title": "CreateArchiveRequest",
        "properties": {
          "ArchiveName": {
            "allOf": [
              {
                "$ref": "#/$defs/ArchiveName"
              },
              {
                "description": "The name for the archive to create."
              }
            ]
          },
          "EventSourceArn": {
            "allOf": [
              {
                "$ref": "#/$defs/Arn"
              },
              {
                "description": "The ARN of the event bus that sends events to the archive."
              }
            ]
          },
          "Description": {
            "allOf": [
              {
                "$ref": "#/$defs/ArchiveDescription"
              },
              {
                "description": "A description for the archive."
              }
            ]
          },
          "EventPattern": {
            "allOf": [
              {
                "$ref": "#/$defs/EventPattern"
              },
              {
                "description": "An event pattern to use to filter events sent to the archive."
              }
            ]
          },
          "RetentionDays": {
            "allOf": [
              {
                "$ref": "#/$defs/RetentionDays"
              },
              {
                "description": "The number of days to retain events for. Default value is 0. If set to 0, events are retained indefinitely"
              }
            ]
          }
        }
      },
      "ArchiveName": {
        "type": "string",
        "pattern": "[\\.\\-_A-Za-z0-9]+",
        "minLength": 1,
        "maxLength": 48
      },
      "Arn": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1600
      },
      "ArchiveDescription": {
        "type": "string",
        "pattern": ".*",
        "maxLength": 512
      },
      "EventPattern": {
        "type": "string"
      },
      "RetentionDays": {
        "type": "integer",
        "minimum": 0
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://events.us-east-1.amazonaws.com/#X-Amz-Target=AWSEvents.CreateArchive"
