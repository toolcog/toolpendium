{
  "name": "UntagResource",
  "description": "Remove tags from an App Runner resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AppRunner.UntagResource"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UntagResourceRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UntagResourceRequest": {
        "type": "object",
        "required": [
          "ResourceArn",
          "TagKeys"
        ],
        "title": "UntagResourceRequest",
        "properties": {
          "ResourceArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AppRunnerResourceArn"
              },
              {
                "description": "<p>The Amazon Resource Name (ARN) of the resource that you want to remove tags from.</p> <p>It must be the ARN of an App Runner resource.</p>"
              }
            ]
          },
          "TagKeys": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKeyList"
              },
              {
                "description": "A list of tag keys that you want to remove."
              }
            ]
          }
        }
      },
      "AppRunnerResourceArn": {
        "type": "string",
        "pattern": "arn:aws(-[\\w]+)*:[a-z0-9-\\\\.]{0,63}:[a-z0-9-\\\\.]{0,63}:[0-9]{12}:(\\w|\\/|-){1,1011}",
        "minLength": 1,
        "maxLength": 1011
      },
      "TagKeyList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TagKey"
        }
      },
      "TagKey": {
        "type": "string",
        "pattern": "^(?!aws:).+",
        "minLength": 1,
        "maxLength": 128
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://apprunner.us-east-1.amazonaws.com/#X-Amz-Target=AppRunner.UntagResource"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
