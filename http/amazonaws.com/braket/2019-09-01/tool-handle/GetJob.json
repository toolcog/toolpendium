      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**algorithmSpecification**",
            "**billableDuration**",
            "**checkpointConfig**",
            "**createdAt**",
            "**deviceConfig**",
            "**endedAt**",
            "**events**",
            "**failureReason**",
            "**hyperParameters**",
            "**inputDataConfig**",
            "**instanceConfig**",
            "**jobArn**",
            "**jobName**",
            "**outputDataConfig**",
            "**roleArn**",
            "**startedAt**",
            "**status**",
            "**stoppingCondition**",
            "**tags**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
