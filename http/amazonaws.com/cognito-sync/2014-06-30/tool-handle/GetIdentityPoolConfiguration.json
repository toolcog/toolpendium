      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "IdentityPoolId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
