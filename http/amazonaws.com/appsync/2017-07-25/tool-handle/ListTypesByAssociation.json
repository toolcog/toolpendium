      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "mergedApiIdentifier",
      "associationId",
      "format"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://appsync.us-east-1.amazonaws.com/v1/mergedApis/{mergedApiIdentifier}/sourceApiAssociations/{associationId}/types#format{?format,nextToken,maxResults}"
    },
    "headers": {
