        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "sourceApiIdentifier",
      "associationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://appsync.us-east-1.amazonaws.com/v1/sourceApis/{sourceApiIdentifier}/mergedApiAssociations/{associationId}"
