{
  "name": "UpdateDomainName",
  "description": "Updates a custom <code>DomainName</code> object.",
  "parameters": {
    "type": "object",
    "properties": {
      "domainName": {
        "type": "string",
        "pattern": "^(\\*[\\w\\d-]*\\.)?([\\w\\d-]+\\.)+[\\w\\d-]+$",
        "minLength": 1,
        "maxLength": 253,
        "description": "The domain name."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
