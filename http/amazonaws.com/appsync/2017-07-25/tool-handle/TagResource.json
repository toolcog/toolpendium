{
  "name": "TagResource",
  "description": "Tags a resource with user-supplied tags.",
  "parameters": {
    "type": "object",
    "properties": {
      "resourceArn": {
        "type": "string",
        "pattern": "^arn:aws:appsync:[A-Za-z0-9_/.-]{0,63}:\\d{12}:apis/[0-9A-Za-z_-]{26}$",
        "minLength": 70,
        "maxLength": 75,
        "description": "The <code>GraphqlApi</code> Amazon Resource Name (ARN)."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "tags"
        ],
        "properties": {
          "tags": {
            "description": "A map with keys of <code>TagKey</code> objects and values of <code>TagValue</code> objects.",
            "type": "object",
            "minProperties": 1,
            "maxProperties": 50,
            "additionalProperties": {
              "$ref": "#/$defs/TagValue"
            }
          }
        }
      }
    },
    "required": [
      "resourceArn",
      "body"
    ],
    "$defs": {
      "TagValue": {
        "type": "string",
        "pattern": "^[\\s\\w+-=\\.:/@]*$",
        "description": "The value for the tag.",
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://appsync.us-east-1.amazonaws.com/v1/tags/{resourceArn}"
    },
    "headers": {
