      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ConfirmDeviceRequest": {
        "type": "object",
        "required": [
          "AccessToken",
          "DeviceKey"
        ],
        "title": "ConfirmDeviceRequest",
        "properties": {
          "AccessToken": {
            "allOf": [
              {
                "$ref": "#/$defs/TokenModelType"
              },
              {
                "description": "A valid access token that Amazon Cognito issued to the user whose device you want to confirm."
              }
            ]
          },
          "DeviceKey": {
            "allOf": [
              {
                "$ref": "#/$defs/DeviceKeyType"
              },
              {
                "description": "The device key."
              }
            ]
          },
          "DeviceSecretVerifierConfig": {
            "allOf": [
              {
                "$ref": "#/$defs/DeviceSecretVerifierConfigType"
              },
              {
                "description": "The configuration of the device secret verifier."
              }
            ]
          },
          "DeviceName": {
            "allOf": [
              {
                "$ref": "#/$defs/DeviceNameType"
              },
              {
                "description": "The device name."
              }
            ]
          }
        },
        "description": "Confirms the device request."
      },
      "TokenModelType": {
        "type": "string",
        "pattern": "[A-Za-z0-9-_=.]+",
        "format": "password"
      },
      "DeviceKeyType": {
        "type": "string",
        "pattern": "[\\w-]+_[0-9a-f-]+",
        "minLength": 1,
        "maxLength": 55
      },
      "DeviceSecretVerifierConfigType": {
        "type": "object",
        "properties": {
          "PasswordVerifier": {
            "allOf": [
              {
                "$ref": "#/$defs/StringType"
              },
              {
                "description": "The password verifier."
              }
            ]
          },
          "Salt": {
            "allOf": [
              {
                "$ref": "#/$defs/StringType"
              },
              {
                "description": "The <a href=\"https://en.wikipedia.org/wiki/Salt_(cryptography)\">salt</a> "
              }
            ]
          }
        },
        "description": "The device verifier against which it is authenticated."
      },
      "StringType": {
        "type": "string",
        "minLength": 0,
        "maxLength": 131072
      },
      "DeviceNameType": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
