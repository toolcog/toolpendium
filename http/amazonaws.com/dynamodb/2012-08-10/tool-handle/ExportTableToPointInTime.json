{
  "name": "ExportTableToPointInTime",
  "description": "Exports table data to an S3 bucket. The table must have point in time recovery enabled, and you can export data from any time within the point in time recovery window.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "DynamoDB_20120810.ExportTableToPointInTime"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ExportTableToPointInTimeInput"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ExportTableToPointInTimeInput": {
        "type": "object",
        "required": [
          "TableArn",
          "S3Bucket"
        ],
        "title": "ExportTableToPointInTimeInput",
        "properties": {
          "TableArn": {
            "allOf": [
              {
                "$ref": "#/$defs/TableArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) associated with the table to export."
              }
            ]
          },
          "ExportTime": {
            "allOf": [
              {
                "$ref": "#/$defs/ExportTime"
              },
              {
                "description": "Time in the past from which to export table data, counted in seconds from the start of the Unix epoch. The table export will be a snapshot of the table's state at this point in time."
              }
            ]
          },
          "ClientToken": {
            "allOf": [
              {
                "$ref": "#/$defs/ClientToken"
              },
              {
                "description": "<p>Providing a <code>ClientToken</code> makes the call to <code>ExportTableToPointInTimeInput</code> idempotent, meaning that multiple identical calls have the same effect as one single call.</p> <p>A client token is valid for 8 hours after the first request that uses it is completed. After 8 hours, any request with the same client token is treated as a new request. Do not resubmit the same request with the same client token for more than 8 hours, or the result might not be idempotent.</p> <p>If you submit a request with the same client token but a change in other parameters within the 8-hour idempotency window, DynamoDB returns an <code>ImportConflictException</code>.</p>"
              }
            ]
          },
          "S3Bucket": {
            "allOf": [
              {
                "$ref": "#/$defs/S3Bucket"
              },
              {
                "description": "The name of the Amazon S3 bucket to export the snapshot to."
              }
            ]
          },
          "S3BucketOwner": {
            "allOf": [
              {
                "$ref": "#/$defs/S3BucketOwner"
              },
              {
                "description": "The ID of the Amazon Web Services account that owns the bucket the export will be stored in."
              }
            ]
          },
          "S3Prefix": {
            "allOf": [
              {
                "$ref": "#/$defs/S3Prefix"
              },
              {
                "description": "The Amazon S3 bucket prefix to use as the file name and path of the exported snapshot."
              }
            ]
          },
          "S3SseAlgorithm": {
            "allOf": [
              {
                "$ref": "#/$defs/S3SseAlgorithm"
              },
              {
                "description": "<p>Type of encryption used on the bucket where export data will be stored. Valid values for <code>S3SseAlgorithm</code> are:</p> <ul> <li> <p> <code>AES256</code> - server-side encryption with Amazon S3 managed keys</p> </li> <li> <p> <code>KMS</code> - server-side encryption with KMS managed keys</p> </li> </ul>"
              }
            ]
          },
          "S3SseKmsKeyId": {
            "allOf": [
              {
                "$ref": "#/$defs/S3SseKmsKeyId"
              },
              {
                "description": "The ID of the KMS managed key used to encrypt the S3 bucket where export data will be stored (if applicable)."
              }
            ]
          },
          "ExportFormat": {
            "allOf": [
              {
                "$ref": "#/$defs/ExportFormat"
              },
              {
                "description": "The format for the exported data. Valid values for <code>ExportFormat</code> are <code>DYNAMODB_JSON</code> or <code>ION</code>."
              }
            ]
          }
        }
      },
      "TableArn": {
        "type": "string"
      },
      "ExportTime": {
        "type": "string",
        "format": "date-time"
      },
      "ClientToken": {
        "type": "string",
        "pattern": "^[^\\$]+$"
      },
      "S3Bucket": {
        "type": "string",
        "pattern": "^[a-z0-9A-Z]+[\\.\\-\\w]*[a-z0-9A-Z]+$",
        "maxLength": 255
      },
      "S3BucketOwner": {
        "type": "string",
        "pattern": "[0-9]{12}"
      },
      "S3Prefix": {
        "type": "string",
        "maxLength": 1024
      },
      "S3SseAlgorithm": {
        "type": "string",
        "enum": [
          "AES256",
          "KMS"
        ]
      },
      "S3SseKmsKeyId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 2048
      },
      "ExportFormat": {
        "type": "string",
        "enum": [
          "DYNAMODB_JSON",
          "ION"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://dynamodb.us-east-1.amazonaws.com/#X-Amz-Target=DynamoDB_20120810.ExportTableToPointInTime"
