      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "loggingOptions"
        ],
        "properties": {
          "loggingOptions": {
            "description": "Contains logging options.",
            "type": "object",
            "properties": {
              "level": {
                "allOf": [
                  {
                    "$ref": "#/$defs/LoggingLevel"
                  },
                  {
                    "description": "The IoT SiteWise logging verbosity level."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "LoggingLevel": {
        "type": "string",
        "enum": [
          "ERROR",
          "INFO",
          "OFF"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
