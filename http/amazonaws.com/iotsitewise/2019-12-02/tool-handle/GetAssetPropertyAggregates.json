{
  "name": "GetAssetPropertyAggregates",
  "description": "<p>Gets aggregated values for an asset property. For more information, see <a href=\"https://docs.aws.amazon.com/iot-sitewise/latest/userguide/query-industrial-data.html#aggregates\">Querying aggregates</a> in the <i>IoT SiteWise User Guide</i>.</p> <p>To identify an asset property, you must specify one of the following:</p> <ul> <li> <p>The <code>assetId</code> and <code>propertyId</code> of an asset property.</p> </li> <li> <p>A <code>propertyAlias</code>, which is a data stream alias (for example, <code>/company/windfarm/3/turbine/7/temperature</code>). To define an asset property's alias, see <a href=\"https://docs.aws.amazon.com/iot-sitewise/latest/APIReference/API_UpdateAssetProperty.html\">UpdateAssetProperty</a>.</p> </li> </ul>",
  "parameters": {
    "type": "object",
    "properties": {
      "assetId": {
        "type": "string",
        "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
        "minLength": 36,
        "maxLength": 36,
        "description": "The ID of the asset."
      },
      "propertyId": {
        "type": "string",
        "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
        "minLength": 36,
        "maxLength": 36,
        "description": "The ID of the asset property."
      },
      "propertyAlias": {
        "type": "string",
        "pattern": "[^\\u0000-\\u001F\\u007F]+",
        "minLength": 1,
        "maxLength": 2048,
        "description": "The alias that identifies the property, such as an OPC-UA server data stream path (for example, <code>/company/windfarm/3/turbine/7/temperature</code>). For more information, see <a href=\"https://docs.aws.amazon.com/iot-sitewise/latest/userguide/connect-data-streams.html\">Mapping industrial data streams to asset properties</a> in the <i>IoT SiteWise User Guide</i>."
      },
      "aggregateTypes": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/AggregateType"
        },
        "minItems": 1,
        "description": "The data aggregating function."
      },
      "resolution": {
        "type": "string",
        "pattern": "1m|15m|1h|1d",
        "minLength": 2,
        "maxLength": 3,
        "description": "The time interval over which to aggregate data."
      },
      "qualities": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Quality"
        },
        "minItems": 1,
        "maxItems": 1,
        "description": "The quality by which to filter asset data."
      },
      "startDate": {
        "type": "string",
        "format": "date-time",
        "description": "The exclusive start of the range from which to query historical data, expressed in seconds in Unix epoch time."
      },
      "endDate": {
        "type": "string",
        "format": "date-time",
        "description": "The inclusive end of the range from which to query historical data, expressed in seconds in Unix epoch time."
      },
      "timeOrdering": {
        "type": "string",
        "enum": [
          "ASCENDING",
          "DESCENDING"
        ],
        "description": "<p>The chronological sorting order of the requested information.</p> <p>Default: <code>ASCENDING</code> </p>"
      },
      "nextToken": {
        "type": "string",
        "pattern": "[A-Za-z0-9+/=]+",
        "minLength": 1,
        "maxLength": 4096,
        "description": "The token to be used for the next set of paginated results."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "description": "<p>The maximum number of results to return for each paginated request. A result set is returned in the two cases, whichever occurs first.</p> <ul> <li> <p>The size of the result set is equal to 1 MB.</p> </li> <li> <p>The number of data points in the result set is equal to the value of <code>maxResults</code>. The maximum value of <code>maxResults</code> is 250.</p> </li> </ul>"
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "aggregateTypes",
      "resolution",
      "startDate",
      "endDate"
    ],
    "$defs": {
      "AggregateType": {
        "type": "string",
        "enum": [
          "AVERAGE",
          "COUNT",
          "MAXIMUM",
          "MINIMUM",
          "SUM",
          "STANDARD_DEVIATION"
        ]
      },
      "Quality": {
        "type": "string",
        "enum": [
          "GOOD",
          "BAD",
          "UNCERTAIN"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://iotsitewise.us-east-1.amazonaws.com/properties/aggregates#aggregateTypes&resolution&startDate&endDate{?assetId,propertyId,propertyAlias,aggregateTypes,resolution,qualities,startDate,endDate,timeOrdering,nextToken,maxResults}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**aggregatedValues**",
            "**nextToken**"
          ]
        },
        {
