{
  "name": "DescribeAssetModel",
  "description": "Retrieves information about an asset model.",
  "parameters": {
    "type": "object",
    "properties": {
      "assetModelId": {
        "type": "string",
        "pattern": "^[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}$",
        "minLength": 36,
        "maxLength": 36,
