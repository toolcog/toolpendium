      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ReleasePhoneNumberRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ReleasePhoneNumberRequest": {
        "type": "object",
        "required": [
          "PhoneNumberId"
        ],
        "title": "ReleasePhoneNumberRequest",
        "properties": {
          "PhoneNumberId": {
            "allOf": [
              {
                "$ref": "#/$defs/PhoneNumberIdOrArn"
              },
              {
                "description": "The PhoneNumberId or PhoneNumberArn of the phone number to release. You can use <a>DescribePhoneNumbers</a> to get the values for PhoneNumberId and PhoneNumberArn."
              }
            ]
          }
        }
      },
      "PhoneNumberIdOrArn": {
        "type": "string",
        "pattern": "[A-Za-z0-9_:/-]+",
        "minLength": 1,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://sms-voice.us-east-1.amazonaws.com/#X-Amz-Target=PinpointSMSVoiceV2.ReleasePhoneNumber"
