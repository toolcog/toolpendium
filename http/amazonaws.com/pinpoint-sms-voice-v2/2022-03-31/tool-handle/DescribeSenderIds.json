            "allOf": [
              {
                "$ref": "#/$defs/SenderIdList"
              },
              {
                "description": "An array of SenderIdAndCountry objects to search for."
              }
            ]
          },
          "Filters": {
            "allOf": [
              {
                "$ref": "#/$defs/SenderIdFilterList"
              },
              {
                "description": "An array of SenderIdFilter objects to filter the results."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "The token to be used for the next set of paginated results. You don't need to supply a value for this field in the initial request."
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of results to return per each request."
              }
            ]
          }
        }
      },
      "SenderIdList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/SenderIdAndCountry"
        },
        "minItems": 0,
        "maxItems": 5
      },
      "SenderIdAndCountry": {
        "type": "object",
        "required": [
          "SenderId",
          "IsoCountryCode"
        ],
        "properties": {
          "SenderId": {
            "allOf": [
              {
                "$ref": "#/$defs/SenderIdOrArn"
              },
              {
                "description": "The unique identifier of the sender."
              }
            ]
          },
          "IsoCountryCode": {
            "allOf": [
              {
                "$ref": "#/$defs/IsoCountryCode"
              },
              {
                "description": "The two-character code, in ISO 3166-1 alpha-2 format, for the country or region. "
              }
            ]
          }
        },
        "description": " The alphanumeric sender ID in a specific country that you want to describe. For more information on sender IDs see <a href=\"https://docs.aws.amazon.com/pinpoint/latest/userguide/channels-sms-awssupport-sender-id.html\">Requesting sender IDs for SMS messaging with Amazon Pinpoint </a> in the <i>Amazon Pinpoint User Guide</i>."
      },
      "SenderIdOrArn": {
        "type": "string",
        "pattern": "[A-Za-z0-9_:/-]+",
        "minLength": 1,
        "maxLength": 256
      },
      "IsoCountryCode": {
        "type": "string",
        "pattern": "[A-Z]{2}",
        "minLength": 2,
        "maxLength": 2
      },
      "SenderIdFilterList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/SenderIdFilter"
        },
        "minItems": 0,
        "maxItems": 20
      },
      "SenderIdFilter": {
        "type": "object",
        "required": [
          "Name",
          "Values"
        ],
        "properties": {
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/SenderIdFilterName"
              },
              {
                "description": "The name of the attribute to filter on."
              }
            ]
          },
          "Values": {
            "allOf": [
              {
                "$ref": "#/$defs/FilterValueList"
              },
              {
                "description": "An array of values to filter for."
              }
            ]
          }
        },
        "description": "The information for a sender ID that meets a specified criteria."
      },
      "SenderIdFilterName": {
        "type": "string",
        "enum": [
          "sender-id",
          "iso-country-code",
          "message-type"
        ]
      },
      "FilterValueList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/FilterValue"
        },
        "minItems": 1,
        "maxItems": 20
      },
      "FilterValue": {
        "type": "string",
        "pattern": "[A-Za-z0-9_-]+",
        "minLength": 1,
        "maxLength": 100
      },
      "NextToken": {
        "type": "string",
        "pattern": ".+",
        "minLength": 1,
        "maxLength": 1024
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      }
    }
  },
  "handler": "http",
  "request": {
