{
  "name": "DescribeKeywords",
  "description": "<p>Describes the specified keywords or all keywords on your origination phone number or pool.</p> <p>A keyword is a word that you can search for on a particular phone number or pool. It is also a specific word or phrase that an end user can send to your number to elicit a response, such as an informational message or a special offer. When your number receives a message that begins with a keyword, Amazon Pinpoint responds with a customizable message.</p> <p>If you specify a keyword that isn't valid, an Error is returned.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "PinpointSMSVoiceV2.DescribeKeywords"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/DescribeKeywordsRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DescribeKeywordsRequest": {
        "type": "object",
        "required": [
          "OriginationIdentity"
        ],
        "title": "DescribeKeywordsRequest",
        "properties": {
          "OriginationIdentity": {
            "allOf": [
              {
                "$ref": "#/$defs/PhoneOrPoolIdOrArn"
              },
              {
                "description": "The origination identity to use such as a PhoneNumberId, PhoneNumberArn, SenderId or SenderIdArn. You can use <a>DescribePhoneNumbers</a> to find the values for PhoneNumberId and PhoneNumberArn while <a>DescribeSenderIds</a> can be used to get the values for SenderId and SenderIdArn."
              }
            ]
          },
          "Keywords": {
            "allOf": [
              {
                "$ref": "#/$defs/KeywordList"
              },
              {
                "description": "An array of keywords to search for."
              }
            ]
          },
          "Filters": {
            "allOf": [
              {
                "$ref": "#/$defs/KeywordFilterList"
              },
              {
                "description": "An array of keyword filters to filter the results."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "The token to be used for the next set of paginated results. You don't need to supply a value for this field in the initial request."
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of results to return per each request."
              }
            ]
          }
        }
      },
      "PhoneOrPoolIdOrArn": {
        "type": "string",
        "pattern": "[A-Za-z0-9_:/-]+",
        "minLength": 1,
        "maxLength": 256
      },
      "KeywordList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Keyword"
        },
        "minItems": 0,
        "maxItems": 5
      },
      "Keyword": {
        "type": "string",
        "pattern": "[ \\S]+",
        "minLength": 1,
        "maxLength": 30
      },
      "KeywordFilterList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/KeywordFilter"
        },
        "minItems": 0,
        "maxItems": 20
      },
      "KeywordFilter": {
        "type": "object",
        "required": [
          "Name",
          "Values"
        ],
        "properties": {
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/KeywordFilterName"
              },
              {
                "description": "The name of the attribute to filter on."
              }
            ]
          },
          "Values": {
            "allOf": [
              {
                "$ref": "#/$defs/FilterValueList"
              },
              {
                "description": "An array values to filter for."
              }
            ]
          }
        },
        "description": "The information for keywords that meet a specified criteria."
      },
      "KeywordFilterName": {
        "type": "string",
        "enum": [
          "keyword-action"
        ]
      },
      "FilterValueList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/FilterValue"
        },
        "minItems": 1,
        "maxItems": 20
      },
      "FilterValue": {
        "type": "string",
        "pattern": "[A-Za-z0-9_-]+",
        "minLength": 1,
        "maxLength": 100
      },
      "NextToken": {
        "type": "string",
        "pattern": ".+",
        "minLength": 1,
        "maxLength": 1024
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://sms-voice.us-east-1.amazonaws.com/#X-Amz-Target=PinpointSMSVoiceV2.DescribeKeywords{?MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
