            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of results to return per each request."
              }
            ]
          }
        }
      },
      "ConfigurationSetNameList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/ConfigurationSetNameOrArn"
        },
        "minItems": 0,
        "maxItems": 5
      },
      "ConfigurationSetNameOrArn": {
        "type": "string",
        "pattern": "[A-Za-z0-9_:/-]+",
        "minLength": 1,
        "maxLength": 256
      },
      "ConfigurationSetFilterList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/ConfigurationSetFilter"
        },
        "minItems": 0,
        "maxItems": 20
      },
      "ConfigurationSetFilter": {
        "type": "object",
        "required": [
          "Name",
          "Values"
        ],
        "properties": {
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/ConfigurationSetFilterName"
              },
              {
                "description": "The name of the attribute to filter on."
              }
            ]
          },
          "Values": {
            "allOf": [
              {
                "$ref": "#/$defs/FilterValueList"
              },
              {
                "description": "An array values to filter for."
              }
            ]
          }
        },
        "description": "The information for configuration sets that meet a specified criteria."
      },
      "ConfigurationSetFilterName": {
        "type": "string",
        "enum": [
          "event-destination-name",
          "matching-event-types",
          "default-message-type",
          "default-sender-id"
        ]
      },
      "FilterValueList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/FilterValue"
        },
        "minItems": 1,
        "maxItems": 20
      },
      "FilterValue": {
        "type": "string",
        "pattern": "[A-Za-z0-9_-]+",
        "minLength": 1,
        "maxLength": 100
      },
      "NextToken": {
        "type": "string",
        "pattern": ".+",
        "minLength": 1,
        "maxLength": 1024
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      }
    }
  },
