{
  "name": "DisassociateOriginationIdentity",
  "description": "<p>Removes the specified origination identity from an existing pool.</p> <p>If the origination identity isn't associated with the specified pool, an Error is returned.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
