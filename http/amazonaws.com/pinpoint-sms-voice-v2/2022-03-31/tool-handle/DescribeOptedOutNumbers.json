              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of results to return per each request."
              }
            ]
          }
        }
      },
      "OptOutListNameOrArn": {
        "type": "string",
        "pattern": "[A-Za-z0-9_:/-]+",
        "minLength": 1,
        "maxLength": 256
      },
      "OptedOutNumberList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/PhoneNumber"
        },
        "minItems": 0,
        "maxItems": 5
      },
      "PhoneNumber": {
        "type": "string",
        "pattern": "\\+?[1-9][0-9]{1,18}",
        "minLength": 1,
        "maxLength": 20
      },
      "OptedOutFilterList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/OptedOutFilter"
        },
        "minItems": 0,
        "maxItems": 20
      },
      "OptedOutFilter": {
