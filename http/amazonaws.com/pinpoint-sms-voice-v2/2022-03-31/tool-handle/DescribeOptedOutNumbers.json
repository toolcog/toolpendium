            "allOf": [
              {
                "$ref": "#/$defs/OptOutListNameOrArn"
              },
              {
                "description": "The OptOutListName or OptOutListArn of the OptOutList. You can use <a>DescribeOptOutLists</a> to find the values for OptOutListName and OptOutListArn."
              }
            ]
          },
          "OptedOutNumbers": {
            "allOf": [
              {
                "$ref": "#/$defs/OptedOutNumberList"
              },
              {
                "description": "An array of phone numbers to search for in the OptOutList."
              }
            ]
          },
          "Filters": {
            "allOf": [
              {
                "$ref": "#/$defs/OptedOutFilterList"
              },
              {
                "description": "An array of OptedOutFilter objects to filter the results on."
              }
            ]
