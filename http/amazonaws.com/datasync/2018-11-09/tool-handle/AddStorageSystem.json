{
  "name": "AddStorageSystem",
  "description": "Creates an Amazon Web Services resource for an on-premises storage system that you want DataSync Discovery to collect information about.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "FmrsService.AddStorageSystem"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/AddStorageSystemRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "AddStorageSystemRequest": {
        "type": "object",
        "required": [
          "ServerConfiguration",
          "SystemType",
          "AgentArns",
          "ClientToken",
          "Credentials"
        ],
        "title": "AddStorageSystemRequest",
        "properties": {
          "ServerConfiguration": {
            "allOf": [
              {
                "$ref": "#/$defs/DiscoveryServerConfiguration"
              },
              {
                "description": "Specifies the server name and network port required to connect with the management interface of your on-premises storage system."
              }
            ]
          },
          "SystemType": {
            "allOf": [
              {
                "$ref": "#/$defs/DiscoverySystemType"
              },
              {
                "description": "<p>Specifies the type of on-premises storage system that you want DataSync Discovery to collect information about.</p> <note> <p>DataSync Discovery currently supports NetApp Fabric-Attached Storage (FAS) and All Flash FAS (AFF) systems running ONTAP 9.7 or later.</p> </note>"
              }
            ]
          },
          "AgentArns": {
            "allOf": [
              {
                "$ref": "#/$defs/DiscoveryAgentArnList"
              },
              {
                "description": "Specifies the Amazon Resource Name (ARN) of the DataSync agent that connects to and reads from your on-premises storage system's management interface. You can only specify one ARN."
              }
            ]
          },
          "CloudWatchLogGroupArn": {
            "allOf": [
              {
                "$ref": "#/$defs/LogGroupArn"
              },
              {
                "description": "Specifies the ARN of the Amazon CloudWatch log group for monitoring and logging discovery job events."
              }
            ]
          },
          "Tags": {
            "allOf": [
              {
                "$ref": "#/$defs/InputTagList"
              },
              {
                "description": "Specifies labels that help you categorize, filter, and search for your Amazon Web Services resources. We recommend creating at least a name tag for your on-premises storage system."
              }
            ]
          },
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/Name"
              },
              {
                "description": "Specifies a familiar name for your on-premises storage system."
              }
            ]
          },
          "ClientToken": {
            "allOf": [
              {
                "$ref": "#/$defs/PtolemyUUID"
              },
              {
                "description": "Specifies a client token to make sure requests with this API operation are idempotent. If you don't specify a client token, DataSync generates one for you automatically."
              }
            ]
          },
          "Credentials": {
            "allOf": [
              {
                "$ref": "#/$defs/Credentials"
              },
              {
                "description": "Specifies the user name and password for accessing your on-premises storage system's management interface."
              }
            ]
          }
        }
      },
      "DiscoveryServerConfiguration": {
        "type": "object",
        "required": [
          "ServerHostname"
        ],
        "properties": {
          "ServerHostname": {
            "allOf": [
              {
                "$ref": "#/$defs/DiscoveryServerHostname"
              },
              {
                "description": "The domain name or IP address of your storage system's management interface."
              }
            ]
          },
          "ServerPort": {
            "allOf": [
              {
                "$ref": "#/$defs/DiscoveryServerPort"
              },
              {
                "description": "The network port for accessing the storage system's management interface."
              }
            ]
          }
        },
        "description": "The network settings that DataSync Discovery uses to connect with your on-premises storage system's management interface."
      },
      "DiscoveryServerHostname": {
        "type": "string",
        "pattern": "^(([a-zA-Z0-9\\-]*[a-zA-Z0-9])\\.)*([A-Za-z0-9\\-]*[A-Za-z0-9])$",
        "maxLength": 255
      },
      "DiscoveryServerPort": {
        "type": "integer",
        "minimum": 1,
        "maximum": 65535
      },
      "DiscoverySystemType": {
        "type": "string",
        "enum": [
          "NetAppONTAP"
        ]
      },
      "DiscoveryAgentArnList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/AgentArn"
        },
        "minItems": 1,
        "maxItems": 1
      },
      "AgentArn": {
        "type": "string",
        "pattern": "^arn:(aws|aws-cn|aws-us-gov|aws-iso|aws-iso-b):datasync:[a-z\\-0-9]+:[0-9]{12}:agent/agent-[0-9a-z]{17}$",
        "maxLength": 128
      },
      "LogGroupArn": {
        "type": "string",
        "pattern": "^arn:(aws|aws-cn|aws-us-gov|aws-iso|aws-iso-b):logs:[a-z\\-0-9]+:[0-9]{12}:log-group:([^:\\*]*)(:\\*)?$",
        "maxLength": 562
      },
      "InputTagList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TagListEntry"
        },
        "minItems": 0,
        "maxItems": 50
      },
      "TagListEntry": {
        "type": "object",
        "required": [
          "Key"
        ],
        "properties": {
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "The key for an Amazon Web Services resource tag."
              }
            ]
          },
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/TagValue"
              },
              {
                "description": "The value for an Amazon Web Services resource tag."
              }
            ]
          }
        },
        "description": "A key-value pair representing a single tag that's been applied to an Amazon Web Services resource."
      },
      "TagKey": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\s+=._:/-]+$",
        "minLength": 1,
        "maxLength": 256
      },
      "TagValue": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\s+=._:@/-]+$",
        "minLength": 1,
        "maxLength": 256
      },
      "Name": {
        "type": "string",
        "pattern": "^[\\p{L}\\p{M}\\p{N}\\s+=._:@\\/-]+$",
        "minLength": 1,
        "maxLength": 256
      },
      "PtolemyUUID": {
        "type": "string",
        "pattern": "[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}"
      },
      "Credentials": {
        "type": "object",
        "required": [
          "Username",
          "Password"
        ],
        "properties": {
          "Username": {
            "allOf": [
              {
                "$ref": "#/$defs/PtolemyUsername"
              },
              {
                "description": "Specifies the user name for your storage system's management interface."
              }
            ]
          },
          "Password": {
            "allOf": [
              {
                "$ref": "#/$defs/PtolemyPassword"
              },
              {
                "description": "Specifies the password for your storage system's management interface."
              }
            ]
          }
        },
        "description": "<p>The credentials that provide DataSync Discovery read access to your on-premises storage system's management interface.</p> <p>DataSync Discovery stores these credentials in <a href=\"https://docs.aws.amazon.com/secretsmanager/latest/userguide/intro.html\">Secrets Manager</a>. For more information, see <a href=\"https://docs.aws.amazon.com/datasync/latest/userguide/discovery-configure-storage.html\">Accessing your on-premises storage system</a>.</p>"
      },
      "PtolemyUsername": {
        "type": "string",
        "pattern": "^(?!.*[:\\\"][^:\"]*$).+$",
        "maxLength": 1024,
        "format": "password"
      },
      "PtolemyPassword": {
        "type": "string",
        "pattern": "^(?!.*[:\\\"][^:\"]*$).+$",
        "maxLength": 1024,
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://datasync.us-east-1.amazonaws.com/#X-Amz-Target=FmrsService.AddStorageSystem"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**StorageSystemArn**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
