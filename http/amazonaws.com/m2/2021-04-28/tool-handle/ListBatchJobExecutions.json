{
  "name": "ListBatchJobExecutions",
  "description": "Lists historical, current, and scheduled batch job executions for a specific application.",
  "parameters": {
    "type": "object",
    "properties": {
      "applicationId": {
        "type": "string",
        "pattern": "^\\S{1,80}$",
        "description": "The unique identifier of the application."
      },
      "executionIds": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Identifier"
        },
        "minItems": 1,
        "maxItems": 10,
        "description": "The unique identifier of each batch job execution."
      },
      "jobName": {
        "type": "string",
        "pattern": "^\\S{1,100}$",
        "description": "The name of each batch job execution."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 2000,
        "description": "The maximum number of batch job executions to return."
      },
      "nextToken": {
        "type": "string",
        "pattern": "^\\S{1,2000}$",
        "description": "A pagination token to control the number of batch job executions displayed in the list."
      },
      "startedAfter": {
        "type": "string",
        "format": "date-time",
        "description": "The time after which the batch job executions started."
      },
      "startedBefore": {
        "type": "string",
        "format": "date-time",
        "description": "The time before the batch job executions started."
      },
      "status": {
        "type": "string",
        "enum": [
          "Submitting",
          "Holding",
          "Dispatching",
          "Running",
          "Cancelling",
          "Cancelled",
          "Succeeded",
          "Failed",
          "Succeeded With Warning"
        ],
        "description": "The status of the batch job executions."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "applicationId"
    ],
    "$defs": {
      "Identifier": {
        "type": "string",
        "pattern": "^\\S{1,80}$"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://m2.us-east-1.amazonaws.com/applications/{applicationId}/batch-job-executions{?executionIds,jobName,maxResults,nextToken,startedAfter,startedBefore,status}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**batchJobExecutions**",
            "**nextToken**"
          ]
        },
