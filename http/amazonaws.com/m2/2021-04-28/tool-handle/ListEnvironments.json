{
  "name": "ListEnvironments",
  "description": "Lists the runtime environments.",
  "parameters": {
    "type": "object",
    "properties": {
      "engineType": {
        "type": "string",
        "enum": [
          "microfocus",
          "bluage"
        ],
        "description": "The engine type for the runtime environment."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 2000,
        "description": "The maximum number of runtime environments to return."
      },
      "names": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/EntityName"
        },
        "minItems": 1,
        "maxItems": 10,
        "description": "The names of the runtime environments. Must be unique within the account."
      },
      "nextToken": {
        "type": "string",
        "pattern": "^\\S{1,2000}$",
        "description": "A pagination token to control the number of runtime environments displayed in the list."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "$defs": {
      "EntityName": {
        "type": "string",
        "pattern": "^[A-Za-z0-9][A-Za-z0-9_\\-]{1,59}$"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://m2.us-east-1.amazonaws.com/environments{?engineType,maxResults,names,nextToken}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**environments**",
            "**nextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
