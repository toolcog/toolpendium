{
  "name": "GetDataSetDetails",
  "description": "Gets the details of a specific data set.",
  "parameters": {
    "type": "object",
    "properties": {
      "applicationId": {
        "type": "string",
        "pattern": "^\\S{1,80}$",
        "description": "The unique identifier of the application that this data set is associated with."
      },
      "dataSetName": {
        "type": "string",
        "pattern": "^\\S{1,200}$",
        "description": "The name of the data set."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "applicationId",
      "dataSetName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://m2.us-east-1.amazonaws.com/applications/{applicationId}/datasets/{dataSetName}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**blocksize**",
            "**creationTime**",
            "**dataSetName**",
            "**dataSetOrg**",
            "**lastReferencedTime**",
            "**lastUpdatedTime**",
            "**location**",
            "**recordLength**"
          ]
        },
        {
