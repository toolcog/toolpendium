{
  "name": "BatchDisassociateClientDeviceFromCoreDevice",
  "description": "Disassociates a list of client devices from a core device. After you disassociate a client device from a core device, the client device won't be able to use cloud discovery to retrieve the core device's connectivity information and certificates.",
  "parameters": {
    "type": "object",
    "properties": {
      "coreDeviceThingName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128,
        "description": "The name of the core device. This is also the name of the IoT thing."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "entries": {
            "description": "The list of client devices to disassociate.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/DisassociateClientDeviceFromCoreDeviceEntry"
            },
            "minItems": 1,
            "maxItems": 100
          }
        }
      }
    },
    "required": [
      "coreDeviceThingName",
      "body"
    ],
    "$defs": {
      "DisassociateClientDeviceFromCoreDeviceEntry": {
        "type": "object",
        "required": [
          "thingName"
        ],
        "properties": {
          "thingName": {
            "allOf": [
              {
                "$ref": "#/$defs/IoTThingName"
              },
              {
                "description": "The name of the IoT thing that represents the client device to disassociate."
              }
            ]
          }
        },
        "description": "Contains a request to disassociate a client device from a core device. The <a href=\"https://docs.aws.amazon.com/greengrass/v2/APIReference/API_BatchDisassociateClientDeviceWithCoreDevice.html\">BatchDisassociateClientDeviceWithCoreDevice</a> operation consumes a list of these requests."
      },
      "IoTThingName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://greengrass.us-east-1.amazonaws.com/greengrass/v2/coreDevices/{coreDeviceThingName}/disassociateClientDevices"
    },
