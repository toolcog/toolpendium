        "type": "string",
        "pattern": "\\d{12}"
      },
      "MemberAccountRuleStatus": {
        "type": "string",
        "enum": [
          "CREATE_SUCCESSFUL",
          "CREATE_IN_PROGRESS",
          "CREATE_FAILED",
          "DELETE_SUCCESSFUL",
          "DELETE_FAILED",
          "DELETE_IN_PROGRESS",
          "UPDATE_SUCCESSFUL",
          "UPDATE_IN_PROGRESS",
          "UPDATE_FAILED"
        ]
      },
