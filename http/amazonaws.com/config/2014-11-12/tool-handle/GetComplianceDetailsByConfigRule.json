              }
            ]
          },
          "ComplianceTypes": {
            "allOf": [
              {
                "$ref": "#/$defs/ComplianceTypes"
              },
              {
                "description": "<p>Filters the results by compliance.</p> <p> <code>INSUFFICIENT_DATA</code> is a valid <code>ComplianceType</code> that is returned when an Config rule cannot be evaluated. However, <code>INSUFFICIENT_DATA</code> cannot be used as a <code>ComplianceType</code> for filtering results.</p>"
              }
            ]
          },
          "Limit": {
            "allOf": [
              {
                "$ref": "#/$defs/Limit"
              },
              {
                "description": "The maximum number of evaluation results returned on each page. The default is 10. You cannot specify a number greater than 100. If you specify 0, Config uses the default."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "The <code>nextToken</code> string returned on a previous page that you use to get the next page of results in a paginated response."
              }
            ]
          }
        },
