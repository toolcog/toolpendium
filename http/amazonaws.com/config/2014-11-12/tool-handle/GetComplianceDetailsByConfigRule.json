        },
        "minItems": 0,
        "maxItems": 3
      },
      "ComplianceType": {
        "type": "string",
        "enum": [
          "COMPLIANT",
          "NON_COMPLIANT",
          "NOT_APPLICABLE",
          "INSUFFICIENT_DATA"
        ]
