{
  "name": "ModifyDataProvider",
  "description": "<p>Modifies the specified data provider using the provided settings.</p> <note> <p>You must remove the data provider from all migration projects before you can modify it.</p> </note>",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AmazonDMSv20160101.ModifyDataProvider"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ModifyDataProviderMessage"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ModifyDataProviderMessage": {
        "type": "object",
        "required": [
          "DataProviderIdentifier"
        ],
        "title": "ModifyDataProviderMessage",
        "properties": {
          "DataProviderIdentifier": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The identifier of the data provider. Identifiers must begin with a letter and must contain only ASCII letters, digits, and hyphens. They can't end with a hyphen, or contain two consecutive hyphens."
              }
            ]
          },
          "DataProviderName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The name of the data provider."
              }
            ]
          },
          "Description": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "A user-friendly description of the data provider."
              }
            ]
          },
          "Engine": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The type of database engine for the data provider. Valid values include <code>\"aurora\"</code>, <code>\"aurora_postgresql\"</code>, <code>\"mysql\"</code>, <code>\"oracle\"</code>, <code>\"postgres\"</code>, and <code>\"sqlserver\"</code>. A value of <code>\"aurora\"</code> represents Amazon Aurora MySQL-Compatible Edition."
              }
            ]
          },
          "ExactSettings": {
            "allOf": [
              {
                "$ref": "#/$defs/BooleanOptional"
              },
              {
                "description": "<p>If this attribute is Y, the current call to <code>ModifyDataProvider</code> replaces all existing data provider settings with the exact settings that you specify in this call. If this attribute is N, the current call to <code>ModifyDataProvider</code> does two things: </p> <ul> <li> <p>It replaces any data provider settings that already exist with new values, for settings with the same names.</p> </li> <li> <p>It creates new data provider settings that you specify in the call, for settings with different names. </p> </li> </ul>"
              }
            ]
          },
          "Settings": {
            "allOf": [
              {
                "$ref": "#/$defs/DataProviderSettings"
              },
              {
                "description": "The settings in JSON format for a data provider."
              }
            ]
          }
        }
      },
      "String": {
        "type": "string"
      },
      "BooleanOptional": {
        "type": "boolean"
      },
      "DataProviderSettings": {
        "type": "object",
        "properties": {
          "PostgreSqlSettings": {
            "$ref": "#/$defs/PostgreSqlDataProviderSettings"
          },
          "MySqlSettings": {
            "$ref": "#/$defs/MySqlDataProviderSettings"
          },
          "OracleSettings": {
            "$ref": "#/$defs/OracleDataProviderSettings"
          },
          "MicrosoftSqlServerSettings": {
            "$ref": "#/$defs/MicrosoftSqlServerDataProviderSettings"
          }
        },
        "description": "Provides information that defines a data provider."
      },
      "PostgreSqlDataProviderSettings": {
        "type": "object",
        "properties": {
          "ServerName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The name of the PostgreSQL server."
              }
            ]
          },
          "Port": {
            "allOf": [
              {
                "$ref": "#/$defs/IntegerOptional"
              },
              {
                "description": "The port value for the PostgreSQL data provider."
              }
            ]
          },
          "DatabaseName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The database name on the PostgreSQL data provider."
              }
            ]
          },
          "SslMode": {
            "allOf": [
              {
                "$ref": "#/$defs/DmsSslModeValue"
              },
              {
                "description": "The SSL mode used to connect to the PostgreSQL data provider. The default value is <code>none</code>."
              }
            ]
          },
          "CertificateArn": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the certificate used for SSL connection."
              }
            ]
          }
        },
        "description": "Provides information that defines a PostgreSQL data provider."
      },
      "IntegerOptional": {
        "type": "integer"
      },
      "DmsSslModeValue": {
        "type": "string",
        "enum": [
          "none",
          "require",
          "verify-ca",
          "verify-full"
        ]
      },
      "MySqlDataProviderSettings": {
        "type": "object",
        "properties": {
          "ServerName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The name of the MySQL server."
              }
            ]
          },
          "Port": {
            "allOf": [
              {
                "$ref": "#/$defs/IntegerOptional"
              },
              {
                "description": "The port value for the MySQL data provider."
              }
            ]
          },
          "SslMode": {
            "allOf": [
              {
                "$ref": "#/$defs/DmsSslModeValue"
              },
              {
                "description": "The SSL mode used to connect to the MySQL data provider. The default value is <code>none</code>."
              }
            ]
          },
          "CertificateArn": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the certificate used for SSL connection."
              }
            ]
          }
        },
        "description": "Provides information that defines a MySQL data provider."
      },
      "OracleDataProviderSettings": {
        "type": "object",
        "properties": {
          "ServerName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The name of the Oracle server."
              }
            ]
          },
          "Port": {
            "allOf": [
              {
                "$ref": "#/$defs/IntegerOptional"
              },
              {
                "description": "The port value for the Oracle data provider."
              }
            ]
          },
          "DatabaseName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The database name on the Oracle data provider."
              }
            ]
          },
          "SslMode": {
            "allOf": [
              {
                "$ref": "#/$defs/DmsSslModeValue"
              },
              {
                "description": "The SSL mode used to connect to the Oracle data provider. The default value is <code>none</code>."
              }
            ]
          },
          "CertificateArn": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the certificate used for SSL connection."
              }
            ]
          },
          "AsmServer": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The address of your Oracle Automatic Storage Management (ASM) server. You can set this value from the <code>asm_server</code> value. You set <code>asm_server</code> as part of the extra connection attribute string to access an Oracle server with Binary Reader that uses ASM. For more information, see <a href=\"https://docs.aws.amazon.com/dms/latest/userguide/CHAP_Source.Oracle.html#dms/latest/userguide/CHAP_Source.Oracle.html#CHAP_Source.Oracle.CDC.Configuration\">Configuration for change data capture (CDC) on an Oracle source database</a>."
              }
            ]
          },
          "SecretsManagerOracleAsmSecretId": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "<p>The identifier of the secret in Secrets Manager that contains the Oracle ASM connection details.</p> <p>Required only if your data provider uses the Oracle ASM server.</p>"
              }
            ]
          },
          "SecretsManagerOracleAsmAccessRoleArn": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The ARN of the IAM role that provides access to the secret in Secrets Manager that contains the Oracle ASM connection details."
              }
            ]
          },
          "SecretsManagerSecurityDbEncryptionSecretId": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The identifier of the secret in Secrets Manager that contains the transparent data encryption (TDE) password. DMS requires this password to access Oracle redo logs encrypted by TDE using Binary Reader."
              }
            ]
          },
          "SecretsManagerSecurityDbEncryptionAccessRoleArn": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The ARN of the IAM role that provides access to the secret in Secrets Manager that contains the TDE password."
              }
            ]
          }
        },
        "description": "Provides information that defines an Oracle data provider."
      },
      "MicrosoftSqlServerDataProviderSettings": {
        "type": "object",
        "properties": {
          "ServerName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The name of the Microsoft SQL Server server."
              }
            ]
          },
          "Port": {
            "allOf": [
              {
                "$ref": "#/$defs/IntegerOptional"
              },
              {
                "description": "The port value for the Microsoft SQL Server data provider."
              }
            ]
          },
          "DatabaseName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The database name on the Microsoft SQL Server data provider."
              }
            ]
          },
          "SslMode": {
            "allOf": [
              {
                "$ref": "#/$defs/DmsSslModeValue"
              },
              {
                "description": "The SSL mode used to connect to the Microsoft SQL Server data provider. The default value is <code>none</code>."
              }
            ]
          },
          "CertificateArn": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the certificate used for SSL connection."
              }
            ]
          }
        },
        "description": "Provides information that defines a Microsoft SQL Server data provider."
      }
    }
  },
