{
  "name": "CreateDataProvider",
  "description": "Creates a data provider using the provided settings. A data provider stores a data store type and location information about your database. ",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AmazonDMSv20160101.CreateDataProvider"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/CreateDataProviderMessage"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "CreateDataProviderMessage": {
        "type": "object",
        "required": [
          "Engine",
          "Settings"
        ],
        "title": "CreateDataProviderMessage",
        "properties": {
          "DataProviderName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "A user-friendly name for the data provider."
              }
            ]
          },
          "Description": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "A user-friendly description of the data provider."
              }
            ]
          },
          "Engine": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The type of database engine for the data provider. Valid values include <code>\"aurora\"</code>, <code>\"aurora_postgresql\"</code>, <code>\"mysql\"</code>, <code>\"oracle\"</code>, <code>\"postgres\"</code>, and <code>\"sqlserver\"</code>. A value of <code>\"aurora\"</code> represents Amazon Aurora MySQL-Compatible Edition."
              }
            ]
          },
          "Settings": {
            "allOf": [
              {
                "$ref": "#/$defs/DataProviderSettings"
              },
              {
                "description": "The settings in JSON format for a data provider."
              }
            ]
          },
          "Tags": {
            "allOf": [
              {
                "$ref": "#/$defs/TagList"
              },
              {
                "description": "One or more tags to be assigned to the data provider."
              }
            ]
          }
        }
      },
      "String": {
        "type": "string"
      },
      "DataProviderSettings": {
        "type": "object",
        "properties": {
          "PostgreSqlSettings": {
            "$ref": "#/$defs/PostgreSqlDataProviderSettings"
          },
          "MySqlSettings": {
            "$ref": "#/$defs/MySqlDataProviderSettings"
          },
          "OracleSettings": {
            "$ref": "#/$defs/OracleDataProviderSettings"
          },
          "MicrosoftSqlServerSettings": {
            "$ref": "#/$defs/MicrosoftSqlServerDataProviderSettings"
          }
        },
        "description": "Provides information that defines a data provider."
      },
      "PostgreSqlDataProviderSettings": {
        "type": "object",
        "properties": {
          "ServerName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The name of the PostgreSQL server."
              }
            ]
          },
          "Port": {
            "allOf": [
              {
                "$ref": "#/$defs/IntegerOptional"
              },
              {
                "description": "The port value for the PostgreSQL data provider."
              }
            ]
          },
          "DatabaseName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The database name on the PostgreSQL data provider."
              }
            ]
          },
          "SslMode": {
            "allOf": [
              {
                "$ref": "#/$defs/DmsSslModeValue"
              },
              {
                "description": "The SSL mode used to connect to the PostgreSQL data provider. The default value is <code>none</code>."
              }
            ]
          },
          "CertificateArn": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the certificate used for SSL connection."
              }
            ]
          }
        },
        "description": "Provides information that defines a PostgreSQL data provider."
      },
      "IntegerOptional": {
        "type": "integer"
      },
      "DmsSslModeValue": {
        "type": "string",
        "enum": [
          "none",
          "require",
          "verify-ca",
          "verify-full"
        ]
      },
      "MySqlDataProviderSettings": {
        "type": "object",
        "properties": {
          "ServerName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
