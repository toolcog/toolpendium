            "allOf": [
              {
                "$ref": "#/$defs/IntegerOptional"
              },
              {
                "description": "The port value for the MySQL data provider."
              }
            ]
          },
          "SslMode": {
            "allOf": [
              {
                "$ref": "#/$defs/DmsSslModeValue"
              },
              {
                "description": "The SSL mode used to connect to the MySQL data provider. The default value is <code>none</code>."
              }
            ]
          },
          "CertificateArn": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the certificate used for SSL connection."
              }
            ]
          }
        },
        "description": "Provides information that defines a MySQL data provider."
      },
      "OracleDataProviderSettings": {
        "type": "object",
        "properties": {
          "ServerName": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The name of the Oracle server."
              }
            ]
          },
          "Port": {
            "allOf": [
              {
                "$ref": "#/$defs/IntegerOptional"
              },
              {
                "description": "The port value for the Oracle data provider."
