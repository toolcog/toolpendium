{
  "name": "ListIdentityResolutionJobs",
  "description": "Lists all of the Identity Resolution Jobs in your domain. The response sorts the list by <code>JobStartTime</code>.",
  "parameters": {
    "type": "object",
    "properties": {
      "DomainName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_-]+$",
        "minLength": 1,
        "maxLength": 64,
        "description": "The unique name of the domain."
      },
      "next-token": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1024,
        "description": "The token for the next set of results. Use the value returned in the previous response in the next request to retrieve the next set of results."
      },
      "max-results": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100,
        "description": "The maximum number of results to return per page."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "DomainName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://profile.us-east-1.amazonaws.com/domains/{DomainName}/identity-resolution-jobs{?next-token,max-results}"
    },
    "headers": {
