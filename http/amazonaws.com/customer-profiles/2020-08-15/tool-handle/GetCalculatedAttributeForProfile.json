{
  "name": "GetCalculatedAttributeForProfile",
  "description": "Retrieve a calculated attribute for a customer profile.",
  "parameters": {
    "type": "object",
    "properties": {
      "DomainName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_-]+$",
        "minLength": 1,
        "maxLength": 64,
        "description": "The unique name of the domain."
      },
      "ProfileId": {
        "type": "string",
        "pattern": "[a-f0-9]{32}",
        "description": "The unique identifier of a customer profile."
      },
      "CalculatedAttributeName": {
        "type": "string",
        "pattern": "^[a-zA-Z_][a-zA-Z_0-9-]*$",
        "minLength": 1,
        "maxLength": 255,
        "description": "The unique name of the calculated attribute."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
