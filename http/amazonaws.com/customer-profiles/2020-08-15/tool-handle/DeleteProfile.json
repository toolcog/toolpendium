      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "ProfileId"
        ],
        "properties": {
          "ProfileId": {
            "description": "The unique identifier of a customer profile.",
            "type": "string",
            "pattern": "[a-f0-9]{32}"
          }
        }
      }
    },
    "required": [
      "DomainName",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://profile.us-east-1.amazonaws.com/domains/{DomainName}/profiles/delete"
