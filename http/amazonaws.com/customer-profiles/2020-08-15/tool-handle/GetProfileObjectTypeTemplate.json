      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**TemplateId**",
            "**SourceName**",
            "**SourceObject**",
            "**AllowProfileCreation**",
            "**SourceLastUpdatedTimestampFormat**",
            "**Fields**",
