{
  "name": "ListAccountIntegrations",
  "description": "Lists all of the integrations associated to a specific URI in the AWS account.",
  "parameters": {
    "type": "object",
    "properties": {
      "next-token": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1024,
        "description": "The pagination token from the previous ListAccountIntegrations API call."
      },
      "max-results": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100,
        "description": "The maximum number of objects returned per page."
      },
      "include-hidden": {
        "type": "boolean",
        "description": "Boolean to indicate if hidden integration should be returned. Defaults to <code>False</code>."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Uri"
        ],
        "properties": {
          "Uri": {
            "description": "The URI of the S3 bucket or any other type of data source.",
            "type": "string",
            "minLength": 1,
            "maxLength": 255
          }
        }
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://profile.us-east-1.amazonaws.com/integrations{?next-token,max-results,include-hidden}"
    },
    "headers": {
