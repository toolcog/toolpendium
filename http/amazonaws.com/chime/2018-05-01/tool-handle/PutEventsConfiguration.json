      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "OutboundEventsHTTPSEndpoint": {
            "description": "HTTPS endpoint that allows the bot to receive outgoing events.",
            "type": "string",
            "format": "password"
          },
          "LambdaFunctionArn": {
            "description": "Lambda function ARN that allows the bot to receive outgoing events.",
            "type": "string",
            "format": "password"
          }
        }
      }
    },
