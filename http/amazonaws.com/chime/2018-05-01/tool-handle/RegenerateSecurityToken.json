{
  "name": "RegenerateSecurityToken",
  "description": "Regenerates the security token for a bot.",
  "parameters": {
    "type": "object",
    "properties": {
      "accountId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The Amazon Chime account ID."
      },
      "botId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The bot ID."
