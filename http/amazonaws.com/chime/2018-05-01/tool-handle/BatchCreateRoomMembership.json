{
  "name": "BatchCreateRoomMembership",
  "description": "Adds up to 50 members to a chat room in an Amazon Chime Enterprise account. Members can be users or bots. The member role designates whether the member is a chat room administrator or a general chat room member.",
  "parameters": {
    "type": "object",
    "properties": {
      "accountId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The Amazon Chime account ID."
      },
      "roomId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The room ID."
      },
      "operation": {
        "type": "string",
        "enum": [
          "batch-create"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "MembershipItemList"
        ],
        "properties": {
          "MembershipItemList": {
            "description": "The list of membership items.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/MembershipItem"
            },
            "maxItems": 50
          }
        }
      }
    },
    "required": [
      "accountId",
      "roomId",
      "operation",
      "body"
    ],
    "$defs": {
      "MembershipItem": {
        "type": "object",
        "properties": {
          "MemberId": {
            "allOf": [
              {
                "$ref": "#/$defs/NonEmptyString"
              },
              {
                "description": "The member ID."
              }
            ]
          },
          "Role": {
            "allOf": [
              {
                "$ref": "#/$defs/RoomMembershipRole"
              },
              {
                "description": "The member role."
              }
            ]
          }
        },
        "description": "Membership details, such as member ID and member role."
      },
      "NonEmptyString": {
        "type": "string",
        "pattern": ".*\\S.*"
      },
      "RoomMembershipRole": {
        "type": "string",
        "enum": [
          "Administrator",
          "Member"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://service.chime.aws.amazon.com/accounts/{accountId}/rooms/{roomId}/memberships#operation=batch-create{?operation}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
