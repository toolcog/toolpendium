      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "meetingId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
