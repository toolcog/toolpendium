        "type": "integer",
        "minimum": 1,
        "maximum": 50,
        "description": "The maximum number of <code>AppInstance</code>s that you want to return."
      },
      "next-token": {
        "type": "string",
        "pattern": ".*",
        "minLength": 0,
        "maxLength": 2048,
