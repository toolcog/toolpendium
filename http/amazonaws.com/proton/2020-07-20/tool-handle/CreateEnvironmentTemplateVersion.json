        "pattern": "^[!-~]*$",
        "minLength": 0,
        "maxLength": 64
      },
      "Description": {
        "type": "string",
        "minLength": 0,
        "maxLength": 500,
        "format": "password"
      },
      "TemplateVersionPart": {
        "type": "string",
        "pattern": "^(0|([1-9]{1}\\d*))$",
        "minLength": 1,
        "maxLength": 20
      },
      "TemplateVersionSourceInput": {
