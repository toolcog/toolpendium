                "$ref": "#/$defs/TemplateVersionPart"
              },
              {
                "description": "The major version of the service template that was used to create the service that the pipeline is associated with."
              }
            ]
          },
          "templateMinorVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/TemplateVersionPart"
              },
              {
                "description": "The minor version of the service template that was used to create the service that the pipeline is associated with."
              }
            ]
          }
        }
      },
      "DeploymentUpdateType": {
        "type": "string",
        "enum": [
          "NONE",
          "CURRENT_VERSION",
          "MINOR_VERSION",
          "MAJOR_VERSION"
        ]
      },
      "ResourceName": {
        "type": "string",
        "pattern": "^[0-9A-Za-z]+[0-9A-Za-z_\\-]*$",
        "minLength": 1,
        "maxLength": 100
      },
      "SpecContents": {
        "type": "string",
        "minLength": 1,
        "maxLength": 51200,
        "format": "password"
      },
      "TemplateVersionPart": {
        "type": "string",
        "pattern": "^(0|([1-9]{1}\\d*))$",
        "minLength": 1,
        "maxLength": 20
      }
    }
  },
