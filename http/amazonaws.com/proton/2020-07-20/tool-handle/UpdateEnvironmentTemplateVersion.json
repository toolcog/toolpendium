        "pattern": "^(0|([1-9]{1}\\d*))$",
        "minLength": 1,
        "maxLength": 20
      },
      "TemplateVersionStatus": {
        "type": "string",
        "enum": [
          "REGISTRATION_IN_PROGRESS",
          "REGISTRATION_FAILED",
          "DRAFT",
          "PUBLISHED"
        ]
