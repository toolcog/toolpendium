          "$ref": "#/$defs/EnvironmentTemplateFilter"
        }
      },
      "EnvironmentTemplateFilter": {
        "type": "object",
        "required": [
          "majorVersion",
          "templateName"
        ],
        "properties": {
          "majorVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/TemplateVersionPart"
              },
              {
                "description": "Include <code>majorVersion</code> to filter search for a major version."
              }
