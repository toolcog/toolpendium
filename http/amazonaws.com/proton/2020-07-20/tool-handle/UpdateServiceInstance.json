              }
            ]
          },
          "templateMajorVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/TemplateVersionPart"
              },
              {
                "description": "The major version of the service template to update."
              }
            ]
          },
          "templateMinorVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/TemplateVersionPart"
              },
              {
                "description": "The minor version of the service template to update."
              }
            ]
          }
        }
      },
