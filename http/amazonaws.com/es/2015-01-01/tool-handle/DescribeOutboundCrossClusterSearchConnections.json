{
  "name": "DescribeOutboundCrossClusterSearchConnections",
  "description": "Lists all the outbound cross-cluster search connections for a source domain.",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
