{
  "name": "DescribeBotAlias",
  "description": "Get information about a specific bot alias.",
  "parameters": {
    "type": "object",
    "properties": {
      "botAliasId": {
        "type": "string",
        "pattern": "^(\\bTSTALIASID\\b|[0-9a-zA-Z]+)$",
        "minLength": 10,
        "maxLength": 10,
        "description": "The identifier of the bot alias to describe."
      },
      "botId": {
        "type": "string",
        "pattern": "^[0-9a-zA-Z]+$",
        "minLength": 10,
        "maxLength": 10,
        "description": "The identifier of the bot associated with the bot alias to describe."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "botAliasId",
      "botId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://models-v2-lex.us-east-1.amazonaws.com/bots/{botId}/botaliases/{botAliasId}/"
    },
