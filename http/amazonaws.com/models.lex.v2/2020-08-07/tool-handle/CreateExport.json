{
  "name": "CreateExport",
  "description": "<p>Creates a zip archive containing the contents of a bot or a bot locale. The archive contains a directory structure that contains JSON files that define the bot.</p> <p>You can create an archive that contains the complete definition of a bot, or you can specify that the archive contain only the definition of a single bot locale.</p> <p>For more information about exporting bots, and about the structure of the export archive, see <a href=\"https://docs.aws.amazon.com/lexv2/latest/dg/importing-exporting.html\"> Importing and exporting bots </a> </p>",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "resourceSpecification",
          "fileFormat"
        ],
        "properties": {
          "resourceSpecification": {
            "description": "Provides information about the bot or bot locale that you want to export. You can specify the <code>botExportSpecification</code> or the <code>botLocaleExportSpecification</code>, but not both.",
            "type": "object",
            "properties": {
              "botExportSpecification": {
                "allOf": [
                  {
                    "$ref": "#/$defs/BotExportSpecification"
                  },
                  {
                    "description": "Parameters for exporting a bot."
                  }
                ]
              },
              "botLocaleExportSpecification": {
                "allOf": [
                  {
                    "$ref": "#/$defs/BotLocaleExportSpecification"
                  },
                  {
                    "description": "Parameters for exporting a bot locale."
                  }
                ]
              },
              "customVocabularyExportSpecification": {
                "allOf": [
                  {
                    "$ref": "#/$defs/CustomVocabularyExportSpecification"
                  },
                  {
                    "description": "The parameters required to export a custom vocabulary."
                  }
                ]
              },
              "testSetExportSpecification": {
                "allOf": [
                  {
                    "$ref": "#/$defs/TestSetExportSpecification"
                  },
                  {
                    "description": "Specifications for the test set that is exported as a resource."
                  }
                ]
              }
            }
          },
          "fileFormat": {
            "description": "The file format of the bot or bot locale definition files.",
            "type": "string",
            "enum": [
              "LexJson",
              "TSV",
              "CSV"
            ]
          },
          "filePassword": {
            "description": "An password to use to encrypt the exported archive. Using a password is optional, but you should encrypt the archive to protect the data in transit between Amazon Lex and your local computer.",
            "type": "string",
            "minLength": 1,
            "maxLength": 1024,
            "format": "password"
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "BotExportSpecification": {
        "type": "object",
        "required": [
          "botId",
          "botVersion"
        ],
        "properties": {
          "botId": {
            "allOf": [
              {
                "$ref": "#/$defs/Id"
              },
              {
                "description": "The identifier of the bot assigned by Amazon Lex."
              }
            ]
          },
          "botVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/BotVersion"
              },
              {
                "description": "The version of the bot that was exported. This will be either <code>DRAFT</code> or the version number."
              }
            ]
          }
        },
        "description": "Provides the identity of a the bot that was exported."
      },
      "Id": {
        "type": "string",
        "pattern": "^[0-9a-zA-Z]+$",
        "minLength": 10,
        "maxLength": 10
      },
      "BotVersion": {
        "type": "string",
        "pattern": "^(DRAFT|[0-9]+)$",
        "minLength": 1,
        "maxLength": 5
      },
      "BotLocaleExportSpecification": {
        "type": "object",
        "required": [
          "botId",
          "botVersion",
          "localeId"
        ],
        "properties": {
          "botId": {
            "allOf": [
              {
                "$ref": "#/$defs/Id"
              },
              {
                "description": "The identifier of the bot to create the locale for."
              }
            ]
          },
          "botVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/BotVersion"
              },
              {
                "description": "The version of the bot to export."
              }
            ]
          },
          "localeId": {
            "allOf": [
              {
                "$ref": "#/$defs/LocaleId"
              },
              {
                "description": "The identifier of the language and locale to export. The string must match one of the locales in the bot."
              }
            ]
          }
        },
        "description": "Provides the bot locale parameters required for exporting a bot locale."
      },
      "LocaleId": {
        "type": "string"
      },
      "CustomVocabularyExportSpecification": {
        "type": "object",
        "required": [
          "botId",
          "botVersion",
          "localeId"
        ],
        "properties": {
          "botId": {
            "allOf": [
              {
                "$ref": "#/$defs/Id"
              },
              {
                "description": "The identifier of the bot that contains the custom vocabulary to export."
              }
            ]
          },
          "botVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/BotVersion"
              },
              {
                "description": "The version of the bot that contains the custom vocabulary to export."
              }
            ]
          },
          "localeId": {
            "allOf": [
              {
                "$ref": "#/$defs/LocaleId"
              },
              {
                "description": "The locale of the bot that contains the custom vocabulary to export."
              }
            ]
          }
        },
        "description": "Provides the parameters required for exporting a custom vocabulary."
      },
      "TestSetExportSpecification": {
        "type": "object",
        "required": [
          "testSetId"
        ],
        "properties": {
          "testSetId": {
            "allOf": [
              {
                "$ref": "#/$defs/Id"
              },
              {
                "description": "The unique identifier of the test set."
              }
            ]
          }
        },
        "description": "Contains information about the test set that is exported."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://models-v2-lex.us-east-1.amazonaws.com/exports/"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
