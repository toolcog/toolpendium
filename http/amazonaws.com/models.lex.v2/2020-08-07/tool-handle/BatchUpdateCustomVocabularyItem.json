{
  "name": "BatchUpdateCustomVocabularyItem",
  "description": "Update a batch of custom vocabulary items for a given bot locale's custom vocabulary.",
  "parameters": {
    "type": "object",
    "properties": {
      "botId": {
        "type": "string",
        "pattern": "^[0-9a-zA-Z]+$",
        "minLength": 10,
        "maxLength": 10,
        "description": "The identifier of the bot associated with this custom vocabulary"
      },
      "botVersion": {
        "type": "string",
        "pattern": "^(DRAFT|[0-9]+)$",
        "minLength": 1,
        "maxLength": 5,
        "description": "The identifier of the version of the bot associated with this custom vocabulary."
      },
      "localeId": {
        "type": "string",
        "description": "The identifier of the language and locale where this custom vocabulary is used. The string must match one of the supported locales. For more information, see <a href=\"https://docs.aws.amazon.com/lexv2/latest/dg/how-languages.html\"> Supported Languages </a>."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "customVocabularyItemList"
        ],
        "properties": {
          "customVocabularyItemList": {
            "description": "A list of custom vocabulary items with updated fields. Each entry must contain a phrase and can optionally contain a displayAs and/or a weight.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/CustomVocabularyItem"
            },
            "minItems": 1,
            "maxItems": 10
          }
        }
      }
    },
    "required": [
      "botId",
      "botVersion",
      "localeId",
      "body"
    ],
    "$defs": {
      "CustomVocabularyItem": {
        "type": "object",
        "required": [
          "itemId",
          "phrase"
        ],
        "properties": {
          "itemId": {
            "allOf": [
              {
                "$ref": "#/$defs/ItemId"
              },
              {
                "description": "The unique item identifer for the custom vocabulary item from the custom vocabulary list."
              }
            ]
          },
          "phrase": {
            "allOf": [
              {
                "$ref": "#/$defs/Phrase"
              },
              {
                "description": "The unique phrase for the custom vocabulary item from the custom vocabulary list."
              }
            ]
          },
          "weight": {
            "allOf": [
              {
                "$ref": "#/$defs/Weight"
              },
              {
                "description": "The weight assigned for the custom vocabulary item from the custom vocabulary list."
              }
            ]
          },
          "displayAs": {
            "allOf": [
              {
                "$ref": "#/$defs/Phrase"
              },
              {
                "description": "The DisplayAs value for the custom vocabulary item from the custom vocabulary list."
              }
            ]
          }
        },
        "description": "The unique custom vocabulary item from the custom vocabulary list."
      },
      "ItemId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 32
      },
      "Phrase": {
        "type": "string",
        "minLength": 1,
        "maxLength": 100
      },
      "Weight": {
        "type": "integer",
        "minimum": 1,
        "maximum": 3
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://models-v2-lex.us-east-1.amazonaws.com/bots/{botId}/botversions/{botVersion}/botlocales/{localeId}/customvocabulary/DEFAULT/batchupdate"
    },
