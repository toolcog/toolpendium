{
  "name": "DescribeCustomVocabularyMetadata",
  "description": "Provides metadata information about a custom vocabulary.",
  "parameters": {
    "type": "object",
    "properties": {
      "botId": {
        "type": "string",
        "pattern": "^[0-9a-zA-Z]+$",
        "minLength": 10,
        "maxLength": 10,
        "description": "The unique identifier of the bot that contains the custom vocabulary."
      },
      "botVersion": {
        "type": "string",
        "pattern": "^(DRAFT|[0-9]+)$",
        "minLength": 1,
        "maxLength": 5,
        "description": "The bot version of the bot to return metadata for."
      },
      "localeId": {
        "type": "string",
        "description": "The locale to return the custom vocabulary information for. The locale must be <code>en_GB</code>."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "botId",
      "botVersion",
      "localeId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://models-v2-lex.us-east-1.amazonaws.com/bots/{botId}/botversions/{botVersion}/botlocales/{localeId}/customvocabulary/DEFAULT/metadata"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
