{
  "name": "StartTestExecution",
  "description": "The action to start test set execution.",
  "parameters": {
    "type": "object",
    "properties": {
      "testSetId": {
        "type": "string",
        "pattern": "^[0-9a-zA-Z]+$",
        "minLength": 10,
        "maxLength": 10,
        "description": "The test set Id for the test set execution."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "target",
          "apiMode"
        ],
        "properties": {
          "target": {
            "description": "Contains information about the bot used for the test execution.",
            "type": "object",
            "properties": {
              "botAliasTarget": {
                "allOf": [
                  {
                    "$ref": "#/$defs/BotAliasTestExecutionTarget"
                  },
                  {
                    "description": "Contains information about the bot alias used for the test execution."
                  }
                ]
              }
            }
          },
          "apiMode": {
            "description": "Indicates whether we use streaming or non-streaming APIs for the test set execution. For streaming, StartConversation Runtime API is used. Whereas, for non-streaming, RecognizeUtterance and RecognizeText Amazon Lex Runtime API are used.",
            "type": "string",
            "enum": [
              "Streaming",
              "NonStreaming"
            ]
          },
          "testExecutionModality": {
            "description": "Indicates whether audio or text is used.",
            "type": "string",
            "enum": [
              "Text",
              "Audio"
            ]
          }
        }
      }
    },
    "required": [
      "testSetId",
      "body"
    ],
    "$defs": {
      "BotAliasTestExecutionTarget": {
        "type": "object",
        "required": [
          "botId",
          "botAliasId",
          "localeId"
        ],
        "properties": {
          "botId": {
            "allOf": [
              {
                "$ref": "#/$defs/Id"
              },
              {
                "description": "The bot Id of the bot alias used in the test set execution."
              }
            ]
          },
          "botAliasId": {
            "allOf": [
              {
                "$ref": "#/$defs/BotAliasId"
              },
              {
                "description": "The bot alias Id of the bot alias used in the test set execution."
              }
            ]
          },
          "localeId": {
            "allOf": [
              {
                "$ref": "#/$defs/LocaleId"
              },
              {
                "description": "The locale Id of the bot alias used in the test set execution."
              }
            ]
          }
        },
        "description": "The target Amazon S3 location for the test set execution using a bot alias."
      },
      "Id": {
        "type": "string",
        "pattern": "^[0-9a-zA-Z]+$",
        "minLength": 10,
        "maxLength": 10
      },
      "BotAliasId": {
        "type": "string",
        "pattern": "^(\\bTSTALIASID\\b|[0-9a-zA-Z]+)$",
        "minLength": 10,
        "maxLength": 10
      },
      "LocaleId": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://models-v2-lex.us-east-1.amazonaws.com/testsets/{testSetId}/testexecutions"
    },
    "headers": {
