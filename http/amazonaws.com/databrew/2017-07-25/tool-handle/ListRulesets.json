      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://databrew.us-east-1.amazonaws.com/rulesets{?targetArn,maxResults,nextToken,MaxResults,NextToken}"
