{
  "name": "DescribeJobRun",
  "description": "Represents one run of a DataBrew job.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "minLength": 1,
        "maxLength": 240,
        "description": "The name of the job being processed during this run."
      },
      "runId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255,
        "description": "The unique identifier of the job run."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "name",
      "runId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://databrew.us-east-1.amazonaws.com/jobs/{name}/jobRun/{runId}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Attempt**",
            "**CompletedOn**",
            "**DatasetName**",
            "**ErrorMessage**",
            "**ExecutionTime**",
            "**JobName**",
            "**ProfileConfiguration**",
            "**ValidationConfigurations**",
            "**RunId**",
            "**State**",
            "**LogSubscription**",
            "**LogGroupName**",
            "**Outputs**",
            "**DataCatalogOutputs**",
            "**DatabaseOutputs**",
            [
              "**RecipeReference**: Represents the name and version of a DataBrew recipe.",
              {
                "$ul": [
                  "**Name**",
                  "**RecipeVersion**"
                ]
              }
            ],
            "**StartedBy**",
            "**StartedOn**",
            "**JobSample**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
