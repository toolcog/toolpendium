{
  "name": "CreateApplication",
  "description": "Creates a new application that is the top-level node in a hierarchy of related cloud resource abstractions.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
