      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**arn**",
            "**name**",
            "**description**",
            "**attributes**",
            "**creationTime**",
            "**lastUpdateTime**",
            "**tags**",
