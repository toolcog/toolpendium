{
  "name": "ListCatalogItems",
  "description": "<p>Lists the items in the catalog.</p> <p>Use filters to return specific results. If you specify multiple filters, the results include only the resources that match all of the specified filters. For a filter where you can specify multiple values, the results include items that match any of the values that you specify for the filter.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "NextToken": {
        "type": "string",
        "pattern": "^(\\d+)##(\\S+)$",
        "description": "",
        "minLength": 1,
        "maxLength": 2048
      },
      "MaxResults": {
        "type": "integer",
        "description": "",
        "minimum": 1,
        "maximum": 1000
      },
      "ItemClassFilter": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/CatalogItemClass"
        },
        "description": "Filters the results by item class."
      },
      "SupportedStorageFilter": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/SupportedStorageEnum"
        },
        "description": "Filters the results by storage option."
      },
      "EC2FamilyFilter": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Family"
        },
        "description": "Filters the results by EC2 family (for example, M5)."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "$defs": {
      "CatalogItemClass": {
        "type": "string",
        "enum": [
          "RACK",
          "SERVER"
        ]
      },
      "SupportedStorageEnum": {
        "type": "string",
        "enum": [
          "EBS",
          "S3"
        ]
      },
      "Family": {
        "type": "string",
        "pattern": "[a-z0-9]+",
        "minLength": 1,
        "maxLength": 10
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://outposts.us-east-1.amazonaws.com/catalog/items{?NextToken,MaxResults,ItemClassFilter,SupportedStorageFilter,EC2FamilyFilter}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**CatalogItems**",
            "**NextToken**: The pagination token."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
