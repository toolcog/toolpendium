{
  "name": "ListOrders",
  "description": "Lists the Outpost orders for your Amazon Web Services account.",
  "parameters": {
    "type": "object",
    "properties": {
      "OutpostIdentifierFilter": {
        "type": "string",
        "pattern": "^(arn:aws([a-z-]+)?:outposts:[a-z\\d-]+:\\d{12}:outpost/)?op-[a-f0-9]{17}$",
        "minLength": 1,
        "maxLength": 180,
        "description": " The ID or the Amazon Resource Name (ARN) of the Outpost. "
      },
      "NextToken": {
        "type": "string",
        "pattern": "^(\\d+)##(\\S+)$",
        "description": "",
        "minLength": 1,
        "maxLength": 2048
      },
      "MaxResults": {
        "type": "integer",
        "description": "",
        "minimum": 1,
        "maximum": 1000
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://outposts.us-east-1.amazonaws.com/list-orders{?OutpostIdentifierFilter,NextToken,MaxResults}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
