{
  "name": "BatchResetAlarm",
  "description": "Resets one or more alarms. The alarms return to the <code>NORMAL</code> state after you reset them.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
