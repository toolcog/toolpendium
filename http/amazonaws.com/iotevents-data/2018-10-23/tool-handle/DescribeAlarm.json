        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "alarmModelName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://data.iotevents.us-east-1.amazonaws.com/alarms/{alarmModelName}/keyValues/{?keyValue}"
    },
