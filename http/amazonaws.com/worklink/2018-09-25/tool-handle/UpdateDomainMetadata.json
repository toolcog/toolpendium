{
  "name": "UpdateDomainMetadata",
  "description": "Updates domain metadata, such as DisplayName.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
