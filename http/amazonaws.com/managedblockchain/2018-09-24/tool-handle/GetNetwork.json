      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "networkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://managedblockchain.us-east-1.amazonaws.com/networks/{networkId}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
