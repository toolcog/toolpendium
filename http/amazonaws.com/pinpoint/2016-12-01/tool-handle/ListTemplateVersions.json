      "template-name",
      "template-type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://pinpoint.us-east-1.amazonaws.com/v1/templates/{template-name}/{template-type}/versions{?next-token,page-size}"
