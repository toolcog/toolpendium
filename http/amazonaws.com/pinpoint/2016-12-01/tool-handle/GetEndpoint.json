{
  "name": "GetEndpoint",
  "description": "Retrieves information about the settings and attributes of a specific endpoint for an application.",
  "parameters": {
    "type": "object",
    "properties": {
      "application-id": {
        "type": "string",
        "description": "The unique identifier for the application. This identifier is displayed as the <b>Project ID</b> on the Amazon Pinpoint console."
      },
