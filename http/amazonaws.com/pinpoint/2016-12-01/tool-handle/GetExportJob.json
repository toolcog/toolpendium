{
  "name": "GetExportJob",
  "description": "Retrieves information about the status and settings of a specific export job for an application.",
  "parameters": {
    "type": "object",
    "properties": {
      "application-id": {
        "type": "string",
        "description": "The unique identifier for the application. This identifier is displayed as the <b>Project ID</b> on the Amazon Pinpoint console."
      },
