{
  "name": "SendOTPMessage",
  "description": "Send an OTP message",
  "parameters": {
    "type": "object",
    "properties": {
      "application-id": {
        "type": "string",
        "description": "The unique ID of your Amazon Pinpoint application."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "SendOTPMessageRequestParameters"
        ],
        "properties": {
          "SendOTPMessageRequestParameters": {
            "description": "Send OTP message request parameters.",
            "type": "object",
            "properties": {
              "AllowedAttempts": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__integer"
                  },
                  {
                    "description": "The attempts allowed to validate an OTP."
                  }
                ]
              },
              "BrandName": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__string"
                  },
                  {
                    "description": "The brand name that will be substituted into the OTP message body. Should be owned by calling AWS account."
                  }
                ]
              },
              "Channel": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__string"
                  },
                  {
                    "description": "Channel type for the OTP message. Supported values: [SMS]."
                  }
                ]
              },
              "CodeLength": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__integer"
                  },
                  {
                    "description": "The number of characters in the generated OTP."
                  }
                ]
              },
              "DestinationIdentity": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__string"
                  },
                  {
                    "description": "The destination identity to send OTP to."
                  }
                ]
              },
              "EntityId": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__string"
                  },
                  {
                    "description": "A unique Entity ID received from DLT after entity registration is approved."
                  }
                ]
              },
              "Language": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__string"
                  },
                  {
                    "description": "The language to be used for the outgoing message body containing the OTP."
                  }
                ]
              },
              "OriginationIdentity": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__string"
                  },
                  {
                    "description": "The origination identity used to send OTP from."
                  }
                ]
              },
              "ReferenceId": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__string"
                  },
                  {
                    "description": "Developer-specified reference identifier. Required to match during OTP verification."
                  }
                ]
              },
              "TemplateId": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__string"
                  },
                  {
                    "description": "A unique Template ID received from DLT after entity registration is approved."
                  }
                ]
              },
              "ValidityPeriod": {
                "allOf": [
                  {
                    "$ref": "#/$defs/__integer"
                  },
                  {
                    "description": "The time in minutes before the OTP is no longer valid."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "application-id",
      "body"
    ],
    "$defs": {
      "__integer": {
        "type": "integer"
      },
      "__string": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://pinpoint.us-east-1.amazonaws.com/v1/apps/{application-id}/otp"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
