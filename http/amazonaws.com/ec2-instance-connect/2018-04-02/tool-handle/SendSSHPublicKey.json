{
  "name": "SendSSHPublicKey",
  "description": "Pushes an SSH public key to the specified EC2 instance for use by the specified user. The key remains for 60 seconds. For more information, see <a href=\"https://docs.aws.amazon.com/AWSEC2/latest/UserGuide/Connect-using-EC2-Instance-Connect.html\">Connect to your Linux instance using EC2 Instance Connect</a> in the <i>Amazon EC2 User Guide</i>.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSEC2InstanceConnectService.SendSSHPublicKey"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/SendSSHPublicKeyRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "SendSSHPublicKeyRequest": {
        "type": "object",
        "required": [
          "InstanceId",
          "InstanceOSUser",
          "SSHPublicKey"
        ],
        "title": "SendSSHPublicKeyRequest",
        "properties": {
          "InstanceId": {
            "allOf": [
              {
                "$ref": "#/$defs/InstanceId"
              },
              {
                "description": "The ID of the EC2 instance."
              }
            ]
          },
          "InstanceOSUser": {
            "allOf": [
              {
                "$ref": "#/$defs/InstanceOSUser"
              },
              {
                "description": "The OS user on the EC2 instance for whom the key can be used to authenticate."
              }
            ]
          },
          "SSHPublicKey": {
            "allOf": [
              {
                "$ref": "#/$defs/SSHPublicKey"
              },
              {
                "description": "The public key material. To use the public key, you must have the matching private key."
              }
            ]
          },
          "AvailabilityZone": {
            "allOf": [
              {
                "$ref": "#/$defs/AvailabilityZone"
              },
              {
                "description": "The Availability Zone in which the EC2 instance was launched."
              }
            ]
          }
        }
      },
      "InstanceId": {
        "type": "string",
        "pattern": "^i-[a-f0-9]+$",
        "minLength": 10,
        "maxLength": 32
      },
      "InstanceOSUser": {
        "type": "string",
        "pattern": "^[A-Za-z_][A-Za-z0-9\\@\\._-]{0,30}[A-Za-z0-9\\$_-]?$",
        "minLength": 1,
        "maxLength": 32
      },
      "SSHPublicKey": {
        "type": "string",
        "minLength": 80,
        "maxLength": 4096
      },
      "AvailabilityZone": {
        "type": "string",
        "pattern": "^(\\w+-){2,3}\\d+\\w+$",
        "minLength": 6,
        "maxLength": 32
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://ec2-instance-connect.us-east-1.amazonaws.com/#X-Amz-Target=AWSEC2InstanceConnectService.SendSSHPublicKey"
    },
    "headers": {
