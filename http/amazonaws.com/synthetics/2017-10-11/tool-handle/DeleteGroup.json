    "type": "object",
    "properties": {
      "groupIdentifier": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128,
        "description": "Specifies which group to delete. You can specify the group name, the ARN, or the group ID as the <code>GroupIdentifier</code>."
      },
