        "title": "AssociateS3ResourcesRequest",
        "properties": {
          "memberAccountId": {
            "allOf": [
              {
                "$ref": "#/$defs/AWSAccountId"
              },
              {
                "description": "(Discontinued) The ID of the Amazon Macie Classic member account whose resources you want to associate with Macie Classic."
              }
            ]
          },
          "s3Resources": {
            "allOf": [
              {
                "$ref": "#/$defs/S3ResourcesClassification"
              },
              {
                "description": "(Discontinued) The S3 resources that you want to associate with Amazon Macie Classic for monitoring and data classification."
              }
            ]
          }
        }
      },
      "AWSAccountId": {
        "type": "string",
        "pattern": "[0-9]{12}"
      },
      "S3ResourcesClassification": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/S3ResourceClassification"
        }
      },
      "S3ResourceClassification": {
        "type": "object",
        "required": [
          "bucketName",
          "classificationType"
        ],
        "properties": {
          "bucketName": {
            "allOf": [
              {
                "$ref": "#/$defs/BucketName"
              },
              {
                "description": "(Discontinued) The name of the S3 bucket that you want to associate with Amazon Macie Classic."
              }
            ]
          },
          "prefix": {
            "allOf": [
              {
                "$ref": "#/$defs/Prefix"
              },
              {
                "description": "(Discontinued) The prefix of the S3 bucket that you want to associate with Amazon Macie Classic."
              }
            ]
          },
          "classificationType": {
            "allOf": [
              {
                "$ref": "#/$defs/ClassificationType"
              },
              {
                "description": "(Discontinued) The classification type that you want to specify for the resource associated with Amazon Macie Classic. "
              }
            ]
          }
        },
        "description": "(Discontinued) The S3 resources that you want to associate with Amazon Macie Classic for monitoring and data classification. This data type is used as a request parameter in the <code>AssociateS3Resources</code> action and a response parameter in the <code>ListS3Resources</code> action. "
      },
      "BucketName": {
        "type": "string",
        "maxLength": 500
      },
      "Prefix": {
        "type": "string",
        "maxLength": 10000
      },
      "ClassificationType": {
        "type": "object",
        "required": [
          "oneTime",
          "continuous"
        ],
        "properties": {
          "oneTime": {
            "allOf": [
              {
                "$ref": "#/$defs/S3OneTimeClassificationType"
              },
              {
                "description": "(Discontinued) A one-time classification of all of the existing objects in a specified S3 bucket. "
              }
            ]
          },
          "continuous": {
            "allOf": [
              {
                "$ref": "#/$defs/S3ContinuousClassificationType"
              },
              {
                "description": "(Discontinued) A continuous classification of the objects that are added to a specified S3 bucket. Amazon Macie Classic begins performing continuous classification after a bucket is successfully associated with Macie Classic."
              }
            ]
          }
        },
        "description": "(Discontinued) The classification type that Amazon Macie Classic applies to the associated S3 resources."
      },
      "S3OneTimeClassificationType": {
        "type": "string",
        "enum": [
          "FULL",
          "NONE"
        ]
      },
      "S3ContinuousClassificationType": {
        "type": "string",
        "enum": [
          "FULL"
        ]
      }
    }
