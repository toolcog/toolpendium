{
  "name": "UpdateColumnStatisticsForTable",
  "description": "<p>Creates or updates table statistics of columns.</p> <p>The Identity and Access Management (IAM) permission required for this operation is <code>UpdateTable</code>.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSGlue.UpdateColumnStatisticsForTable"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateColumnStatisticsForTableRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateColumnStatisticsForTableRequest": {
        "type": "object",
        "required": [
          "DatabaseName",
          "TableName",
          "ColumnStatisticsList"
        ],
        "title": "UpdateColumnStatisticsForTableRequest",
        "properties": {
          "CatalogId": {
            "allOf": [
              {
                "$ref": "#/$defs/CatalogIdString"
              },
              {
                "description": "The ID of the Data Catalog where the partitions in question reside. If none is supplied, the Amazon Web Services account ID is used by default."
              }
            ]
          },
          "DatabaseName": {
            "allOf": [
              {
                "$ref": "#/$defs/NameString"
              },
              {
                "description": "The name of the catalog database where the partitions reside."
              }
            ]
          },
          "TableName": {
            "allOf": [
              {
                "$ref": "#/$defs/NameString"
              },
              {
                "description": "The name of the partitions' table."
              }
            ]
          },
          "ColumnStatisticsList": {
            "allOf": [
              {
                "$ref": "#/$defs/UpdateColumnStatisticsList"
              },
              {
                "description": "A list of the column statistics."
              }
            ]
          }
        }
      },
      "CatalogIdString": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255,
        "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\t]*"
      },
      "NameString": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255,
        "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\t]*"
      },
      "UpdateColumnStatisticsList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/ColumnStatistics"
        },
        "minItems": 0,
        "maxItems": 25
      },
      "ColumnStatistics": {
        "type": "object",
        "required": [
          "ColumnName",
          "ColumnType",
          "AnalyzedTime",
          "StatisticsData"
        ],
        "properties": {
          "ColumnName": {
            "allOf": [
              {
                "$ref": "#/$defs/NameString"
              },
              {
                "description": "Name of column which statistics belong to."
              }
            ]
          },
          "ColumnType": {
            "allOf": [
              {
                "$ref": "#/$defs/TypeString"
              },
              {
                "description": "The data type of the column."
              }
            ]
          },
          "AnalyzedTime": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "The timestamp of when column statistics were generated."
              }
            ]
          },
          "StatisticsData": {
            "allOf": [
              {
                "$ref": "#/$defs/ColumnStatisticsData"
              },
              {
                "description": "A <code>ColumnStatisticData</code> object that contains the statistics data values."
              }
            ]
          }
        },
        "description": "Represents the generated column-level statistics for a table or partition."
      },
      "TypeString": {
        "type": "string",
        "minLength": 0,
        "maxLength": 20000,
        "x-pattern": "[\\u0020-\\uD7FF\\uE000-\\uFFFD\\uD800\\uDC00-\\uDBFF\\uDFFF\\t]*"
      },
      "Timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "ColumnStatisticsData": {
        "type": "object",
        "required": [
          "Type"
        ],
        "properties": {
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/ColumnStatisticsType"
              },
              {
                "description": "The type of column statistics data."
              }
            ]
          },
          "BooleanColumnStatisticsData": {
            "allOf": [
              {
                "$ref": "#/$defs/BooleanColumnStatisticsData"
              },
              {
                "description": "Boolean column statistics data."
              }
            ]
          },
          "DateColumnStatisticsData": {
            "allOf": [
              {
                "$ref": "#/$defs/DateColumnStatisticsData"
              },
              {
                "description": "Date column statistics data."
              }
            ]
          },
          "DecimalColumnStatisticsData": {
            "allOf": [
              {
                "$ref": "#/$defs/DecimalColumnStatisticsData"
              },
              {
                "description": " Decimal column statistics data. UnscaledValues within are Base64-encoded binary objects storing big-endian, two's complement representations of the decimal's unscaled value. "
              }
            ]
          },
          "DoubleColumnStatisticsData": {
            "allOf": [
              {
                "$ref": "#/$defs/DoubleColumnStatisticsData"
              },
              {
                "description": "Double column statistics data."
              }
            ]
          },
          "LongColumnStatisticsData": {
            "allOf": [
              {
                "$ref": "#/$defs/LongColumnStatisticsData"
              },
              {
                "description": "Long column statistics data."
              }
            ]
          },
          "StringColumnStatisticsData": {
            "allOf": [
              {
                "$ref": "#/$defs/StringColumnStatisticsData"
              },
              {
                "description": "String column statistics data."
              }
            ]
          },
          "BinaryColumnStatisticsData": {
            "allOf": [
              {
                "$ref": "#/$defs/BinaryColumnStatisticsData"
              },
              {
                "description": "Binary column statistics data."
              }
            ]
          }
        },
        "description": "Contains the individual types of column statistics data. Only one data object should be set and indicated by the <code>Type</code> attribute."
      },
      "ColumnStatisticsType": {
        "type": "string",
        "enum": [
          "BOOLEAN",
          "DATE",
          "DECIMAL",
          "DOUBLE",
          "LONG",
          "STRING",
          "BINARY"
        ]
      },
      "BooleanColumnStatisticsData": {
        "type": "object",
        "required": [
          "NumberOfTrues",
          "NumberOfFalses",
          "NumberOfNulls"
        ],
        "properties": {
          "NumberOfTrues": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of true values in the column."
              }
            ]
          },
          "NumberOfFalses": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of false values in the column."
              }
            ]
          },
          "NumberOfNulls": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of null values in the column."
              }
            ]
          }
        },
        "description": "Defines column statistics supported for Boolean data columns."
      },
      "NonNegativeLong": {
        "type": "integer",
        "minimum": 0
      },
      "DateColumnStatisticsData": {
        "type": "object",
        "required": [
          "NumberOfNulls",
          "NumberOfDistinctValues"
        ],
        "properties": {
          "MinimumValue": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "The lowest value in the column."
              }
            ]
          },
          "MaximumValue": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "The highest value in the column."
              }
            ]
          },
          "NumberOfNulls": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of null values in the column."
              }
            ]
          },
          "NumberOfDistinctValues": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of distinct values in a column."
              }
            ]
          }
        },
        "description": "Defines column statistics supported for timestamp data columns."
      },
      "DecimalColumnStatisticsData": {
        "type": "object",
        "required": [
          "NumberOfNulls",
          "NumberOfDistinctValues"
        ],
        "properties": {
          "MinimumValue": {
            "allOf": [
              {
                "$ref": "#/$defs/DecimalNumber"
              },
              {
                "description": "The lowest value in the column."
              }
            ]
          },
          "MaximumValue": {
            "allOf": [
              {
                "$ref": "#/$defs/DecimalNumber"
              },
              {
                "description": "The highest value in the column."
              }
            ]
          },
          "NumberOfNulls": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of null values in the column."
              }
            ]
          },
          "NumberOfDistinctValues": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of distinct values in a column."
              }
            ]
          }
        },
        "description": "Defines column statistics supported for fixed-point number data columns."
      },
      "DecimalNumber": {
        "type": "object",
        "required": [
          "UnscaledValue",
          "Scale"
        ],
        "properties": {
          "UnscaledValue": {
            "allOf": [
              {
                "$ref": "#/$defs/Blob"
              },
              {
                "description": "The unscaled numeric value."
              }
            ]
          },
          "Scale": {
            "allOf": [
              {
                "$ref": "#/$defs/Integer"
              },
              {
                "description": "The scale that determines where the decimal point falls in the unscaled value."
              }
            ]
          }
        },
        "description": "Contains a numeric value in decimal format."
      },
      "Blob": {
        "type": "string"
      },
      "Integer": {
        "type": "integer"
      },
      "DoubleColumnStatisticsData": {
        "type": "object",
        "required": [
          "NumberOfNulls",
          "NumberOfDistinctValues"
        ],
        "properties": {
          "MinimumValue": {
            "allOf": [
              {
                "$ref": "#/$defs/Double"
              },
              {
                "description": "The lowest value in the column."
              }
            ]
          },
          "MaximumValue": {
            "allOf": [
              {
                "$ref": "#/$defs/Double"
              },
              {
                "description": "The highest value in the column."
              }
            ]
          },
          "NumberOfNulls": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of null values in the column."
              }
            ]
          },
          "NumberOfDistinctValues": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of distinct values in a column."
              }
            ]
          }
        },
        "description": "Defines column statistics supported for floating-point number data columns."
      },
      "Double": {
        "type": "number",
        "format": "double"
      },
      "LongColumnStatisticsData": {
        "type": "object",
        "required": [
          "NumberOfNulls",
          "NumberOfDistinctValues"
        ],
        "properties": {
          "MinimumValue": {
            "allOf": [
              {
                "$ref": "#/$defs/Long"
              },
              {
                "description": "The lowest value in the column."
              }
            ]
          },
          "MaximumValue": {
            "allOf": [
              {
                "$ref": "#/$defs/Long"
              },
              {
                "description": "The highest value in the column."
              }
            ]
          },
          "NumberOfNulls": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of null values in the column."
              }
            ]
          },
          "NumberOfDistinctValues": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of distinct values in a column."
              }
            ]
          }
        },
        "description": "Defines column statistics supported for integer data columns."
      },
      "Long": {
        "type": "integer"
      },
      "StringColumnStatisticsData": {
        "type": "object",
        "required": [
          "MaximumLength",
          "AverageLength",
          "NumberOfNulls",
          "NumberOfDistinctValues"
        ],
        "properties": {
          "MaximumLength": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The size of the longest string in the column."
              }
            ]
          },
          "AverageLength": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeDouble"
              },
              {
                "description": "The average string length in the column."
              }
            ]
          },
          "NumberOfNulls": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of null values in the column."
              }
            ]
          },
          "NumberOfDistinctValues": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of distinct values in a column."
              }
            ]
          }
        },
        "description": "Defines column statistics supported for character sequence data values."
      },
      "NonNegativeDouble": {
        "type": "number",
        "format": "double",
        "minimum": 0
      },
      "BinaryColumnStatisticsData": {
        "type": "object",
        "required": [
          "MaximumLength",
          "AverageLength",
          "NumberOfNulls"
        ],
        "properties": {
          "MaximumLength": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The size of the longest bit sequence in the column."
              }
            ]
          },
          "AverageLength": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeDouble"
              },
              {
                "description": "The average bit sequence length in the column."
              }
            ]
          },
          "NumberOfNulls": {
            "allOf": [
              {
                "$ref": "#/$defs/NonNegativeLong"
              },
              {
                "description": "The number of null values in the column."
              }
            ]
          }
        },
        "description": "Defines column statistics supported for bit sequence data values."
      }
    }
  },
