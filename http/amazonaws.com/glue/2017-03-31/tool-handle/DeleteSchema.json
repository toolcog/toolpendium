      },
      "GlueResourceArn": {
        "type": "string",
        "pattern": "arn:(aws|aws-us-gov|aws-cn):glue:.*",
        "minLength": 1,
        "maxLength": 10240
      },
      "SchemaRegistryNameString": {
        "type": "string",
        "pattern": "[a-zA-Z0-9-_$#.]+",
        "minLength": 1,
        "maxLength": 255
      }
    }
  },
