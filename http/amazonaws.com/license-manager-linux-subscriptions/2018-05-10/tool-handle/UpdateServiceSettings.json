                  }
                ]
              },
              "SourceRegions": {
                "allOf": [
                  {
                    "$ref": "#/$defs/StringList"
                  },
                  {
                    "description": "The Regions in which to discover data for Linux subscriptions."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "OrganizationIntegration": {
        "type": "string",
        "enum": [
          "Enabled",
          "Disabled"
        ]
      },
      "StringList": {
        "type": "array",
        "items": {
