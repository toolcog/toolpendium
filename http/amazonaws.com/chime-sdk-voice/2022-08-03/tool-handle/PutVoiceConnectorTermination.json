{
  "name": "PutVoiceConnectorTermination",
  "description": "Updates a Voice Connector's termination settings.",
  "parameters": {
    "type": "object",
    "properties": {
      "voiceConnectorId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The Voice Connector ID."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Termination"
        ],
        "properties": {
          "Termination": {
            "description": "Termination settings enable SIP hosts to make outbound calls using an Amazon Chime SDK Voice Connector.",
            "type": "object",
            "properties": {
              "CpsLimit": {
                "allOf": [
                  {
                    "$ref": "#/$defs/CpsLimit"
                  },
                  {
                    "description": "The limit on calls per second. Max value based on account service quota. Default value of 1."
                  }
                ]
              },
              "DefaultPhoneNumber": {
                "allOf": [
                  {
                    "$ref": "#/$defs/E164PhoneNumber"
                  },
                  {
                    "description": "The default outbound calling number."
                  }
                ]
              },
              "CallingRegions": {
                "allOf": [
                  {
                    "$ref": "#/$defs/CallingRegionList"
                  },
                  {
                    "description": "The countries to which calls are allowed, in ISO 3166-1 alpha-2 format. Required."
                  }
                ]
              },
              "CidrAllowedList": {
                "allOf": [
                  {
                    "$ref": "#/$defs/StringList"
                  },
                  {
                    "description": "The IP addresses allowed to make calls, in CIDR format."
                  }
                ]
              },
              "Disabled": {
                "allOf": [
                  {
                    "$ref": "#/$defs/Boolean"
                  },
                  {
                    "description": "When termination is disabled, outbound calls cannot be made."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "voiceConnectorId",
      "body"
    ],
    "$defs": {
      "CpsLimit": {
        "type": "integer",
        "minimum": 1
      },
      "E164PhoneNumber": {
        "type": "string",
        "pattern": "^\\+?[1-9]\\d{1,14}$",
        "format": "password"
      },
      "CallingRegionList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/CallingRegion"
        }
      },
      "CallingRegion": {
        "type": "string"
      },
      "StringList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/String"
        }
      },
      "String": {
        "type": "string"
      },
      "Boolean": {
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://voice-chime.us-east-1.amazonaws.com/voice-connectors/{voiceConnectorId}/termination"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
