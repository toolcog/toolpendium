{
  "name": "CreateVoiceProfileDomain",
  "description": "<p>Creates a voice profile domain, a collection of voice profiles, their voice prints, and encrypted enrollment audio.</p> <important> <p>Before creating any voice profiles, you must provide all notices and obtain all consents from the speaker as required under applicable privacy and biometrics laws, and as required under the <a href=\"https://aws.amazon.com/service-terms/\">AWS service terms</a> for the Amazon Chime SDK.</p> </important> <p>For more information about voice profile domains, see <a href=\"https://docs.aws.amazon.com/chime-sdk/latest/dg/pstn-voice-analytics.html\">Using Amazon Chime SDK Voice Analytics</a> in the <i>Amazon Chime SDK Developer Guide</i>.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Name",
          "ServerSideEncryptionConfiguration"
        ],
        "properties": {
          "Name": {
            "description": "The name of the voice profile domain.",
            "type": "string",
            "pattern": "[a-zA-Z0-9 _.-]+",
            "minLength": 1,
            "maxLength": 256
          },
          "Description": {
            "description": "A description of the voice profile domain.",
            "type": "string",
            "minLength": 0,
            "maxLength": 1024
          },
          "ServerSideEncryptionConfiguration": {
            "description": "<p>A structure that contains the configuration settings for server-side encryption.</p> <note> <p>We only support symmetric keys. Do not use asymmetric or HMAC keys, or KMS aliases.</p> </note>",
            "type": "object",
            "properties": {
              "KmsKeyArn": {
                "allOf": [
                  {
                    "$ref": "#/$defs/Arn"
                  },
                  {
                    "description": "The ARN of the KMS key used to encrypt the enrollment data in a voice profile domain. Asymmetric customer managed keys are not supported."
                  }
                ]
              }
            }
          },
          "ClientRequestToken": {
            "description": "The unique identifier for the client request. Use a different token for different domain creation requests.",
            "type": "string",
            "pattern": "^[-_a-zA-Z0-9]*$\\{2,64\\}$"
          },
          "Tags": {
            "description": "The tags assigned to the domain.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/Tag"
            },
            "minItems": 1,
            "maxItems": 50
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "Arn": {
        "type": "string",
        "pattern": "^arn[\\/\\:\\-\\_\\.a-zA-Z0-9]+$",
        "minLength": 1,
        "maxLength": 1024,
        "format": "password"
      },
      "Tag": {
        "type": "object",
        "required": [
          "Key",
          "Value"
        ],
        "properties": {
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "The tag's key."
              }
            ]
          },
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/TagValue"
              },
              {
                "description": "The tag's value."
              }
            ]
          }
        },
        "description": "Describes a tag applied to a resource."
      },
      "TagKey": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128,
        "format": "password"
      },
      "TagValue": {
        "type": "string",
        "minLength": 0,
        "maxLength": 256,
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://voice-chime.us-east-1.amazonaws.com/voice-profile-domains"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
