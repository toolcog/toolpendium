{
  "name": "PutVoiceConnectorEmergencyCallingConfiguration",
  "description": "Updates a Voice Connector's emergency calling configuration.",
  "parameters": {
    "type": "object",
    "properties": {
      "voiceConnectorId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The Voice Connector ID."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "EmergencyCallingConfiguration"
        ],
        "properties": {
          "EmergencyCallingConfiguration": {
            "description": "The emergency calling configuration details associated with an Amazon Chime SDK Voice Connector.",
            "type": "object",
            "properties": {
              "DNIS": {
                "allOf": [
                  {
                    "$ref": "#/$defs/DNISEmergencyCallingConfigurationList"
                  },
                  {
                    "description": "The Dialed Number Identification Service (DNIS) emergency calling configuration details."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "voiceConnectorId",
      "body"
    ],
    "$defs": {
      "DNISEmergencyCallingConfigurationList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/DNISEmergencyCallingConfiguration"
        }
      },
      "DNISEmergencyCallingConfiguration": {
        "type": "object",
        "required": [
          "EmergencyPhoneNumber",
          "CallingCountry"
        ],
        "properties": {
          "EmergencyPhoneNumber": {
            "allOf": [
              {
                "$ref": "#/$defs/E164PhoneNumber"
              },
              {
                "description": "The DNIS phone number that you route emergency calls to, in E.164 format."
              }
            ]
          },
          "TestPhoneNumber": {
            "allOf": [
              {
                "$ref": "#/$defs/E164PhoneNumber"
              },
              {
                "description": "The DNIS phone number for routing test emergency calls to, in E.164 format."
              }
            ]
          },
          "CallingCountry": {
            "allOf": [
              {
                "$ref": "#/$defs/Alpha2CountryCode"
              },
              {
                "description": "The country from which emergency calls are allowed, in ISO 3166-1 alpha-2 format."
              }
            ]
          }
        },
        "description": "The Dialed Number Identification Service (DNIS) emergency calling configuration details associated with an Amazon Chime SDK Voice Connector's emergency calling configuration."
      },
      "E164PhoneNumber": {
        "type": "string",
        "pattern": "^\\+?[1-9]\\d{1,14}$",
        "format": "password"
      },
      "Alpha2CountryCode": {
        "type": "string",
        "pattern": "[A-Z]{2}"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://voice-chime.us-east-1.amazonaws.com/voice-connectors/{voiceConnectorId}/emergency-calling-configuration"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
