        "type": "object",
        "required": [
          "PhoneNumberIds"
        ],
        "properties": {
          "PhoneNumberIds": {
            "description": "List of phone number IDs.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/String"
            },
            "minItems": 1
          }
        }
      }
    },
    "required": [
