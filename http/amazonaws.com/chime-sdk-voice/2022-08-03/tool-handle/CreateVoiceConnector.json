              "eu-central-1",
              "eu-west-1",
              "eu-west-2",
              "ap-northeast-2",
              "ap-northeast-1",
              "ap-southeast-1",
              "ap-southeast-2"
            ]
          },
          "RequireEncryption": {
            "description": "Enables or disables encryption for the Voice Connector.",
            "type": "boolean"
          },
          "Tags": {
            "description": "The tags assigned to the Voice Connector.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/Tag"
            },
            "minItems": 1,
            "maxItems": 50
          }
        }
      }
    },
    "required": [
      "body"
    ],
