{
  "name": "UntagResource",
  "description": "Removes tags from a resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "operation": {
        "type": "string",
        "enum": [
          "untag-resource"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "ResourceARN",
          "TagKeys"
        ],
        "properties": {
          "ResourceARN": {
            "description": "The ARN of the resource having its tags removed.",
            "type": "string",
            "pattern": "^arn[\\/\\:\\-\\_\\.a-zA-Z0-9]+$",
            "minLength": 1,
            "maxLength": 1024,
            "format": "password"
          },
          "TagKeys": {
            "description": "The keys of the tags being removed from the resource.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/TagKey"
            },
            "minItems": 1,
            "maxItems": 50
          }
        }
      }
    },
    "required": [
      "operation",
      "body"
    ],
    "$defs": {
      "TagKey": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128,
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://voice-chime.us-east-1.amazonaws.com/tags#operation=untag-resource{?operation}"
    },
    "headers": {
