      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "TransactionId",
          "VoiceProfileDomainId"
        ],
        "properties": {
          "TransactionId": {
            "description": "The transaction ID of the call being analyzed.",
            "type": "string",
            "pattern": ".*\\S.*",
            "minLength": 1,
            "maxLength": 256
          },
          "VoiceProfileDomainId": {
            "description": "The ID of the voice profile domain that will store the voice profile.",
            "type": "string",
            "pattern": ".*\\S.*",
            "minLength": 1,
            "maxLength": 256
          },
          "ClientRequestToken": {
            "description": "The unique identifier for the client request. Use a different token for different speaker search tasks.",
            "type": "string",
            "pattern": "^[-_a-zA-Z0-9]*$\\{2,64\\}$"
          },
          "CallLeg": {
            "description": "Specifies which call leg to stream for speaker search.",
            "type": "string",
            "enum": [
              "Caller",
              "Callee"
            ]
          }
        }
      }
    },
    "required": [
      "VoiceConnectorId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
