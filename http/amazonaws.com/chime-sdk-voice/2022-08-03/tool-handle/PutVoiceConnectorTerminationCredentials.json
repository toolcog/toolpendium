{
  "name": "PutVoiceConnectorTerminationCredentials",
  "description": "Updates a Voice Connector's termination credentials.",
  "parameters": {
    "type": "object",
    "properties": {
      "voiceConnectorId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The Voice Connector ID."
      },
      "operation": {
        "type": "string",
        "enum": [
          "put"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "Credentials": {
            "description": "The termination credentials being updated.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/Credential"
            }
          }
        }
      }
    },
    "required": [
      "voiceConnectorId",
      "operation",
      "body"
    ],
    "$defs": {
      "Credential": {
        "type": "object",
        "properties": {
          "Username": {
            "allOf": [
              {
                "$ref": "#/$defs/SensitiveString"
              },
              {
                "description": "The RFC2617 compliant user name associated with the SIP credentials, in US-ASCII format."
              }
            ]
          },
          "Password": {
            "allOf": [
              {
                "$ref": "#/$defs/SensitiveString"
              },
              {
                "description": "The RFC2617 compliant password associated with the SIP credentials, in US-ASCII format."
              }
            ]
          }
        },
        "description": "The SIP credentials used to authenticate requests to an Amazon Chime SDK Voice Connector."
      },
      "SensitiveString": {
        "type": "string",
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://voice-chime.us-east-1.amazonaws.com/voice-connectors/{voiceConnectorId}/termination/credentials#operation=put{?operation}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
