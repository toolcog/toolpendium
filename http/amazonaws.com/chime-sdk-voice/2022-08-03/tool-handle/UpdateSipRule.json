{
  "name": "UpdateSipRule",
  "description": "Updates the details of the specified SIP rule.",
  "parameters": {
    "type": "object",
    "properties": {
      "sipRuleId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "description": "The SIP rule ID."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Name"
        ],
        "properties": {
          "Name": {
            "description": "The new name for the specified SIP rule.",
            "type": "string",
            "pattern": "[a-zA-Z0-9 _.-]+",
            "minLength": 1,
            "maxLength": 256
          },
          "Disabled": {
            "description": "The new value that indicates whether the rule is disabled.",
            "type": "boolean"
          },
          "TargetApplications": {
            "description": "The new list of target applications.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/SipRuleTargetApplication"
            },
            "minItems": 1,
            "maxItems": 25
          }
        }
      }
    },
    "required": [
      "sipRuleId",
      "body"
    ],
    "$defs": {
      "SipRuleTargetApplication": {
        "type": "object",
        "properties": {
          "SipMediaApplicationId": {
            "allOf": [
              {
                "$ref": "#/$defs/NonEmptyString"
              },
              {
                "description": "The ID of a rule's target SIP media application."
              }
            ]
          },
          "Priority": {
            "allOf": [
              {
                "$ref": "#/$defs/SipApplicationPriority"
              },
              {
                "description": "The priority setting of a rule's target SIP media application."
              }
            ]
          },
          "AwsRegion": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "The AWS Region of a rule's target SIP media application."
              }
            ]
          }
        },
        "description": "A target SIP media application and other details, such as priority and AWS Region, to be specified in the SIP rule. Only one SIP rule per AWS Region can be provided."
      },
      "NonEmptyString": {
        "type": "string",
        "pattern": ".*\\S.*"
      },
      "SipApplicationPriority": {
        "type": "integer",
        "minimum": 1
      },
      "String": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://voice-chime.us-east-1.amazonaws.com/sip-rules/{sipRuleId}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
