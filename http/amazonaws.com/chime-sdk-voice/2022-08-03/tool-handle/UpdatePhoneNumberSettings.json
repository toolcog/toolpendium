      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "CallingName"
        ],
        "properties": {
          "CallingName": {
            "description": "The default outbound calling name for the account.",
            "type": "string",
            "pattern": "^$|^[a-zA-Z0-9 ]{2,15}$",
            "format": "password"
          }
        }
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
