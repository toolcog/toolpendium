{
  "name": "RestorePhoneNumber",
  "description": "Restores a deleted phone number.",
  "parameters": {
    "type": "object",
    "properties": {
      "phoneNumberId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "format": "password",
        "description": "The ID of the phone number being restored."
      },
      "operation": {
        "type": "string",
        "enum": [
          "restore"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
