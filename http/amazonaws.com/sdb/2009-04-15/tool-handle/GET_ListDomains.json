{
  "name": "GET_ListDomains",
  "description": " The <code>ListDomains</code> operation lists all domains associated with the Access Key ID. It returns domain names up to the limit set by <a href=\"#MaxNumberOfDomains\">MaxNumberOfDomains</a>. A <a href=\"#NextToken\">NextToken</a> is returned if there are more than <code>MaxNumberOfDomains</code> domains. Calling <code>ListDomains</code> successive times with the <code>NextToken</code> provided by the operation returns up to <code>MaxNumberOfDomains</code> more domain names with each successive operation call. ",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxNumberOfDomains": {
        "type": "integer",
        "description": "The maximum number of domain names you want returned. The range is 1 to 100. The default setting is 100."
      },
      "NextToken": {
        "type": "string",
        "description": "A string informing Amazon SimpleDB where to start the next list of domain names."
      },
      "Action": {
        "type": "string",
        "enum": [
          "ListDomains"
        ]
      },
      "Version": {
        "type": "string",
        "enum": [
          "2009-04-15"
        ]
      },
      "AWSAccessKeyId": {
        "type": "string"
      },
      "SignatureMethod": {
        "type": "string"
      },
      "SignatureVersion": {
        "type": "string"
      },
      "Timestamp": {
        "type": "string"
      },
      "Signature": {
        "type": "string"
      }
    },
    "required": [
      "Action",
      "Version",
      "AWSAccessKeyId",
      "SignatureMethod",
      "SignatureVersion",
      "Timestamp",
      "Signature"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://sdb.amazonaws.com/#Action=ListDomains{?MaxNumberOfDomains,NextToken,Action,Version,AWSAccessKeyId,SignatureMethod,SignatureVersion,Timestamp,Signature}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DomainNames**",
            "**NextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The specified NextToken is not valid. ",
        "**Key properties:**",
        {
          "$ul": [
            "**BoxUsage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
