{
  "name": "RegisterAccount",
  "description": " Enables Audit Manager for the specified Amazon Web Services account. ",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
