        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "kmsKey": {
            "description": " The KMS key details. ",
            "type": "string",
            "pattern": "^arn:.*:kms:.*|DEFAULT",
            "minLength": 7,
            "maxLength": 2048
          },
          "delegatedAdminAccount": {
            "description": " The delegated administrator account for Audit Manager. ",
            "type": "string",
            "pattern": "^[0-9]{12}$",
            "minLength": 12,
            "maxLength": 12
          }
        }
      }
    },
    "required": [
