    "$defs": {
      "PutKMSEncryptionKeyRequest": {
        "type": "object",
        "required": [
          "kmsEncryptionKeyArn"
        ],
        "title": "PutKMSEncryptionKeyRequest",
        "properties": {
          "kmsEncryptionKeyArn": {
            "allOf": [
              {
                "$ref": "#/$defs/KmsEncryptionKeyArn"
              },
              {
                "description": "<p>The KMS encryption key ARN.</p> <p>The KMS key must be single-Region key. Amazon Fraud Detector does not support multi-Region KMS key.</p>"
              }
            ]
          }
        }
      },
      "KmsEncryptionKeyArn": {
        "type": "string",
        "pattern": "^DEFAULT|arn:[a-zA-Z0-9-]+:kms:[a-zA-Z0-9-]+:\\d{12}:key\\/\\w{8}-\\w{4}-\\w{4}-\\w{4}-\\w{12}$",
        "minLength": 7,
        "maxLength": 90
      }
    }
  },
  "handler": "http",
  "request": {
