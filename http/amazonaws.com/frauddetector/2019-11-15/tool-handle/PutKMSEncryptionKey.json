{
  "name": "PutKMSEncryptionKey",
  "description": "Specifies the KMS key to be used to encrypt content in Amazon Fraud Detector.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
