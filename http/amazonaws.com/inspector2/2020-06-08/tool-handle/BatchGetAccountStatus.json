{
  "name": "BatchGetAccountStatus",
  "description": "Retrieves the Amazon Inspector status of multiple Amazon Web Services accounts within your environment.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "accountIds": {
            "description": "The 12-digit Amazon Web Services account IDs of the accounts to retrieve Amazon Inspector status for.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/AccountId"
            },
            "minItems": 0,
            "maxItems": 100
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "AccountId": {
        "type": "string",
        "pattern": "^\\d{12}$",
        "minLength": 12,
        "maxLength": 12
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://inspector2.us-east-1.amazonaws.com/status/batch/get"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
