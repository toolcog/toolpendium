{
  "name": "GetMatchingJob",
  "description": "Gets the status, metrics, and errors (if there are any) that are associated with a job.",
  "parameters": {
    "type": "object",
    "properties": {
      "jobId": {
        "type": "string",
        "pattern": "^[a-f0-9]{32}$",
        "description": "The ID of the job."
      },
      "workflowName": {
        "type": "string",
        "pattern": "^[a-zA-Z_0-9-]*$",
        "minLength": 0,
        "maxLength": 255,
        "description": "The name of the workflow."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "jobId",
      "workflowName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://entityresolution.us-east-1.amazonaws.com/matchingworkflows/{workflowName}/jobs/{jobId}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
