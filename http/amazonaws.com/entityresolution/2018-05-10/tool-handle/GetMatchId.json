{
  "name": "GetMatchId",
  "description": "Returns the corresponding Match ID of a customer record if the record has been processed.",
  "parameters": {
    "type": "object",
    "properties": {
      "workflowName": {
        "type": "string",
        "pattern": "^[a-zA-Z_0-9-]*$",
        "minLength": 0,
        "maxLength": 255,
        "description": "The name of the workflow."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "record"
        ],
        "properties": {
          "record": {
            "description": "The record to fetch the Match ID for.",
            "type": "object",
            "additionalProperties": {
              "$ref": "#/$defs/RecordAttributeMapValueString"
            }
          }
        }
      }
    },
    "required": [
      "workflowName",
      "body"
    ],
    "$defs": {
      "RecordAttributeMapValueString": {
        "type": "string",
        "pattern": "^[a-zA-Z_0-9-.@ ()+\\t]*$",
        "minLength": 0,
        "maxLength": 255
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://entityresolution.us-east-1.amazonaws.com/matchingworkflows/{workflowName}/matches"
    },
    "headers": {
