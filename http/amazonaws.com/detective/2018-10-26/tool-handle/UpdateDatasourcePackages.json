{
  "name": "UpdateDatasourcePackages",
  "description": "Starts a data source packages for the behavior graph.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
