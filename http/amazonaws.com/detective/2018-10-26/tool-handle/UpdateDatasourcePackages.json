      "body": {
        "type": "object",
        "required": [
          "GraphArn",
          "DatasourcePackages"
        ],
        "properties": {
          "GraphArn": {
            "description": "The ARN of the behavior graph.",
            "type": "string",
            "pattern": "^arn:aws[-\\w]{0,10}?:detective:[-\\w]{2,20}?:\\d{12}?:graph:[abcdef\\d]{32}?$"
          },
          "DatasourcePackages": {
            "description": "The data source package start for the behavior graph.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/DatasourcePackage"
            },
            "minItems": 1,
            "maxItems": 25
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "DatasourcePackage": {
        "type": "string",
        "enum": [
          "DETECTIVE_CORE",
          "EKS_AUDIT",
          "ASFF_SECURITYHUB_FINDING"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.detective.us-east-1.amazonaws.com/graph/datasources/update"
    },
    "headers": {
