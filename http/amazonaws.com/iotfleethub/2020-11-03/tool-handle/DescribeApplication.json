      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "applicationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
