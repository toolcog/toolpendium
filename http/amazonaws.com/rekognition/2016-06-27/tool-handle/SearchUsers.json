{
  "name": "SearchUsers",
  "description": "Searches for UserIDs within a collection based on a <code>FaceId</code> or <code>UserId</code>. This API can be used to find the closest UserID (with a highest similarity) to associate a face. The request must be provided with either <code>FaceId</code> or <code>UserId</code>. The operation returns an array of UserID that match the <code>FaceId</code> or <code>UserId</code>, ordered by similarity score with the highest similarity first. ",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "RekognitionService.SearchUsers"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/SearchUsersRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "SearchUsersRequest": {
        "type": "object",
        "required": [
          "CollectionId"
        ],
        "title": "SearchUsersRequest",
        "properties": {
          "CollectionId": {
            "allOf": [
              {
                "$ref": "#/$defs/CollectionId"
              },
              {
                "description": "The ID of an existing collection containing the UserID, used with a UserId or FaceId. If a FaceId is provided, UserId isnâ€™t required to be present in the Collection."
              }
            ]
          },
          "UserId": {
            "allOf": [
              {
                "$ref": "#/$defs/UserId"
              },
              {
                "description": "ID for the existing User."
              }
            ]
          },
          "FaceId": {
            "allOf": [
              {
                "$ref": "#/$defs/FaceId"
              },
              {
                "description": "ID for the existing face."
              }
            ]
          },
          "UserMatchThreshold": {
            "allOf": [
              {
                "$ref": "#/$defs/Percent"
              },
              {
                "description": "Optional value that specifies the minimum confidence in the matched UserID to return. Default value of 80."
              }
            ]
          },
          "MaxUsers": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxUserResults"
              },
              {
                "description": "Maximum number of identities to return."
              }
            ]
          }
        }
      },
      "CollectionId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_.\\-]+",
        "minLength": 1,
        "maxLength": 255
      },
      "UserId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_.\\-:]+",
        "minLength": 1,
        "maxLength": 128
      },
      "FaceId": {
        "type": "string",
        "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
      },
      "Percent": {
        "type": "number",
        "format": "float",
        "minimum": 0,
        "maximum": 100
      },
      "MaxUserResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 500
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://rekognition.us-east-1.amazonaws.com/#X-Amz-Target=RekognitionService.SearchUsers"
