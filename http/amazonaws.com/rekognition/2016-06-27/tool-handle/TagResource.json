      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/TagResourceRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "TagResourceRequest": {
        "type": "object",
        "required": [
          "ResourceArn",
          "Tags"
        ],
        "title": "TagResourceRequest",
        "properties": {
          "ResourceArn": {
            "allOf": [
              {
                "$ref": "#/$defs/ResourceArn"
              },
              {
                "description": " Amazon Resource Name (ARN) of the model, collection, or stream processor that you want to assign the tags to. "
              }
            ]
          },
          "Tags": {
            "allOf": [
              {
                "$ref": "#/$defs/TagMap"
              },
              {
                "description": " The key-value tags to assign to the resource. "
              }
            ]
          }
        }
      },
      "ResourceArn": {
        "type": "string",
        "minLength": 20,
        "maxLength": 2048
      },
      "TagMap": {
        "type": "object",
        "minProperties": 0,
        "maxProperties": 200,
        "additionalProperties": {
          "$ref": "#/$defs/TagValue"
        }
      },
      "TagValue": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 0,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
