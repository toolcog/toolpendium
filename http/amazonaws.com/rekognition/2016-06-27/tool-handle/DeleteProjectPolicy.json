      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DeleteProjectPolicyRequest": {
        "type": "object",
        "required": [
          "ProjectArn",
          "PolicyName"
        ],
        "title": "DeleteProjectPolicyRequest",
        "properties": {
          "ProjectArn": {
            "allOf": [
              {
                "$ref": "#/$defs/ProjectArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the project that the project policy you want to delete is attached to."
              }
            ]
          },
          "PolicyName": {
            "allOf": [
              {
                "$ref": "#/$defs/ProjectPolicyName"
              },
              {
                "description": "The name of the policy that you want to delete."
              }
            ]
          },
          "PolicyRevisionId": {
            "allOf": [
              {
                "$ref": "#/$defs/ProjectPolicyRevisionId"
              },
              {
                "description": "The ID of the project policy revision that you want to delete."
              }
            ]
          }
        }
      },
      "ProjectArn": {
        "type": "string",
        "pattern": "(^arn:[a-z\\d-]+:rekognition:[a-z\\d-]+:\\d{12}:project\\/[a-zA-Z0-9_.\\-]{1,255}\\/[0-9]+$)",
        "minLength": 20,
        "maxLength": 2048
      },
      "ProjectPolicyName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_.\\-]+",
        "minLength": 1,
        "maxLength": 128
      },
      "ProjectPolicyRevisionId": {
        "type": "string",
        "pattern": "[0-9A-Fa-f]+",
        "maxLength": 64
      }
    }
  },
