      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/DeleteFacesRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DeleteFacesRequest": {
        "type": "object",
        "required": [
          "CollectionId",
          "FaceIds"
        ],
        "title": "DeleteFacesRequest",
        "properties": {
          "CollectionId": {
            "allOf": [
              {
                "$ref": "#/$defs/CollectionId"
              },
              {
                "description": "Collection from which to remove the specific faces."
              }
            ]
          },
          "FaceIds": {
            "allOf": [
              {
                "$ref": "#/$defs/FaceIdList"
              },
              {
                "description": "An array of face IDs to delete."
              }
            ]
          }
        }
      },
      "CollectionId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_.\\-]+",
        "minLength": 1,
        "maxLength": 255
      },
      "FaceIdList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/FaceId"
        },
        "minItems": 1,
        "maxItems": 4096
      },
      "FaceId": {
        "type": "string",
        "pattern": "[0-9a-f]{8}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{4}-[0-9a-f]{12}"
      }
    }
  },
  "handler": "http",
  "request": {
