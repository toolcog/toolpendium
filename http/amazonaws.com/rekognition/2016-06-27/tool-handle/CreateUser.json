    "$defs": {
      "CreateUserRequest": {
        "type": "object",
        "required": [
          "CollectionId",
          "UserId"
        ],
        "title": "CreateUserRequest",
        "properties": {
          "CollectionId": {
            "allOf": [
              {
                "$ref": "#/$defs/CollectionId"
              },
              {
                "description": "The ID of an existing collection to which the new UserID needs to be created."
              }
            ]
          },
          "UserId": {
            "allOf": [
              {
                "$ref": "#/$defs/UserId"
              },
              {
                "description": "ID for the UserID to be created. This ID needs to be unique within the collection."
              }
            ]
          },
          "ClientRequestToken": {
            "allOf": [
              {
                "$ref": "#/$defs/ClientRequestToken"
              },
              {
                "description": "Idempotent token used to identify the request to <code>CreateUser</code>. If you use the same token with multiple <code>CreateUser</code> requests, the same response is returned. Use ClientRequestToken to prevent the same request from being processed more than once."
              }
            ]
          }
        }
      },
      "CollectionId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_.\\-]+",
        "minLength": 1,
        "maxLength": 255
      },
      "UserId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_.\\-:]+",
        "minLength": 1,
        "maxLength": 128
      },
      "ClientRequestToken": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9-_]+$",
        "minLength": 1,
        "maxLength": 64
      }
    }
  },
