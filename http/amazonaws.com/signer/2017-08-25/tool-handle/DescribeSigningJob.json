      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**jobId**",
            "**source**",
            "**signingMaterial**",
            "**platformId**",
            "**platformDisplayName**",
            "**profileName**",
            "**profileVersion**",
            "**overrides**",
            "**signingParameters**",
            "**createdAt**",
            "**completedAt**",
            "**signatureExpiresAt**",
            "**requestedBy**",
            "**status**",
            "**statusReason**",
            "**revocationRecord**",
            "**signedObject**",
            "**jobOwner**",
            "**jobInvoker**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
