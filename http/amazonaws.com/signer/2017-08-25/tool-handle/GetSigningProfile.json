{
  "name": "GetSigningProfile",
  "description": "Returns information on a specific signing profile.",
  "parameters": {
    "type": "object",
    "properties": {
      "profileName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_]{2,}",
        "minLength": 2,
        "maxLength": 64,
        "description": "The name of the target signing profile."
      },
      "profileOwner": {
        "type": "string",
        "pattern": "^[0-9]{12}$",
        "minLength": 12,
        "maxLength": 12,
        "description": "The AWS account ID of the profile owner."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "profileName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://signer.us-east-1.amazonaws.com/signing-profiles/{profileName}{?profileOwner}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**profileName**",
            "**profileVersion**",
            "**profileVersionArn**",
            [
              "**revocationRecord**: Revocation information for a signing profile.",
              {
                "$ul": [
                  "**revocationEffectiveFrom**",
                  "**revokedAt**",
                  "**revokedBy**"
                ]
              }
            ],
            "**signingMaterial**",
            "**platformId**",
            "**platformDisplayName**",
            [
              "**signatureValidityPeriod**: The validity period for a signing job.",
              {
                "$ul": [
                  "**value**",
                  "**type**"
                ]
              }
            ],
            "**overrides**",
            "**signingParameters**",
            "**status**",
            "**statusReason**",
            "**arn**",
            "**tags**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "482": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "483": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
