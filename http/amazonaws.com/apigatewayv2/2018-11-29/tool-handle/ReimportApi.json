      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "body"
        ],
        "properties": {
          "body": {
            "description": "The OpenAPI definition. Supported only for HTTP APIs.",
            "type": "string"
          }
        }
      }
    },
    "required": [
      "apiId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://apigateway.us-east-1.amazonaws.com/v2/apis/{apiId}{?basepath,failOnWarnings}"
    },
    "headers": {
