      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "apiId",
      "outputType",
      "specification"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apigateway.us-east-1.amazonaws.com/v2/apis/{apiId}/exports/{specification}#outputType{?exportVersion,includeExtensions,outputType,stageName}"
    },
    "headers": {
