{
  "name": "DeleteClientBranding",
  "description": "<p>Deletes customized client branding. Client branding allows you to customize your WorkSpace's client login portal. You can tailor your login portal company logo, the support email address, support link, link to reset password, and a custom message for users trying to sign in.</p> <p>After you delete your customized client branding, your login portal reverts to the default client branding.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "WorkspacesService.DeleteClientBranding"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/DeleteClientBrandingRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DeleteClientBrandingRequest": {
        "type": "object",
        "required": [
          "ResourceId",
          "Platforms"
        ],
        "title": "DeleteClientBrandingRequest",
        "properties": {
          "ResourceId": {
            "allOf": [
              {
                "$ref": "#/$defs/DirectoryId"
              },
              {
                "description": "The directory identifier of the WorkSpace for which you want to delete client branding."
              }
            ]
          },
          "Platforms": {
            "allOf": [
              {
                "$ref": "#/$defs/ClientDeviceTypeList"
              },
              {
                "description": "The device type for which you want to delete client branding."
              }
            ]
          }
        }
      },
      "DirectoryId": {
        "type": "string",
        "pattern": "^d-[0-9a-f]{8,63}$",
        "minLength": 10,
        "maxLength": 65
      },
      "ClientDeviceTypeList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/ClientDeviceType"
        },
        "minItems": 1,
        "maxItems": 6
      },
      "ClientDeviceType": {
        "type": "string",
        "enum": [
          "DeviceTypeWindows",
          "DeviceTypeOsx",
          "DeviceTypeAndroid",
          "DeviceTypeIos",
          "DeviceTypeLinux",
          "DeviceTypeWeb"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
