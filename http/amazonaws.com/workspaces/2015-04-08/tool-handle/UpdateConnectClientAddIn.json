    "$defs": {
      "UpdateConnectClientAddInRequest": {
        "type": "object",
        "required": [
          "AddInId",
          "ResourceId"
        ],
        "title": "UpdateConnectClientAddInRequest",
        "properties": {
          "AddInId": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonUuid"
              },
              {
                "description": "The identifier of the client add-in to update."
              }
            ]
          },
          "ResourceId": {
            "allOf": [
              {
                "$ref": "#/$defs/DirectoryId"
              },
              {
                "description": "The directory identifier for which the client add-in is configured."
              }
            ]
          },
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/AddInName"
              },
              {
                "description": "The name of the client add-in."
              }
            ]
          },
          "URL": {
            "allOf": [
              {
                "$ref": "#/$defs/AddInUrl"
              },
              {
                "description": "The endpoint URL of the Amazon Connect client add-in."
              }
            ]
          }
        }
      },
      "AmazonUuid": {
        "type": "string",
        "pattern": "[a-f0-9]{8}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{4}-[a-f0-9]{12}",
        "minLength": 36,
        "maxLength": 36
      },
      "DirectoryId": {
        "type": "string",
        "pattern": "^d-[0-9a-f]{8,63}$",
        "minLength": 10,
        "maxLength": 65
      },
      "AddInName": {
        "type": "string",
        "pattern": "^.*$",
        "minLength": 1,
        "maxLength": 64
      },
      "AddInUrl": {
        "type": "string",
        "pattern": "^(http|https)\\://\\S+",
        "minLength": 1,
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
