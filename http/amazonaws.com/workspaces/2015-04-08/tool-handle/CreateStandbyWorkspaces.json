        "type": "string",
        "pattern": "^ws-[0-9a-z]{8,63}$"
      },
      "VolumeEncryptionKey": {
        "type": "string"
      },
      "DirectoryId": {
        "type": "string",
        "pattern": "^d-[0-9a-f]{8,63}$",
        "minLength": 10,
        "maxLength": 65
      },
      "TagList": {
        "type": "array",
