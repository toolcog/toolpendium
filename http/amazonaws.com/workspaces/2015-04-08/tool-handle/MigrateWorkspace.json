              {
                "$ref": "#/$defs/BundleId"
              },
              {
                "description": "The identifier of the target bundle type to migrate the WorkSpace to."
              }
            ]
          }
        }
      },
      "WorkspaceId": {
        "type": "string",
        "pattern": "^ws-[0-9a-z]{8,63}$"
      },
