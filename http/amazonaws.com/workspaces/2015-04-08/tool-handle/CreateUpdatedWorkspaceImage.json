    "$defs": {
      "CreateUpdatedWorkspaceImageRequest": {
        "type": "object",
        "required": [
          "Name",
          "Description",
          "SourceImageId"
        ],
        "title": "CreateUpdatedWorkspaceImageRequest",
        "properties": {
