        "items": {
          "allOf": [
            {
              "$ref": "#/$defs/TrafficMirrorNetworkService"
            },
            {
              "xml": {
                "name": "item"
              }
            }
          ]
        },
        "description": "The network service, for example Amazon DNS, that you want to mirror."
      },
      "RemoveNetworkService": {
