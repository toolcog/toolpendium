      },
      "DestinationPrefixListId": {
        "type": "string",
        "description": " Use a prefix list in place of <code>DestinationCidrBlock</code>. You cannot use <code>DestinationPrefixListId</code> and <code>DestinationCidrBlock</code> in the same request. "
      },
      "Action": {
        "type": "string",
