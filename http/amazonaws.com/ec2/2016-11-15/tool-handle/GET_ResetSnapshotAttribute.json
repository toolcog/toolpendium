      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "Attribute",
      "SnapshotId",
      "Action",
      "Version"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
