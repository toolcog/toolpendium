      },
      "body": {
        "type": "object",
        "required": [
          "controlIdentifier",
          "targetIdentifier"
        ],
        "properties": {
          "controlIdentifier": {
            "description": "The ARN of the control. Only <b>Strongly recommended</b> and <b>Elective</b> controls are permitted, with the exception of the <b>Region deny</b> guardrail.",
            "type": "string",
            "pattern": "^arn:aws[0-9a-zA-Z_\\-:\\/]+$",
            "minLength": 20,
            "maxLength": 2048
          },
          "targetIdentifier": {
