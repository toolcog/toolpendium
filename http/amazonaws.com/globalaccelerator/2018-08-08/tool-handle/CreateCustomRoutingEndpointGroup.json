          "EndpointGroupRegion",
          "DestinationConfigurations",
          "IdempotencyToken"
        ],
        "title": "CreateCustomRoutingEndpointGroupRequest",
        "properties": {
          "ListenerArn": {
            "allOf": [
              {
                "$ref": "#/$defs/GenericString"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the listener for a custom routing endpoint."
              }
            ]
          },
          "EndpointGroupRegion": {
            "allOf": [
              {
                "$ref": "#/$defs/GenericString"
              },
              {
                "description": "The Amazon Web Services Region where the endpoint group is located. A listener can have only one endpoint group in a specific Region."
              }
            ]
          },
          "DestinationConfigurations": {
            "allOf": [
              {
                "$ref": "#/$defs/CustomRoutingDestinationConfigurations"
              },
              {
                "description": "Sets the port range and protocol for all endpoints (virtual private cloud subnets) in a custom routing endpoint group to accept client traffic on."
              }
            ]
          },
          "IdempotencyToken": {
            "allOf": [
              {
                "$ref": "#/$defs/IdempotencyToken"
              },
              {
                "description": "A unique, case-sensitive identifier that you provide to ensure the idempotency—that is, the uniqueness—of the request."
              }
            ]
          }
        }
      },
      "GenericString": {
        "type": "string",
        "maxLength": 255
      },
      "CustomRoutingDestinationConfigurations": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/CustomRoutingDestinationConfiguration"
        },
        "minItems": 1,
        "maxItems": 100
      },
      "CustomRoutingDestinationConfiguration": {
        "type": "object",
        "required": [
          "FromPort",
          "ToPort",
          "Protocols"
        ],
        "properties": {
          "FromPort": {
            "allOf": [
              {
                "$ref": "#/$defs/PortNumber"
              },
              {
                "description": "The first port, inclusive, in the range of ports for the endpoint group that is associated with a custom routing accelerator."
              }
            ]
          },
          "ToPort": {
            "allOf": [
              {
                "$ref": "#/$defs/PortNumber"
              },
              {
                "description": "The last port, inclusive, in the range of ports for the endpoint group that is associated with a custom routing accelerator."
              }
            ]
          },
          "Protocols": {
            "allOf": [
              {
                "$ref": "#/$defs/CustomRoutingProtocols"
              },
              {
                "description": "The protocol for the endpoint group that is associated with a custom routing accelerator. The protocol can be either TCP or UDP."
              }
            ]
          }
        },
        "description": "For a custom routing accelerator, sets the port range and protocol for all endpoints (virtual private cloud subnets) in an endpoint group to accept client traffic on."
      },
      "PortNumber": {
        "type": "integer",
        "minimum": 1,
        "maximum": 65535
      },
      "CustomRoutingProtocols": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/CustomRoutingProtocol"
        },
        "minItems": 1,
        "maxItems": 2
      },
      "CustomRoutingProtocol": {
        "type": "string",
        "enum": [
          "TCP",
          "UDP"
        ]
      },
      "IdempotencyToken": {
        "type": "string",
        "maxLength": 255
      }
    }
