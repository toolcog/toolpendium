{
  "name": "UntagResource",
  "description": "Untag a resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "arn": {
        "type": "string",
        "pattern": "^arn:.*",
        "description": "",
        "minLength": 20,
        "maxLength": 500
      },
      "tagKeys": {
        "type": "array",
        "description": "",
        "items": {
          "$ref": "#/$defs/TagKey"
        },
        "minItems": 0,
        "maxItems": 50
