    ],
    "$defs": {
      "DialRequest": {
        "type": "object",
        "required": [
          "attributes",
          "clientToken",
          "expirationTime",
          "phoneNumber"
        ],
        "properties": {
          "attributes": {
            "$ref": "#/$defs/Attributes"
          },
          "clientToken": {
            "$ref": "#/$defs/ClientToken"
          },
          "expirationTime": {
            "$ref": "#/$defs/TimeStamp"
          },
          "phoneNumber": {
            "$ref": "#/$defs/DestinationPhoneNumber"
          }
        },
        "description": "A dial request for a campaign."
      },
      "Attributes": {
        "type": "object",
        "description": "A custom key-value pair using an attribute map. The attributes are standard Amazon Connect attributes, and can be accessed in contact flows just like any other contact attributes.",
        "additionalProperties": {
          "$ref": "#/$defs/AttributeValue"
        }
      },
      "AttributeValue": {
        "type": "string",
        "description": "The value of the attribute.",
        "minLength": 0,
        "maxLength": 32767
      },
      "ClientToken": {
        "type": "string",
        "description": "Client provided parameter used for idempotency. Its value must be unique for each request.",
        "minLength": 0,
        "maxLength": 64
      },
      "TimeStamp": {
        "type": "string",
        "description": "Timestamp with no UTC offset or timezone",
        "format": "date-time"
      },
      "DestinationPhoneNumber": {
        "type": "string",
        "description": "The phone number of the customer, in E.164 format.",
        "minLength": 0,
        "maxLength": 20,
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://connect-campaigns.us-east-1.amazonaws.com/campaigns/{id}/dial-requests"
