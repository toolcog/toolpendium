{
  "name": "TagResource",
  "description": "Adds or updates tags for a resource in Shield.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSShield_20160616.TagResource"
        ]
