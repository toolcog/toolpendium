{
  "name": "AssociateProactiveEngagementDetails",
  "description": "<p>Initializes proactive engagement and sets the list of contacts for the Shield Response Team (SRT) to use. You must provide at least one phone number in the emergency contact list. </p> <p>After you have initialized proactive engagement using this call, to disable or enable proactive engagement, use the calls <code>DisableProactiveEngagement</code> and <code>EnableProactiveEngagement</code>. </p> <note> <p>This call defines the list of email addresses and phone numbers that the SRT can use to contact you for escalations to the SRT and to initiate proactive customer support.</p> <p>The contacts that you provide in the request replace any contacts that were already defined. If you already have contacts defined and want to use them, retrieve the list using <code>DescribeEmergencyContactSettings</code> and then provide it to this call. </p> </note>",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSShield_20160616.AssociateProactiveEngagementDetails"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/AssociateProactiveEngagementDetailsRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "AssociateProactiveEngagementDetailsRequest": {
        "type": "object",
        "required": [
          "EmergencyContactList"
        ],
        "title": "AssociateProactiveEngagementDetailsRequest",
        "properties": {
          "EmergencyContactList": {
            "allOf": [
              {
                "$ref": "#/$defs/EmergencyContactList"
              },
              {
                "description": "<p>A list of email addresses and phone numbers that the Shield Response Team (SRT) can use to contact you for escalations to the SRT and to initiate proactive customer support. </p> <p>To enable proactive engagement, the contact list must include at least one phone number.</p> <note> <p>The contacts that you provide here replace any contacts that were already defined. If you already have contacts defined and want to use them, retrieve the list using <code>DescribeEmergencyContactSettings</code> and then provide it here. </p> </note>"
              }
            ]
          }
        }
      },
      "EmergencyContactList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/EmergencyContact"
        },
        "minItems": 0,
        "maxItems": 10
      },
      "EmergencyContact": {
        "type": "object",
        "required": [
          "EmailAddress"
        ],
        "properties": {
          "EmailAddress": {
            "allOf": [
              {
                "$ref": "#/$defs/EmailAddress"
              },
              {
                "description": "The email address for the contact."
              }
            ]
          },
          "PhoneNumber": {
            "allOf": [
              {
                "$ref": "#/$defs/PhoneNumber"
              },
              {
                "description": "The phone number for the contact."
              }
            ]
          },
          "ContactNotes": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactNotes"
              },
              {
                "description": "Additional notes regarding the contact. "
              }
            ]
          }
        },
        "description": "Contact information that the SRT can use to contact you if you have proactive engagement enabled, for escalations to the SRT and to initiate proactive customer support."
      },
      "EmailAddress": {
        "type": "string",
        "pattern": "^\\S+@\\S+\\.\\S+$",
        "minLength": 1,
        "maxLength": 150
      },
      "PhoneNumber": {
        "type": "string",
        "pattern": "^\\+[1-9]\\d{1,14}$",
        "minLength": 1,
        "maxLength": 16
      },
      "ContactNotes": {
        "type": "string",
        "pattern": "^[\\w\\s\\.\\-,:/()+@]*$",
        "minLength": 1,
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://shield.us-east-1.amazonaws.com/#X-Amz-Target=AWSShield_20160616.AssociateProactiveEngagementDetails"
    },
