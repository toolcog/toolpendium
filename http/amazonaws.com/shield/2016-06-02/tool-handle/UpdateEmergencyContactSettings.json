{
  "name": "UpdateEmergencyContactSettings",
  "description": "Updates the details of the list of email addresses and phone numbers that the Shield Response Team (SRT) can use to contact you if you have proactive engagement enabled, for escalations to the SRT and to initiate proactive customer support.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSShield_20160616.UpdateEmergencyContactSettings"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateEmergencyContactSettingsRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateEmergencyContactSettingsRequest": {
        "type": "object",
        "title": "UpdateEmergencyContactSettingsRequest",
        "properties": {
          "EmergencyContactList": {
            "allOf": [
              {
                "$ref": "#/$defs/EmergencyContactList"
              },
              {
                "description": "<p>A list of email addresses and phone numbers that the Shield Response Team (SRT) can use to contact you if you have proactive engagement enabled, for escalations to the SRT and to initiate proactive customer support.</p> <p>If you have proactive engagement enabled, the contact list must include at least one phone number.</p>"
              }
            ]
          }
        }
      },
      "EmergencyContactList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/EmergencyContact"
        },
        "minItems": 0,
        "maxItems": 10
      },
      "EmergencyContact": {
        "type": "object",
        "required": [
          "EmailAddress"
        ],
        "properties": {
          "EmailAddress": {
            "allOf": [
              {
                "$ref": "#/$defs/EmailAddress"
              },
              {
                "description": "The email address for the contact."
              }
            ]
          },
          "PhoneNumber": {
            "allOf": [
              {
                "$ref": "#/$defs/PhoneNumber"
              },
              {
                "description": "The phone number for the contact."
              }
            ]
          },
          "ContactNotes": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactNotes"
              },
              {
                "description": "Additional notes regarding the contact. "
              }
            ]
          }
        },
        "description": "Contact information that the SRT can use to contact you if you have proactive engagement enabled, for escalations to the SRT and to initiate proactive customer support."
      },
      "EmailAddress": {
        "type": "string",
        "pattern": "^\\S+@\\S+\\.\\S+$",
        "minLength": 1,
        "maxLength": 150
      },
      "PhoneNumber": {
        "type": "string",
        "pattern": "^\\+[1-9]\\d{1,14}$",
        "minLength": 1,
        "maxLength": 16
      },
      "ContactNotes": {
        "type": "string",
        "pattern": "^[\\w\\s\\.\\-,:/()+@]*$",
        "minLength": 1,
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
