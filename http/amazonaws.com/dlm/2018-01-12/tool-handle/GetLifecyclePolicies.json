{
  "name": "GetLifecyclePolicies",
  "description": "<p>Gets summary information about all or the specified data lifecycle policies.</p> <p>To get complete information about a policy, use <a>GetLifecyclePolicy</a>.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "policyIds": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/PolicyId"
        },
        "description": "The identifiers of the data lifecycle policies."
      },
      "state": {
        "type": "string",
        "enum": [
          "ENABLED",
          "DISABLED",
          "ERROR"
        ],
        "description": "The activation state."
      },
      "resourceTypes": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/ResourceTypeValues"
        },
        "minItems": 1,
        "maxItems": 1,
        "description": "The resource type."
      },
      "targetTags": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TagFilter"
        },
        "minItems": 1,
        "maxItems": 50,
        "description": "<p>The target tag for a policy.</p> <p>Tags are strings in the format <code>key=value</code>.</p>"
      },
      "tagsToAdd": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TagFilter"
        },
        "minItems": 0,
        "maxItems": 50,
        "description": "<p>The tags to add to objects created by the policy.</p> <p>Tags are strings in the format <code>key=value</code>.</p> <p>These user-defined tags are added in addition to the Amazon Web Services-added lifecycle tags.</p>"
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "$defs": {
      "PolicyId": {
        "type": "string",
        "pattern": "policy-[A-Za-z0-9]+",
        "minLength": 0,
        "maxLength": 64
      },
      "ResourceTypeValues": {
        "type": "string",
        "enum": [
          "VOLUME",
          "INSTANCE"
        ]
      },
      "TagFilter": {
        "type": "string",
        "pattern": "[\\p{all}]*",
        "minLength": 0,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://dlm.us-east-1.amazonaws.com/policies{?policyIds,state,resourceTypes,targetTags,tagsToAdd}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Policies**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
