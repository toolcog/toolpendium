{
  "name": "ListLocations",
  "description": "Lists all custom and Amazon Web Services locations.",
  "parameters": {
    "type": "object",
    "properties": {
      "Limit": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "GameLift.ListLocations"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
