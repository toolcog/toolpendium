        "type": "object",
        "required": [
          "GameServerGroupName",
          "ResumeActions"
        ],
        "title": "ResumeGameServerGroupInput",
        "properties": {
          "GameServerGroupName": {
            "allOf": [
              {
                "$ref": "#/$defs/GameServerGroupNameOrArn"
              },
              {
                "description": "A unique identifier for the game server group. Use either the name or ARN value."
              }
            ]
          },
          "ResumeActions": {
            "allOf": [
