{
  "name": "DeregisterCompute",
  "description": "Removes a compute resource from the specified fleet. Deregister your compute resources before you delete the compute.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "GameLift.DeregisterCompute"
        ]
      },
