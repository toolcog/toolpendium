        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/DeregisterComputeInput"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DeregisterComputeInput": {
        "type": "object",
        "required": [
          "FleetId",
          "ComputeName"
        ],
        "title": "DeregisterComputeInput",
        "properties": {
          "FleetId": {
            "allOf": [
              {
                "$ref": "#/$defs/FleetIdOrArn"
              },
              {
                "description": "&gt;A unique identifier for the fleet the compute resource is registered to."
              }
            ]
          },
          "ComputeName": {
            "allOf": [
              {
                "$ref": "#/$defs/ComputeNameOrArn"
              },
              {
                "description": "The name of the compute resource you want to delete."
              }
            ]
          }
        }
      },
      "FleetIdOrArn": {
        "type": "string",
        "pattern": "^fleet-\\S+|^arn:.*:fleet\\/fleet-\\S+"
      },
      "ComputeNameOrArn": {
        "type": "string",
        "pattern": "[a-zA-Z0-9\\-]+|^arn:.*:compute\\/[a-zA-Z0-9\\-]+",
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
