      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DescribeComputeInput": {
        "type": "object",
        "required": [
          "FleetId",
          "ComputeName"
        ],
        "title": "DescribeComputeInput",
        "properties": {
          "FleetId": {
            "allOf": [
              {
                "$ref": "#/$defs/FleetIdOrArn"
              },
              {
                "description": "A unique identifier for the fleet the compute is registered to."
              }
            ]
          },
          "ComputeName": {
            "allOf": [
              {
                "$ref": "#/$defs/ComputeNameOrArn"
              },
              {
                "description": "A descriptive label that is associated with the compute resource registered to your fleet."
              }
            ]
          }
        }
      },
      "FleetIdOrArn": {
        "type": "string",
        "pattern": "^fleet-\\S+|^arn:.*:fleet\\/fleet-\\S+"
      },
      "ComputeNameOrArn": {
        "type": "string",
        "pattern": "[a-zA-Z0-9\\-]+|^arn:.*:compute\\/[a-zA-Z0-9\\-]+",
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
