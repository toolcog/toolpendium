              }
            ]
          },
          "vpceConfigurationArns": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonResourceNames"
              },
              {
                "description": "An array of ARNs included in the VPC endpoint configuration."
              }
            ]
          }
        },
        "description": "Configuration settings for a remote access session, including billing method."
      },
      "BillingMethod": {
        "type": "string",
        "enum": [
          "METERED",
          "UNMETERED"
        ]
      },
      "AmazonResourceNames": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/AmazonResourceName"
        }
      },
      "InteractionMode": {
        "type": "string",
        "enum": [
          "INTERACTIVE",
          "NO_VIDEO",
          "VIDEO_ONLY"
        ],
        "minLength": 0,
        "maxLength": 64
      }
    }
