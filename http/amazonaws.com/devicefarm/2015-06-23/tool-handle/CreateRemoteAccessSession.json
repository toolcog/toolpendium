        "type": "string",
        "pattern": "^arn:.+",
        "minLength": 32,
        "maxLength": 1011
      },
      "SshPublicKey": {
        "type": "string",
        "minLength": 0,
        "maxLength": 8192
      },
      "Boolean": {
        "type": "boolean"
      },
      "Name": {
