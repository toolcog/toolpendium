{
  "name": "ListJobRuns",
  "description": "Lists job runs based on a set of parameters.",
  "parameters": {
    "type": "object",
    "properties": {
      "applicationId": {
        "type": "string",
        "pattern": "[0-9a-z]+",
        "minLength": 1,
        "maxLength": 64,
        "description": "The ID of the application for which to list the job run."
      },
      "nextToken": {
        "type": "string",
        "pattern": "[A-Za-z0-9_=-]+",
        "minLength": 1,
        "maxLength": 1024,
        "description": "The token for the next set of job run results."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 50,
        "description": "The maximum number of job runs that can be listed."
      },
      "createdAtAfter": {
        "type": "string",
        "format": "date-time",
        "description": "The lower bound of the option to filter by creation date and time."
      },
      "createdAtBefore": {
        "type": "string",
        "format": "date-time",
        "description": "The upper bound of the option to filter by creation date and time."
      },
      "states": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/JobRunState"
        },
        "minItems": 0,
        "maxItems": 8,
        "description": "An optional filter for job run states. Note that if this filter contains multiple states, the resulting list will be grouped by the state."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "applicationId"
    ],
    "$defs": {
      "JobRunState": {
        "type": "string",
        "enum": [
          "SUBMITTED",
          "PENDING",
          "SCHEDULED",
          "RUNNING",
          "SUCCESS",
          "FAILED",
          "CANCELLING",
          "CANCELLED"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://emr-serverless.us-east-1.amazonaws.com/applications/{applicationId}/jobruns{?nextToken,maxResults,createdAtAfter,createdAtBefore,states}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**jobRuns**",
            "**nextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
