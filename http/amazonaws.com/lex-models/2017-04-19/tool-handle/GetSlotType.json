      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**name**",
            "**description**",
            "**enumerationValues**",
            "**lastUpdatedDate**",
            "**createdDate**",
            "**version**",
            "**checksum**",
            "**valueSelectionStrategy**",
            "**parentSlotTypeSignature**",
            "**slotTypeConfigurations**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
