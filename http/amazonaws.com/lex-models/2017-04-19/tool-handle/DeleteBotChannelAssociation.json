{
  "name": "DeleteBotChannelAssociation",
  "description": "<p>Deletes the association between an Amazon Lex bot and a messaging platform.</p> <p>This operation requires permission for the <code>lex:DeleteBotChannelAssociation</code> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "pattern": "^([A-Za-z]_?)+$",
        "minLength": 1,
        "maxLength": 100,
        "description": "The name of the association. The name is case sensitive. "
      },
      "botName": {
        "type": "string",
        "pattern": "^([A-Za-z]_?)+$",
        "minLength": 2,
        "maxLength": 50,
        "description": "The name of the Amazon Lex bot."
      },
      "aliasName": {
        "type": "string",
        "pattern": "^([A-Za-z]_?)+$",
        "minLength": 1,
        "maxLength": 100,
        "description": "An alias that points to the specific version of the Amazon Lex bot to which this association is being made."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "name",
      "botName",
      "aliasName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://models.lex.us-east-1.amazonaws.com/bots/{botName}/aliases/{aliasName}/channels/{name}"
    },
    "headers": {
