{
  "name": "DescribeEndpoints",
  "description": "Send an request with an empty body to the regional API endpoint to get your account API endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
