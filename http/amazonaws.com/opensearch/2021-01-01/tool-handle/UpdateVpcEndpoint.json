{
  "name": "UpdateVpcEndpoint",
  "description": "Modifies an Amazon OpenSearch Service-managed interface VPC endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
