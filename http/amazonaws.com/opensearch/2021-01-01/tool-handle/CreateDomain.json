      },
      "StartAt": {
        "type": "string",
        "format": "date-time"
      },
      "Duration": {
        "type": "object",
        "properties": {
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/DurationValue"
              },
              {
