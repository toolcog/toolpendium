      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Account"
        ],
        "properties": {
          "Account": {
            "description": "The account ID to revoke access from.",
            "type": "string",
            "pattern": "^[0-9]+$"
          }
        }
      }
    },
    "required": [
      "DomainName",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://es.us-east-1.amazonaws.com/2021-01-01/opensearch/domain/{DomainName}/revokeVpcEndpointAccess"
    },
