{
  "name": "PutClassificationExportConfiguration",
  "description": "Creates or updates the configuration settings for storing data classification results.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
