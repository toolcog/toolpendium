{
  "name": "UpdateSnapshot",
  "description": "Updates the name of an Amazon FSx for OpenZFS snapshot.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSSimbaAPIService_v20180301.UpdateSnapshot"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
