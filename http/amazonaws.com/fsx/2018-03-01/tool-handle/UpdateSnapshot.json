        "$ref": "#/$defs/UpdateSnapshotRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateSnapshotRequest": {
        "type": "object",
        "required": [
          "Name",
          "SnapshotId"
        ],
        "title": "UpdateSnapshotRequest",
        "properties": {
          "ClientRequestToken": {
            "$ref": "#/$defs/ClientRequestToken"
          },
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/SnapshotName"
              },
              {
                "description": "The name of the snapshot to update."
              }
            ]
          },
          "SnapshotId": {
            "allOf": [
              {
                "$ref": "#/$defs/SnapshotId"
              },
              {
                "description": "The ID of the snapshot that you want to update, in the format <code>fsvolsnap-0123456789abcdef0</code>."
              }
            ]
          }
        }
      },
      "ClientRequestToken": {
        "type": "string",
        "pattern": "[A-za-z0-9_.-]{0,63}$",
        "description": "(Optional) An idempotency token for resource creation, in a string of up to 63 ASCII characters. This token is automatically filled on your behalf when you use the Command Line Interface (CLI) or an Amazon Web Services SDK.",
        "minLength": 1,
        "maxLength": 63
      },
      "SnapshotName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_:.-]{1,203}$",
        "minLength": 1,
        "maxLength": 203
      },
      "SnapshotId": {
        "type": "string",
        "pattern": "^((fs)?volsnap-[0-9a-f]{8,})$",
        "minLength": 11,
        "maxLength": 28
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://fsx.us-east-1.amazonaws.com/#X-Amz-Target=AWSSimbaAPIService_v20180301.UpdateSnapshot"
