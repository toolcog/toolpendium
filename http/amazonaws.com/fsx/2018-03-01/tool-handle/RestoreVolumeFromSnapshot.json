{
  "name": "RestoreVolumeFromSnapshot",
  "description": "Returns an Amazon FSx for OpenZFS volume to the state saved by the specified snapshot.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSSimbaAPIService_v20180301.RestoreVolumeFromSnapshot"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/RestoreVolumeFromSnapshotRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "RestoreVolumeFromSnapshotRequest": {
        "type": "object",
        "required": [
          "VolumeId",
          "SnapshotId"
        ],
        "title": "RestoreVolumeFromSnapshotRequest",
        "properties": {
          "ClientRequestToken": {
            "$ref": "#/$defs/ClientRequestToken"
          },
          "VolumeId": {
            "allOf": [
              {
                "$ref": "#/$defs/VolumeId"
              },
              {
                "description": "The ID of the volume that you are restoring."
              }
            ]
          },
          "SnapshotId": {
            "allOf": [
              {
                "$ref": "#/$defs/SnapshotId"
              },
              {
                "description": "The ID of the source snapshot. Specifies the snapshot that you are restoring from."
              }
            ]
          },
          "Options": {
            "allOf": [
              {
                "$ref": "#/$defs/RestoreOpenZFSVolumeOptions"
              },
              {
                "description": "<p>The settings used when restoring the specified volume from snapshot.</p> <ul> <li> <p> <code>DELETE_INTERMEDIATE_SNAPSHOTS</code> - Deletes snapshots between the current state and the specified snapshot. If there are intermediate snapshots and this option isn't used, <code>RestoreVolumeFromSnapshot</code> fails.</p> </li> <li> <p> <code>DELETE_CLONED_VOLUMES</code> - Deletes any dependent clone volumes created from intermediate snapshots. If there are any dependent clone volumes and this option isn't used, <code>RestoreVolumeFromSnapshot</code> fails.</p> </li> </ul>"
              }
            ]
          }
        }
      },
      "ClientRequestToken": {
        "type": "string",
        "pattern": "[A-za-z0-9_.-]{0,63}$",
        "description": "(Optional) An idempotency token for resource creation, in a string of up to 63 ASCII characters. This token is automatically filled on your behalf when you use the Command Line Interface (CLI) or an Amazon Web Services SDK.",
        "minLength": 1,
        "maxLength": 63
      },
      "VolumeId": {
        "type": "string",
        "pattern": "^(fsvol-[0-9a-f]{17,})$",
        "minLength": 23,
        "maxLength": 23
      },
      "SnapshotId": {
        "type": "string",
        "pattern": "^((fs)?volsnap-[0-9a-f]{8,})$",
        "minLength": 11,
        "maxLength": 28
      },
      "RestoreOpenZFSVolumeOptions": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/RestoreOpenZFSVolumeOption"
        },
        "maxItems": 2
      },
      "RestoreOpenZFSVolumeOption": {
        "type": "string",
        "enum": [
          "DELETE_INTERMEDIATE_SNAPSHOTS",
          "DELETE_CLONED_VOLUMES"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://fsx.us-east-1.amazonaws.com/#X-Amz-Target=AWSSimbaAPIService_v20180301.RestoreVolumeFromSnapshot"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
