        "items": {
          "$ref": "#/$defs/AlternateDNSName"
        },
        "maxItems": 50
      },
      "AlternateDNSName": {
        "type": "string",
        "pattern": "^[^\\u0000\\u0085\\u2028\\u2029\\r\\n]{4,253}$",
        "minLength": 4,
        "maxLength": 253
      }
    }
  },
