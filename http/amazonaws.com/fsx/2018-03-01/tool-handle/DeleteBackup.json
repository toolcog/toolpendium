              },
              {
                "description": "The ID of the backup that you want to delete."
              }
            ]
          },
          "ClientRequestToken": {
            "allOf": [
              {
                "$ref": "#/$defs/ClientRequestToken"
              },
              {
                "description": "A string of up to 63 ASCII characters that Amazon FSx uses to ensure idempotent deletion. This parameter is automatically filled on your behalf when using the CLI or SDK."
              }
            ]
          }
        },
        "description": "The request object for the <code>DeleteBackup</code> operation."
      },
      "BackupId": {
        "type": "string",
        "pattern": "^(backup-[0-9a-f]{8,})$",
        "description": "The ID of the source backup. Specifies the backup that you are copying.",
        "minLength": 12,
        "maxLength": 128
      },
      "ClientRequestToken": {
        "type": "string",
        "pattern": "[A-za-z0-9_.-]{0,63}$",
