      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Arn**",
            "**Type**",
            "**TypeName**",
            "**DefaultVersionId**",
            "**IsDefaultVersion**",
            "**TypeTestsStatus**",
            "**TypeTestsStatusDescription**",
            "**Description**",
            "**Schema**",
            "**ProvisioningType**",
            "**DeprecatedStatus**",
            "**LoggingConfig**",
            "**RequiredActivatedTypes**",
            "**ExecutionRoleArn**",
            "**Visibility**",
            "**SourceUrl**",
            "**DocumentationUrl**",
            "**LastUpdated**",
            "**TimeCreated**",
            "**ConfigurationSchema**",
            "**PublisherId**",
            "**OriginalTypeName**",
            "**OriginalTypeArn**",
            "**PublicVersionNumber**",
            "**LatestPublicVersion**",
            "**IsActivated**",
            "**AutoUpdate**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
