{
  "name": "DescribeJobExecution",
  "description": "Gets details of a job execution.",
  "parameters": {
    "type": "object",
    "properties": {
      "jobId": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_-]+|^\\$next",
        "description": "The unique identifier assigned to this job when it was created."
      },
      "thingName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9:_-]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "The thing name associated with the device the job execution is running on."
      },
      "includeJobDocument": {
        "type": "boolean",
        "description": "Optional. When set to true, the response contains the job document. The default is false."
      },
      "executionNumber": {
        "type": "integer",
        "description": "Optional. A number that identifies a particular job execution on a particular device. If not specified, the latest job execution is returned."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "jobId",
      "thingName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://data.jobs.iot.us-east-1.amazonaws.com/things/{thingName}/jobs/{jobId}{?includeJobDocument,executionNumber}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
