{
  "name": "ListNodes",
  "description": "Returns a list of nodes.",
  "parameters": {
    "type": "object",
    "properties": {
      "category": {
        "type": "string",
        "enum": [
          "BUSINESS_LOGIC",
          "ML_MODEL",
          "MEDIA_SOURCE",
          "MEDIA_SINK"
        ],
        "description": "Search for nodes by category."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 0,
        "maximum": 25,
        "description": "The maximum number of nodes to return in one page of results."
      },
      "nextToken": {
        "type": "string",
        "pattern": "^.+$",
        "minLength": 1,
        "maxLength": 4096,
        "description": "Specify the pagination token from a previous request to retrieve the next page of results."
      },
      "ownerAccount": {
        "type": "string",
        "pattern": "^[0-9a-z\\_]+$",
        "minLength": 1,
        "maxLength": 12,
        "description": "Search for nodes by the account ID of the nodes' owner."
      },
      "packageName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\-\\_]+$",
        "minLength": 1,
        "maxLength": 128,
        "description": "Search for nodes by name."
      },
      "packageVersion": {
        "type": "string",
        "pattern": "^([0-9]+)\\.([0-9]+)$",
        "minLength": 1,
        "maxLength": 255,
        "description": "Search for nodes by version."
      },
      "patchVersion": {
        "type": "string",
        "pattern": "^[a-z0-9]+$",
        "minLength": 1,
        "maxLength": 255,
        "description": "Search for nodes by patch version."
      },
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://panorama.us-east-1.amazonaws.com/nodes{?category,maxResults,nextToken,ownerAccount,packageName,packageVersion,patchVersion,MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**NextToken**",
            "**Nodes**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
