{
  "name": "CreatePackageImportJob",
  "description": "Imports a node package.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "ClientToken",
          "InputConfig",
          "JobType",
          "OutputConfig"
        ],
        "properties": {
          "ClientToken": {
            "description": "A client token for the package import job.",
            "type": "string",
            "pattern": "^[a-zA-Z0-9\\-\\_]+$",
            "minLength": 1,
            "maxLength": 64
          },
          "InputConfig": {
            "description": "A configuration for a package import job.",
            "type": "object",
            "properties": {
              "PackageVersionInputConfig": {
                "allOf": [
                  {
                    "$ref": "#/$defs/PackageVersionInputConfig"
                  },
                  {
                    "description": "The package version's input configuration."
                  }
                ]
              }
            }
          },
          "JobTags": {
            "description": "Tags for the package import job.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/JobResourceTags"
            }
          },
          "JobType": {
            "description": "A job type for the package import job.",
            "type": "string",
            "enum": [
              "NODE_PACKAGE_VERSION",
              "MARKETPLACE_NODE_PACKAGE_VERSION"
            ]
          },
          "OutputConfig": {
            "description": "An output configuration for a package import job.",
            "type": "object",
            "properties": {
              "PackageVersionOutputConfig": {
                "allOf": [
                  {
                    "$ref": "#/$defs/PackageVersionOutputConfig"
                  },
                  {
                    "description": "The package version's output configuration."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "PackageVersionInputConfig": {
        "type": "object",
        "required": [
          "S3Location"
        ],
        "properties": {
          "S3Location": {
            "allOf": [
              {
                "$ref": "#/$defs/S3Location"
              },
              {
                "description": "A location in Amazon S3."
              }
            ]
          }
        },
        "description": "A package version input configuration."
      },
      "S3Location": {
        "type": "object",
        "required": [
          "BucketName",
          "ObjectKey"
        ],
        "properties": {
          "BucketName": {
            "allOf": [
              {
                "$ref": "#/$defs/BucketName"
              },
              {
                "description": "A bucket name."
              }
            ]
          },
          "ObjectKey": {
            "allOf": [
              {
                "$ref": "#/$defs/ObjectKey"
              },
              {
                "description": "An object key."
              }
            ]
          },
          "Region": {
            "allOf": [
              {
                "$ref": "#/$defs/Region"
              },
              {
                "description": "The bucket's Region."
              }
            ]
          }
        },
        "description": "A location in Amazon S3."
      },
      "BucketName": {
        "type": "string",
        "pattern": "^.+$",
        "minLength": 1,
        "maxLength": 255
      },
      "ObjectKey": {
        "type": "string",
        "pattern": "^.+$",
        "minLength": 1,
        "maxLength": 255
      },
      "Region": {
        "type": "string",
        "pattern": "^.+$",
        "minLength": 1,
        "maxLength": 255
      },
      "JobResourceTags": {
        "type": "object",
        "required": [
          "ResourceType",
          "Tags"
        ],
        "properties": {
          "ResourceType": {
            "allOf": [
              {
                "$ref": "#/$defs/JobResourceType"
              },
              {
                "description": "The job's type."
              }
            ]
          },
          "Tags": {
            "allOf": [
              {
                "$ref": "#/$defs/TagMap"
              },
              {
                "description": "The job's tags."
              }
            ]
          }
        },
        "description": "Tags for a job."
      },
      "JobResourceType": {
        "type": "string",
        "enum": [
          "PACKAGE"
        ]
      },
      "TagMap": {
        "type": "object",
        "minProperties": 0,
        "maxProperties": 50,
        "additionalProperties": {
          "$ref": "#/$defs/TagValue"
        }
      },
      "TagValue": {
        "type": "string",
        "pattern": "^.*$",
        "minLength": 0,
        "maxLength": 256
      },
      "PackageVersionOutputConfig": {
        "type": "object",
        "required": [
          "PackageName",
          "PackageVersion"
        ],
        "properties": {
          "MarkLatest": {
            "allOf": [
              {
                "$ref": "#/$defs/MarkLatestPatch"
              },
              {
                "description": "Indicates that the version is recommended for all users."
              }
            ]
          },
          "PackageName": {
            "allOf": [
              {
                "$ref": "#/$defs/NodePackageName"
              },
              {
                "description": "The output's package name."
              }
            ]
          },
          "PackageVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/NodePackageVersion"
              },
              {
                "description": "The output's package version."
              }
            ]
          }
        },
        "description": "A package version output configuration."
      },
      "MarkLatestPatch": {
        "type": "boolean"
      },
      "NodePackageName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\-\\_]+$",
        "minLength": 1,
        "maxLength": 128
      },
      "NodePackageVersion": {
        "type": "string",
        "pattern": "^([0-9]+)\\.([0-9]+)$",
        "minLength": 1,
        "maxLength": 255
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://panorama.us-east-1.amazonaws.com/packages/import-jobs"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**JobId**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
