{
  "name": "ListDevices",
  "description": "Returns a list of devices.",
  "parameters": {
    "type": "object",
    "properties": {
      "DeviceAggregatedStatusFilter": {
        "type": "string",
        "enum": [
          "ERROR",
          "AWAITING_PROVISIONING",
          "PENDING",
          "FAILED",
          "DELETING",
          "ONLINE",
          "OFFLINE",
          "LEASE_EXPIRED",
          "UPDATE_NEEDED",
          "REBOOTING"
        ],
        "description": "Filter based on a device's status."
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 0,
        "maximum": 25,
        "description": "The maximum number of devices to return in one page of results."
      },
      "NameFilter": {
        "type": "string",
        "description": "Filter based on device's name. Prefixes supported."
      },
      "NextToken": {
        "type": "string",
        "pattern": "^.+$",
        "minLength": 1,
        "maxLength": 4096,
        "description": "Specify the pagination token from a previous request to retrieve the next page of results."
      },
      "SortBy": {
        "type": "string",
        "enum": [
          "DEVICE_ID",
          "CREATED_TIME",
          "NAME",
          "DEVICE_AGGREGATED_STATUS"
        ],
        "description": "The target column to be sorted on. Default column sort is CREATED_TIME."
      },
      "SortOrder": {
        "type": "string",
        "enum": [
          "ASCENDING",
          "DESCENDING"
        ],
        "description": "The sorting order for the returned list. SortOrder is DESCENDING by default based on CREATED_TIME. Otherwise, SortOrder is ASCENDING."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://panorama.us-east-1.amazonaws.com/devices{?DeviceAggregatedStatusFilter,MaxResults,NameFilter,NextToken,SortBy,SortOrder}"
