{
  "name": "DescribePackage",
  "description": "Returns information about a package.",
  "parameters": {
    "type": "object",
    "properties": {
      "PackageId": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\-\\_\\/]+$",
        "minLength": 1,
        "maxLength": 255,
        "description": "The package's ID."
      },
      "X-Amz-Content-Sha256": {
