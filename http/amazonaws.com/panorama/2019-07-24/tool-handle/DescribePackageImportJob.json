      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ClientToken**",
            "**CreatedTime**",
            "**InputConfig**",
            "**JobId**",
            "**JobTags**",
            "**JobType**",
            "**LastUpdatedTime**",
