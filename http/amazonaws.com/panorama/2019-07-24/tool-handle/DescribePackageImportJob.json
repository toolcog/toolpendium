{
  "name": "DescribePackageImportJob",
  "description": "Returns information about a package import job.",
  "parameters": {
    "type": "object",
    "properties": {
      "JobId": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\-\\_]+$",
        "minLength": 1,
        "maxLength": 255,
        "description": "The job's ID."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
