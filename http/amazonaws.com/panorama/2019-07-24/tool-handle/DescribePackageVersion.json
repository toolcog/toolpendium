{
  "name": "DescribePackageVersion",
  "description": "Returns information about a package version.",
  "parameters": {
    "type": "object",
    "properties": {
      "OwnerAccount": {
        "type": "string",
        "pattern": "^[0-9a-z\\_]+$",
        "minLength": 1,
        "maxLength": 12,
        "description": "The version's owner account."
      },
      "PackageId": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\-\\_\\/]+$",
        "minLength": 1,
        "maxLength": 255,
        "description": "The version's ID."
      },
      "PackageVersion": {
        "type": "string",
        "pattern": "^([0-9]+)\\.([0-9]+)$",
        "minLength": 1,
        "maxLength": 255,
        "description": "The version's version."
      },
      "PatchVersion": {
        "type": "string",
        "pattern": "^[a-z0-9]+$",
        "minLength": 1,
        "maxLength": 255,
        "description": "The version's patch version."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "PackageId",
      "PackageVersion"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://panorama.us-east-1.amazonaws.com/packages/metadata/{PackageId}/versions/{PackageVersion}{?OwnerAccount,PatchVersion}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**IsLatestPatch**",
            "**OwnerAccount**",
            "**PackageArn**",
            "**PackageId**",
            "**PackageName**",
            "**PackageVersion**",
            "**PatchVersion**",
            "**RegisteredTime**",
            "**Status**",
            "**StatusDescription**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
