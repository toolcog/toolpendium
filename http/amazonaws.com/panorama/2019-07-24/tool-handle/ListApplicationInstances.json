{
  "name": "ListApplicationInstances",
  "description": "Returns a list of application instances.",
  "parameters": {
    "type": "object",
    "properties": {
      "deviceId": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\-\\_]+$",
        "minLength": 1,
        "maxLength": 255,
        "description": "The application instances' device ID."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 0,
        "maximum": 25,
        "description": "The maximum number of application instances to return in one page of results."
      },
      "nextToken": {
        "type": "string",
        "pattern": "^.+$",
        "minLength": 1,
        "maxLength": 4096,
        "description": "Specify the pagination token from a previous request to retrieve the next page of results."
      },
      "statusFilter": {
        "type": "string",
        "enum": [
          "DEPLOYMENT_SUCCEEDED",
          "DEPLOYMENT_ERROR",
          "REMOVAL_SUCCEEDED",
          "REMOVAL_FAILED",
          "PROCESSING_DEPLOYMENT",
          "PROCESSING_REMOVAL",
          "DEPLOYMENT_FAILED"
        ],
        "description": "Only include instances with a specific status."
      },
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
