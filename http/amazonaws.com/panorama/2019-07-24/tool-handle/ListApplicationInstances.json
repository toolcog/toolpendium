      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://panorama.us-east-1.amazonaws.com/application-instances{?deviceId,maxResults,nextToken,statusFilter,MaxResults,NextToken}"
    },
    "headers": {
