  "name": "UpdateSpace",
  "description": "Changes one or more values for a space.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "pattern": "[a-zA-Z0-9]+(?:[-_\\.][a-zA-Z0-9]+)*",
        "minLength": 3,
        "maxLength": 63,
        "description": "The name of the space."
      },
      "body": {
        "type": "object",
        "properties": {
          "description": {
            "description": "The description of the space.",
