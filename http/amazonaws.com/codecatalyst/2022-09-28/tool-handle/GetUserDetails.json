{
  "name": "GetUserDetails",
  "description": "Returns information about a user. ",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "minLength": 1,
        "maxLength": 256,
        "description": "The system-generated unique ID of the user. "
      },
      "userName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9]{3,100}",
        "minLength": 3,
        "maxLength": 100,
        "description": "The name of the user as displayed in Amazon CodeCatalyst."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://codecatalyst.global.api.aws/userDetails{?id,userName}"
    }
  },
