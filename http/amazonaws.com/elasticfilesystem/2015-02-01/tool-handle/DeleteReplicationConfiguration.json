      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "SourceFileSystemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://elasticfilesystem.us-east-1.amazonaws.com/2015-02-01/file-systems/{SourceFileSystemId}/replication-configuration"
    },
