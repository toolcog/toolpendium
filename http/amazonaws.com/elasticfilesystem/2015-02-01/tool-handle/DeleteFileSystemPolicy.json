    "type": "object",
    "properties": {
      "FileSystemId": {
        "type": "string",
        "pattern": "^(arn:aws[-a-z]*:elasticfilesystem:[0-9a-z-:]+:file-system/fs-[0-9a-f]{8,40}|fs-[0-9a-f]{8,40})$",
        "maxLength": 128,
        "description": "Specifies the EFS file system for which to delete the <code>FileSystemPolicy</code>."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
