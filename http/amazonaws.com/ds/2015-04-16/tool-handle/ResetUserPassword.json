      "body"
    ],
    "$defs": {
      "ResetUserPasswordRequest": {
        "type": "object",
        "required": [
          "DirectoryId",
          "UserName",
          "NewPassword"
        ],
        "title": "ResetUserPasswordRequest",
        "properties": {
          "DirectoryId": {
            "allOf": [
              {
                "$ref": "#/$defs/DirectoryId"
              },
              {
                "description": "Identifier of the Managed Microsoft AD or Simple AD directory in which the user resides."
              }
            ]
          },
          "UserName": {
            "allOf": [
              {
                "$ref": "#/$defs/CustomerUserName"
              },
              {
                "description": "The user name of the user whose password will be reset."
              }
            ]
          },
          "NewPassword": {
            "allOf": [
              {
                "$ref": "#/$defs/UserPassword"
              },
              {
                "description": "The new password that will be reset."
              }
            ]
          }
        }
      },
      "DirectoryId": {
        "type": "string",
        "pattern": "^d-[0-9a-f]{10}$"
      },
      "CustomerUserName": {
        "type": "string",
        "pattern": "^(?!.*\\\\|.*\"|.*\\/|.*\\[|.*\\]|.*:|.*;|.*\\||.*=|.*,|.*\\+|.*\\*|.*\\?|.*<|.*>|.*@).*$",
        "minLength": 1,
        "maxLength": 64
      },
      "UserPassword": {
        "type": "string",
        "minLength": 1,
        "maxLength": 127,
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
