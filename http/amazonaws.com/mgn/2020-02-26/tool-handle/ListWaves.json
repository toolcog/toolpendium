{
  "name": "ListWaves",
  "description": "Retrieves all waves or multiple waves by ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "maxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "nextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "accountID": {
            "description": "Request account ID.",
            "type": "string",
            "pattern": "[0-9]{12,}",
            "minLength": 12,
            "maxLength": 12
          },
          "filters": {
            "description": "Waves list filters.",
            "type": "object",
            "properties": {
              "isArchived": {
                "allOf": [
                  {
                    "$ref": "#/$defs/Boolean"
                  },
                  {
                    "description": "Filter waves list by archival status."
                  }
                ]
              },
              "waveIDs": {
                "allOf": [
                  {
                    "$ref": "#/$defs/WaveIDsFilter"
                  },
                  {
                    "description": "Filter waves list by wave ID."
                  }
                ]
              }
            }
          },
          "maxResults": {
            "description": "Maximum results to return when listing waves.",
            "type": "integer",
            "minimum": 1,
            "maximum": 1000
          },
          "nextToken": {
            "description": "Request next token.",
            "type": "string",
            "minLength": 0,
            "maxLength": 2048
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "Boolean": {
        "type": "boolean"
      },
      "WaveIDsFilter": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/WaveID"
        },
        "minItems": 0,
        "maxItems": 200
      },
      "WaveID": {
        "type": "string",
        "pattern": "^wave-[0-9a-zA-Z]{17}$",
        "minLength": 22,
        "maxLength": 22
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://mgn.us-east-1.amazonaws.com/ListWaves{?maxResults,nextToken}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**nextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
