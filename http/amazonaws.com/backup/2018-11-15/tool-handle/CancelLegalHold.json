      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "legalHoldId",
      "cancelDescription"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://backup.us-east-1.amazonaws.com/legal-holds/{legalHoldId}#cancelDescription{?cancelDescription,retainRecordInDays}"
    },
