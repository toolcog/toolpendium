      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**BackupVaultName**",
            "**BackupVaultArn**",
            "**EncryptionKeyArn**",
            "**CreationDate**",
            "**CreatorRequestId**",
            "**NumberOfRecoveryPoints**",
