              {
                "$ref": "#/$defs/ProductSource"
              },
              {
                "description": "Access level of the source of the product."
              }
            ]
          }
        }
      },
      "AcceptLanguage": {
        "type": "string",
        "maxLength": 100
      },
      "Id": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_\\-]*",
        "minLength": 1,
        "maxLength": 100
      },
      "ProductViewFilters": {
        "type": "object",
        "additionalProperties": {
          "$ref": "#/$defs/ProductViewFilterValues"
        }
      },
      "ProductViewFilterValues": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/ProductViewFilterValue"
