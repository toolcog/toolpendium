          "Filters": {
            "allOf": [
              {
                "$ref": "#/$defs/ProductViewFilters"
              },
              {
                "description": "The search filters. If no search filters are specified, the output includes all products to which the administrator has access."
              }
            ]
          },
          "SortBy": {
            "allOf": [
              {
                "$ref": "#/$defs/ProductViewSortBy"
