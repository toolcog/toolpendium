        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "AppMonitorName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://rum.us-east-1.amazonaws.com/rummetrics/{AppMonitorName}/metricsdestination{?maxResults,nextToken,MaxResults,NextToken}"
