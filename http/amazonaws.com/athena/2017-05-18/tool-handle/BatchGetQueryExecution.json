      "BatchGetQueryExecutionInput": {
        "type": "object",
        "required": [
          "QueryExecutionIds"
        ],
        "title": "BatchGetQueryExecutionInput",
        "properties": {
          "QueryExecutionIds": {
            "allOf": [
              {
                "$ref": "#/$defs/QueryExecutionIdList"
              },
              {
                "description": "An array of query execution IDs."
              }
            ]
          }
        },
        "description": "Contains an array of query execution IDs."
      },
      "QueryExecutionIdList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/QueryExecutionId"
        },
        "minItems": 1,
        "maxItems": 50
      },
      "QueryExecutionId": {
        "type": "string",
        "pattern": "\\S+",
        "minLength": 1,
        "maxLength": 128
      }
    }
  },
  "handler": "http",
  "request": {
