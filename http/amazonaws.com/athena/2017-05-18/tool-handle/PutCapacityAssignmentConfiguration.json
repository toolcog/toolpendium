{
  "name": "PutCapacityAssignmentConfiguration",
  "description": "Puts a new capacity assignment configuration for a specified capacity reservation. If a capacity assignment configuration already exists for the capacity reservation, replaces the existing capacity assignment configuration.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AmazonAthena.PutCapacityAssignmentConfiguration"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/PutCapacityAssignmentConfigurationInput"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "PutCapacityAssignmentConfigurationInput": {
        "type": "object",
        "required": [
          "CapacityReservationName",
          "CapacityAssignments"
        ],
        "title": "PutCapacityAssignmentConfigurationInput",
        "properties": {
          "CapacityReservationName": {
            "allOf": [
              {
                "$ref": "#/$defs/CapacityReservationName"
              },
              {
                "description": "The name of the capacity reservation to put a capacity assignment configuration for."
              }
            ]
          },
          "CapacityAssignments": {
            "allOf": [
              {
                "$ref": "#/$defs/CapacityAssignmentsList"
              },
              {
                "description": "The list of assignments for the capacity assignment configuration."
              }
            ]
          }
        }
      },
      "CapacityReservationName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9._-]+",
        "minLength": 1,
        "maxLength": 128
      },
      "CapacityAssignmentsList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/CapacityAssignment"
        }
      },
      "CapacityAssignment": {
        "type": "object",
        "properties": {
          "WorkGroupNames": {
            "allOf": [
              {
                "$ref": "#/$defs/WorkGroupNamesList"
              },
              {
                "description": "The list of workgroup names for the capacity assignment."
              }
            ]
          }
        },
        "description": "A mapping between one or more workgroups and a capacity reservation."
      },
      "WorkGroupNamesList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/WorkGroupName"
        }
      },
      "WorkGroupName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9._-]{1,128}"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://athena.us-east-1.amazonaws.com/#X-Amz-Target=AmazonAthena.PutCapacityAssignmentConfiguration"
    },
    "headers": {
