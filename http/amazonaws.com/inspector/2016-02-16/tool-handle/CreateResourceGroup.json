        "type": "object",
        "required": [
          "resourceGroupTags"
        ],
        "title": "CreateResourceGroupRequest",
        "properties": {
          "resourceGroupTags": {
            "allOf": [
              {
                "$ref": "#/$defs/ResourceGroupTags"
              },
              {
                "description": "<p>A collection of keys and an array of possible values, '[{\"key\":\"key1\",\"values\":[\"Value1\",\"Value2\"]},{\"key\":\"Key2\",\"values\":[\"Value3\"]}]'.</p> <p>For example,'[{\"key\":\"Name\",\"values\":[\"TestEC2Instance\"]}]'.</p>"
              }
            ]
          }
        }
      },
      "ResourceGroupTags": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/ResourceGroupTag"
        },
        "minItems": 1,
        "maxItems": 10
      },
      "ResourceGroupTag": {
        "type": "object",
        "required": [
          "key"
        ],
        "properties": {
          "key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "A tag key."
              }
            ]
          },
          "value": {
            "allOf": [
              {
                "$ref": "#/$defs/TagValue"
              },
              {
                "description": "The value assigned to a tag key."
              }
            ]
          }
        },
        "description": "This data type is used as one of the elements of the <a>ResourceGroup</a> data type."
      },
      "TagKey": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128
      },
      "TagValue": {
        "type": "string",
        "minLength": 1,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
