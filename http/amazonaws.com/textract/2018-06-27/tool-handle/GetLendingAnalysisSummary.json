      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "GetLendingAnalysisSummaryRequest": {
        "type": "object",
        "required": [
          "JobId"
        ],
        "title": "GetLendingAnalysisSummaryRequest",
        "properties": {
          "JobId": {
            "allOf": [
              {
                "$ref": "#/$defs/JobId"
              },
              {
                "description": " A unique identifier for the lending or text-detection job. The <code>JobId</code> is returned from StartLendingAnalysis. A <code>JobId</code> value is only valid for 7 days."
              }
            ]
          }
        }
      },
      "JobId": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9-_]+$",
        "minLength": 1,
        "maxLength": 64
      }
    }
  },
  "handler": "http",
