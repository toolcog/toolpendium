        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "savingsPlanArns": {
            "description": "The Amazon Resource Names (ARN) of the Savings Plans.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/SavingsPlanArn"
            },
            "maxItems": 100
          },
          "savingsPlanIds": {
            "description": "The IDs of the Savings Plans.",
            "type": "array",
