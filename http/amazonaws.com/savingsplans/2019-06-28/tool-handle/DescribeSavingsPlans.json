            "items": {
              "$ref": "#/$defs/SavingsPlanFilter"
            }
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "SavingsPlanArn": {
        "type": "string",
        "pattern": "arn:aws:[a-z]+:([a-z]{2}-[a-z]+-\\d{1}|):(\\d{12}):savingsplan\\/([0-9a-f]{8}-[0-9a-f]{4}-[1-5][0-9a-f]{3}-[89ab][0-9a-f]{3}-[0-9a-f]{12})$"
      },
      "SavingsPlanId": {
        "type": "string"
      },
      "SavingsPlanState": {
        "type": "string",
        "enum": [
          "payment-pending",
          "payment-failed",
          "active",
          "retired",
          "queued",
          "queued-deleted"
        ]
      },
      "SavingsPlanFilter": {
        "type": "object",
        "properties": {
          "name": {
            "allOf": [
              {
                "$ref": "#/$defs/SavingsPlansFilterName"
              },
              {
                "description": "The filter name."
              }
            ]
          },
          "values": {
            "allOf": [
              {
                "$ref": "#/$defs/ListOfStrings"
              },
              {
                "description": "The filter value."
              }
            ]
          }
        },
        "description": "Information about a filter."
      },
      "SavingsPlansFilterName": {
        "type": "string",
        "enum": [
          "region",
          "ec2-instance-family",
          "commitment",
          "upfront",
          "term",
          "savings-plan-type",
          "payment-option",
          "start",
          "end"
        ]
      },
      "ListOfStrings": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/String"
        }
      },
      "String": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://savingsplans.us-east-1.amazonaws.com/DescribeSavingsPlans"
    },
    "headers": {
