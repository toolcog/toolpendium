    "type": "object",
    "properties": {
      "TopicArn": {
        "type": "string",
        "description": "The ARN of the topic for which you wish to find subscriptions."
      },
      "NextToken": {
        "type": "string",
        "description": "Token returned by the previous <code>ListSubscriptionsByTopic</code> request."
      },
      "Action": {
        "type": "string",
