{
  "name": "DeleteSite",
  "description": "Deletes an existing site. The site cannot be associated with any device or link.",
  "parameters": {
    "type": "object",
    "properties": {
      "globalNetworkId": {
        "type": "string",
        "pattern": "[\\s\\S]*",
        "minLength": 0,
        "maxLength": 50,
        "description": "The ID of the global network."
      },
      "siteId": {
        "type": "string",
        "pattern": "[\\s\\S]*",
        "minLength": 0,
        "maxLength": 50,
        "description": "The ID of the site."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "globalNetworkId",
      "siteId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://networkmanager.us-east-1.amazonaws.com/global-networks/{globalNetworkId}/sites/{siteId}"
    },
    "headers": {
