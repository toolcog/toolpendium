      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "DeviceId",
          "LinkId"
        ],
        "properties": {
          "DeviceId": {
            "description": "The ID of the device.",
            "type": "string",
            "pattern": "[\\s\\S]*",
            "minLength": 0,
            "maxLength": 50
          },
          "LinkId": {
            "description": "The ID of the link.",
            "type": "string",
            "pattern": "[\\s\\S]*",
            "minLength": 0,
