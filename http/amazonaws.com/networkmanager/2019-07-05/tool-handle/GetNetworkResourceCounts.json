      }
    },
    "required": [
      "globalNetworkId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://networkmanager.us-east-1.amazonaws.com/global-networks/{globalNetworkId}/network-resource-count{?resourceType,maxResults,nextToken,MaxResults,NextToken}"
