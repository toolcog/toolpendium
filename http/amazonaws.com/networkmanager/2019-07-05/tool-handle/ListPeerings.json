{
  "name": "ListPeerings",
  "description": "Lists the peerings for a core network.",
  "parameters": {
    "type": "object",
    "properties": {
      "coreNetworkId": {
        "type": "string",
        "pattern": "^core-network-([0-9a-f]{8,17})$",
        "minLength": 0,
        "maxLength": 50,
        "description": "The ID of a core network."
      },
      "peeringType": {
        "type": "string",
        "enum": [
          "TRANSIT_GATEWAY"
        ],
        "description": "Returns a list of a peering requests."
      },
      "edgeLocation": {
        "type": "string",
        "pattern": "[\\s\\S]*",
        "minLength": 1,
        "maxLength": 63,
        "description": "Returns a list edge locations for the "
      },
      "state": {
        "type": "string",
        "enum": [
          "CREATING",
          "FAILED",
          "AVAILABLE",
          "DELETING"
        ],
        "description": "Returns a list of the peering request states."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 500,
        "description": "The maximum number of results to return."
      },
      "nextToken": {
        "type": "string",
        "pattern": "[\\s\\S]*",
        "minLength": 0,
        "maxLength": 2048,
        "description": "The token for the next page of results."
      },
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://networkmanager.us-east-1.amazonaws.com/peerings{?coreNetworkId,peeringType,edgeLocation,state,maxResults,nextToken,MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Peerings**",
            "**NextToken**"
          ]
