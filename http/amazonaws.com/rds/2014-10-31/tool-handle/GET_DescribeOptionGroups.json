{
  "name": "GET_DescribeOptionGroups",
  "description": "Describes the available option groups.",
  "parameters": {
    "type": "object",
    "properties": {
      "OptionGroupName": {
        "type": "string",
        "description": "The name of the option group to describe. Can't be supplied together with EngineName or MajorEngineVersion."
