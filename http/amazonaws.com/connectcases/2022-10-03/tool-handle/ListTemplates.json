{
  "name": "ListTemplates",
  "description": "Lists all of the templates in a Cases domain. Each list item is a condensed summary object of the template. ",
  "parameters": {
    "type": "object",
    "properties": {
      "domainId": {
        "type": "string",
        "minLength": 1,
        "maxLength": 500,
        "description": "The unique identifier of the Cases domain. "
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100,
        "description": "The maximum number of results to return per page."
      },
      "nextToken": {
        "type": "string",
        "minLength": 0,
        "maxLength": 9000,
        "description": "The token for the next set of results. Use the value returned in the previous response in the next request to retrieve the next set of results."
      },
      "status": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TemplateStatus"
        },
        "minItems": 1,
        "maxItems": 2,
        "description": "A list of status values to filter on."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "domainId"
    ],
    "$defs": {
      "TemplateStatus": {
        "type": "string",
        "enum": [
          "Active",
          "Inactive"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://cases.us-east-1.amazonaws.com/domains/{domainId}/templates-list{?maxResults,nextToken,status}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**nextToken**",
            "**templates**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
