{
  "name": "CreateCustomPlugin",
  "description": "Creates a custom plugin using the specified properties.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
