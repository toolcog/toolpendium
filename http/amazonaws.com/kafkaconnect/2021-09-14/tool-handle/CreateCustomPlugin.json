      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "contentType",
          "location",
          "name"
        ],
        "properties": {
          "contentType": {
            "description": "The type of the plugin file.",
            "type": "string",
            "enum": [
              "JAR",
              "ZIP"
            ]
          },
          "description": {
            "description": "A summary description of the custom plugin.",
            "type": "string",
            "minLength": 0,
            "maxLength": 1024
          },
          "location": {
            "description": "Information about the location of a custom plugin.",
            "type": "object",
            "properties": {
              "s3Location": {
                "allOf": [
                  {
                    "$ref": "#/$defs/S3Location"
                  },
                  {
                    "description": "The S3 bucket Amazon Resource Name (ARN), file key, and object version of the plugin file stored in Amazon S3."
                  }
                ]
              }
            }
          },
          "name": {
            "description": "The name of the custom plugin.",
            "type": "string",
            "minLength": 1,
            "maxLength": 128
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "S3Location": {
        "type": "object",
        "required": [
          "bucketArn",
          "fileKey"
        ],
        "properties": {
          "bucketArn": {
            "allOf": [
              {
                "$ref": "#/$defs/__string"
              },
              {
                "description": "The Amazon Resource Name (ARN) of an S3 bucket."
              }
            ]
          },
          "fileKey": {
            "allOf": [
              {
                "$ref": "#/$defs/__string"
              },
              {
                "description": "The file key for an object in an S3 bucket."
              }
            ]
          },
          "objectVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/__string"
              },
              {
                "description": "The version of an object in an S3 bucket."
              }
            ]
          }
        },
        "description": "The location of an object in Amazon S3."
      },
      "__string": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://kafkaconnect.us-east-1.amazonaws.com/v1/custom-plugins"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
