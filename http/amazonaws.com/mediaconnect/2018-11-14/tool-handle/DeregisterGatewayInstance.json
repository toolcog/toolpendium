        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "gatewayInstanceArn"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://mediaconnect.us-east-1.amazonaws.com/v1/gateway-instances/{gatewayInstanceArn}{?force}"
