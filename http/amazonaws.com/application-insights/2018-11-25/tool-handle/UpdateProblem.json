{
  "name": "UpdateProblem",
  "description": "Updates the visibility of the problem or specifies the problem as <code>RESOLVED</code>.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "EC2WindowsBarleyService.UpdateProblem"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
