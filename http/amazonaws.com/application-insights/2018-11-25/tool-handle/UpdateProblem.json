        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateProblemRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateProblemRequest": {
        "type": "object",
        "required": [
          "ProblemId"
        ],
        "title": "UpdateProblemRequest",
        "properties": {
          "ProblemId": {
            "allOf": [
              {
                "$ref": "#/$defs/ProblemId"
              },
              {
                "description": "The ID of the problem."
              }
            ]
          },
          "UpdateStatus": {
            "allOf": [
              {
                "$ref": "#/$defs/UpdateStatus"
              },
              {
                "description": "The status of the problem. Arguments can be passed for only problems that show a status of <code>RECOVERING</code>."
              }
            ]
          },
          "Visibility": {
            "allOf": [
              {
                "$ref": "#/$defs/Visibility"
              },
              {
                "description": "The visibility of a problem. When you pass a value of <code>IGNORED</code>, the problem is removed from the default view, and all notifications for the problem are suspended. When <code>VISIBLE</code> is passed, the <code>IGNORED</code> action is reversed."
              }
            ]
          }
        }
      },
      "ProblemId": {
        "type": "string",
        "pattern": "p-[0-9a-fA-F]{8}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{4}\\-[0-9a-fA-F]{12}",
        "minLength": 38,
        "maxLength": 38
      },
      "UpdateStatus": {
        "type": "string",
        "enum": [
          "RESOLVED"
        ]
      },
      "Visibility": {
        "type": "string",
        "enum": [
          "IGNORED",
          "VISIBLE"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://applicationinsights.us-east-1.amazonaws.com/#X-Amz-Target=EC2WindowsBarleyService.UpdateProblem"
