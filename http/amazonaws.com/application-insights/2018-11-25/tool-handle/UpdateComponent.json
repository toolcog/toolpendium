    "$defs": {
      "UpdateComponentRequest": {
        "type": "object",
        "required": [
          "ResourceGroupName",
          "ComponentName"
        ],
        "title": "UpdateComponentRequest",
        "properties": {
          "ResourceGroupName": {
            "allOf": [
              {
                "$ref": "#/$defs/ResourceGroupName"
              },
              {
                "description": "The name of the resource group."
              }
            ]
          },
          "ComponentName": {
            "allOf": [
