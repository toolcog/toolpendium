                "$ref": "#/$defs/CustomComponentName"
              },
              {
                "description": "The name of the component."
              }
            ]
          },
          "NewComponentName": {
            "allOf": [
              {
                "$ref": "#/$defs/CustomComponentName"
              },
              {
                "description": "The new name of the component."
              }
            ]
          },
          "ResourceList": {
            "allOf": [
              {
                "$ref": "#/$defs/ResourceList"
              },
              {
                "description": "The list of resource ARNs that belong to the component."
              }
            ]
          }
        }
      },
      "ResourceGroupName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9\\.\\-_]*",
        "minLength": 1,
        "maxLength": 256
      },
      "CustomComponentName": {
        "type": "string",
        "pattern": "^[\\d\\w\\-_\\.+]*$",
        "minLength": 1,
        "maxLength": 128
      },
      "ResourceList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/ResourceARN"
        }
      },
      "ResourceARN": {
        "type": "string",
        "pattern": "^arn:aws(-\\w+)*:[\\w\\d-]+:([\\w\\d-]*)?:[\\w\\d_-]*([:/].+)*$",
        "minLength": 1,
        "maxLength": 1011
      }
    }
  },
