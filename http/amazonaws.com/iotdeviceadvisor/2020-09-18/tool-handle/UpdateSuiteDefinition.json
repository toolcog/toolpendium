{
  "name": "UpdateSuiteDefinition",
  "description": "<p>Updates a Device Advisor test suite.</p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">UpdateSuiteDefinition</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "suiteDefinitionId": {
        "type": "string",
        "minLength": 12,
        "maxLength": 36,
        "description": "Suite definition ID of the test suite to be updated."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "suiteDefinitionConfiguration"
        ],
        "properties": {
          "suiteDefinitionConfiguration": {
            "description": "Gets the suite definition configuration.",
            "type": "object",
            "properties": {
              "suiteDefinitionName": {
                "allOf": [
                  {
                    "$ref": "#/$defs/SuiteDefinitionName"
                  },
                  {
                    "description": "Gets the suite definition name. This is a required parameter."
                  }
                ]
              },
              "devices": {
                "allOf": [
                  {
                    "$ref": "#/$defs/DeviceUnderTestList"
                  },
                  {
                    "description": "Gets the devices configured."
                  }
                ]
              },
              "intendedForQualification": {
                "allOf": [
                  {
                    "$ref": "#/$defs/IntendedForQualificationBoolean"
                  },
                  {
                    "description": "Gets the tests intended for qualification in a suite."
                  }
                ]
              },
              "isLongDurationTest": {
                "allOf": [
                  {
                    "$ref": "#/$defs/IsLongDurationTestBoolean"
                  },
                  {
                    "description": "Verifies if the test suite is a long duration test."
                  }
                ]
              },
              "rootGroup": {
                "allOf": [
                  {
                    "$ref": "#/$defs/RootGroup"
                  },
                  {
                    "description": "<p>Gets the test suite root group. This is a required parameter. For updating or creating the latest qualification suite, if <code>intendedForQualification</code> is set to true, <code>rootGroup</code> can be an empty string. If <code>intendedForQualification</code> is false, <code>rootGroup</code> cannot be an empty string. If <code>rootGroup</code> is empty, and <code>intendedForQualification</code> is set to true, all the qualification tests are included, and the configuration is default.</p> <p> For a qualification suite, the minimum length is 0, and the maximum is 2048. For a non-qualification suite, the minimum length is 1, and the maximum is 2048. </p>"
                  }
                ]
              },
              "devicePermissionRoleArn": {
                "allOf": [
                  {
                    "$ref": "#/$defs/AmazonResourceName"
                  },
                  {
                    "description": "Gets the device permission ARN. This is a required parameter."
                  }
                ]
              },
              "protocol": {
                "allOf": [
                  {
                    "$ref": "#/$defs/Protocol"
                  },
                  {
                    "description": "Sets the MQTT protocol that is configured in the suite definition."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "suiteDefinitionId",
      "body"
    ],
    "$defs": {
      "SuiteDefinitionName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 256
      },
      "DeviceUnderTestList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/DeviceUnderTest"
        },
        "minItems": 0,
        "maxItems": 2
      },
      "DeviceUnderTest": {
        "type": "object",
        "properties": {
          "thingArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonResourceName"
              },
              {
                "description": "Lists device's thing ARN."
              }
            ]
          },
          "certificateArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonResourceName"
              },
              {
                "description": "Lists device's certificate ARN."
              }
            ]
          },
          "deviceRoleArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonResourceName"
              },
              {
                "description": "Lists device's role ARN."
              }
            ]
          }
        },
        "description": "Information of a test device. A thing ARN, certificate ARN or device role ARN is required."
      },
      "AmazonResourceName": {
        "type": "string",
        "minLength": 20,
        "maxLength": 2048
      },
      "IntendedForQualificationBoolean": {
        "type": "boolean"
      },
      "IsLongDurationTestBoolean": {
        "type": "boolean"
      },
      "RootGroup": {
        "type": "string",
        "minLength": 0,
        "maxLength": 2048
      },
      "Protocol": {
        "type": "string",
        "enum": [
          "MqttV3_1_1",
          "MqttV5",
          "MqttV3_1_1_OverWebSocket",
          "MqttV5_OverWebSocket"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "http://api.iotdeviceadvisor.us-east-1.amazonaws.com/suiteDefinitions/{suiteDefinitionId}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**suiteDefinitionId**",
            "**suiteDefinitionArn**",
            "**suiteDefinitionName**",
            "**suiteDefinitionVersion**",
            "**createdAt**",
            "**lastUpdatedAt**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
