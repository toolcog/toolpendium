{
  "name": "GetEndpoint",
  "description": "Gets information about an Device Advisor endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "thingArn": {
        "type": "string",
        "minLength": 20,
        "maxLength": 2048,
        "description": "The thing ARN of the device. This is an optional parameter."
      },
      "certificateArn": {
        "type": "string",
        "minLength": 20,
        "maxLength": 2048,
        "description": "The certificate ARN of the device. This is an optional parameter."
      },
      "deviceRoleArn": {
        "type": "string",
        "minLength": 20,
        "maxLength": 2048,
        "description": "The device role ARN of the device. This is an optional parameter."
      },
      "authenticationMethod": {
        "type": "string",
        "enum": [
          "X509ClientCertificate",
          "SignatureVersion4"
        ],
        "description": "The authentication method used during the device connection."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.iotdeviceadvisor.us-east-1.amazonaws.com/endpoint{?thingArn,certificateArn,deviceRoleArn,authenticationMethod}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**endpoint**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "482": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
