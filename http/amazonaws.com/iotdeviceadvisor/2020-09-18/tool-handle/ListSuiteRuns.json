{
  "name": "ListSuiteRuns",
  "description": "<p>Lists runs of the specified Device Advisor test suite. You can list all runs of the test suite, or the runs of a specific version of the test suite.</p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">ListSuiteRuns</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "suiteDefinitionId": {
        "type": "string",
        "minLength": 12,
        "maxLength": 36,
        "description": "Lists the test suite runs of the specified test suite based on suite definition ID."
      },
      "suiteDefinitionVersion": {
        "type": "string",
        "minLength": 2,
        "maxLength": 255,
        "description": "Must be passed along with <code>suiteDefinitionId</code>. Lists the test suite runs of the specified test suite based on suite definition version."
      },
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 50,
        "description": "The maximum number of results to return at once."
      },
      "nextToken": {
        "type": "string",
        "maxLength": 2000,
        "description": "A token to retrieve the next set of results."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.iotdeviceadvisor.us-east-1.amazonaws.com/suiteRuns{?suiteDefinitionId,suiteDefinitionVersion,maxResults,nextToken}"
    },
    "headers": {
