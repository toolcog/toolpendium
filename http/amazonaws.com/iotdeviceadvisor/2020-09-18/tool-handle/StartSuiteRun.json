{
  "name": "StartSuiteRun",
  "description": "<p>Starts a Device Advisor test suite run.</p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">StartSuiteRun</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "suiteDefinitionId": {
        "type": "string",
        "minLength": 12,
        "maxLength": 36,
        "description": "Suite definition ID of the test suite."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "suiteRunConfiguration"
        ],
        "properties": {
          "suiteDefinitionVersion": {
            "description": "Suite definition version of the test suite.",
            "type": "string",
            "minLength": 2,
            "maxLength": 255
          },
          "suiteRunConfiguration": {
            "description": "Gets suite run configuration.",
            "type": "object",
            "properties": {
              "primaryDevice": {
                "allOf": [
                  {
                    "$ref": "#/$defs/DeviceUnderTest"
                  },
                  {
                    "description": "Sets the primary device for the test suite run. This requires a thing ARN or a certificate ARN."
                  }
                ]
              },
              "selectedTestList": {
                "allOf": [
                  {
                    "$ref": "#/$defs/SelectedTestList"
                  },
                  {
                    "description": "Sets test case list."
                  }
                ]
              },
              "parallelRun": {
                "allOf": [
                  {
                    "$ref": "#/$defs/ParallelRun"
                  },
                  {
                    "description": "TRUE if multiple test suites run in parallel."
                  }
                ]
              }
            }
          },
          "tags": {
            "description": "The tags to be attached to the suite run.",
            "type": "object",
            "minProperties": 0,
            "maxProperties": 50,
            "additionalProperties": {
              "$ref": "#/$defs/String256"
            }
          }
        }
      }
    },
    "required": [
      "suiteDefinitionId",
      "body"
    ],
    "$defs": {
      "DeviceUnderTest": {
        "type": "object",
        "properties": {
          "thingArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonResourceName"
              },
              {
                "description": "Lists device's thing ARN."
              }
            ]
          },
          "certificateArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonResourceName"
              },
              {
                "description": "Lists device's certificate ARN."
              }
            ]
          },
          "deviceRoleArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonResourceName"
              },
              {
                "description": "Lists device's role ARN."
              }
            ]
          }
        },
        "description": "Information of a test device. A thing ARN, certificate ARN or device role ARN is required."
      },
      "AmazonResourceName": {
        "type": "string",
        "minLength": 20,
        "maxLength": 2048
      },
      "SelectedTestList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/UUID"
        },
        "minItems": 0,
        "maxItems": 100
      },
      "UUID": {
        "type": "string",
        "minLength": 12,
        "maxLength": 36
      },
      "ParallelRun": {
        "type": "boolean"
      },
      "String256": {
        "type": "string",
        "minLength": 1,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.iotdeviceadvisor.us-east-1.amazonaws.com/suiteDefinitions/{suiteDefinitionId}/suiteRuns"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**suiteRunId**",
            "**suiteRunArn**",
            "**createdAt**",
            "**endpoint**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "482": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
