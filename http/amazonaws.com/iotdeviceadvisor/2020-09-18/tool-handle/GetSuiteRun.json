      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "suiteDefinitionId",
      "suiteRunId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://api.iotdeviceadvisor.us-east-1.amazonaws.com/suiteDefinitions/{suiteDefinitionId}/suiteRuns/{suiteRunId}"
