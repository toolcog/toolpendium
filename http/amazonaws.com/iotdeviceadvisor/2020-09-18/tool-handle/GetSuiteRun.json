{
  "name": "GetSuiteRun",
  "description": "<p>Gets information about a Device Advisor test suite run.</p> <p>Requires permission to access the <a href=\"https://docs.aws.amazon.com/service-authorization/latest/reference/list_awsiot.html#awsiot-actions-as-permissions\">GetSuiteRun</a> action.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "suiteDefinitionId": {
        "type": "string",
        "minLength": 12,
        "maxLength": 36,
        "description": "Suite definition ID for the test suite run."
      },
      "suiteRunId": {
        "type": "string",
        "minLength": 12,
        "maxLength": 36,
        "description": "Suite run ID for the test suite run."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
