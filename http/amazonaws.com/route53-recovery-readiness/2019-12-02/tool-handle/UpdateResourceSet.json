            "pattern": "AWS::[A-Za-z0-9]+::[A-Za-z0-9]+"
          },
          "resources": {
            "description": "A list of resource objects.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/Resource"
            }
          }
        }
