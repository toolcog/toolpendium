        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "ControlPanelArn",
          "ControlPanelName"
        ],
        "properties": {
          "ControlPanelArn": {
            "description": "The Amazon Resource Name (ARN) of the control panel.",
            "type": "string",
            "pattern": "^[A-Za-z0-9:\\/_-]*$",
            "minLength": 1,
            "maxLength": 256
          },
          "ControlPanelName": {
            "description": "The name of the control panel.",
            "type": "string",
            "pattern": "^\\S+$",
            "minLength": 1,
            "maxLength": 64
          }
        }
