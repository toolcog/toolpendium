    "$defs": {
      "DeleteOrganizationalUnitRequest": {
        "type": "object",
        "required": [
          "OrganizationalUnitId"
        ],
        "title": "DeleteOrganizationalUnitRequest",
        "properties": {
          "OrganizationalUnitId": {
            "allOf": [
              {
                "$ref": "#/$defs/OrganizationalUnitId"
              },
              {
                "description": "<p>The unique identifier (ID) of the organizational unit that you want to delete. You can get the ID from the <a>ListOrganizationalUnitsForParent</a> operation.</p> <p>The <a href=\"http://wikipedia.org/wiki/regex\">regex pattern</a> for an organizational unit ID string requires \"ou-\" followed by from 4 to 32 lowercase letters or digits (the ID of the root that contains the OU). This string is followed by a second \"-\" dash and from 8 to 32 additional lowercase letters or digits.</p>"
              }
            ]
          }
        }
      },
      "OrganizationalUnitId": {
        "type": "string",
        "pattern": "^ou-[0-9a-z]{4,32}-[a-z0-9]{8,32}$",
        "maxLength": 68
      }
    }
  },
  "handler": "http",
  "request": {
