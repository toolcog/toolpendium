            "allOf": [
              {
                "$ref": "#/$defs/PolicyId"
              },
              {
                "description": "<p>The unique identifier (ID) of the policy that you want to delete. You can get the ID from the <a>ListPolicies</a> or <a>ListPoliciesForTarget</a> operations.</p> <p>The <a href=\"http://wikipedia.org/wiki/regex\">regex pattern</a> for a policy ID string requires \"p-\" followed by from 8 to 128 lowercase or uppercase letters, digits, or the underscore character (_).</p>"
              }
            ]
          }
        }
      },
      "PolicyId": {
        "type": "string",
        "pattern": "^p-[0-9a-zA-Z_]{8,128}$",
        "maxLength": 130
      }
    }
