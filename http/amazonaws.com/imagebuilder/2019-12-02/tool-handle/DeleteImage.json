        "type": "string",
        "pattern": "^arn:aws[^:]*:imagebuilder:[^:]+:(?:[0-9]{12}|aws):image/[a-z0-9-_]+/[0-9]+\\.[0-9]+\\.[0-9]+/[0-9]+$",
        "description": "The Amazon Resource Name (ARN) of the Image Builder image resource to delete."
      },
      "X-Amz-Content-Sha256": {
