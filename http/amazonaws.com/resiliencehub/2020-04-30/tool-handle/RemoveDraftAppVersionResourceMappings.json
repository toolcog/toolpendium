{
  "name": "RemoveDraftAppVersionResourceMappings",
  "description": "Removes resource mappings from a draft application version.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
