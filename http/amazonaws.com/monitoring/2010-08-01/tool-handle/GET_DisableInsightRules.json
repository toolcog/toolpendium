{
  "name": "GET_DisableInsightRules",
  "description": "Disables the specified Contributor Insights rules. When rules are disabled, they do not analyze log groups and do not incur costs.",
  "parameters": {
    "type": "object",
    "properties": {
      "RuleNames": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/InsightRuleName"
        },
        "description": "An array of the rule names to disable. If you need to find out the names of your rules, use <a href=\"https://docs.aws.amazon.com/AmazonCloudWatch/latest/APIReference/API_DescribeInsightRules.html\">DescribeInsightRules</a>."
      },
      "Action": {
        "type": "string",
        "enum": [
          "DisableInsightRules"
        ]
      },
      "Version": {
        "type": "string",
        "enum": [
          "2010-08-01"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "RuleNames",
      "Action",
      "Version"
    ],
    "$defs": {
      "InsightRuleName": {
        "type": "string",
        "pattern": "[\\x20-\\x7E]+",
        "minLength": 1,
        "maxLength": 128
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
