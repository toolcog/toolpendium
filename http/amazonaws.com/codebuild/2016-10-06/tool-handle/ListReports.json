{
  "name": "ListReports",
  "description": " Returns a list of ARNs for the reports in the current Amazon Web Services account. ",
  "parameters": {
    "type": "object",
    "properties": {
      "maxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "nextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "CodeBuild_20161006.ListReports"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
