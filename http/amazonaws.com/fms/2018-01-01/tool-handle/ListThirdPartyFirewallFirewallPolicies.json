{
  "name": "ListThirdPartyFirewallFirewallPolicies",
  "description": "Retrieves a list of all of the third-party firewall policies that are associated with the third-party firewall administrator's account.",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSFMS_20180101.ListThirdPartyFirewallFirewallPolicies"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
