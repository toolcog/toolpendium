        "maxItems": 6
      },
      "SubnetId": {
        "type": "string",
        "pattern": "^subnet-([0-9a-f]{8}|[0-9a-f]{17})$",
        "minLength": 1,
        "maxLength": 32
      },
      "VpcId": {
        "type": "string",
