      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://workdocs.us-east-1.amazonaws.com/api/v1/activities{?startTime,endTime,organizationId,activityTypes,resourceId,userId,includeIndirectActivities,limit,marker,Limit,Marker}"
    },
    "headers": {
