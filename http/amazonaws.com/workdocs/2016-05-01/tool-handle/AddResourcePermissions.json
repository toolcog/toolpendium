{
  "name": "AddResourcePermissions",
  "description": "Creates a set of permissions for the specified folder or document. The resource permissions are overwritten if the principals already have different permissions.",
  "parameters": {
    "type": "object",
    "properties": {
      "Authentication": {
        "type": "string",
        "minLength": 1,
        "maxLength": 8199,
        "format": "password",
        "description": "Amazon WorkDocs authentication token. Not required when using Amazon Web Services administrator credentials to access the API."
      },
      "ResourceId": {
        "type": "string",
        "pattern": "[\\w+-.@]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "The ID of the resource."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Principals"
        ],
        "properties": {
          "Principals": {
            "description": "The users, groups, or organization being granted permission.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/SharePrincipal"
            }
          },
          "NotificationOptions": {
            "description": "Set of options which defines notification preferences of given action.",
            "type": "object",
            "properties": {
              "SendEmail": {
                "allOf": [
                  {
                    "$ref": "#/$defs/BooleanType"
                  },
                  {
                    "description": "Boolean value to indicate an email notification should be sent to the recipients."
                  }
                ]
              },
              "EmailMessage": {
                "allOf": [
                  {
                    "$ref": "#/$defs/MessageType"
                  },
                  {
                    "description": "Text value to be included in the email body."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "ResourceId",
      "body"
    ],
    "$defs": {
      "SharePrincipal": {
        "type": "object",
        "required": [
          "Id",
          "Type",
          "Role"
        ],
        "properties": {
          "Id": {
            "allOf": [
              {
                "$ref": "#/$defs/IdType"
              },
              {
                "description": "The ID of the recipient."
              }
            ]
          },
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/PrincipalType"
              },
              {
                "description": "The type of the recipient."
              }
            ]
          },
          "Role": {
            "allOf": [
              {
                "$ref": "#/$defs/RoleType"
              },
              {
                "description": "The role of the recipient."
              }
            ]
          }
        },
        "description": "Describes the recipient type and ID, if available."
      },
      "IdType": {
        "type": "string",
        "pattern": "[&\\w+-.@]+",
        "minLength": 1,
        "maxLength": 256
      },
      "PrincipalType": {
        "type": "string",
        "enum": [
          "USER",
          "GROUP",
          "INVITE",
          "ANONYMOUS",
          "ORGANIZATION"
        ]
      },
      "RoleType": {
        "type": "string",
        "enum": [
          "VIEWER",
          "CONTRIBUTOR",
          "OWNER",
          "COOWNER"
        ]
      },
      "BooleanType": {
        "type": "boolean"
      },
      "MessageType": {
        "type": "string",
        "minLength": 0,
        "maxLength": 2048,
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://workdocs.us-east-1.amazonaws.com/api/v1/resources/{ResourceId}/permissions"
    },
    "headers": {
      "Authentication": {
        "$": "Authentication"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
