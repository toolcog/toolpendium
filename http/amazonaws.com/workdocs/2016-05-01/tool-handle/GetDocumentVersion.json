{
  "name": "GetDocumentVersion",
  "description": "Retrieves version metadata for the specified document.",
  "parameters": {
    "type": "object",
    "properties": {
      "Authentication": {
        "type": "string",
        "minLength": 1,
        "maxLength": 8199,
        "format": "password",
        "description": "Amazon WorkDocs authentication token. Not required when using Amazon Web Services administrator credentials to access the API."
      },
      "DocumentId": {
        "type": "string",
        "pattern": "[\\w+-.@]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "The ID of the document."
      },
      "VersionId": {
        "type": "string",
        "pattern": "[\\w+-.@]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "The version ID of the document."
      },
      "fields": {
        "type": "string",
        "pattern": "[\\w,]+",
        "minLength": 1,
        "maxLength": 256,
        "description": "A comma-separated list of values. Specify \"SOURCE\" to include a URL for the source document."
      },
      "includeCustomMetadata": {
        "type": "boolean",
        "description": "Set this to TRUE to include custom metadata in the response."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "DocumentId",
      "VersionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://workdocs.us-east-1.amazonaws.com/api/v1/documents/{DocumentId}/versions/{VersionId}{?fields,includeCustomMetadata}"
    },
    "headers": {
      "Authentication": {
        "$": "Authentication"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Metadata**",
            "**CustomMetadata**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
