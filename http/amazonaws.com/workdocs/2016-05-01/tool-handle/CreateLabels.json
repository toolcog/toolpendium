{
  "name": "CreateLabels",
  "description": "Adds the specified list of labels to the given resource (a document or folder)",
  "parameters": {
    "type": "object",
    "properties": {
      "ResourceId": {
        "type": "string",
        "pattern": "[\\w+-.@]+",
        "minLength": 1,
        "maxLength": 128,
        "description": "The ID of the resource."
      },
      "Authentication": {
        "type": "string",
        "minLength": 1,
        "maxLength": 8199,
        "format": "password",
        "description": "Amazon WorkDocs authentication token. Not required when using Amazon Web Services administrator credentials to access the API."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Labels"
        ],
        "properties": {
          "Labels": {
            "description": "List of labels to add to the resource.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/SharedLabel"
            },
            "maxItems": 20
          }
        }
      }
    },
    "required": [
      "ResourceId",
      "body"
    ],
    "$defs": {
      "SharedLabel": {
        "type": "string",
        "pattern": "[a-zA-Z0-9._+-/=][a-zA-Z0-9 ._+-/=]*",
        "minLength": 1,
        "maxLength": 32
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://workdocs.us-east-1.amazonaws.com/api/v1/resources/{ResourceId}/labels"
    },
    "headers": {
      "Authentication": {
        "$": "Authentication"
      },
