{
  "name": "UntagResource",
  "description": "Removes specified tags from an AWS OpsWorks-CM server or backup.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "OpsWorksCM_V2016_11_01.UntagResource"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UntagResourceRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UntagResourceRequest": {
        "type": "object",
        "required": [
          "ResourceArn",
          "TagKeys"
        ],
        "title": "UntagResourceRequest",
        "properties": {
          "ResourceArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AWSOpsWorksCMResourceArn"
              },
              {
                "description": "The Amazon Resource Number (ARN) of a resource from which you want to remove tags. For example, <code>arn:aws:opsworks-cm:us-west-2:123456789012:server/test-owcm-server/EXAMPLE-66b0-4196-8274-d1a2bEXAMPLE</code>."
              }
            ]
          },
          "TagKeys": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKeyList"
              },
              {
                "description": "The keys of tags that you want to remove."
              }
            ]
          }
        }
      },
      "AWSOpsWorksCMResourceArn": {
        "type": "string",
        "pattern": "arn:aws.*:opsworks-cm:.*:[0-9]{12}:.*"
      },
      "TagKeyList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TagKey"
        },
        "minItems": 0,
        "maxItems": 200
      },
      "TagKey": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 1,
        "maxLength": 128
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://opsworks-cm.us-east-1.amazonaws.com/#X-Amz-Target=OpsWorksCM_V2016_11_01.UntagResource"
    },
    "headers": {
