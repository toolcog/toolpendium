      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "TransactionId": {
            "description": "The transaction to extend.",
            "type": "string",
            "pattern": "[\\p{L}\\p{N}\\p{P}]*",
            "minLength": 1,
            "maxLength": 255
          }
        }
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
