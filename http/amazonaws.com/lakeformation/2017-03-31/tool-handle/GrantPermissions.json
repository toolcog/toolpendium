        "properties": {
          "CatalogId": {
            "allOf": [
              {
                "$ref": "#/$defs/CatalogIdString"
              },
              {
                "description": "The identifier for the Data Catalog where the location is registered with Lake Formation. By default, it is the account ID of the caller."
              }
