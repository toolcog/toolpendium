            ]
          },
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/NameString"
              },
              {
                "description": "The name of the data cells filter. "
              }
            ]
          }
        },
        "description": "A structure for a data cells filter resource. "
      },
