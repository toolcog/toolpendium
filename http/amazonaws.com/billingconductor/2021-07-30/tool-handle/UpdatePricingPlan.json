{
  "name": "UpdatePricingPlan",
  "description": "This updates an existing pricing plan. ",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
