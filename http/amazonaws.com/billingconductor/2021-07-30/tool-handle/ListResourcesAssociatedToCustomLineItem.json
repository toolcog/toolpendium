{
  "name": "ListResourcesAssociatedToCustomLineItem",
  "description": " List the resources that are associated to a custom line item. ",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Arn"
        ],
        "properties": {
          "BillingPeriod": {
            "description": " The billing period for which the resource associations will be listed. ",
            "type": "string",
            "pattern": "\\d{4}-(0?[1-9]|1[012])"
          },
          "Arn": {
            "description": " The ARN of the custom line item for which the resource associations will be listed. ",
            "type": "string",
            "pattern": "(arn:aws(-cn)?:billingconductor::[0-9]{12}:customlineitem/)?[a-zA-Z0-9]{10}"
          },
          "MaxResults": {
            "description": " (Optional) The maximum number of resource associations to be retrieved. ",
            "type": "integer",
            "minimum": 1,
            "maximum": 100
          },
          "NextToken": {
            "description": " (Optional) The pagination token that's returned by a previous request. ",
            "type": "string"
          },
          "Filters": {
            "description": " A filter that specifies the type of resource associations that should be retrieved for a custom line item. ",
            "type": "object",
            "properties": {
              "Relationship": {
                "allOf": [
                  {
                    "$ref": "#/$defs/CustomLineItemRelationship"
                  },
                  {
                    "description": " The type of relationship between the custom line item and the associated resource. "
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "CustomLineItemRelationship": {
        "type": "string",
        "enum": [
          "PARENT",
          "CHILD"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://billingconductor.us-east-1.amazonaws.com/list-resources-associated-to-custom-line-item{?MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
