{
  "name": "UpdateBillingGroup",
  "description": "This updates an existing billing group. ",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Arn"
        ],
        "properties": {
          "Arn": {
            "description": "The Amazon Resource Name (ARN) of the billing group being updated. ",
            "type": "string",
            "pattern": "(arn:aws(-cn)?:billingconductor::[0-9]{12}:billinggroup/)?[0-9]{12}"
          },
          "Name": {
            "description": "The name of the billing group. The names must be unique to each billing group. ",
            "type": "string",
            "pattern": "[a-zA-Z0-9_\\+=\\.\\-@]+",
            "minLength": 1,
            "maxLength": 128,
            "format": "password"
          },
          "Status": {
            "description": "The status of the billing group. Only one of the valid values can be used. ",
            "type": "string",
            "enum": [
              "ACTIVE",
              "PRIMARY_ACCOUNT_MISSING"
            ]
          },
          "ComputationPreference": {
            "description": "The preferences and settings that will be used to compute the Amazon Web Services charges for a billing group.",
            "type": "object",
            "properties": {
              "PricingPlanArn": {
                "allOf": [
                  {
                    "$ref": "#/$defs/PricingPlanFullArn"
                  },
                  {
                    "description": " The Amazon Resource Name (ARN) of the pricing plan that's used to compute the Amazon Web Services charges for a billing group. "
                  }
                ]
              }
            }
          },
          "Description": {
            "description": "A description of the billing group. ",
            "type": "string",
            "minLength": 0,
            "maxLength": 1024,
            "format": "password"
          },
          "AccountGrouping": {
            "description": "Specifies if the billing group has the following features enabled.",
            "type": "object",
            "properties": {
              "AutoAssociate": {
                "allOf": [
                  {
                    "$ref": "#/$defs/Boolean"
                  },
                  {
                    "description": "Specifies if this billing group will automatically associate newly added Amazon Web Services accounts that join your consolidated billing family."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "PricingPlanFullArn": {
        "type": "string",
        "pattern": "arn:aws(-cn)?:billingconductor::[0-9]{12}:pricingplan/[a-zA-Z0-9]{10}"
      },
      "Boolean": {
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://billingconductor.us-east-1.amazonaws.com/update-billing-group"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
