      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "InstanceId",
      "UserId"
    ]
  },
  "handler": "http",
  "request": {
