      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/RollbackApplicationRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "RollbackApplicationRequest": {
        "type": "object",
        "required": [
          "ApplicationName",
          "CurrentApplicationVersionId"
        ],
        "title": "RollbackApplicationRequest",
