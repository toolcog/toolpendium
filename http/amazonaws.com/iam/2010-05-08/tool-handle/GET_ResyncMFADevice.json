      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "UserName",
      "SerialNumber",
      "AuthenticationCode1",
      "AuthenticationCode2",
      "Action",
      "Version"
    ]
  },
  "handler": "http",
  "request": {
