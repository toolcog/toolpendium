          "ApplicationName",
          "CurrentApplicationVersionId",
          "CloudWatchLoggingOption"
        ],
        "title": "AddApplicationCloudWatchLoggingOptionRequest",
        "properties": {
          "ApplicationName": {
            "allOf": [
              {
                "$ref": "#/$defs/ApplicationName"
              },
              {
                "description": "The Kinesis Analytics application name."
              }
            ]
          },
          "CurrentApplicationVersionId": {
            "allOf": [
              {
                "$ref": "#/$defs/ApplicationVersionId"
              },
              {
                "description": "The version ID of the Kinesis Analytics application."
              }
            ]
          },
          "CloudWatchLoggingOption": {
            "allOf": [
              {
                "$ref": "#/$defs/CloudWatchLoggingOption"
              },
              {
                "description": "Provides the CloudWatch log stream Amazon Resource Name (ARN) and the IAM role ARN. Note: To write application messages to CloudWatch, the IAM role that is used must have the <code>PutLogEvents</code> policy action enabled."
              }
            ]
          }
        }
      },
      "ApplicationName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_.-]+",
        "minLength": 1,
        "maxLength": 128
      },
      "ApplicationVersionId": {
        "type": "integer",
        "minimum": 1,
        "maximum": 999999999
      },
      "CloudWatchLoggingOption": {
        "type": "object",
        "required": [
          "LogStreamARN",
          "RoleARN"
        ],
        "properties": {
          "LogStreamARN": {
            "allOf": [
              {
                "$ref": "#/$defs/LogStreamARN"
              },
              {
                "description": "ARN of the CloudWatch log to receive application messages."
              }
            ]
          },
          "RoleARN": {
            "allOf": [
              {
                "$ref": "#/$defs/RoleARN"
              },
              {
                "description": "IAM ARN of the role to use to send application messages. Note: To write application messages to CloudWatch, the IAM role that is used must have the <code>PutLogEvents</code> policy action enabled."
              }
            ]
          }
        },
        "description": "Provides a description of CloudWatch logging options, including the log stream Amazon Resource Name (ARN) and the role ARN."
      },
      "LogStreamARN": {
        "type": "string",
        "pattern": "arn:.*",
        "minLength": 1,
        "maxLength": 2048
      },
      "RoleARN": {
        "type": "string",
        "pattern": "arn:aws:iam::\\d{12}:role/?[a-zA-Z_0-9+=,.@\\-_/]+",
        "minLength": 1,
        "maxLength": 2048
      }
    }
  },
