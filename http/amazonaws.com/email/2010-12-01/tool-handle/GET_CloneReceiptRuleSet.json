      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "RuleSetName",
      "OriginalRuleSetName",
      "Action",
      "Version"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
