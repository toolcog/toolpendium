      "NextToken": {
        "type": "string",
        "description": "An array the contains the name and creation time stamp for each template in your Amazon SES account."
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 50,
        "description": "The maximum number of custom verification email templates to return. This value must be at least 1 and less than or equal to 50. If you do not specify a value, or if you specify a value less than 1 or greater than 50, the operation will return up to 50 results."
      },
      "Action": {
        "type": "string",
