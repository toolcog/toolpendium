{
  "name": "GET_UpdateConfigurationSetReputationMetricsEnabled",
  "description": "<p>Enables or disables the publishing of reputation metrics for emails sent using a specific configuration set in a given AWS Region. Reputation metrics include bounce and complaint rates. These metrics are published to Amazon CloudWatch. By using CloudWatch, you can create alarms when bounce or complaint rates exceed certain thresholds.</p> <p>You can execute this operation no more than once per second.</p>",
  "parameters": {
    "type": "object",
    "properties": {
      "ConfigurationSetName": {
        "type": "string",
        "description": "The name of the configuration set that you want to update."
      },
      "Enabled": {
        "type": "boolean",
        "description": "Describes whether or not Amazon SES will publish reputation metrics for the configuration set, such as bounce and complaint rates, to Amazon CloudWatch."
      },
      "Action": {
        "type": "string",
        "enum": [
          "UpdateConfigurationSetReputationMetricsEnabled"
        ]
      },
      "Version": {
        "type": "string",
        "enum": [
          "2010-12-01"
        ]
      },
      "X-Amz-Content-Sha256": {
