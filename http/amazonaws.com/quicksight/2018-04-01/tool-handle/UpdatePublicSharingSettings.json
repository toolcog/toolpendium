      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "PublicSharingEnabled": {
            "description": "A Boolean value that indicates whether public sharing is turned on for an Amazon QuickSight account.",
            "type": "boolean"
          }
        }
      }
    },
    "required": [
      "AwsAccountId",
      "body"
    ]
  },
  "handler": "http",
