{
  "name": "UpdateTopicRefreshSchedule",
  "description": "Updates a topic refresh schedule.",
  "parameters": {
    "type": "object",
    "properties": {
      "AwsAccountId": {
        "type": "string",
        "pattern": "^[0-9]{12}$",
        "minLength": 12,
        "maxLength": 12,
        "description": "The ID of the Amazon Web Services account that contains the topic whose refresh schedule you want to update."
      },
      "TopicId": {
        "type": "string",
        "pattern": "^[A-Za-z0-9-_.\\\\+]*$",
        "maxLength": 256,
        "description": "The ID of the topic that you want to modify. This ID is unique per Amazon Web Services Region for each Amazon Web Services account."
      },
      "DatasetId": {
        "type": "string",
        "description": "The ID of the dataset."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "RefreshSchedule"
        ],
        "properties": {
          "RefreshSchedule": {
            "description": "A structure that represents a topic refresh schedule.",
            "type": "object",
            "properties": {
              "IsEnabled": {
                "allOf": [
                  {
                    "$ref": "#/$defs/NullableBoolean"
                  },
                  {
                    "description": "A Boolean value that controls whether to schedule is enabled."
                  }
                ]
              },
              "BasedOnSpiceSchedule": {
                "allOf": [
                  {
                    "$ref": "#/$defs/Boolean"
                  },
                  {
                    "description": "A Boolean value that controls whether to schedule runs at the same schedule that is specified in SPICE dataset."
                  }
                ]
              },
              "StartingAt": {
                "allOf": [
                  {
                    "$ref": "#/$defs/Timestamp"
                  },
                  {
                    "description": "The starting date and time for the refresh schedule."
                  }
                ]
              },
              "Timezone": {
                "allOf": [
                  {
                    "$ref": "#/$defs/LimitedString"
                  },
                  {
                    "description": "The timezone that you want the refresh schedule to use."
                  }
                ]
              },
              "RepeatAt": {
                "allOf": [
                  {
                    "$ref": "#/$defs/LimitedString"
                  },
                  {
                    "description": "The time of day when the refresh should run, for example, Monday-Sunday."
                  }
                ]
              },
              "TopicScheduleType": {
                "allOf": [
                  {
                    "$ref": "#/$defs/TopicScheduleType"
                  },
                  {
                    "description": "The type of refresh schedule. Valid values for this structure are <code>HOURLY</code>, <code>DAILY</code>, <code>WEEKLY</code>, and <code>MONTHLY</code>."
                  }
                ]
              }
            }
          }
        }
      }
    },
    "required": [
      "AwsAccountId",
      "TopicId",
      "DatasetId",
      "body"
    ],
    "$defs": {
      "NullableBoolean": {
        "type": "boolean"
      },
      "Boolean": {
        "type": "boolean"
      },
      "Timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "LimitedString": {
        "type": "string",
        "maxLength": 256
      },
      "TopicScheduleType": {
        "type": "string",
        "enum": [
          "HOURLY",
          "DAILY",
          "WEEKLY",
          "MONTHLY"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://quicksight.us-east-1.amazonaws.com/accounts/{AwsAccountId}/topics/{TopicId}/schedules/{DatasetId}"
    },
    "headers": {
