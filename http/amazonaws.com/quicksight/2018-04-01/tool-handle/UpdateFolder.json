        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "Name"
        ],
        "properties": {
          "Name": {
            "description": "The name of the folder.",
            "type": "string",
            "minLength": 1,
            "maxLength": 200
          }
        }
      }
    },
    "required": [
      "AwsAccountId",
      "FolderId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
