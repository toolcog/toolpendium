      }
    },
    "required": [
      "AwsAccountId",
      "AnalysisId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://quicksight.us-east-1.amazonaws.com/accounts/{AwsAccountId}/analyses/{AnalysisId}/definition"
