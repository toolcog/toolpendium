{
  "name": "ListChannelsAssociatedWithChannelFlow",
  "description": "Lists all channels associated with a specified channel flow. You can associate a channel flow with multiple channels, but you can only associate a channel with one channel flow. This is a developer API.",
  "parameters": {
    "type": "object",
    "properties": {
      "channel-flow-arn": {
        "type": "string",
        "pattern": "arn:[a-z0-9-\\.]{1,63}:[a-z0-9-\\.]{0,63}:[a-z0-9-\\.]{0,63}:[a-z0-9-\\.]{0,63}:[^/].{0,1023}",
        "minLength": 5,
        "maxLength": 1600,
        "description": "The ARN of the channel flow."
      },
      "max-results": {
        "type": "integer",
        "minimum": 1,
        "maximum": 50,
        "description": "The maximum number of channels that you want to return."
      },
      "next-token": {
        "type": "string",
        "pattern": ".*",
        "minLength": 0,
        "maxLength": 2048,
        "format": "password",
        "description": "The token passed by previous API calls until all requested channels are returned."
      },
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "scope": {
        "type": "string",
        "enum": [
          "channel-flow-associations"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "channel-flow-arn",
      "scope"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://messaging-chime.us-east-1.amazonaws.com/channels#scope=channel-flow-associations&channel-flow-arn{?channel-flow-arn,max-results,next-token,MaxResults,NextToken,scope}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
