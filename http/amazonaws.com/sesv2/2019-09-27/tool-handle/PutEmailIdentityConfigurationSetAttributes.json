{
  "name": "PutEmailIdentityConfigurationSetAttributes",
  "description": "Used to associate a configuration set with an email identity.",
  "parameters": {
    "type": "object",
    "properties": {
      "EmailIdentity": {
        "type": "string",
        "minLength": 1,
        "description": "The email address or domain to associate with a configuration set."
