{
  "name": "PutDedicatedIpPoolScalingAttributes",
  "description": "<p>Used to convert a dedicated IP pool to a different scaling mode.</p> <note> <p> <code>MANAGED</code> pools cannot be converted to <code>STANDARD</code> scaling mode.</p> </note>",
  "parameters": {
    "type": "object",
    "properties": {
      "PoolName": {
        "type": "string",
        "description": "The name of the dedicated IP pool."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
