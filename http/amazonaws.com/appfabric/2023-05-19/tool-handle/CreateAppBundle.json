{
  "name": "CreateAppBundle",
  "description": "Creates an app bundle to collect data from an application using AppFabric.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
