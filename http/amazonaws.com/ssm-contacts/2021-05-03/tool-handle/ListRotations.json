      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ListRotationsRequest": {
        "type": "object",
        "title": "ListRotationsRequest",
        "properties": {
          "RotationNamePrefix": {
            "allOf": [
              {
                "$ref": "#/$defs/RotationName"
              },
              {
                "description": "A filter to include rotations in list results based on their common prefix. For example, entering prod returns a list of all rotation names that begin with <code>prod</code>, such as <code>production</code> and <code>prod-1</code>."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/PaginationToken"
              },
              {
                "description": "A token to start the list. Use this token to get the next set of results."
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of items to return for this call. The call also returns a token that you can specify in a subsequent call to get the next set of results."
              }
            ]
          }
        }
      },
      "RotationName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_\\-\\s\\.]*$",
        "minLength": 1,
        "maxLength": 255
      },
      "PaginationToken": {
        "type": "string",
        "pattern": "^[\\\\\\/a-zA-Z0-9_+=\\-]*$",
        "maxLength": 2048
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 0,
        "maximum": 1024
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://ssm-contacts.us-east-1.amazonaws.com/#X-Amz-Target=SSMContacts.ListRotations{?MaxResults,NextToken}"
    },
    "headers": {
