        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ActivateContactChannelRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ActivateContactChannelRequest": {
        "type": "object",
        "required": [
          "ContactChannelId",
          "ActivationCode"
        ],
        "title": "ActivateContactChannelRequest",
        "properties": {
          "ContactChannelId": {
            "allOf": [
              {
                "$ref": "#/$defs/SsmContactsArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the contact channel."
              }
            ]
          },
          "ActivationCode": {
            "allOf": [
              {
                "$ref": "#/$defs/ActivationCode"
              },
              {
                "description": "The code sent to the contact channel when it was created in the contact."
              }
            ]
          }
        }
      },
      "SsmContactsArn": {
        "type": "string",
        "pattern": "arn:(aws|aws-cn|aws-us-gov):ssm-contacts:[-\\w+=\\/,.@]*:[0-9]+:([\\w+=\\/,.@:-]+)*",
        "minLength": 1,
        "maxLength": 2048
      },
      "ActivationCode": {
        "type": "string",
        "pattern": "^[0-9]*$",
        "minLength": 6,
        "maxLength": 10
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
