{
  "name": "ListContacts",
  "description": "Lists all contacts and escalation plans in Incident Manager.",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SSMContacts.ListContacts"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ListContactsRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ListContactsRequest": {
        "type": "object",
        "title": "ListContactsRequest",
        "properties": {
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/PaginationToken"
              },
              {
                "description": "The pagination token to continue to the next page of results."
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of contacts and escalation plans per page of results."
              }
            ]
          },
          "AliasPrefix": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactAlias"
              },
              {
                "description": "Used to list only contacts who's aliases start with the specified prefix."
              }
            ]
          },
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/ContactType"
              },
              {
                "description": "The type of contact. A contact is type <code>PERSONAL</code> and an escalation plan is type <code>ESCALATION</code>."
              }
            ]
          }
        }
      },
      "PaginationToken": {
        "type": "string",
        "pattern": "^[\\\\\\/a-zA-Z0-9_+=\\-]*$",
        "maxLength": 2048
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 0,
        "maximum": 1024
      },
      "ContactAlias": {
        "type": "string",
        "pattern": "^[a-z0-9_\\-]*$",
        "minLength": 1,
        "maxLength": 255
      },
      "ContactType": {
        "type": "string",
        "enum": [
          "PERSONAL",
          "ESCALATION",
          "ONCALL_SCHEDULE"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://ssm-contacts.us-east-1.amazonaws.com/#X-Amz-Target=SSMContacts.ListContacts{?MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**NextToken**",
            "**Contacts**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
