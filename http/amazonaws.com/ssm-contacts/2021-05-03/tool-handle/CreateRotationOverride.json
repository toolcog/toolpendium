        "type": "object",
        "required": [
          "RotationId",
          "NewContactIds",
          "StartTime",
          "EndTime"
        ],
        "title": "CreateRotationOverrideRequest",
        "properties": {
          "RotationId": {
            "allOf": [
              {
                "$ref": "#/$defs/SsmContactsArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the rotation to create an override for."
              }
            ]
          },
          "NewContactIds": {
            "allOf": [
              {
                "$ref": "#/$defs/RotationOverrideContactsArnList"
              },
              {
                "description": "<p>The Amazon Resource Names (ARNs) of the contacts to replace those in the current on-call rotation with.</p> <p>If you want to include any current team members in the override shift, you must include their ARNs in the new contact ID list.</p>"
              }
            ]
          },
          "StartTime": {
            "allOf": [
              {
                "$ref": "#/$defs/DateTime"
              },
              {
                "description": "The date and time when the override goes into effect."
              }
            ]
          },
          "EndTime": {
            "allOf": [
              {
                "$ref": "#/$defs/DateTime"
              },
              {
                "description": "The date and time when the override ends."
              }
            ]
          },
          "IdempotencyToken": {
            "allOf": [
              {
                "$ref": "#/$defs/IdempotencyToken"
              },
              {
                "description": "A token that ensures that the operation is called only once with the specified details."
              }
            ]
          }
        }
      },
      "SsmContactsArn": {
        "type": "string",
        "pattern": "arn:(aws|aws-cn|aws-us-gov):ssm-contacts:[-\\w+=\\/,.@]*:[0-9]+:([\\w+=\\/,.@:-]+)*",
        "minLength": 1,
        "maxLength": 2048
      },
      "RotationOverrideContactsArnList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/SsmContactsArn"
        },
        "minItems": 0,
        "maxItems": 30
      },
      "DateTime": {
        "type": "string",
        "format": "date-time"
      },
      "IdempotencyToken": {
        "type": "string",
        "pattern": "^[\\\\\\/a-zA-Z0-9_+=\\-]*$",
        "maxLength": 2048
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
