{
  "name": "UpdateWatchlist",
  "description": "Updates the specified watchlist. Every domain has a default watchlist which cannot be updated. ",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "VoiceID.UpdateWatchlist"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateWatchlistRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateWatchlistRequest": {
        "type": "object",
        "required": [
          "DomainId",
          "WatchlistId"
        ],
        "title": "UpdateWatchlistRequest",
        "properties": {
          "Description": {
            "allOf": [
              {
                "$ref": "#/$defs/WatchlistDescription"
              },
              {
                "description": "A brief description about this watchlist."
              }
            ]
          },
          "DomainId": {
            "allOf": [
              {
                "$ref": "#/$defs/DomainId"
              },
              {
                "description": "The identifier of the domain that contains the watchlist."
              }
            ]
          },
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/WatchlistName"
              },
              {
                "description": "The name of the watchlist."
              }
            ]
          },
          "WatchlistId": {
            "allOf": [
              {
                "$ref": "#/$defs/WatchlistId"
              },
              {
                "description": "The identifier of the watchlist to be updated."
              }
            ]
          }
        }
      },
      "WatchlistDescription": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-%@]*)$",
        "minLength": 1,
        "maxLength": 1024,
        "format": "password"
      },
      "DomainId": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9]{22}$",
        "minLength": 22,
        "maxLength": 22
      },
      "WatchlistName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9][a-zA-Z0-9_-]*$",
        "minLength": 1,
        "maxLength": 256,
        "format": "password"
      },
      "WatchlistId": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9]{22}$",
        "minLength": 22,
        "maxLength": 22
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://voiceid.us-east-1.amazonaws.com/#X-Amz-Target=VoiceID.UpdateWatchlist"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
