{
  "name": "UntagResource",
  "description": "Removes specified tags from a specified Amazon Connect Voice ID resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "VoiceID.UntagResource"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UntagResourceRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UntagResourceRequest": {
        "type": "object",
        "required": [
          "ResourceArn",
          "TagKeys"
        ],
        "title": "UntagResourceRequest",
        "properties": {
          "ResourceArn": {
            "allOf": [
              {
                "$ref": "#/$defs/AmazonResourceName"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the Voice ID resource you want to remove tags from."
              }
            ]
          },
          "TagKeys": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKeyList"
              },
              {
                "description": "The list of tag keys you want to remove from the specified resource."
              }
            ]
          }
        }
      },
      "AmazonResourceName": {
        "type": "string",
        "pattern": "^arn:aws(-[^:]+)?:voiceid.+:[0-9]{12}:domain/[a-zA-Z0-9]{22}$",
        "minLength": 1,
        "maxLength": 1011
      },
      "TagKeyList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TagKey"
        },
        "minItems": 0,
        "maxItems": 200
      },
      "TagKey": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 1,
        "maxLength": 128,
        "format": "password"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://voiceid.us-east-1.amazonaws.com/#X-Amz-Target=VoiceID.UntagResource"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
