{
  "name": "ListEnvironments",
  "description": "Lists Amazon Web Services Migration Hub Refactor Spaces environments owned by a caller account or shared with the caller account. ",
  "parameters": {
    "type": "object",
    "properties": {
      "maxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100,
        "description": "The maximum number of results to return with a single call. To retrieve the remaining results, make another call with the returned <code>nextToken</code> value."
      },
      "nextToken": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9/\\+\\=]{0,2048}$",
