{
  "name": "CreatePipeline",
  "description": "Creates a pipeline using a JSON pipeline definition.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.CreatePipeline"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/CreatePipelineRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "CreatePipelineRequest": {
        "type": "object",
        "required": [
          "PipelineName",
          "ClientRequestToken",
          "RoleArn"
        ],
        "title": "CreatePipelineRequest",
        "properties": {
          "PipelineName": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineName"
              },
              {
                "description": "The name of the pipeline."
              }
            ]
          },
          "PipelineDisplayName": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineName"
              },
              {
                "description": "The display name of the pipeline."
              }
            ]
          },
          "PipelineDefinition": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineDefinition"
              },
              {
                "description": "The <a href=\"https://aws-sagemaker-mlops.github.io/sagemaker-model-building-pipeline-definition-JSON-schema/\">JSON pipeline definition</a> of the pipeline."
              }
            ]
          },
          "PipelineDefinitionS3Location": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineDefinitionS3Location"
              },
              {
                "description": "The location of the pipeline definition stored in Amazon S3. If specified, SageMaker will retrieve the pipeline definition from this location."
              }
            ]
          },
          "PipelineDescription": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineDescription"
              },
              {
                "description": "A description of the pipeline."
              }
            ]
          },
          "ClientRequestToken": {
            "allOf": [
              {
                "$ref": "#/$defs/IdempotencyToken"
              },
              {
                "description": "A unique, case-sensitive identifier that you provide to ensure the idempotency of the operation. An idempotent operation completes no more than one time."
              }
            ]
          },
          "RoleArn": {
            "allOf": [
              {
                "$ref": "#/$defs/RoleArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the role used by the pipeline to access and create resources."
              }
            ]
          },
          "Tags": {
            "allOf": [
              {
                "$ref": "#/$defs/TagList"
              },
              {
                "description": "A list of tags to apply to the created pipeline."
              }
            ]
          },
          "ParallelismConfiguration": {
            "allOf": [
              {
                "$ref": "#/$defs/ParallelismConfiguration"
              },
              {
                "description": "This is the configuration that controls the parallelism of the pipeline. If specified, it applies to all runs of this pipeline by default."
              }
            ]
          }
        }
      },
      "PipelineName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,255}",
        "minLength": 1,
        "maxLength": 256
      },
      "PipelineDefinition": {
        "type": "string",
        "pattern": ".*(?:[ \\r\\n\\t].*)*",
        "minLength": 1,
        "maxLength": 1048576
      },
      "PipelineDefinitionS3Location": {
        "type": "object",
        "required": [
          "Bucket",
          "ObjectKey"
        ],
        "properties": {
          "Bucket": {
            "allOf": [
              {
                "$ref": "#/$defs/BucketName"
              },
              {
                "description": "Name of the S3 bucket."
              }
            ]
          },
          "ObjectKey": {
            "allOf": [
              {
                "$ref": "#/$defs/Key"
              },
              {
                "description": "The object key (or key name) uniquely identifies the object in an S3 bucket. "
              }
            ]
          },
          "VersionId": {
            "allOf": [
              {
                "$ref": "#/$defs/VersionId"
              },
              {
                "description": "Version Id of the pipeline definition file. If not specified, Amazon SageMaker will retrieve the latest version."
              }
            ]
          }
        },
        "description": "The location of the pipeline definition stored in Amazon S3."
      },
      "BucketName": {
        "type": "string",
        "pattern": "[a-z0-9][\\.\\-a-z0-9]{1,61}[a-z0-9]",
        "minLength": 3,
        "maxLength": 63
      },
      "Key": {
        "type": "string",
        "pattern": ".+",
        "minLength": 1,
        "maxLength": 1024
      },
      "VersionId": {
        "type": "string",
        "pattern": ".+",
        "minLength": 1,
        "maxLength": 1024
      },
      "PipelineDescription": {
        "type": "string",
        "pattern": ".*",
        "minLength": 0,
        "maxLength": 3072
      },
      "IdempotencyToken": {
        "type": "string",
        "minLength": 32,
        "maxLength": 128
      },
      "RoleArn": {
        "type": "string",
        "pattern": "^arn:aws[a-z\\-]*:iam::\\d{12}:role/?[a-zA-Z_0-9+=,.@\\-_/]+$",
        "minLength": 20,
        "maxLength": 2048
      },
      "TagList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Tag"
        },
        "minItems": 0,
        "maxItems": 50
      },
      "Tag": {
        "type": "object",
        "required": [
          "Key",
          "Value"
        ],
        "properties": {
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "The tag key. Tag keys must be unique per resource."
              }
            ]
          },
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/TagValue"
              },
              {
                "description": "The tag value."
              }
            ]
          }
        },
        "description": "<p>A tag object that consists of a key and an optional value, used to manage metadata for SageMaker Amazon Web Services resources.</p> <p>You can add tags to notebook instances, training jobs, hyperparameter tuning jobs, batch transform jobs, models, labeling jobs, work teams, endpoint configurations, and endpoints. For more information on adding tags to SageMaker resources, see <a href=\"https://docs.aws.amazon.com/sagemaker/latest/APIReference/API_AddTags.html\">AddTags</a>.</p> <p>For more information on adding metadata to your Amazon Web Services resources with tagging, see <a href=\"https://docs.aws.amazon.com/general/latest/gr/aws_tagging.html\">Tagging Amazon Web Services resources</a>. For advice on best practices for managing Amazon Web Services resources with tagging, see <a href=\"https://d1.awsstatic.com/whitepapers/aws-tagging-best-practices.pdf\">Tagging Best Practices: Implement an Effective Amazon Web Services Resource Tagging Strategy</a>.</p>"
      },
      "TagKey": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 1,
        "maxLength": 128
      },
      "TagValue": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 0,
        "maxLength": 256
      },
      "ParallelismConfiguration": {
        "type": "object",
        "required": [
          "MaxParallelExecutionSteps"
        ],
        "properties": {
          "MaxParallelExecutionSteps": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxParallelExecutionSteps"
              },
              {
                "description": "The max number of steps that can be executed in parallel. "
              }
            ]
          }
        },
        "description": "Configuration that controls the parallelism of the pipeline. By default, the parallelism configuration specified applies to all executions of the pipeline unless overridden."
      },
      "MaxParallelExecutionSteps": {
        "type": "integer",
        "minimum": 1
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.CreatePipeline"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**PipelineArn**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
