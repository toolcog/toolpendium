{
  "name": "ListImages",
  "description": "Lists the images in your account and their properties. The list can be filtered by creation time or modified time, and whether the image name contains a specified string.",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.ListImages"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ListImagesRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ListImagesRequest": {
        "type": "object",
        "title": "ListImagesRequest",
        "properties": {
          "CreationTimeAfter": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "A filter that returns only images created on or after the specified time."
              }
            ]
          },
          "CreationTimeBefore": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "A filter that returns only images created on or before the specified time."
              }
            ]
          },
          "LastModifiedTimeAfter": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "A filter that returns only images modified on or after the specified time."
              }
            ]
          },
          "LastModifiedTimeBefore": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "A filter that returns only images modified on or before the specified time."
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of images to return in the response. The default value is 10. "
              }
            ]
          },
          "NameContains": {
            "allOf": [
              {
                "$ref": "#/$defs/ImageNameContains"
              },
              {
                "description": "A filter that returns only images whose name contains the specified string."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "If the previous call to <code>ListImages</code> didn't return the full set of images, the call returns a token for getting the next set of images."
              }
            ]
          },
          "SortBy": {
            "allOf": [
              {
                "$ref": "#/$defs/ImageSortBy"
              },
              {
                "description": "The property used to sort results. The default value is <code>CREATION_TIME</code>."
              }
            ]
          },
          "SortOrder": {
            "allOf": [
              {
                "$ref": "#/$defs/ImageSortOrder"
              },
              {
                "description": "The sort order. The default value is <code>DESCENDING</code>."
              }
            ]
          }
        }
      },
      "Timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      },
      "ImageNameContains": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9\\-.]+$",
        "maxLength": 63
      },
      "NextToken": {
        "type": "string",
        "pattern": ".*",
        "maxLength": 8192
      },
      "ImageSortBy": {
        "type": "string",
        "enum": [
          "CREATION_TIME",
          "LAST_MODIFIED_TIME",
          "IMAGE_NAME"
        ]
      },
      "ImageSortOrder": {
        "type": "string",
        "enum": [
          "ASCENDING",
          "DESCENDING"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.ListImages{?MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Images**",
            "**NextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
