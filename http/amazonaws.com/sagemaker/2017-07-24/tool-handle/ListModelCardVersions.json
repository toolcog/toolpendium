        ],
        "title": "ListModelCardVersionsRequest",
        "properties": {
          "CreationTimeAfter": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "Only list model card versions that were created after the time specified."
              }
            ]
          },
          "CreationTimeBefore": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "Only list model card versions that were created before the time specified."
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of model card versions to list."
              }
            ]
          },
          "ModelCardName": {
            "allOf": [
              {
                "$ref": "#/$defs/EntityName"
              },
              {
                "description": "List model card versions for the model card with the specified name."
              }
            ]
          },
          "ModelCardStatus": {
            "allOf": [
              {
                "$ref": "#/$defs/ModelCardStatus"
              },
              {
                "description": "Only list model card versions with the specified approval status."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "If the response to a previous <code>ListModelCardVersions</code> request was truncated, the response includes a <code>NextToken</code>. To retrieve the next set of model card versions, use the token in the next request."
              }
            ]
          },
          "SortBy": {
            "allOf": [
              {
                "$ref": "#/$defs/ModelCardVersionSortBy"
              },
              {
                "description": "Sort listed model card versions by version. Sorts by version by default."
              }
            ]
          },
          "SortOrder": {
            "allOf": [
              {
                "$ref": "#/$defs/ModelCardSortOrder"
              },
              {
                "description": "Sort model card versions by ascending or descending order."
              }
            ]
          }
        }
      },
      "Timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      },
      "EntityName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}$",
        "minLength": 1,
        "maxLength": 63
      },
      "ModelCardStatus": {
        "type": "string",
        "enum": [
          "Draft",
          "PendingReview",
          "Approved",
          "Archived"
        ]
      },
      "NextToken": {
        "type": "string",
        "pattern": ".*",
        "maxLength": 8192
      },
      "ModelCardVersionSortBy": {
        "type": "string",
        "enum": [
          "Version"
        ]
      },
      "ModelCardSortOrder": {
        "type": "string",
        "enum": [
          "Ascending",
          "Descending"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
