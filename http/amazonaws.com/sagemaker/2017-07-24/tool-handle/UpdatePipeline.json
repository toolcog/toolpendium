{
  "name": "UpdatePipeline",
  "description": "Updates a pipeline.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.UpdatePipeline"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdatePipelineRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdatePipelineRequest": {
        "type": "object",
        "required": [
          "PipelineName"
        ],
        "title": "UpdatePipelineRequest",
        "properties": {
          "PipelineName": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineName"
              },
              {
                "description": "The name of the pipeline to update."
              }
            ]
          },
          "PipelineDisplayName": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineName"
              },
              {
                "description": "The display name of the pipeline."
              }
            ]
          },
          "PipelineDefinition": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineDefinition"
              },
              {
                "description": "The JSON pipeline definition."
              }
            ]
          },
          "PipelineDefinitionS3Location": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineDefinitionS3Location"
              },
              {
                "description": "The location of the pipeline definition stored in Amazon S3. If specified, SageMaker will retrieve the pipeline definition from this location."
              }
            ]
          },
          "PipelineDescription": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineDescription"
              },
              {
                "description": "The description of the pipeline."
              }
            ]
          },
          "RoleArn": {
            "allOf": [
              {
                "$ref": "#/$defs/RoleArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) that the pipeline uses to execute."
              }
            ]
          },
          "ParallelismConfiguration": {
            "allOf": [
              {
                "$ref": "#/$defs/ParallelismConfiguration"
              },
              {
                "description": "If specified, it applies to all executions of this pipeline by default."
              }
            ]
          }
        }
      },
      "PipelineName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,255}",
        "minLength": 1,
        "maxLength": 256
      },
      "PipelineDefinition": {
        "type": "string",
        "pattern": ".*(?:[ \\r\\n\\t].*)*",
        "minLength": 1,
        "maxLength": 1048576
      },
      "PipelineDefinitionS3Location": {
        "type": "object",
        "required": [
          "Bucket",
          "ObjectKey"
        ],
        "properties": {
          "Bucket": {
            "allOf": [
              {
                "$ref": "#/$defs/BucketName"
              },
              {
                "description": "Name of the S3 bucket."
              }
            ]
          },
          "ObjectKey": {
            "allOf": [
              {
                "$ref": "#/$defs/Key"
              },
              {
                "description": "The object key (or key name) uniquely identifies the object in an S3 bucket. "
              }
            ]
          },
          "VersionId": {
            "allOf": [
              {
                "$ref": "#/$defs/VersionId"
              },
              {
                "description": "Version Id of the pipeline definition file. If not specified, Amazon SageMaker will retrieve the latest version."
              }
            ]
          }
        },
        "description": "The location of the pipeline definition stored in Amazon S3."
      },
      "BucketName": {
        "type": "string",
        "pattern": "[a-z0-9][\\.\\-a-z0-9]{1,61}[a-z0-9]",
        "minLength": 3,
        "maxLength": 63
      },
      "Key": {
        "type": "string",
        "pattern": ".+",
        "minLength": 1,
        "maxLength": 1024
      },
      "VersionId": {
        "type": "string",
        "pattern": ".+",
        "minLength": 1,
        "maxLength": 1024
      },
      "PipelineDescription": {
        "type": "string",
        "pattern": ".*",
        "minLength": 0,
        "maxLength": 3072
      },
      "RoleArn": {
        "type": "string",
        "pattern": "^arn:aws[a-z\\-]*:iam::\\d{12}:role/?[a-zA-Z_0-9+=,.@\\-_/]+$",
        "minLength": 20,
        "maxLength": 2048
      },
      "ParallelismConfiguration": {
        "type": "object",
        "required": [
          "MaxParallelExecutionSteps"
        ],
        "properties": {
          "MaxParallelExecutionSteps": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxParallelExecutionSteps"
              },
              {
                "description": "The max number of steps that can be executed in parallel. "
              }
            ]
          }
        },
        "description": "Configuration that controls the parallelism of the pipeline. By default, the parallelism configuration specified applies to all executions of the pipeline unless overridden."
      },
      "MaxParallelExecutionSteps": {
        "type": "integer",
        "minimum": 1
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.UpdatePipeline"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**PipelineArn**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
