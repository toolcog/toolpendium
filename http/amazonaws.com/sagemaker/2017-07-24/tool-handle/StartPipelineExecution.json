{
  "name": "StartPipelineExecution",
  "description": "Starts a pipeline execution.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.StartPipelineExecution"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/StartPipelineExecutionRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "StartPipelineExecutionRequest": {
        "type": "object",
        "required": [
          "PipelineName",
          "ClientRequestToken"
        ],
        "title": "StartPipelineExecutionRequest",
        "properties": {
          "PipelineName": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineNameOrArn"
              },
              {
                "description": "The name or Amazon Resource Name (ARN) of the pipeline."
              }
            ]
          },
          "PipelineExecutionDisplayName": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineExecutionName"
              },
              {
                "description": "The display name of the pipeline execution."
              }
            ]
          },
          "PipelineParameters": {
            "allOf": [
              {
                "$ref": "#/$defs/ParameterList"
              },
              {
                "description": "Contains a list of pipeline parameters. This list can be empty. "
              }
            ]
          },
          "PipelineExecutionDescription": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineExecutionDescription"
              },
              {
                "description": "The description of the pipeline execution."
              }
            ]
          },
          "ClientRequestToken": {
            "allOf": [
              {
                "$ref": "#/$defs/IdempotencyToken"
              },
              {
                "description": "A unique, case-sensitive identifier that you provide to ensure the idempotency of the operation. An idempotent operation completes no more than once."
              }
            ]
          },
          "ParallelismConfiguration": {
            "allOf": [
              {
                "$ref": "#/$defs/ParallelismConfiguration"
              },
              {
                "description": "This configuration, if specified, overrides the parallelism configuration of the parent pipeline for this specific run."
              }
            ]
          },
          "SelectiveExecutionConfig": {
            "allOf": [
              {
                "$ref": "#/$defs/SelectiveExecutionConfig"
              },
              {
                "description": "The selective execution configuration applied to the pipeline run."
              }
            ]
          }
        }
      },
      "PipelineNameOrArn": {
        "type": "string",
        "pattern": "(arn:aws[a-z\\-]*:sagemaker:[a-z0-9\\-]*:[0-9]{12}:pipeline/.*)?([a-zA-Z0-9](-*[a-zA-Z0-9]){0,255})",
        "minLength": 1,
        "maxLength": 2048
      },
      "PipelineExecutionName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,81}",
        "minLength": 1,
        "maxLength": 82
      },
      "ParameterList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Parameter"
        },
        "minItems": 0,
        "maxItems": 50
      },
      "Parameter": {
        "type": "object",
        "required": [
          "Name",
          "Value"
        ],
        "properties": {
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineParameterName"
              },
              {
                "description": "The name of the parameter to assign a value to. This parameter name must match a named parameter in the pipeline definition."
              }
            ]
          },
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/String1024"
              },
              {
                "description": "The literal value for the parameter."
              }
            ]
          }
        },
        "description": "Assigns a value to a named Pipeline parameter."
      },
      "PipelineParameterName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,255}$",
        "minLength": 1,
        "maxLength": 256
      },
      "String1024": {
        "type": "string",
        "maxLength": 1024
      },
      "PipelineExecutionDescription": {
        "type": "string",
        "pattern": ".*",
        "minLength": 0,
        "maxLength": 3072
      },
      "IdempotencyToken": {
        "type": "string",
        "minLength": 32,
        "maxLength": 128
      },
      "ParallelismConfiguration": {
        "type": "object",
        "required": [
          "MaxParallelExecutionSteps"
        ],
        "properties": {
          "MaxParallelExecutionSteps": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxParallelExecutionSteps"
              },
              {
                "description": "The max number of steps that can be executed in parallel. "
              }
            ]
          }
        },
        "description": "Configuration that controls the parallelism of the pipeline. By default, the parallelism configuration specified applies to all executions of the pipeline unless overridden."
      },
      "MaxParallelExecutionSteps": {
        "type": "integer",
        "minimum": 1
      },
      "SelectiveExecutionConfig": {
        "type": "object",
        "required": [
          "SourcePipelineExecutionArn",
          "SelectedSteps"
        ],
        "properties": {
          "SourcePipelineExecutionArn": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineExecutionArn"
              },
              {
                "description": "The ARN from a reference execution of the current pipeline. Used to copy input collaterals needed for the selected steps to run. The execution status of the pipeline can be either <code>Failed</code> or <code>Success</code>."
              }
            ]
          },
          "SelectedSteps": {
            "allOf": [
              {
                "$ref": "#/$defs/SelectedStepList"
              },
              {
                "description": "A list of pipeline steps to run. All step(s) in all path(s) between two selected steps should be included."
              }
            ]
          }
        },
        "description": "The selective execution configuration applied to the pipeline run."
      },
      "PipelineExecutionArn": {
        "type": "string",
        "pattern": "^arn:aws[a-z\\-]*:sagemaker:[a-z0-9\\-]*:[0-9]{12}:pipeline\\/.*\\/execution\\/.*$",
        "maxLength": 256
      },
      "SelectedStepList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/SelectedStep"
        },
        "minItems": 1,
        "maxItems": 50
      },
      "SelectedStep": {
        "type": "object",
        "required": [
          "StepName"
        ],
        "properties": {
          "StepName": {
            "allOf": [
              {
                "$ref": "#/$defs/String256"
              },
              {
                "description": "The name of the pipeline step."
              }
            ]
          }
        },
        "description": "A step selected to run in selective execution mode."
      },
      "String256": {
        "type": "string",
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.StartPipelineExecution"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**PipelineExecutionArn**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
