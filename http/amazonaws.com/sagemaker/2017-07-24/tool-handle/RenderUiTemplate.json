{
  "name": "RenderUiTemplate",
  "description": "Renders the UI template so that you can preview the worker's experience. ",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.RenderUiTemplate"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/RenderUiTemplateRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "RenderUiTemplateRequest": {
        "type": "object",
        "required": [
          "Task",
          "RoleArn"
        ],
        "title": "RenderUiTemplateRequest",
        "properties": {
          "UiTemplate": {
            "allOf": [
              {
                "$ref": "#/$defs/UiTemplate"
              },
              {
                "description": "A <code>Template</code> object containing the worker UI template to render."
              }
            ]
          },
          "Task": {
            "allOf": [
              {
                "$ref": "#/$defs/RenderableTask"
              },
              {
                "description": "A <code>RenderableTask</code> object containing a representative task to render."
              }
            ]
          },
          "RoleArn": {
            "allOf": [
              {
                "$ref": "#/$defs/RoleArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) that has access to the S3 objects that are used by the template."
              }
            ]
          },
          "HumanTaskUiArn": {
            "allOf": [
              {
                "$ref": "#/$defs/HumanTaskUiArn"
              },
              {
                "description": "<p>The <code>HumanTaskUiArn</code> of the worker UI that you want to render. Do not provide a <code>HumanTaskUiArn</code> if you use the <code>UiTemplate</code> parameter.</p> <p>See a list of available Human Ui Amazon Resource Names (ARNs) in <a href=\"https://docs.aws.amazon.com/sagemaker/latest/APIReference/API_UiConfig.html\">UiConfig</a>.</p>"
              }
            ]
          }
        }
      },
      "UiTemplate": {
        "type": "object",
        "required": [
          "Content"
        ],
        "properties": {
          "Content": {
            "allOf": [
              {
                "$ref": "#/$defs/TemplateContent"
              },
              {
                "description": "The content of the Liquid template for the worker user interface."
              }
            ]
          }
        },
        "description": "The Liquid template for the worker user interface."
      },
      "TemplateContent": {
        "type": "string",
        "pattern": "[\\S\\s]+",
        "minLength": 1,
        "maxLength": 128000
      },
      "RenderableTask": {
        "type": "object",
        "required": [
          "Input"
        ],
        "properties": {
          "Input": {
            "allOf": [
              {
                "$ref": "#/$defs/TaskInput"
              },
              {
                "description": "A JSON object that contains values for the variables defined in the template. It is made available to the template under the substitution variable <code>task.input</code>. For example, if you define a variable <code>task.input.text</code> in your template, you can supply the variable in the JSON object as <code>\"text\": \"sample text\"</code>."
              }
            ]
          }
        },
        "description": "Contains input values for a task."
      },
      "TaskInput": {
        "type": "string",
        "pattern": "[\\S\\s]+",
        "minLength": 2,
        "maxLength": 128000
      },
      "RoleArn": {
        "type": "string",
        "pattern": "^arn:aws[a-z\\-]*:iam::\\d{12}:role/?[a-zA-Z_0-9+=,.@\\-_/]+$",
        "minLength": 20,
        "maxLength": 2048
      },
      "HumanTaskUiArn": {
        "type": "string",
        "pattern": "arn:aws[a-z\\-]*:sagemaker:[a-z0-9\\-]*:[0-9]{12}:human-task-ui/.*",
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.RenderUiTemplate"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**RenderedContent**",
            "**Errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
