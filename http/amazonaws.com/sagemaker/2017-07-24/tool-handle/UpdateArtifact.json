{
  "name": "UpdateArtifact",
  "description": "Updates an artifact.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.UpdateArtifact"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateArtifactRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateArtifactRequest": {
        "type": "object",
        "required": [
          "ArtifactArn"
        ],
        "title": "UpdateArtifactRequest",
        "properties": {
          "ArtifactArn": {
            "allOf": [
              {
                "$ref": "#/$defs/ArtifactArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the artifact to update."
              }
            ]
          },
          "ArtifactName": {
            "allOf": [
              {
                "$ref": "#/$defs/ExperimentEntityName"
              },
              {
                "description": "The new name for the artifact."
              }
            ]
          },
          "Properties": {
            "allOf": [
              {
                "$ref": "#/$defs/LineageEntityParameters"
              },
              {
                "description": "The new list of properties. Overwrites the current property list."
              }
            ]
          },
          "PropertiesToRemove": {
            "allOf": [
              {
                "$ref": "#/$defs/ListLineageEntityParameterKey"
              },
              {
                "description": "A list of properties to remove."
              }
            ]
          }
        }
      },
      "ArtifactArn": {
        "type": "string",
        "pattern": "arn:aws[a-z\\-]*:sagemaker:[a-z0-9\\-]*:[0-9]{12}:artifact/.*",
        "maxLength": 256
      },
      "ExperimentEntityName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,119}",
        "minLength": 1,
        "maxLength": 120
      },
      "LineageEntityParameters": {
        "type": "object",
        "maxProperties": 30,
        "additionalProperties": {
          "$ref": "#/$defs/StringParameterValue"
        }
      },
      "StringParameterValue": {
        "type": "string",
        "pattern": ".*",
        "maxLength": 256
      },
      "ListLineageEntityParameterKey": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/StringParameterValue"
        }
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.UpdateArtifact"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ArtifactArn**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "481": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
