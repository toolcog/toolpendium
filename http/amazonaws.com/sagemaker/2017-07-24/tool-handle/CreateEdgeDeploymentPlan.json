{
  "name": "CreateEdgeDeploymentPlan",
  "description": "Creates an edge deployment plan, consisting of multiple stages. Each stage may have a different deployment configuration and devices.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.CreateEdgeDeploymentPlan"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/CreateEdgeDeploymentPlanRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "CreateEdgeDeploymentPlanRequest": {
        "type": "object",
        "required": [
          "EdgeDeploymentPlanName",
          "ModelConfigs",
          "DeviceFleetName"
        ],
        "title": "CreateEdgeDeploymentPlanRequest",
        "properties": {
          "EdgeDeploymentPlanName": {
            "allOf": [
              {
                "$ref": "#/$defs/EntityName"
              },
              {
                "description": "The name of the edge deployment plan."
              }
            ]
          },
          "ModelConfigs": {
            "allOf": [
              {
                "$ref": "#/$defs/EdgeDeploymentModelConfigs"
              },
              {
                "description": "List of models associated with the edge deployment plan."
              }
            ]
          },
          "DeviceFleetName": {
            "allOf": [
              {
                "$ref": "#/$defs/EntityName"
              },
              {
                "description": "The device fleet used for this edge deployment plan."
              }
            ]
          },
          "Stages": {
            "allOf": [
              {
                "$ref": "#/$defs/DeploymentStages"
              },
              {
                "description": "List of stages of the edge deployment plan. The number of stages is limited to 10 per deployment."
              }
            ]
          },
          "Tags": {
            "allOf": [
              {
                "$ref": "#/$defs/TagList"
              },
              {
                "description": "List of tags with which to tag the edge deployment plan."
              }
            ]
          }
        }
      },
      "EntityName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}$",
        "minLength": 1,
        "maxLength": 63
      },
      "EdgeDeploymentModelConfigs": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/EdgeDeploymentModelConfig"
        }
      },
      "EdgeDeploymentModelConfig": {
        "type": "object",
        "required": [
          "ModelHandle",
          "EdgePackagingJobName"
        ],
        "properties": {
          "ModelHandle": {
            "allOf": [
              {
                "$ref": "#/$defs/EntityName"
              },
              {
                "description": "The name the device application uses to reference this model."
              }
            ]
          },
          "EdgePackagingJobName": {
            "allOf": [
              {
                "$ref": "#/$defs/EntityName"
              },
              {
                "description": "The edge packaging job associated with this deployment."
              }
            ]
          }
        },
        "description": "Contains information about the configuration of a model in a deployment."
      },
      "DeploymentStages": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/DeploymentStage"
        }
      },
      "DeploymentStage": {
        "type": "object",
        "required": [
          "StageName",
          "DeviceSelectionConfig"
        ],
        "properties": {
          "StageName": {
            "allOf": [
              {
                "$ref": "#/$defs/EntityName"
              },
              {
                "description": "The name of the stage."
              }
            ]
          },
          "DeviceSelectionConfig": {
            "allOf": [
              {
                "$ref": "#/$defs/DeviceSelectionConfig"
              },
              {
                "description": "Configuration of the devices in the stage."
              }
            ]
          },
          "DeploymentConfig": {
            "allOf": [
              {
                "$ref": "#/$defs/EdgeDeploymentConfig"
              },
              {
                "description": "Configuration of the deployment details."
              }
            ]
          }
        },
        "description": "Contains information about a stage in an edge deployment plan."
      },
      "DeviceSelectionConfig": {
        "type": "object",
        "required": [
          "DeviceSubsetType"
        ],
        "properties": {
          "DeviceSubsetType": {
            "allOf": [
              {
                "$ref": "#/$defs/DeviceSubsetType"
              },
              {
                "description": "Type of device subsets to deploy to the current stage."
              }
            ]
          },
          "Percentage": {
            "allOf": [
              {
                "$ref": "#/$defs/Percentage"
              },
              {
                "description": "Percentage of devices in the fleet to deploy to the current stage."
              }
            ]
          },
          "DeviceNames": {
            "allOf": [
              {
                "$ref": "#/$defs/DeviceNames"
              },
              {
                "description": "List of devices chosen to deploy."
              }
            ]
          },
          "DeviceNameContains": {
            "allOf": [
              {
                "$ref": "#/$defs/DeviceName"
              },
              {
                "description": "A filter to select devices with names containing this name."
              }
            ]
          }
        },
        "description": "Contains information about the configurations of selected devices."
      },
      "DeviceSubsetType": {
        "type": "string",
        "enum": [
          "PERCENTAGE",
          "SELECTION",
          "NAMECONTAINS"
        ]
      },
      "Percentage": {
        "type": "integer",
        "maximum": 100
      },
      "DeviceNames": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/DeviceName"
        }
      },
      "DeviceName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}$",
        "minLength": 1,
        "maxLength": 63
      },
      "EdgeDeploymentConfig": {
        "type": "object",
        "required": [
          "FailureHandlingPolicy"
        ],
        "properties": {
          "FailureHandlingPolicy": {
            "allOf": [
              {
                "$ref": "#/$defs/FailureHandlingPolicy"
              },
              {
                "description": "Toggle that determines whether to rollback to previous configuration if the current deployment fails. By default this is turned on. You may turn this off if you want to investigate the errors yourself."
              }
            ]
          }
        },
        "description": "Contains information about the configuration of a deployment."
      },
      "FailureHandlingPolicy": {
        "type": "string",
        "enum": [
          "ROLLBACK_ON_FAILURE",
          "DO_NOTHING"
        ]
      },
      "TagList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Tag"
        },
        "minItems": 0,
        "maxItems": 50
      },
      "Tag": {
        "type": "object",
        "required": [
          "Key",
          "Value"
        ],
        "properties": {
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "The tag key. Tag keys must be unique per resource."
              }
            ]
          },
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/TagValue"
              },
              {
                "description": "The tag value."
              }
            ]
          }
        },
        "description": "<p>A tag object that consists of a key and an optional value, used to manage metadata for SageMaker Amazon Web Services resources.</p> <p>You can add tags to notebook instances, training jobs, hyperparameter tuning jobs, batch transform jobs, models, labeling jobs, work teams, endpoint configurations, and endpoints. For more information on adding tags to SageMaker resources, see <a href=\"https://docs.aws.amazon.com/sagemaker/latest/APIReference/API_AddTags.html\">AddTags</a>.</p> <p>For more information on adding metadata to your Amazon Web Services resources with tagging, see <a href=\"https://docs.aws.amazon.com/general/latest/gr/aws_tagging.html\">Tagging Amazon Web Services resources</a>. For advice on best practices for managing Amazon Web Services resources with tagging, see <a href=\"https://d1.awsstatic.com/whitepapers/aws-tagging-best-practices.pdf\">Tagging Best Practices: Implement an Effective Amazon Web Services Resource Tagging Strategy</a>.</p>"
      },
      "TagKey": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 1,
        "maxLength": 128
      },
      "TagValue": {
        "type": "string",
        "pattern": "^([\\p{L}\\p{Z}\\p{N}_.:/=+\\-@]*)$",
        "minLength": 0,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.CreateEdgeDeploymentPlan"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**EdgeDeploymentPlanArn**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
