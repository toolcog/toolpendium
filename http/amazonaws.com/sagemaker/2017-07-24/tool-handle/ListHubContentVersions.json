{
  "name": "ListHubContentVersions",
  "description": "<p>List hub content versions.</p> <note> <p>Hub APIs are only callable through SageMaker Studio.</p> </note>",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.ListHubContentVersions"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ListHubContentVersionsRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ListHubContentVersionsRequest": {
        "type": "object",
        "required": [
          "HubName",
          "HubContentType",
          "HubContentName"
        ],
        "title": "ListHubContentVersionsRequest",
        "properties": {
          "HubName": {
            "allOf": [
              {
                "$ref": "#/$defs/HubName"
              },
              {
                "description": "The name of the hub to list the content versions of."
              }
            ]
          },
          "HubContentType": {
            "allOf": [
              {
                "$ref": "#/$defs/HubContentType"
              },
              {
                "description": "The type of hub content to list versions of."
              }
            ]
          },
          "HubContentName": {
            "allOf": [
              {
                "$ref": "#/$defs/HubContentName"
              },
              {
                "description": "The name of the hub content."
              }
            ]
          },
          "MinVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/HubContentVersion"
              },
              {
                "description": "The lower bound of the hub content versions to list."
              }
            ]
          },
          "MaxSchemaVersion": {
            "allOf": [
              {
                "$ref": "#/$defs/DocumentSchemaVersion"
              },
              {
                "description": "The upper bound of the hub content schema version."
              }
            ]
          },
          "CreationTimeBefore": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "Only list hub content versions that were created before the time specified."
              }
            ]
          },
          "CreationTimeAfter": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "Only list hub content versions that were created after the time specified."
              }
            ]
          },
          "SortBy": {
            "allOf": [
              {
                "$ref": "#/$defs/HubContentSortBy"
              },
              {
                "description": "Sort hub content versions by either name or creation time."
              }
            ]
          },
          "SortOrder": {
            "allOf": [
              {
                "$ref": "#/$defs/SortOrder"
              },
              {
                "description": "Sort hub content versions by ascending or descending order."
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of hub content versions to list."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "If the response to a previous <code>ListHubContentVersions</code> request was truncated, the response includes a <code>NextToken</code>. To retrieve the next set of hub content versions, use the token in the next request."
              }
            ]
          }
        }
      },
      "HubName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}",
        "maxLength": 63
      },
      "HubContentType": {
        "type": "string",
        "enum": [
          "Model",
          "Notebook"
        ]
      },
      "HubContentName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}",
        "maxLength": 63
      },
      "HubContentVersion": {
        "type": "string",
        "pattern": "^\\d{1,4}.\\d{1,4}.\\d{1,4}$",
        "minLength": 5,
        "maxLength": 14
      },
      "DocumentSchemaVersion": {
        "type": "string",
        "pattern": "^\\d{1,4}.\\d{1,4}.\\d{1,4}$",
        "minLength": 5,
        "maxLength": 14
      },
      "Timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "HubContentSortBy": {
        "type": "string",
        "enum": [
          "HubContentName",
          "CreationTime",
          "HubContentStatus"
        ]
      },
      "SortOrder": {
        "type": "string",
        "enum": [
          "Ascending",
          "Descending"
        ]
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      },
      "NextToken": {
        "type": "string",
        "pattern": ".*",
        "maxLength": 8192
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.ListHubContentVersions"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**HubContentSummaries**",
            "**NextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
