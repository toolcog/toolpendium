    "$defs": {
      "UpdateMonitoringAlertRequest": {
        "type": "object",
        "required": [
          "MonitoringScheduleName",
          "MonitoringAlertName",
          "DatapointsToAlert",
          "EvaluationPeriod"
        ],
        "title": "UpdateMonitoringAlertRequest",
        "properties": {
          "MonitoringScheduleName": {
            "allOf": [
              {
                "$ref": "#/$defs/MonitoringScheduleName"
              },
              {
                "description": "The name of a monitoring schedule."
              }
            ]
          },
          "MonitoringAlertName": {
            "allOf": [
              {
                "$ref": "#/$defs/MonitoringAlertName"
              },
              {
                "description": "The name of a monitoring alert."
              }
            ]
          },
          "DatapointsToAlert": {
            "allOf": [
              {
                "$ref": "#/$defs/MonitoringDatapointsToAlert"
              },
              {
                "description": "Within <code>EvaluationPeriod</code>, how many execution failures will raise an alert."
              }
            ]
          },
          "EvaluationPeriod": {
            "allOf": [
              {
                "$ref": "#/$defs/MonitoringEvaluationPeriod"
              },
              {
                "description": "The number of most recent monitoring executions to consider when evaluating alert status."
              }
            ]
          }
        }
      },
      "MonitoringScheduleName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}$",
        "minLength": 1,
        "maxLength": 63
      },
      "MonitoringAlertName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}$",
        "minLength": 1,
        "maxLength": 63
      },
      "MonitoringDatapointsToAlert": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      },
      "MonitoringEvaluationPeriod": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      }
    }
  },
  "handler": "http",
  "request": {
