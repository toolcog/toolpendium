{
  "name": "UpdatePipelineExecution",
  "description": "Updates a pipeline execution.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.UpdatePipelineExecution"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdatePipelineExecutionRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdatePipelineExecutionRequest": {
        "type": "object",
        "required": [
          "PipelineExecutionArn"
        ],
        "title": "UpdatePipelineExecutionRequest",
        "properties": {
          "PipelineExecutionArn": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineExecutionArn"
              },
              {
                "description": "The Amazon Resource Name (ARN) of the pipeline execution."
              }
            ]
          },
          "PipelineExecutionDescription": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineExecutionDescription"
              },
              {
                "description": "The description of the pipeline execution."
              }
            ]
          },
          "PipelineExecutionDisplayName": {
            "allOf": [
              {
                "$ref": "#/$defs/PipelineExecutionName"
              },
              {
                "description": "The display name of the pipeline execution."
              }
            ]
          },
          "ParallelismConfiguration": {
            "allOf": [
              {
                "$ref": "#/$defs/ParallelismConfiguration"
              },
              {
                "description": "This configuration, if specified, overrides the parallelism configuration of the parent pipeline for this specific run."
              }
            ]
          }
        }
      },
      "PipelineExecutionArn": {
        "type": "string",
        "pattern": "^arn:aws[a-z\\-]*:sagemaker:[a-z0-9\\-]*:[0-9]{12}:pipeline\\/.*\\/execution\\/.*$",
        "maxLength": 256
      },
      "PipelineExecutionDescription": {
        "type": "string",
        "pattern": ".*",
        "minLength": 0,
        "maxLength": 3072
      },
      "PipelineExecutionName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,81}",
        "minLength": 1,
        "maxLength": 82
      },
      "ParallelismConfiguration": {
        "type": "object",
        "required": [
          "MaxParallelExecutionSteps"
        ],
        "properties": {
          "MaxParallelExecutionSteps": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxParallelExecutionSteps"
              },
              {
                "description": "The max number of steps that can be executed in parallel. "
              }
            ]
          }
        },
        "description": "Configuration that controls the parallelism of the pipeline. By default, the parallelism configuration specified applies to all executions of the pipeline unless overridden."
      },
      "MaxParallelExecutionSteps": {
        "type": "integer",
        "minimum": 1
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.UpdatePipelineExecution"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
