{
  "name": "ListMonitoringExecutions",
  "description": "Returns list of all monitoring job executions.",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "SageMaker.ListMonitoringExecutions"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ListMonitoringExecutionsRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ListMonitoringExecutionsRequest": {
        "type": "object",
        "title": "ListMonitoringExecutionsRequest",
        "properties": {
          "MonitoringScheduleName": {
            "allOf": [
              {
                "$ref": "#/$defs/MonitoringScheduleName"
              },
              {
                "description": "Name of a specific schedule to fetch jobs for."
              }
            ]
          },
          "EndpointName": {
            "allOf": [
              {
                "$ref": "#/$defs/EndpointName"
              },
              {
                "description": "Name of a specific endpoint to fetch jobs for."
              }
            ]
          },
          "SortBy": {
            "allOf": [
              {
                "$ref": "#/$defs/MonitoringExecutionSortKey"
              },
              {
                "description": "Whether to sort results by <code>Status</code>, <code>CreationTime</code>, <code>ScheduledTime</code> field. The default is <code>CreationTime</code>."
              }
            ]
          },
          "SortOrder": {
            "allOf": [
              {
                "$ref": "#/$defs/SortOrder"
              },
              {
                "description": "Whether to sort the results in <code>Ascending</code> or <code>Descending</code> order. The default is <code>Descending</code>."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "The token returned if the response is truncated. To retrieve the next set of job executions, use it in the next request."
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of jobs to return in the response. The default value is 10."
              }
            ]
          },
          "ScheduledTimeBefore": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "Filter for jobs scheduled before a specified time."
              }
            ]
          },
          "ScheduledTimeAfter": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "Filter for jobs scheduled after a specified time."
              }
            ]
          },
          "CreationTimeBefore": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "A filter that returns only jobs created before a specified time."
              }
            ]
          },
          "CreationTimeAfter": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "A filter that returns only jobs created after a specified time."
              }
            ]
          },
          "LastModifiedTimeBefore": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "A filter that returns only jobs modified after a specified time."
              }
            ]
          },
          "LastModifiedTimeAfter": {
            "allOf": [
              {
                "$ref": "#/$defs/Timestamp"
              },
              {
                "description": "A filter that returns only jobs modified before a specified time."
              }
            ]
          },
          "StatusEquals": {
            "allOf": [
              {
                "$ref": "#/$defs/ExecutionStatus"
              },
              {
                "description": "A filter that retrieves only jobs with a specific status."
              }
            ]
          },
          "MonitoringJobDefinitionName": {
            "allOf": [
              {
                "$ref": "#/$defs/MonitoringJobDefinitionName"
              },
              {
                "description": "Gets a list of the monitoring job runs of the specified monitoring job definitions."
              }
            ]
          },
          "MonitoringTypeEquals": {
            "allOf": [
              {
                "$ref": "#/$defs/MonitoringType"
              },
              {
                "description": "A filter that returns only the monitoring job runs of the specified monitoring type."
              }
            ]
          }
        }
      },
      "MonitoringScheduleName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}$",
        "minLength": 1,
        "maxLength": 63
      },
      "EndpointName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9]){0,62}",
        "maxLength": 63
      },
      "MonitoringExecutionSortKey": {
        "type": "string",
        "enum": [
          "CreationTime",
          "ScheduledTime",
          "Status"
        ]
      },
      "SortOrder": {
        "type": "string",
        "enum": [
          "Ascending",
          "Descending"
        ]
      },
      "NextToken": {
        "type": "string",
        "pattern": ".*",
        "maxLength": 8192
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      },
      "Timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "ExecutionStatus": {
        "type": "string",
        "enum": [
          "Pending",
          "Completed",
          "CompletedWithViolations",
          "InProgress",
          "Failed",
          "Stopping",
          "Stopped"
        ]
      },
      "MonitoringJobDefinitionName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9](-*[a-zA-Z0-9])*$",
        "minLength": 1,
        "maxLength": 63
      },
      "MonitoringType": {
        "type": "string",
        "enum": [
          "DataQuality",
          "ModelQuality",
          "ModelBias",
          "ModelExplainability"
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://api.sagemaker.us-east-1.amazonaws.com/#X-Amz-Target=SageMaker.ListMonitoringExecutions{?MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Target": {
        "$": "X-Amz-Target"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**MonitoringExecutionSummaries**",
            "**NextToken**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
