{
  "name": "GetAccessToken",
  "description": "Gets a temporary access token to use with AssumeRoleWithWebIdentity. Access tokens are valid for one hour.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AWSLicenseManager.GetAccessToken"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/GetAccessTokenRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "GetAccessTokenRequest": {
        "type": "object",
        "required": [
          "Token"
        ],
        "title": "GetAccessTokenRequest",
        "properties": {
          "Token": {
            "allOf": [
              {
                "$ref": "#/$defs/TokenString"
              },
              {
                "description": "Refresh token, encoded as a JWT token."
              }
            ]
          },
          "TokenProperties": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxSize3StringList"
              },
              {
                "description": "Token properties to validate against those present in the JWT token."
              }
            ]
          }
        }
      },
      "TokenString": {
        "type": "string",
        "pattern": "\\S+",
        "maxLength": 4096
      },
      "MaxSize3StringList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/String"
        },
        "maxItems": 3
      },
      "String": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://license-manager.us-east-1.amazonaws.com/#X-Amz-Target=AWSLicenseManager.GetAccessToken"
    },
    "headers": {
      "X-Amz-Target": {
