          "PENDING_ACCEPT",
          "REJECTED",
          "ACTIVE",
          "FAILED_WORKFLOW",
          "DELETED",
          "PENDING_DELETE",
          "DISABLED",
          "WORKFLOW_COMPLETED"
        ]
      },
      "StatusReasonMessage": {
        "type": "string",
