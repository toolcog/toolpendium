      }
    },
    "required": [
      "Id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://route53.amazonaws.com/2013-04-01/hostedzone/{Id}/authorizevpcassociation"
