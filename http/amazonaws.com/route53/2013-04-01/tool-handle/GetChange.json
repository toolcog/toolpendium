    "type": "object",
    "properties": {
      "Id": {
        "type": "string",
        "minLength": 1,
        "maxLength": 6500,
        "description": "The ID of the change batch request. The value that you specify here is the value that <code>ChangeResourceRecordSets</code> returned in the <code>Id</code> element when you submitted the request."
