{
  "name": "ListCidrBlocks",
  "description": "Returns a paginated list of location objects and their CIDR blocks.",
  "parameters": {
    "type": "object",
    "properties": {
      "CidrCollectionId": {
        "type": "string",
        "pattern": "[0-9a-f]{8}-(?:[0-9a-f]{4}-){3}[0-9a-f]{12}",
        "description": "The UUID of the CIDR collection."
      },
      "location": {
        "type": "string",
        "pattern": "[0-9A-Za-z_\\-]+",
        "minLength": 1,
        "maxLength": 16,
        "description": "The name of the CIDR collection location."
      },
      "nexttoken": {
        "type": "string",
        "maxLength": 1024,
        "description": "An opaque pagination token to indicate where the service is to begin enumerating results."
      },
      "maxresults": {
        "type": "string",
        "description": "Maximum number of results you want returned."
      },
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "CidrCollectionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://route53.amazonaws.com/2013-04-01/cidrcollection/{CidrCollectionId}/cidrblocks{?location,nexttoken,maxresults,MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**NextToken**",
            "**CidrBlocks**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
