      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdatePrimaryEmailAddressRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdatePrimaryEmailAddressRequest": {
        "type": "object",
        "required": [
          "OrganizationId",
          "EntityId",
          "Email"
        ],
        "title": "UpdatePrimaryEmailAddressRequest",
        "properties": {
          "OrganizationId": {
            "allOf": [
              {
                "$ref": "#/$defs/OrganizationId"
              },
              {
                "description": "The organization that contains the user, group, or resource to update."
              }
            ]
          },
          "EntityId": {
            "allOf": [
              {
                "$ref": "#/$defs/WorkMailIdentifier"
              },
              {
                "description": "The user, group, or resource to update."
              }
            ]
          },
          "Email": {
            "allOf": [
              {
                "$ref": "#/$defs/EmailAddress"
              },
              {
                "description": "The value of the email to be updated as primary."
              }
            ]
          }
        }
      },
      "OrganizationId": {
        "type": "string",
        "pattern": "^m-[0-9a-f]{32}$",
        "minLength": 34,
        "maxLength": 34
      },
      "WorkMailIdentifier": {
        "type": "string",
        "minLength": 12,
        "maxLength": 256
      },
      "EmailAddress": {
        "type": "string",
        "pattern": "[a-zA-Z0-9._%+-]{1,64}@[a-zA-Z0-9.-]+\\.[a-zA-Z-]{2,}",
        "minLength": 1,
        "maxLength": 254
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://workmail.us-east-1.amazonaws.com/#X-Amz-Target=WorkMailService.UpdatePrimaryEmailAddress"
