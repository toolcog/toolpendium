      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "DeleteUserRequest": {
        "type": "object",
        "required": [
          "OrganizationId",
          "UserId"
        ],
        "title": "DeleteUserRequest",
        "properties": {
          "OrganizationId": {
            "allOf": [
