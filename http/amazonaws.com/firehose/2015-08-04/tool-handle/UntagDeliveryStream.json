        "type": "object",
        "required": [
          "DeliveryStreamName",
          "TagKeys"
        ],
        "title": "UntagDeliveryStreamInput",
        "properties": {
          "DeliveryStreamName": {
            "allOf": [
              {
                "$ref": "#/$defs/DeliveryStreamName"
              },
              {
                "description": "The name of the delivery stream."
              }
            ]
          },
          "TagKeys": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKeyList"
              },
              {
                "description": "A list of tag keys. Each corresponding tag is removed from the delivery stream."
              }
            ]
          }
        }
      },
      "DeliveryStreamName": {
        "type": "string",
        "pattern": "[a-zA-Z0-9_.-]+",
        "minLength": 1,
        "maxLength": 64
      },
      "TagKeyList": {
        "type": "array",
        "items": {
