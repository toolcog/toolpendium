        },
        "minItems": 1,
        "maxItems": 50
      },
      "TagKey": {
        "type": "string",
        "pattern": "^(?!aws:)[\\p{L}\\p{Z}\\p{N}_.:\\/=+\\-@%]*$",
        "minLength": 1,
        "maxLength": 128
      }
    }
