{
  "name": "PutObject",
  "description": "Upload object that can store object metadata String and data blob in single API call using inline chunk field.",
  "parameters": {
    "type": "object",
    "properties": {
      "jobId": {
        "type": "string",
        "description": "Backup job Id for the in-progress backup."
      },
      "objectName": {
        "type": "string",
        "description": "The name of the Object to be uploaded."
      },
      "metadata-string": {
        "type": "string",
        "description": "Store user defined metadata like backup checksum, disk ids, restore metadata etc."
      },
      "length": {
        "type": "integer",
        "description": "Length of the inline chunk data."
      },
      "checksum": {
        "type": "string",
        "description": "Inline chunk checksum"
      },
      "checksum-algorithm": {
        "type": "string",
        "description": "Inline chunk checksum algorithm"
      },
      "object-checksum": {
        "type": "string",
        "description": "object checksum"
      },
      "object-checksum-algorithm": {
        "type": "string",
        "enum": [
          "SUMMARY"
        ],
        "description": "object checksum algorithm"
      },
      "throwOnDuplicate": {
        "type": "boolean",
        "description": "Throw an exception if Object name is already exist."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "properties": {
          "InlineChunk": {
            "description": "Inline chunk data to be uploaded.",
            "type": "string"
          }
        }
      }
    },
    "required": [
      "jobId",
      "objectName",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://backupstorage.us-east-1.amazonaws.com/backup-jobs/{jobId}/object/{objectName}/put-object{?metadata-string,length,checksum,checksum-algorithm,object-checksum,object-checksum-algorithm,throwOnDuplicate}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**InlineChunkChecksum**",
            "**InlineChunkChecksumAlgorithm**",
            "**ObjectChecksum**",
            "**ObjectChecksumAlgorithm**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
