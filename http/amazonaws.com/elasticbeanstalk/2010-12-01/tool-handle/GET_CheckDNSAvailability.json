{
  "name": "GET_CheckDNSAvailability",
  "description": "Checks if the specified CNAME is available.",
  "parameters": {
    "type": "object",
    "properties": {
      "CNAMEPrefix": {
        "type": "string",
        "minLength": 4,
        "maxLength": 63,
        "description": "The prefix used when this CNAME is reserved."
      },
      "Action": {
        "type": "string",
        "enum": [
          "CheckDNSAvailability"
        ]
      },
      "Version": {
        "type": "string",
        "enum": [
          "2010-12-01"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "CNAMEPrefix",
      "Action",
      "Version"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://elasticbeanstalk.us-east-1.amazonaws.com/#Action=CheckDNSAvailability{?CNAMEPrefix,Action,Version}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Indicates if the specified CNAME is available.",
        "**Key properties:**",
        {
          "$ul": [
            "**Available**",
            "**FullyQualifiedCNAME**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
