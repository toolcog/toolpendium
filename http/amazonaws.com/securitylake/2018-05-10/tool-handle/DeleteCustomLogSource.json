{
  "name": "DeleteCustomLogSource",
  "description": "Removes a custom log source from Amazon Security Lake, to stop sending data from the custom source to Security Lake.",
  "parameters": {
    "type": "object",
    "properties": {
      "sourceName": {
        "type": "string",
        "pattern": "^[\\\\\\w\\-_:/.]*$",
        "minLength": 1,
        "maxLength": 64,
        "description": "The source name of custom log source that you want to delete."
      },
      "sourceVersion": {
        "type": "string",
        "pattern": "^[A-Za-z0-9\\-\\.\\_]*$",
        "minLength": 1,
        "maxLength": 32,
        "description": "The source version for the third-party custom source. You can limit the custom source removal to the specified source version."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "sourceName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://securitylake.us-east-1.amazonaws.com/v1/datalake/logsources/custom/{sourceName}{?sourceVersion}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
