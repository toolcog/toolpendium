{
  "name": "CreateStudio",
  "description": "Creates a new Amazon EMR Studio.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "ElasticMapReduce.CreateStudio"
