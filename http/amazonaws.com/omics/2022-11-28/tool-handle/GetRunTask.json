{
  "name": "GetRunTask",
  "description": "Gets information about a workflow run task.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "[0-9]+",
        "minLength": 1,
        "maxLength": 18,
        "description": "The task's ID."
      },
