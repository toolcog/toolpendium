{
  "name": "GetReference",
  "description": "Gets a reference file.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "[0-9]+",
        "minLength": 10,
        "maxLength": 36,
        "description": "The reference's ID."
      },
      "referenceStoreId": {
        "type": "string",
        "pattern": "[0-9]+",
        "minLength": 10,
        "maxLength": 36,
        "description": "The reference's store ID."
      },
      "Range": {
        "type": "string",
        "pattern": "[\\p{N}||\\p{P}]+",
        "minLength": 1,
        "maxLength": 127,
        "description": "The range to retrieve."
      },
      "partNumber": {
        "type": "integer",
        "minimum": 1,
        "maximum": 10000,
        "description": "The part number to retrieve."
      },
      "file": {
        "type": "string",
        "enum": [
          "SOURCE",
          "INDEX"
        ],
        "description": "The file to retrieve."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "id",
      "referenceStoreId",
      "partNumber"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://omics.us-east-1.amazonaws.com/referencestore/{referenceStoreId}/reference/{id}#partNumber{?partNumber,file}"
    },
    "headers": {
      "Range": {
        "$": "Range"
      },
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
