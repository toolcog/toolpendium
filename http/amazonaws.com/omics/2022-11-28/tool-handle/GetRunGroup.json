{
  "name": "GetRunGroup",
  "description": "Gets information about a workflow run group.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "[0-9]+",
        "minLength": 1,
        "maxLength": 18,
        "description": "The group's ID."
      },
      "X-Amz-Content-Sha256": {
