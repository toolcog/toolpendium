{
  "name": "GetRun",
  "description": "Gets information about a workflow run.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "pattern": "[0-9]+",
        "minLength": 1,
        "maxLength": 18,
        "description": "The run's ID."
      },
      "export": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/RunExport"
        },
        "minItems": 0,
        "maxItems": 32,
        "description": "The run's export format."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "id"
    ],
    "$defs": {
      "RunExport": {
        "type": "string",
        "enum": [
          "DEFINITION"
        ],
        "minLength": 1,
        "maxLength": 64
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://omics.us-east-1.amazonaws.com/run/{id}{?export}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**arn**",
            "**id**",
            "**status**",
            "**workflowId**",
            "**workflowType**",
            "**runId**",
            "**roleArn**",
            "**name**",
            "**runGroupId**",
            "**priority**",
            "**definition**",
            "**digest**",
            "**parameters**",
            "**storageCapacity**",
            "**outputUri**",
            "**logLevel**",
            "**resourceDigests**",
            "**startedBy**",
            "**creationTime**",
            "**startTime**",
            "**stopTime**",
            "**statusMessage**",
            "**tags**",
            "**accelerators**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
