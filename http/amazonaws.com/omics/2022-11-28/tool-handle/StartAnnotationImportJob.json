{
  "name": "StartAnnotationImportJob",
  "description": "Starts an annotation import job.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "type": "object",
        "required": [
          "destinationName",
          "roleArn",
          "items"
        ],
        "properties": {
          "destinationName": {
            "description": "A destination annotation store for the job.",
            "type": "string",
            "pattern": "([a-z]){1}([a-z0-9_]){2,254}",
            "minLength": 3,
            "maxLength": 255
          },
          "roleArn": {
            "description": "A service role for the job.",
            "type": "string",
            "pattern": "arn:([^: ]*):([^: ]*):([^: ]*):([0-9]{12}):([^: ]*)",
            "minLength": 20,
            "maxLength": 2048
          },
          "items": {
            "description": "Items to import.",
            "type": "array",
            "items": {
              "$ref": "#/$defs/AnnotationImportItemSource"
            },
            "minItems": 1
          },
          "formatOptions": {
            "description": "Formatting options for a file.",
            "type": "object",
            "properties": {
              "tsvOptions": {
                "allOf": [
                  {
                    "$ref": "#/$defs/TsvOptions"
                  },
                  {
                    "description": "Options for a TSV file."
                  }
                ]
              },
              "vcfOptions": {
                "allOf": [
                  {
                    "$ref": "#/$defs/VcfOptions"
                  },
                  {
                    "description": "Options for a VCF file."
                  }
                ]
              }
            }
          },
          "runLeftNormalization": {
            "description": "The job's left normalization setting.",
            "type": "boolean"
          },
          "annotationFields": {
            "description": " The annotation schema generated by the parsed annotation data. ",
            "type": "object",
            "additionalProperties": {
              "$ref": "#/$defs/AnnotationFieldMapValueString"
            }
          }
        }
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "AnnotationImportItemSource": {
        "type": "object",
        "required": [
          "source"
        ],
        "properties": {
          "source": {
            "allOf": [
              {
                "$ref": "#/$defs/S3Uri"
              },
              {
                "description": "The source file's location in Amazon S3."
              }
            ]
          }
        },
        "description": "A source for an annotation import job."
      },
      "S3Uri": {
        "type": "string",
        "pattern": "s3://([a-z0-9][a-z0-9-.]{1,61}[a-z0-9])/(.{1,1024})"
      },
      "TsvOptions": {
        "type": "object",
        "properties": {
          "readOptions": {
            "allOf": [
              {
                "$ref": "#/$defs/ReadOptions"
              },
              {
                "description": "The file's read options."
              }
            ]
          }
        },
        "description": "Formatting options for a TSV file."
      },
      "ReadOptions": {
        "type": "object",
        "properties": {
          "sep": {
            "allOf": [
              {
                "$ref": "#/$defs/Separator"
              },
              {
                "description": "The file's field separator."
              }
            ]
          },
          "encoding": {
            "allOf": [
              {
                "$ref": "#/$defs/Encoding"
              },
              {
                "description": "The file's encoding."
              }
            ]
          },
          "quote": {
            "allOf": [
              {
                "$ref": "#/$defs/Quote"
              },
              {
                "description": "The file's quote character."
              }
            ]
          },
          "quoteAll": {
            "allOf": [
              {
                "$ref": "#/$defs/QuoteAll"
              },
              {
                "description": "Whether all values need to be quoted, or just those that contain quotes."
              }
            ]
          },
          "escape": {
            "allOf": [
              {
                "$ref": "#/$defs/EscapeChar"
              },
              {
                "description": "A character for escaping quotes in the file."
              }
            ]
          },
          "escapeQuotes": {
            "allOf": [
              {
                "$ref": "#/$defs/EscapeQuotes"
              },
              {
                "description": "Whether quotes need to be escaped in the file."
              }
            ]
          },
          "comment": {
            "allOf": [
              {
                "$ref": "#/$defs/CommentChar"
              },
              {
                "description": "The file's comment character."
              }
            ]
          },
          "header": {
            "allOf": [
              {
                "$ref": "#/$defs/Header"
              },
              {
                "description": "Whether the file has a header row."
              }
            ]
          },
          "lineSep": {
            "allOf": [
              {
                "$ref": "#/$defs/LineSep"
              },
              {
                "description": "A line separator for the file."
              }
            ]
          }
        },
        "description": "Read options for an annotation import job."
      },
      "Separator": {
        "type": "string",
        "minLength": 1,
        "maxLength": 20
      },
      "Encoding": {
        "type": "string",
        "minLength": 1,
        "maxLength": 20
      },
      "Quote": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1
      },
      "QuoteAll": {
        "type": "boolean"
      },
      "EscapeChar": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1
      },
      "EscapeQuotes": {
        "type": "boolean"
      },
      "CommentChar": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1
      },
      "Header": {
        "type": "boolean"
      },
      "LineSep": {
        "type": "string",
        "minLength": 1,
        "maxLength": 20
      },
      "VcfOptions": {
        "type": "object",
        "properties": {
          "ignoreQualField": {
            "allOf": [
              {
                "$ref": "#/$defs/Boolean"
              },
              {
                "description": "The file's ignore qual field setting."
              }
            ]
          },
          "ignoreFilterField": {
            "allOf": [
              {
                "$ref": "#/$defs/Boolean"
              },
              {
                "description": "The file's ignore filter field setting."
              }
            ]
          }
        },
        "description": "Formatting options for a VCF file."
      },
      "Boolean": {
        "type": "boolean"
      },
      "AnnotationFieldMapValueString": {
        "type": "string",
        "minLength": 1,
        "maxLength": 21
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://omics.us-east-1.amazonaws.com/import/annotation"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**jobId**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
