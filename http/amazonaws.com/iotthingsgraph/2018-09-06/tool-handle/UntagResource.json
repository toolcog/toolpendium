{
  "name": "UntagResource",
  "description": "Removes a tag from the specified resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "IotThingsGraphFrontEndService.UntagResource"
