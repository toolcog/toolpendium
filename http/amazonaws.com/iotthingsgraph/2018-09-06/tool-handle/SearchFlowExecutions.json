        "type": "string"
      },
      "Timestamp": {
        "type": "string",
        "format": "date-time"
      },
      "NextToken": {
        "type": "string"
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 250
      }
    }
  },
  "handler": "http",
  "request": {
