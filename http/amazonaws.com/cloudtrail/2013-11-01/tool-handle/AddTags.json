    "$defs": {
      "AddTagsRequest": {
        "type": "object",
        "required": [
          "ResourceId",
          "TagsList"
        ],
        "title": "AddTagsRequest",
        "properties": {
          "ResourceId": {
            "allOf": [
              {
                "$ref": "#/$defs/String"
              },
              {
                "description": "<p>Specifies the ARN of the trail, event data store, or channel to which one or more tags will be added.</p> <p>The format of a trail ARN is: <code>arn:aws:cloudtrail:us-east-2:123456789012:trail/MyTrail</code> </p> <p>The format of an event data store ARN is: <code>arn:aws:cloudtrail:us-east-2:123456789012:eventdatastore/EXAMPLE-f852-4e8f-8bd1-bcf6cEXAMPLE</code> </p> <p>The format of a channel ARN is: <code>arn:aws:cloudtrail:us-east-2:123456789012:channel/01234567890</code> </p>"
              }
            ]
          },
          "TagsList": {
            "allOf": [
              {
                "$ref": "#/$defs/TagsList"
              },
              {
                "description": "Contains a list of tags, up to a limit of 50"
              }
            ]
          }
        },
        "description": "Specifies the tags to add to a trail, event data store, or channel."
      },
      "String": {
        "type": "string"
      },
      "TagsList": {
        "type": "array",
        "description": "A list of tags.",
        "items": {
          "$ref": "#/$defs/Tag"
        },
        "maxItems": 200
      },
      "Tag": {
        "type": "object",
        "required": [
          "Key"
        ],
        "properties": {
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "The key in a key-value pair. The key must be must be no longer than 128 Unicode characters. The key must be unique for the resource to which it applies."
              }
            ]
          },
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/TagValue"
              },
              {
                "description": "The value in a key-value pair of a tag. The value must be no longer than 256 Unicode characters."
              }
            ]
          }
        },
        "description": "A custom key-value pair associated with a resource such as a CloudTrail trail, event data store, or channel."
      },
      "TagKey": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128
      },
      "TagValue": {
        "type": "string",
        "minLength": 1,
        "maxLength": 256
      }
    }
  },
