{
  "name": "CreateChannel",
  "description": "Creates a channel for CloudTrail to ingest events from a partner or external source. After you create a channel, a CloudTrail Lake event data store can log events from the partner or source that you specify.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "com.amazonaws.cloudtrail.v20131101.CloudTrail_20131101.CreateChannel"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/CreateChannelRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "CreateChannelRequest": {
        "type": "object",
        "required": [
          "Name",
          "Source",
          "Destinations"
        ],
        "title": "CreateChannelRequest",
        "properties": {
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/ChannelName"
              },
              {
                "description": "The name of the channel."
              }
            ]
          },
          "Source": {
            "allOf": [
              {
                "$ref": "#/$defs/Source"
              },
              {
                "description": "<p>The name of the partner or external event source. You cannot change this name after you create the channel. A maximum of one channel is allowed per source.</p> <p> A source can be either <code>Custom</code> for all valid non-Amazon Web Services events, or the name of a partner event source. For information about the source names for available partners, see <a href=\"https://docs.aws.amazon.com/awscloudtrail/latest/userguide/query-event-data-store-integration.html#cloudtrail-lake-partner-information\">Additional information about integration partners</a> in the CloudTrail User Guide. </p>"
              }
            ]
          },
          "Destinations": {
            "allOf": [
              {
                "$ref": "#/$defs/Destinations"
              },
              {
                "description": "One or more event data stores to which events arriving through a channel will be logged."
              }
            ]
          },
          "Tags": {
            "$ref": "#/$defs/TagsList"
          }
        }
      },
      "ChannelName": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9._\\-]+$",
        "minLength": 3,
        "maxLength": 128
      },
      "Source": {
        "type": "string",
        "pattern": ".*",
        "minLength": 1,
        "maxLength": 256
      },
      "Destinations": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/Destination"
        },
        "minItems": 1,
        "maxItems": 200
      },
      "Destination": {
        "type": "object",
        "required": [
          "Type",
          "Location"
        ],
        "properties": {
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/DestinationType"
              },
              {
                "description": "The type of destination for events arriving from a channel. For channels used for a CloudTrail Lake integration, the value is <code>EventDataStore</code>. For service-linked channels, the value is <code>AWS_SERVICE</code>. "
              }
            ]
          },
          "Location": {
            "allOf": [
              {
                "$ref": "#/$defs/Location"
              },
              {
                "description": " For channels used for a CloudTrail Lake integration, the location is the ARN of an event data store that receives events from a channel. For service-linked channels, the location is the name of the Amazon Web Services service."
              }
            ]
          }
        },
        "description": "Contains information about the destination receiving events."
      },
      "DestinationType": {
        "type": "string",
        "enum": [
          "EVENT_DATA_STORE",
          "AWS_SERVICE"
        ]
      },
      "Location": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9._/\\-:]+$",
        "minLength": 3,
        "maxLength": 1024
      },
      "TagsList": {
        "type": "array",
        "description": "A list of tags.",
        "items": {
          "$ref": "#/$defs/Tag"
        },
        "maxItems": 200
      },
      "Tag": {
        "type": "object",
        "required": [
          "Key"
        ],
        "properties": {
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/TagKey"
              },
              {
                "description": "The key in a key-value pair. The key must be must be no longer than 128 Unicode characters. The key must be unique for the resource to which it applies."
              }
            ]
          },
          "Value": {
            "allOf": [
              {
                "$ref": "#/$defs/TagValue"
              },
              {
                "description": "The value in a key-value pair of a tag. The value must be no longer than 256 Unicode characters."
              }
            ]
          }
        },
        "description": "A custom key-value pair associated with a resource such as a CloudTrail trail, event data store, or channel."
      },
      "TagKey": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128
      },
      "TagValue": {
        "type": "string",
        "minLength": 1,
        "maxLength": 256
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
