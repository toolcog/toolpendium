      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**completionTime**",
            "**id**",
            "**importName**",
            "**inputS3Bucket**",
            "**inputS3Key**",
            "**numberOfRecordsFailed**",
            "**numberOfRecordsSuccess**",
            "**startTime**",
            "**status**",
