{
  "name": "DeleteRoute",
  "description": "Deletes an existing route.",
  "parameters": {
    "type": "object",
    "properties": {
      "meshName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 255,
        "description": "The name of the service mesh in which to delete the route."
