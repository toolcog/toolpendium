      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/ListRulesRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "ListRulesRequest": {
        "type": "object",
        "title": "ListRulesRequest",
        "properties": {
          "NamePrefix": {
            "allOf": [
              {
                "$ref": "#/$defs/RuleName"
              },
              {
                "description": "The prefix matching the rule name."
              }
            ]
          },
          "EventBusName": {
            "allOf": [
              {
                "$ref": "#/$defs/EventBusNameOrArn"
              },
              {
                "description": "The name or ARN of the event bus to list the rules for. If you omit this, the default event bus is used."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "The token returned by a previous call to retrieve the next set of results."
              }
            ]
          },
          "Limit": {
            "allOf": [
              {
                "$ref": "#/$defs/LimitMax100"
              },
              {
                "description": "The maximum number of results to return."
              }
            ]
          }
        }
      },
      "RuleName": {
        "type": "string",
        "pattern": "[\\.\\-_A-Za-z0-9]+",
        "minLength": 1,
        "maxLength": 64
      },
      "EventBusNameOrArn": {
        "type": "string",
        "pattern": "(arn:aws[\\w-]*:events:[a-z]{2}-[a-z]+-[\\w-]+:[0-9]{12}:event-bus\\/)?[/\\.\\-_A-Za-z0-9]+",
        "minLength": 1,
        "maxLength": 1600
      },
      "NextToken": {
        "type": "string",
        "minLength": 1,
        "maxLength": 2048
      },
      "LimitMax100": {
        "type": "integer",
        "minimum": 1,
        "maximum": 100
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
