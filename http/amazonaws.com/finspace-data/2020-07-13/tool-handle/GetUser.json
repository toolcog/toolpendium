{
  "name": "GetUser",
  "description": "Retrieves details for a specific user.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "string",
        "pattern": ".*\\S.*",
        "minLength": 1,
        "maxLength": 26,
        "description": "The unique identifier of the user to get data for."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
