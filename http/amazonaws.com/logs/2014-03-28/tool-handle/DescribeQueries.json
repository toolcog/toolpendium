          }
        }
      },
      "LogGroupName": {
        "type": "string",
        "pattern": "[\\.\\-_/#A-Za-z0-9]+",
        "minLength": 1,
        "maxLength": 512
      },
      "QueryStatus": {
        "type": "string",
        "enum": [
          "Scheduled",
          "Running",
          "Complete",
          "Failed",
          "Cancelled",
          "Timeout",
          "Unknown"
        ]
      },
      "DescribeQueriesMaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 1000
      },
      "NextToken": {
        "type": "string",
        "description": "The token for the next set of items to return. The token expires after 24 hours.",
        "minLength": 1
      }
    }
