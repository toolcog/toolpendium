{
  "name": "UntagResource",
  "description": "Removes one or more tags from the specified resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "Logs_20140328.UntagResource"
