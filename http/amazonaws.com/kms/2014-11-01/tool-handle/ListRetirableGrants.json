        "type": "string",
        "description": "Pagination limit"
      },
      "Marker": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "TrentService.ListRetirableGrants"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
