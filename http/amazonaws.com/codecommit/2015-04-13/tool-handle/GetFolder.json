{
  "name": "GetFolder",
  "description": "Returns the contents of a specified folder in a repository.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "CodeCommit_20150413.GetFolder"
