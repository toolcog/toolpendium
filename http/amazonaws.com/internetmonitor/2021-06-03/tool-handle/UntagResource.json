{
  "name": "UntagResource",
  "description": "Removes a tag from a resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "ResourceArn": {
        "type": "string",
        "pattern": "arn:.*",
        "minLength": 20,
        "maxLength": 512,
        "description": "The Amazon Resource Name (ARN) for a tag you remove a resource from."
      },
      "tagKeys": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/TagKey"
        },
        "minItems": 0,
        "maxItems": 200,
        "description": "Tag keys that you remove from a resource."
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "ResourceArn",
      "tagKeys"
    ],
    "$defs": {
      "TagKey": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://internetmonitor.us-east-1.api.aws/tags/{ResourceArn}#tagKeys{?tagKeys}"
    },
    "headers": {
      "X-Amz-Content-Sha256": {
        "$": "X-Amz-Content-Sha256"
      },
      "X-Amz-Date": {
        "$": "X-Amz-Date"
      },
      "X-Amz-Algorithm": {
        "$": "X-Amz-Algorithm"
      },
      "X-Amz-Credential": {
        "$": "X-Amz-Credential"
      },
      "X-Amz-Security-Token": {
        "$": "X-Amz-Security-Token"
      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    }
  },
