    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AmazonSSM.UpdateAssociationStatus"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateAssociationStatusRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "UpdateAssociationStatusRequest": {
        "type": "object",
        "required": [
          "Name",
          "InstanceId",
          "AssociationStatus"
        ],
        "title": "UpdateAssociationStatusRequest",
        "properties": {
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/DocumentARN"
              },
              {
                "description": "The name of the SSM document."
              }
            ]
          },
          "InstanceId": {
            "allOf": [
              {
                "$ref": "#/$defs/InstanceId"
              },
              {
                "description": "The managed node ID."
              }
            ]
          },
          "AssociationStatus": {
            "allOf": [
              {
                "$ref": "#/$defs/AssociationStatus"
              },
              {
                "description": "The association status."
              }
            ]
          }
        }
      },
      "DocumentARN": {
        "type": "string",
        "pattern": "^[a-zA-Z0-9_\\-.:/]{3,128}$"
      },
      "InstanceId": {
        "type": "string",
        "pattern": "(^i-(\\w{8}|\\w{17})$)|(^mi-\\w{17}$)"
      },
      "AssociationStatus": {
        "type": "object",
        "required": [
          "Date",
          "Name",
          "Message"
        ],
        "properties": {
          "Date": {
            "allOf": [
              {
                "$ref": "#/$defs/DateTime"
              },
              {
                "description": "The date when the status changed."
              }
            ]
          },
          "Name": {
            "allOf": [
              {
                "$ref": "#/$defs/AssociationStatusName"
              },
              {
                "description": "The status."
              }
            ]
          },
          "Message": {
            "allOf": [
              {
                "$ref": "#/$defs/StatusMessage"
              },
              {
                "description": "The reason for the status."
              }
            ]
          },
          "AdditionalInfo": {
            "allOf": [
              {
                "$ref": "#/$defs/StatusAdditionalInfo"
              },
              {
                "description": "A user-defined string."
              }
            ]
          }
        },
        "description": "Describes an association status."
      },
      "DateTime": {
        "type": "string",
        "format": "date-time"
      },
      "AssociationStatusName": {
        "type": "string",
        "enum": [
          "Pending",
          "Success",
          "Failed"
        ]
      },
      "StatusMessage": {
        "type": "string",
        "minLength": 1,
        "maxLength": 1024
      },
      "StatusAdditionalInfo": {
        "type": "string",
        "maxLength": 1024
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
