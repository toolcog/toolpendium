{
  "name": "GetPatchBaseline",
  "description": "Retrieves information about a patch baseline.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AmazonSSM.GetPatchBaseline"
