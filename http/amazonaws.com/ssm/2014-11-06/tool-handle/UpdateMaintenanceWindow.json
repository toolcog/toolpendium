      },
      "X-Amz-Signature": {
        "$": "X-Amz-Signature"
      },
      "X-Amz-SignedHeaders": {
        "$": "X-Amz-SignedHeaders"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**WindowId**",
            "**Name**",
            "**Description**",
            "**StartDate**",
            "**EndDate**",
            "**Schedule**",
            "**ScheduleTimezone**",
            "**ScheduleOffset**",
            "**Duration**",
            "**Cutoff**",
            "**AllowUnassociatedTargets**",
            "**Enabled**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "480": {
      "$encode": "markdown",
