{
  "name": "GetOpsSummary",
  "description": "View a summary of operations metadata (OpsData) based on specified filters and aggregators. OpsData can include information about Amazon Web Services Systems Manager OpsCenter operational workitems (OpsItems) as well as information about any Amazon Web Services resource or service configured to report OpsData to Amazon Web Services Systems Manager Explorer. ",
  "parameters": {
    "type": "object",
    "properties": {
      "MaxResults": {
        "type": "string",
        "description": "Pagination limit"
      },
      "NextToken": {
        "type": "string",
        "description": "Pagination token"
      },
      "X-Amz-Target": {
        "type": "string",
        "enum": [
          "AmazonSSM.GetOpsSummary"
        ]
      },
      "X-Amz-Content-Sha256": {
        "type": "string"
      },
      "X-Amz-Date": {
        "type": "string"
      },
      "X-Amz-Algorithm": {
        "type": "string"
      },
      "X-Amz-Credential": {
        "type": "string"
      },
      "X-Amz-Security-Token": {
        "type": "string"
      },
      "X-Amz-Signature": {
        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/GetOpsSummaryRequest"
      }
    },
    "required": [
      "X-Amz-Target",
      "body"
    ],
    "$defs": {
      "GetOpsSummaryRequest": {
        "type": "object",
        "title": "GetOpsSummaryRequest",
        "properties": {
          "SyncName": {
            "allOf": [
              {
                "$ref": "#/$defs/ResourceDataSyncName"
              },
              {
                "description": "Specify the name of a resource data sync to get."
              }
            ]
          },
          "Filters": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsFilterList"
              },
              {
                "description": "Optional filters used to scope down the returned OpsData. "
              }
            ]
          },
          "Aggregators": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsAggregatorList"
              },
              {
                "description": "Optional aggregators that return counts of OpsData based on one or more expressions."
              }
            ]
          },
          "ResultAttributes": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsResultAttributeList"
              },
              {
                "description": "The OpsData data type to return."
              }
            ]
          },
          "NextToken": {
            "allOf": [
              {
                "$ref": "#/$defs/NextToken"
              },
              {
                "description": "A token to start the list. Use this token to get the next set of results. "
              }
            ]
          },
          "MaxResults": {
            "allOf": [
              {
                "$ref": "#/$defs/MaxResults"
              },
              {
                "description": "The maximum number of items to return for this call. The call also returns a token that you can specify in a subsequent call to get the next set of results."
              }
            ]
          }
        }
      },
      "ResourceDataSyncName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 64
      },
      "OpsFilterList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/OpsFilter"
        },
        "minItems": 1,
        "maxItems": 5
      },
      "OpsFilter": {
        "type": "object",
        "required": [
          "Key",
          "Values"
        ],
        "properties": {
          "Key": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsFilterKey"
              },
              {
                "description": "The name of the filter."
              }
            ]
          },
          "Values": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsFilterValueList"
              },
              {
                "description": "The filter value."
              }
            ]
          },
          "Type": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsFilterOperatorType"
              },
              {
                "description": "The type of filter."
              }
            ]
          }
        },
        "description": "A filter for viewing OpsData summaries."
      },
      "OpsFilterKey": {
        "type": "string",
        "minLength": 1,
        "maxLength": 200
      },
      "OpsFilterValueList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/OpsFilterValue"
        },
        "minItems": 1,
        "maxItems": 40
      },
      "OpsFilterValue": {
        "type": "string",
        "minLength": 0,
        "maxLength": 256
      },
      "OpsFilterOperatorType": {
        "type": "string",
        "enum": [
          "Equal",
          "NotEqual",
          "BeginWith",
          "LessThan",
          "GreaterThan",
          "Exists"
        ]
      },
      "OpsAggregatorList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/OpsAggregator"
        },
        "minItems": 1,
        "maxItems": 12
      },
      "OpsAggregator": {
        "type": "object",
        "properties": {
          "AggregatorType": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsAggregatorType"
              },
              {
                "description": "Either a <code>Range</code> or <code>Count</code> aggregator for limiting an OpsData summary."
              }
            ]
          },
          "TypeName": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsDataTypeName"
              },
              {
                "description": "The data type name to use for viewing counts of OpsData."
              }
            ]
          },
          "AttributeName": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsDataAttributeName"
              },
              {
                "description": "The name of an OpsData attribute on which to limit the count of OpsData."
              }
            ]
          },
          "Values": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsAggregatorValueMap"
              },
              {
                "description": "The aggregator value."
              }
            ]
          },
          "Filters": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsFilterList"
              },
              {
                "description": "The aggregator filters."
              }
            ]
          },
          "Aggregators": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsAggregatorList"
              },
              {
                "description": "A nested aggregator for viewing counts of OpsData."
              }
            ]
          }
        },
        "description": "One or more aggregators for viewing counts of OpsData using different dimensions such as <code>Source</code>, <code>CreatedTime</code>, or <code>Source and CreatedTime</code>, to name a few."
      },
      "OpsAggregatorType": {
        "type": "string",
        "pattern": "^(range|count|sum)",
        "minLength": 1,
        "maxLength": 20
      },
      "OpsDataTypeName": {
        "type": "string",
        "pattern": "^(AWS|Custom):.*$",
        "minLength": 1,
        "maxLength": 100
      },
      "OpsDataAttributeName": {
        "type": "string",
        "minLength": 1,
        "maxLength": 128
      },
      "OpsAggregatorValueMap": {
        "type": "object",
        "minProperties": 0,
        "maxProperties": 5,
        "additionalProperties": {
          "$ref": "#/$defs/OpsAggregatorValue"
        }
      },
      "OpsAggregatorValue": {
        "type": "string",
        "minLength": 0,
        "maxLength": 2048
      },
      "OpsResultAttributeList": {
        "type": "array",
        "items": {
          "$ref": "#/$defs/OpsResultAttribute"
        },
        "minItems": 1
      },
      "OpsResultAttribute": {
        "type": "object",
        "required": [
          "TypeName"
        ],
        "properties": {
          "TypeName": {
            "allOf": [
              {
                "$ref": "#/$defs/OpsDataTypeName"
              },
              {
                "description": "Name of the data type. Valid value: <code>AWS:OpsItem</code>, <code>AWS:EC2InstanceInformation</code>, <code>AWS:OpsItemTrendline</code>, or <code>AWS:ComplianceSummary</code>."
              }
            ]
          }
        },
        "description": "The OpsItem data type to return."
      },
      "NextToken": {
        "type": "string"
      },
      "MaxResults": {
        "type": "integer",
        "minimum": 1,
        "maximum": 50
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://ssm.us-east-1.amazonaws.com/#X-Amz-Target=AmazonSSM.GetOpsSummary{?MaxResults,NextToken}"
    },
    "headers": {
      "X-Amz-Target": {
