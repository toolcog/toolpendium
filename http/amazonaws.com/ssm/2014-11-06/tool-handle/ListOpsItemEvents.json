        "type": "array",
        "items": {
          "$ref": "#/$defs/OpsItemEventFilterValue"
        }
      },
      "OpsItemEventFilterValue": {
        "type": "string",
        "pattern": "^(oi)-[0-9a-f]{12}$",
        "minLength": 1,
        "maxLength": 15
      },
      "OpsItemEventFilterOperator": {
        "type": "string",
        "enum": [
