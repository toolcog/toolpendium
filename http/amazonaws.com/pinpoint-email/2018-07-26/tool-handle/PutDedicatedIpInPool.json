        "type": "object",
        "required": [
          "DestinationPoolName"
        ],
        "properties": {
          "DestinationPoolName": {
            "description": "The name of a dedicated IP pool.",
            "type": "string"
          }
        }
      }
    },
    "required": [
      "IP",
      "body"
    ]
  },
  "handler": "http",
