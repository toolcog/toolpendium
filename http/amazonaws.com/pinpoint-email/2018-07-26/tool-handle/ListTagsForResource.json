        "type": "string"
      },
      "X-Amz-SignedHeaders": {
        "type": "string"
      }
    },
    "required": [
      "ResourceArn"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://email.us-east-1.amazonaws.com/v1/email/tags#ResourceArn{?ResourceArn}"
    },
