{
  "name": "updateFilterProperty",
  "description": "Updates view's filter property.",
  "parameters": {
    "type": "object",
    "properties": {
      "viewId": {
        "format": "int64",
        "type": "integer",
        "description": "view's identifier"
      },
      "filterProperty": {
        "type": "string",
        "description": "view's filter property name"
      },
      "body": {
        "$ref": "#/components/schemas/FilterPropertyDTO",
        "description": "Updated view's filter property."
      }
    },
    "required": [
      "viewId",
      "filterProperty",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/browser/views/{viewId}/filter/{filterProperty}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
