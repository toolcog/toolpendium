{
  "name": "getLocalSettings",
  "description": "Returns view's local settings (for current user).",
  "parameters": {
    "type": "object",
    "properties": {
      "viewId": {
        "format": "int64",
        "type": "integer",
        "description": "view's identifier"
      }
    },
    "required": [
      "viewId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/browser/views/{viewId}/settings/local"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**maxLinesInRow**",
            "**maxRows**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
