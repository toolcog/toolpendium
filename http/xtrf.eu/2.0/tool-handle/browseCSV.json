{
  "name": "browseCSV",
  "description": "Searches for data (ie. customer, task, etc) and returns it in a CSV form.",
  "parameters": {
    "type": "object",
    "properties": {
      "viewId": {
        "format": "int64",
        "type": "integer",
        "description": "view's identifier"
      },
      "separator": {
        "type": "string",
        "description": "csv field separator"
      },
      "secondarySeparator": {
        "type": "string",
        "description": "secondary csv field separator"
      },
      "additionalOrder": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/browser/csv{?viewId,separator,secondarySeparator,additionalOrder}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
