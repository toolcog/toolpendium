{
  "name": "getAllActive",
  "description": "Returns active workflows list",
  "parameters": {
    "type": "object",
    "properties": {
      "nameEquals": {
        "type": "string",
        "description": "exact name of entity"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/services/active{?nameEquals}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**: should this value be available on the XTRF selection lists",
            "**default**: should this value be set by default in XTRF selection lists",
            "**id**: internal identifier",
            "**name**: localised name (formatted in the current user's locale)",
            "**preferred**: should this value be available on the top of XTRF selection lists, in the Preferred section"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
