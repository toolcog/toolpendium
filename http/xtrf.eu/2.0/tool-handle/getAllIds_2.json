{
  "name": "getAllIds_2",
  "description": "Returns clients' internal identifiers.",
  "parameters": {
    "type": "object",
    "properties": {
      "updatedSince": {
        "format": "int64",
        "type": "integer",
        "description": "only clients modified since this timestamp"
      },
      "nameEquals": {
        "type": "string",
        "description": "exact name of client"
      },
      "emailEquals": {
        "type": "string",
        "description": "exact email of client"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/customers/ids{?updatedSince,nameEquals,emailEquals}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
