{
  "name": "getPayments_1",
  "description": "Returns all payments for the vendor invoice.",
  "parameters": {
    "type": "object",
    "properties": {
      "invoiceId": {
        "format": "int64",
        "type": "integer",
        "description": "vendor invoice's internal identifier"
      }
    },
    "required": [
      "invoiceId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/accounting/providers/invoices/{invoiceId}/payments"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**amount**",
            "**notes**",
            [
              "**paymentDate**",
              {
                "$ul": [
                  "**value**"
                ]
              }
            ],
            "**paymentMethodId**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
