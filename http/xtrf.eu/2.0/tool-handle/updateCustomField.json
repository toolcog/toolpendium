{
  "name": "updateCustomField",
  "description": "Updates given custom field of a given client.",
  "parameters": {
    "type": "object",
    "properties": {
      "customerId": {
        "format": "int64",
        "type": "integer",
        "description": "client's internal identifier"
      },
      "customFieldKey": {
        "type": "string",
        "description": "custom field's key"
      },
      "body": {
        "$ref": "#/components/schemas/CustomFieldDTO",
        "description": "Updated custom field of a given client."
      }
    },
    "required": [
      "customerId",
      "customFieldKey",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/customers/{customerId}/customFields/{customFieldKey}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**key**",
            "**name**",
            "**type**",
            "**value**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
