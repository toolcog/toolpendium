{
  "name": "changePassword",
  "description": "Sets user's password to a new value.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "format": "int64",
        "type": "integer",
        "description": "user's internal identifier"
      },
      "body": {
        "properties": {
          "newPassword": {
            "description": "new password",
            "type": "string"
          },
          "oldPassword": {
            "description": "old password",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/users/{userId}/password"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
