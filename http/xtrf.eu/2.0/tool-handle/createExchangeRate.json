{
  "name": "createExchangeRate",
  "description": "Adding currency exchange rates via API",
  "parameters": {
    "type": "object",
    "properties": {
      "isoCode": {
        "type": "string",
        "description": "iso code, https://www.xe.com/iso4217.php"
      },
      "body": {
        "$ref": "#/components/schemas/CurrencyHistoryDTO",
        "description": "Adding new currency exchange rates"
      }
    },
    "required": [
      "isoCode",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/dictionaries/currency/{isoCode}/exchangeRate"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
