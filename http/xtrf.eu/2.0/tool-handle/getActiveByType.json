{
  "name": "getActiveByType",
  "description": "Returns active values from a given dictionary.",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "type": "string",
        "description": "dictionary type"
      },
      "nameEquals": {
        "type": "string",
        "description": "exact name of entity"
      }
    },
    "required": [
      "type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/dictionaries/{type}/active{?nameEquals}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**",
            [
              "**andClearEventsQueue**",
              {
                "$ul": [
                  "**all**",
                  "**empty**",
                  "**readyToBeDispatched**"
                ]
              }
            ],
            "**auditDisplayName**",
            "**auditPath**",
            "**classNameKey**",
            "**classSimpleName**",
            "**compoundId**",
            "**defaultEntity**",
            "**displayName**",
            "**entityMarkedAsNotSupposedToBePersisted**",
            "**eventsQueueReadyToBeDispatched**",
            "**id**",
            [
              "**identifier**",
              {
                "$ul": [
                  "**compoundId**",
                  "**id**"
                ]
              }
            ],
            "**internalDescription**",
            "**lastModificationDate**",
            "**name**",
            "**packedCompoundId**",
            "**preferedEntity**",
            "**preferred**",
            "**version**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
