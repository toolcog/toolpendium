{
  "name": "getByIsoCode",
  "description": "Returns currency exchange rates.",
  "parameters": {
    "type": "object",
    "properties": {
      "isoCode": {
        "type": "string",
        "description": "iso code, https://www.xe.com/iso4217.php"
      }
    },
    "required": [
      "isoCode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/dictionaries/currency/{isoCode}/exchangeRate"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**dateFrom**",
              {
                "$ul": [
                  "**value**"
                ]
              }
            ],
            "**exchangeRate**",
            [
              "**lastModification**",
              {
                "$ul": [
                  "**value**"
                ]
              }
            ],
            "**originDetails**",
            [
              "**publicationDate**",
              {
                "$ul": [
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
