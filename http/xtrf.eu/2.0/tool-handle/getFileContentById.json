{
  "name": "getFileContentById",
  "description": "Downloads a file content.",
  "parameters": {
    "type": "object",
    "properties": {
      "fileId": {
        "type": "string",
        "description": "file's internal identifier"
      },
      "fileName": {
        "type": "string",
        "description": "file's name"
      }
    },
    "required": [
      "fileId",
      "fileName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/v2/projects/files/{fileId}/download/{fileName}"
    }
  },
  "responses": {}
}
