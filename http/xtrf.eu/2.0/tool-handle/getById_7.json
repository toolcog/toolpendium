{
  "name": "getById_7",
  "description": "Returns project details. If the specified project ID refers to Smart Project, 400 Bad Request is returned instead.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "type": "string",
        "description": "project's internal identifier"
      },
      "embed": {
        "type": "string",
        "description": "list of additional fields which should be embedded in the response (available options: tasks)"
      }
    },
    "required": [
      "projectId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/projects/{projectId}{?embed}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**categoriesIds**",
            "**contactPersonId**",
            [
              "**contacts**",
              {
                "$ul": [
                  "**additionalIds**",
                  "**primaryId**",
                  "**sendBackToId**"
                ]
              }
            ],
            [
              "**customFields**",
              {
                "$ul": [
                  "**customFields**",
                  "**empty**"
                ]
              }
            ],
            "**customerId**",
            [
              "**dates**",
              {
                "$ul": [
                  [
                    "**actualDeliveryDate**",
                    {
                      "$ul": [
                        "**value**"
                      ]
                    }
                  ],
                  [
                    "**actualStartDate**",
                    {
                      "$ul": [
                        "**value**"
                      ]
                    }
                  ],
                  [
                    "**deadline**",
                    {
                      "$ul": [
                        "**value**"
                      ]
                    }
                  ],
                  [
                    "**startDate**",
                    {
                      "$ul": [
                        "**value**"
                      ]
                    }
                  ]
                ]
              }
            ],
            [
              "**finance**",
              {
                "$ul": [
                  "**ROI**",
                  "**currencyId**",
                  "**margin**",
                  "**payables**",
                  "**profit**",
                  "**receivables**",
                  "**totalAgreed**",
                  "**totalCost**"
                ]
              }
            ],
            "**id**",
            "**idNumber**",
            [
              "**instructions**",
              {
                "$ul": [
                  "**forProvider**",
                  "**fromCustomer**",
                  "**internal**",
                  "**notes**",
                  "**paymentNoteForCustomer**",
                  "**paymentNoteForVendor**"
                ]
              }
            ],
            "**isClassicProject**",
            "**name**",
            "**projectId**",
            "**projectManagerId**",
            "**specializationId**",
            "**status**",
            "**tasks**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
