{
  "name": "updateInstructions",
  "description": "Updates instructions for a job.",
  "parameters": {
    "type": "object",
    "properties": {
      "jobId": {
        "type": "string",
        "description": "job's internal identifier"
      },
      "body": {
        "$ref": "#/components/schemas/InstructionsDTO",
        "description": "Updated instructions for a job."
      }
    },
    "required": [
      "jobId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/jobs/{jobId}/instructions"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
