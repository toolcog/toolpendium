{
  "name": "getActive",
  "description": "Returns active dictionary entities for all types.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/dictionaries/active"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**calculationUnit**",
            "**category**",
            "**country**",
            "**currency**",
            "**industry**",
            "**jobType**",
            "**language**",
            "**leadSource**",
            "**personDepartment**",
            "**personPosition**",
            "**province**",
            "**specialization**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
