{
  "name": "browseJSON",
  "description": "Searches for data (ie. customer, task, etc) and returns it in a tabular form.",
  "parameters": {
    "type": "object",
    "properties": {
      "viewId": {
        "format": "int64",
        "type": "integer",
        "description": "view's identifier"
      },
      "page": {
        "default": 0,
        "format": "int32",
        "type": "integer"
      },
      "additionalOrder": {
        "type": "string"
      },
      "useDeferredColumns": {
        "type": "string"
      },
      "maxRows": {
        "default": 0,
        "format": "int32",
        "type": "integer",
        "description": "overrides view's default rows limit, supported values 10 to 1000"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/browser{?viewId,page,additionalOrder,useDeferredColumns,maxRows}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
