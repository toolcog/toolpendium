{
  "name": "updateSettings",
  "description": "Updates view's settings.",
  "parameters": {
    "type": "object",
    "properties": {
      "viewId": {
        "format": "int64",
        "type": "integer",
        "description": "view's identifier"
      },
      "body": {
        "$ref": "#/components/schemas/SettingsDTO",
        "description": "Updated view's settings."
      }
    },
    "required": [
      "viewId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/browser/views/{viewId}/settings"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**local**",
              {
                "$ul": [
                  "**maxLinesInRow**",
                  "**maxRows**"
                ]
              }
            ],
            "**name**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
