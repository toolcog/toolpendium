{
  "name": "updateName",
  "description": "Updates name of a given task.",
  "parameters": {
    "type": "object",
    "properties": {
      "taskId": {
        "type": "string",
        "description": "task's internal identifier"
      },
      "body": {
        "$ref": "#/$defs/StringDTO"
      }
    },
    "required": [
      "taskId",
      "body"
    ],
    "$defs": {
      "StringDTO": {
        "properties": {
          "value": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://presentation.s.xtrf.eu/home-api/tasks/{taskId}/name"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**value**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
