{
  "name": "changePassword",
  "description": "Change Password",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "string"
      },
      "body": {
        "properties": {
          "newPassword": {
            "properties": {
              "value": {
                "example": "uTVM,TPw55",
                "type": "string"
              }
            },
            "type": "object"
          },
          "oldPassword": {
            "properties": {
              "value": {
                "example": "{{password}}",
                "type": "string"
              }
            },
            "type": "object"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://okta.local/api/v1/users/{userId}/credentials/change_password"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
