{
  "name": "changeRecoveryQuestion",
  "description": "Change Recovery Question",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "string"
      },
      "body": {
        "properties": {
          "password": {
            "properties": {
              "value": {
                "example": "{{password}}",
                "type": "string"
              }
            },
            "type": "object"
          },
          "recovery_question": {
            "properties": {
              "answer": {
                "example": "My recovery credentials are updated",
                "type": "string"
              },
              "question": {
                "example": "What happens when I update my question",
                "type": "string"
              }
            },
            "type": "object"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://okta.local/api/v1/users/{userId}/credentials/change_recovery_question"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
