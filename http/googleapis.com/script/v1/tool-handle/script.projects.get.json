{
  "name": "script_projects_get",
  "description": "Gets a script project's metadata.",
  "parameters": {
    "type": "object",
    "properties": {
      "scriptId": {
        "type": "string",
        "description": "The script project's Drive ID."
      },
      "$.xgafv": {
        "enum": [
          "1",
          "2"
        ],
        "type": "string",
        "description": "V1 error format."
      },
      "access_token": {
        "type": "string",
        "description": "OAuth access token."
      },
      "alt": {
        "enum": [
          "json",
          "media",
          "proto"
        ],
        "type": "string",
        "description": "Data format for response."
      },
      "callback": {
        "type": "string",
        "description": "JSONP"
      },
      "fields": {
        "type": "string",
        "description": "Selector specifying which fields to include in a partial response."
      },
      "key": {
        "type": "string",
        "description": "API key. Your API key identifies your project and provides you with API access, quota, and reports. Required unless you provide an OAuth 2.0 token."
      },
      "oauth_token": {
        "type": "string",
        "description": "OAuth 2.0 token for the current user."
      },
      "prettyPrint": {
        "type": "boolean",
        "description": "Returns response with indentations and line breaks."
      },
      "quotaUser": {
        "type": "string",
        "description": "Available to use for quota purposes for server-side applications. Can be any arbitrary string assigned to a user, but should not exceed 40 characters."
      },
      "upload_protocol": {
        "type": "string",
        "description": "Upload protocol for media (e.g. \"raw\", \"multipart\")."
      },
      "uploadType": {
        "type": "string",
        "description": "Legacy upload protocol for media (e.g. \"media\", \"multipart\")."
      }
    },
    "required": [
      "scriptId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://script.googleapis.com//v1/projects/{scriptId}{?$.xgafv,access_token,alt,callback,fields,key,oauth_token,prettyPrint,quotaUser,upload_protocol,uploadType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The script project resource.",
        "**Key properties:**",
        {
          "$ul": [
            "**createTime**: When the script was created.",
            [
              "**creator**: A simple user profile resource.",
              {
                "$ul": [
                  "**domain**: The user's domain.",
                  "**email**: The user's identifying email address.",
                  "**name**: The user's display name.",
                  "**photoUrl**: The user's photo."
                ]
              }
            ],
            [
              "**lastModifyUser**: A simple user profile resource.",
              {
                "$ul": [
                  "**domain**: The user's domain.",
                  "**email**: The user's identifying email address.",
                  "**name**: The user's display name.",
                  "**photoUrl**: The user's photo."
                ]
              }
            ],
            "**parentId**: The parent's Drive ID that the script will be attached to. This is usually the ID of a Google Document or Google Sheet. This filed is optional, and if not set, a stand-alone script will be created.",
            "**scriptId**: The script project's Drive ID.",
            "**title**: The title for the project.",
            "**updateTime**: When the script was last updated."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
