{
  "name": "postPackageServiceJson",
  "parameters": {
    "type": "object",
    "properties": {
      "path": {
        "type": "string"
      },
      "cmd": {
        "type": "string"
      },
      "groupName": {
        "type": "string"
      },
      "packageName": {
        "type": "string"
      },
      "packageVersion": {
        "type": "string"
      },
      "_charset_": {
        "type": "string"
      },
      "force": {
        "type": "boolean"
      },
      "recursive": {
        "type": "boolean"
      },
      "body": {
        "properties": {
          "package": {
            "format": "binary",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "path",
      "cmd"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//crx/packmgr/service/.json/{path}{?cmd,groupName,packageName,packageVersion,_charset_,force,recursive}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
