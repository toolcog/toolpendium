{
  "name": "postAuthorizableKeystore",
  "parameters": {
    "type": "object",
    "properties": {
      "intermediatePath": {
        "type": "string"
      },
      "authorizableId": {
        "type": "string"
      },
      ":operation": {
        "type": "string"
      },
      "currentPassword": {
        "type": "string"
      },
      "newPassword": {
        "type": "string"
      },
      "rePassword": {
        "type": "string"
      },
      "keyPassword": {
        "type": "string"
      },
      "keyStorePass": {
        "type": "string"
      },
      "alias": {
        "type": "string"
      },
      "newAlias": {
        "type": "string"
      },
      "removeAlias": {
        "type": "string"
      },
      "body": {
        "properties": {
          "cert-chain": {
            "format": "binary",
            "type": "string"
          },
          "keyStore": {
            "format": "binary",
            "type": "string"
          },
          "pk": {
            "format": "binary",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "intermediatePath",
      "authorizableId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//{intermediatePath}/{authorizableId}.ks.html{?:operation,currentPassword,newPassword,rePassword,keyPassword,keyStorePass,alias,newAlias,removeAlias}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**aliases**",
            "**exists**: False if truststore don't exist"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
