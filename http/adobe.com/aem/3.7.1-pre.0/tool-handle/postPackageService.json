{
  "name": "postPackageService",
  "parameters": {
    "type": "object",
    "properties": {
      "cmd": {
        "type": "string"
      }
    },
    "required": [
      "cmd"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//crx/packmgr/service.jsp{?cmd}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
