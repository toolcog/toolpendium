{
  "name": "postNode",
  "parameters": {
    "type": "object",
    "properties": {
      "path": {
        "type": "string"
      },
      "name": {
        "type": "string"
      },
      ":operation": {
        "type": "string"
      },
      "deleteAuthorizable": {
        "type": "string"
      },
      "body": {
        "properties": {
          "file": {
            "format": "binary",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "path",
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//{path}/{name}{?:operation,deleteAuthorizable}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {}
}
