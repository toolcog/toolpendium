{
  "name": "postTruststore",
  "parameters": {
    "type": "object",
    "properties": {
      ":operation": {
        "type": "string"
      },
      "newPassword": {
        "type": "string"
      },
      "rePassword": {
        "type": "string"
      },
      "keyStoreType": {
        "type": "string"
      },
      "removeAlias": {
        "type": "string"
      },
      "body": {
        "properties": {
          "certificate": {
            "format": "binary",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//libs/granite/security/post/truststore{?:operation,newPassword,rePassword,keyStoreType,removeAlias}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
