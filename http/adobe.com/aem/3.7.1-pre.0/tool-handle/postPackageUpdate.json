{
  "name": "postPackageUpdate",
  "parameters": {
    "type": "object",
    "properties": {
      "groupName": {
        "type": "string"
      },
      "packageName": {
        "type": "string"
      },
      "version": {
        "type": "string"
      },
      "path": {
        "type": "string"
      },
      "filter": {
        "type": "string"
      },
      "_charset_": {
        "type": "string"
      }
    },
    "required": [
      "groupName",
      "packageName",
      "version",
      "path"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//crx/packmgr/update.jsp{?groupName,packageName,version,path,filter,_charset_}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
