{
  "name": "sslSetup",
  "parameters": {
    "type": "object",
    "properties": {
      "keystorePassword": {
        "type": "string"
      },
      "keystorePasswordConfirm": {
        "type": "string"
      },
      "truststorePassword": {
        "type": "string"
      },
      "truststorePasswordConfirm": {
        "type": "string"
      },
      "httpsHostname": {
        "type": "string"
      },
      "httpsPort": {
        "type": "string"
      },
      "body": {
        "properties": {
          "certificateFile": {
            "format": "binary",
            "type": "string"
          },
          "privatekeyFile": {
            "format": "binary",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "keystorePassword",
      "keystorePasswordConfirm",
      "truststorePassword",
      "truststorePasswordConfirm",
      "httpsHostname",
      "httpsPort"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//libs/granite/security/post/sslSetup.html{?keystorePassword,keystorePasswordConfirm,truststorePassword,truststorePasswordConfirm,httpsHostname,httpsPort}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
