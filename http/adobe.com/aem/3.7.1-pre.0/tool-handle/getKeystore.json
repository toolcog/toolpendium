{
  "name": "getKeystore",
  "parameters": {
    "type": "object",
    "properties": {
      "intermediatePath": {
        "type": "string"
      },
      "authorizableId": {
        "type": "string"
      }
    },
    "required": [
      "intermediatePath",
      "authorizableId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//{intermediatePath}/{authorizableId}/keystore/store.p12"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
