{
  "name": "clean",
  "description": "clean up the uploaded files",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "HEAD",
    "url": {
      "$uri": "http://restful4up.local/clean"
    }
  },
  "responses": {
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
