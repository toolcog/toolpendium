{
  "name": "generatePartialYaraRule",
  "description": "generate partial YARA rules for give executable. (Rule without the condition section)",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "file": {
            "description": "file",
            "format": "binary",
            "type": "string"
          },
          "is_unpacking_required": {
            "enum": [
              "true",
              "false"
            ],
            "type": "string"
          },
          "minimum_string_length": {
            "type": "string"
          },
          "strings_to_ignore": {
            "items": {
              "type": "string"
            },
            "maxItems": 1000,
            "minItems": 0,
            "type": "array"
          }
        },
        "required": [
          "file"
        ]
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://restful4up.local/generate-partial-yara-rules"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**rule**",
              {
                "$ul": [
                  [
                    "**meta**: Yara rule meta data section",
                    {
                      "$ul": [
                        "**date**: Date of the YARA rule created",
                        "**md5sum**: MD5 check sum",
                        "**sha256sum**: SHA 256 check sum",
                        "**sha512sum**: SHA 512 check sum"
                      ]
                    }
                  ],
                  "**name**: Yara rule name",
                  "**strings**: Yara rule string section"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
