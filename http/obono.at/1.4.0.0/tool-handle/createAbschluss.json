    "type": "object",
    "properties": {
      "registrierkasseUuid": {
        "type": "string",
        "description": "The `_uuid` of the `Registrierkasse` to retrieve the `Beleg` collection."
      },
      "body": {
        "$ref": "#/components/schemas/Abschlussbelegdaten",
        "description": "An object that contains all data for a particular `Abschlussbeleg`."
      }
    },
    "required": [
      "registrierkasseUuid",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/registrierkassen/{registrierkasseUuid}/abschluss"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
