        "type": "string",
        "description": "account address"
      },
      "page": {
        "example": "1",
        "type": "string",
        "description": "Current page, see description for more informatiosn how pagination works"
      }
    },
    "required": [
      "address",
      "page"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
