{
  "name": "resendfailedipn4",
  "description": "Returns all subscriptions/IPNs created with an account.",
  "parameters": {
    "type": "object",
    "properties": {
      "notification_id": {
        "example": "62640817eb594da78c7bee00",
        "type": "string",
        "description": "Notification id"
      },
      "body": {
        "properties": {},
        "type": "object"
      }
    },
    "required": [
      "notification_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/bitcoin/webhooks/notifications/failed/{notification_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
