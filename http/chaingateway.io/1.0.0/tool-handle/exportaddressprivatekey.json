{
  "name": "exportaddressprivatekey",
  "description": "exportAddressPrivateKey",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "example": "Evolo_4215179",
        "type": "string"
      },
      "address": {
        "example": "bc1qz3hdws5lt5ug7qwfv4m4ttanaq67ts77ekv95x",
        "type": "string"
      },
      "body": {
        "properties": {
          "password": {
            "example": "test123",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "name",
      "address"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/bitcoin/wallets/{name}/addresses/{address}/export"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**private_key**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**walletname**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
