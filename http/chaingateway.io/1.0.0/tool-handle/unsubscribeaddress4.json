{
  "name": "unsubscribeaddress4",
  "description": "Deletes an existing subscription/IPN for the given address (and contractaddress).",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "63ef470294d74b0f290b4b64",
        "type": "string",
        "description": "Webhook id"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/bitcoin/webhooks/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**id**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
