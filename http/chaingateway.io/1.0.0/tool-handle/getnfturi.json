{
  "name": "getnfturi",
  "description": "Get the URI of an NFT",
  "parameters": {
    "type": "object",
    "properties": {
      "contract": {
        "example": "0x9db475371b5cc2913d3219f72e16a3f101339a05",
        "type": "string",
        "description": "NFT contract address"
      },
      "token_id": {
        "example": "374",
        "type": "string",
        "description": "Id of the NFT"
      }
    },
    "required": [
      "contract",
      "token_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/ethereum/nfts/{contract}/uri/{token_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**uri**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
