{
  "name": "getalltimehigh",
  "description": "getAllTimeHigh",
  "parameters": {
    "type": "object",
    "properties": {
      "currency1": {
        "example": "btc",
        "type": "string"
      },
      "currenty2": {
        "example": "eur",
        "type": "string"
      }
    },
    "required": [
      "currency1",
      "currenty2"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/markets/prices/{currency1}/{currenty2}/ath"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  [
                    "**bitcoin**",
                    {
                      "$ul": [
                        "**ath**",
                        [
                          "**athDate**",
                          {
                            "$ul": [
                              "**offset**",
                              "**timestamp**",
                              [
                                "**timezone**",
                                {
                                  "$ul": [
                                    [
                                      "**location**",
                                      {
                                        "$ul": [
                                          "**comments**",
                                          "**country_code**",
                                          "**latitude**",
                                          "**longitude**"
                                        ]
                                      }
                                    ],
                                    "**name**",
                                    "**transitions**"
                                  ]
                                }
                              ]
                            ]
                          }
                        ],
                        "**athPercentage**",
                        "**name**",
                        "**symbol**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
