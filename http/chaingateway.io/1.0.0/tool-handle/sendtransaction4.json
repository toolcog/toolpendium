{
  "name": "sendtransaction4",
  "description": "sendTransaction",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "amount": {
            "example": "1",
            "type": "string"
          },
          "password": {
            "example": "Test123",
            "type": "string"
          },
          "speed": {
            "example": "fast",
            "type": "string"
          },
          "subtractfee": {
            "example": false,
            "type": "boolean"
          },
          "to": {
            "example": "bc1qzq8m9ukvvgvj7kmlejnfr2q4ndmk2e4ndtr9rn",
            "type": "string"
          },
          "walletname": {
            "example": "Evolo_6928232",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/bitcoin/transactions"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**error**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
