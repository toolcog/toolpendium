{
  "name": "exportaddress2",
  "description": "exportAddress",
  "parameters": {
    "type": "object",
    "properties": {
      "address": {
        "example": "0xa1f36016221d48ce7f15cde7b826a4fbe09bacce",
        "type": "string",
        "description": "BSC address"
      },
      "body": {
        "properties": {
          "password": {
            "example": "test123",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "address"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/bsc/addresses/export/{address}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**address**",
                  [
                    "**content**",
                    {
                      "$ul": [
                        "**address**",
                        [
                          "**crypto**",
                          {
                            "$ul": [
                              "**cipher**",
                              [
                                "**cipherparams**",
                                {
                                  "$ul": [
                                    "**iv**"
                                  ]
                                }
                              ],
                              "**ciphertext**",
                              "**kdf**",
                              [
                                "**kdfparams**",
                                {
                                  "$ul": [
                                    "**dklen**",
                                    "**n**",
                                    "**p**",
                                    "**r**",
                                    "**salt**"
                                  ]
                                }
                              ],
                              "**mac**"
                            ]
                          }
                        ],
                        "**id**",
                        "**version**"
                      ]
                    }
                  ],
                  "**filename**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
