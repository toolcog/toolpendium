{
  "name": "importaddress2",
  "description": "importAddress",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "content": {
            "properties": {
              "address": {
                "example": "4fb45889cd2d3348348e39c613c41bf1dc43760a",
                "type": "string"
              },
              "crypto": {
                "properties": {
                  "cipher": {
                    "example": "aes-128-ctr",
                    "type": "string"
                  },
                  "cipherparams": {
                    "properties": {
                      "iv": {
                        "example": "a2b3022a9d7d76ade60dd70e5405cc09",
                        "type": "string"
                      }
                    },
                    "type": "object"
                  },
                  "ciphertext": {
                    "example": "027fbbe9d7b91b1a4a3afddcb27a07ac5e7fff550fd167452a6345a961f21a4d",
                    "type": "string"
                  },
                  "kdf": {
                    "example": "scrypt",
                    "type": "string"
                  },
                  "kdfparams": {
                    "properties": {
                      "dklen": {
                        "example": 32,
                        "type": "number"
                      },
                      "n": {
                        "example": 131056,
                        "type": "number"
                      },
                      "p": {
                        "example": 1,
                        "type": "number"
                      },
                      "r": {
                        "example": 6,
                        "type": "number"
                      },
                      "salt": {
                        "example": "fd84f455da4ad1325cf5bc7beb06bf3052919278c76373a67507aaa68bcebab3",
                        "type": "string"
                      }
                    },
                    "type": "object"
                  },
                  "mac": {
                    "example": "d46d36b6facd1f3f5549f01cb5c87de0c01b68b6445410259128c52d515a7b6d",
                    "type": "string"
                  }
                },
                "type": "object"
              },
              "id": {
                "example": "f0449f3f-1ef2-5fb9-bf09-20bb72c47167",
                "type": "string"
              },
              "version": {
                "example": 3,
                "type": "number"
              }
            },
            "type": "object"
          },
          "filename": {
            "example": "UTC--2020-05-01T12-30-12.020Z--4fb45889cd2d3348348e39c613c41bf1dc43760a",
            "type": "string"
          },
          "password": {
            "example": "padN39QkRA2hJ",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/bsc/addresses/import"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**address**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
