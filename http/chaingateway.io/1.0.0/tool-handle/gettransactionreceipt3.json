{
  "name": "gettransactionreceipt3",
  "description": "You can get your transaction decoded by add /decoded to your api call. If you want to get the transaction encoded, youst remove the /decoded part",
  "parameters": {
    "type": "object",
    "properties": {
      "transaction": {
        "example": "dab6bb52e098232a2f115d56419124df1463ae60ac4f2effdb59984c550d8fa9",
        "type": "string",
        "description": "Transaction hash"
      }
    },
    "required": [
      "transaction"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/tron/transactions/{transaction}/receipt/decoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  [
                    "**raw_data**",
                    {
                      "$ul": [
                        "**contract**",
                        "**expiration**",
                        "**ref_block_bytes**",
                        "**ref_block_hash**",
                        "**timestamp**"
                      ]
                    }
                  ],
                  "**raw_data_hex**",
                  "**ret**",
                  "**signature**",
                  "**txID**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
