{
  "name": "getbep20",
  "description": "Receive information of a BEP20 address",
  "parameters": {
    "type": "object",
    "properties": {
      "contract": {
        "example": "0xE6965B4F189DBDB2BD65e60aBAeb531B6fE9580B",
        "type": "string",
        "description": "BSC contract address"
      }
    },
    "required": [
      "contract"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/bsc/bep20/{contract}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**contractaddress**",
                  "**decimals**",
                  "**name**",
                  "**symbol**",
                  "**totalSupply**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
