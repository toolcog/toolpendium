{
  "name": "getblockchaininfo",
  "description": "getBlockchaIninfo",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/bitcoin/info"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**bestblockhash**",
                  "**blocks**",
                  "**chain**",
                  "**chainwork**",
                  "**difficulty**",
                  "**headers**",
                  "**initialblockdownload**",
                  "**mediantime**",
                  "**pruned**",
                  "**size_on_disk**",
                  "**time**",
                  "**verificationprogress**",
                  "**warnings**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
