{
  "name": "getnftowner1",
  "description": "Get the Owner of an NFT",
  "parameters": {
    "type": "object",
    "properties": {
      "contract": {
        "example": "0xd9186b67ae0d3ee0777a607eeb195c5a9fb0affcd78ddcd7481efae4b258c9f5",
        "type": "string",
        "description": "Polygon contract address"
      },
      "token_id": {
        "example": "374",
        "type": "string",
        "description": "Polygon token id"
      }
    },
    "required": [
      "contract",
      "token_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/polygon/nfts/{contract}/owner/{token_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**owner**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
