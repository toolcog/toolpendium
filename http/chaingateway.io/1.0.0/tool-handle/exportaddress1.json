{
  "name": "exportaddress1",
  "description": "exportAddress",
  "parameters": {
    "type": "object",
    "properties": {
      "address": {
        "example": "0xfa343ee01667869cfb409b24ae33f7ce8b110eb0",
        "type": "string",
        "description": "Polygon address"
      }
    },
    "required": [
      "address"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://dev.api.chaingateway.io/v2/polygon/addresses/export/{address}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**address**",
                  [
                    "**content**",
                    {
                      "$ul": [
                        "**address**",
                        [
                          "**crypto**",
                          {
                            "$ul": [
                              "**cipher**",
                              [
                                "**cipherparams**",
                                {
                                  "$ul": [
                                    "**iv**"
                                  ]
                                }
                              ],
                              "**ciphertext**",
                              "**kdf**",
                              [
                                "**kdfparams**",
                                {
                                  "$ul": [
                                    "**dklen**",
                                    "**n**",
                                    "**p**",
                                    "**r**",
                                    "**salt**"
                                  ]
                                }
                              ],
                              "**mac**"
                            ]
                          }
                        ],
                        "**id**",
                        "**version**"
                      ]
                    }
                  ],
                  "**filename**"
                ]
              }
            ],
            "**message**",
            "**ok**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
