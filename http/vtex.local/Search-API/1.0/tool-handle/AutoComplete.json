{
  "name": "AutoComplete",
  "description": "Retrieves product's information related to the searched string.\r\n`{{searchString}} is the part of string the user is looking for.\r\nE.g.: `ref` | `refrig` | `refrigerator`",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "example": "application/json",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "example": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand "
      },
      "productNameContains": {
        "example": "jeans",
        "type": "string",
        "description": "Part of the string that will be searched."
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "productNameContains"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://example.com/.DefaultParameterValue.com.br/buscaautocomplete{?productNameContains}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "The root schema"
        },
        "The root schema comprises the entire JSON document.",
        "**Key properties:**",
        {
          "$ul": [
            "**itemsReturned**: Autocomplete returned items."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
