{
  "name": "Getusage",
  "description": "Retrieves information about the coupon usage.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Describes the type of the content being sent."
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand."
      },
      "couponCode": {
        "example": "test",
        "type": "string",
        "description": "Coupon Code"
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "couponCode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/rnb/pvt/coupon/usage/{couponCode}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": ""
        },
        "**Key properties:**",
        {
          "$ul": [
            "**couponCode**: Coupon code.",
            "**hostName**: Name of the store.",
            [
              "**profileUsages**: Object with information where and when the coupon was used.",
              {
                "$ul": [
                  [
                    "**profileId**: Profile ID.",
                    {
                      "$ul": [
                        "**orderUsage**: Array with all the orders that the coupon was used."
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
