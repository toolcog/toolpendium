{
  "name": "Getarchivedbycouponcode",
  "description": "Retrieves a specific archived coupon by its coupon code.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Describes the type of the content being sent."
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand."
      },
      "couponCode": {
        "example": "promo10",
        "type": "string",
        "description": "Coupon Code"
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "couponCode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/rnb/pvt/archive/coupon/{couponCode}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": ""
        },
        "**Key properties:**",
        {
          "$ul": [
            "**couponCode**: Coupon code.",
            "**expirationIntervalPerUse**: Coupon expiration interval per use.",
            "**isArchived**: Defines if the coupon is archived (`true`) or not (`false`).",
            "**lastModifiedUtc**: Date and time of the last update of the coupon in UTC format.",
            "**maxItemsPerClient**: Maximum items per client that the coupon can be applied.",
            "**utmCampaign**: UTM campaign code.",
            "**utmSource**: UTM source code."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
