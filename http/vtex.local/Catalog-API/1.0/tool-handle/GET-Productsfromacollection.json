{
  "name": "GET-Productsfromacollection",
  "description": "Retrieves information about the products from a collection.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent."
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand."
      },
      "collectionId": {
        "example": 1,
        "type": "integer",
        "description": "Collection's unique identifier."
      },
      "page": {
        "example": 2,
        "type": "integer",
        "description": "Page number."
      },
      "pageSize": {
        "example": 15,
        "type": "integer",
        "description": "Number of the items of the page."
      },
      "Filter": {
        "example": "Pre launch",
        "type": "string",
        "description": "Filter used to refine the Collection's products."
      },
      "Active": {
        "example": true,
        "type": "boolean",
        "description": "Defines if the status of the product is active or not."
      },
      "Visible": {
        "example": true,
        "type": "boolean",
        "description": "Defines if the product is visible on the store or not."
      },
      "CategoryId": {
        "example": 12,
        "type": "integer",
        "description": "Product's Category unique identifier."
      },
      "BrandId": {
        "example": 3,
        "type": "integer",
        "description": "Product's Brand unique identifier."
      },
      "SupplierId": {
        "example": 1,
        "type": "integer",
        "description": "Product's Supplier unique identifier."
      },
      "SalesChannelId": {
        "example": 1,
        "type": "integer",
        "description": "Product's Trade Policy unique identifier."
      },
      "ReleaseFrom": {
        "example": "2069-11-26T15:23:00",
        "type": "string",
        "description": "Product past release date."
      },
      "ReleaseTo": {
        "example": "2069-11-26T15:23:00",
        "type": "string",
        "description": "Product future release date."
      },
      "SpecificationProduct": {
        "example": "M",
        "type": "string",
        "description": "Product Specification Field Value. You must also fill in `SpecificationFieldId` to use this parameter."
      },
      "SpecificationFieldId": {
        "example": 40,
        "type": "integer",
        "description": "Product Specification Field unique identifier."
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "collectionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/catalog/pvt/collection/{collectionId}/products{?page,pageSize,Filter,Active,Visible,CategoryId,BrandId,SupplierId,SalesChannelId,ReleaseFrom,ReleaseTo,SpecificationProduct,SpecificationFieldId}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {}
}
