{
  "name": "RetrysubscriptionbygroupId",
  "description": "Permits the retry of a Subscription group, via API, filtering by groupId and instanceId.",
  "parameters": {
    "type": "object",
    "properties": {
      "groupid": {
        "example": "1",
        "type": "string",
        "description": "Group ID."
      },
      "instanceId": {
        "type": "string",
        "description": "Instance ID."
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand."
      },
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent."
      }
    },
    "required": [
      "groupid",
      "instanceId",
      "Accept",
      "Content-Type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://vtex.local/subscriptions-group/{groupid}/instances/{instanceId}/retry"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      },
      "Content-Type": {
        "$": "Content-Type"
      }
    }
  },
  "responses": {}
}
