{
  "name": "GetConversationMessagebygroupId",
  "description": "Retrieves the conversation of a given Subscription group, filtering by groupId.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent."
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand."
      },
      "groupId": {
        "example": "1",
        "type": "string",
        "description": "Group ID."
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "groupId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/subscriptions-group/{groupId}/conversation-message"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {}
}
