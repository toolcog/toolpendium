{
  "name": "Getsubscriptionstocustomer",
  "description": "Retrieves details of a given customer's subscriptions, searching by that customer's `customerId`.",
  "parameters": {
    "type": "object",
    "properties": {
      "customerId": {
        "example": "user@vtex.com.br",
        "type": "string",
        "description": "Customer ID."
      },
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent."
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand."
      }
    },
    "required": [
      "customerId",
      "Content-Type",
      "Accept"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/subscriptions{?customerId}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {}
}
