{
  "name": "Searchallinvoices",
  "description": "Returns all invoices according to the informed query params in the request.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "The Media type of the body of the request. Default value for payment provider protocol is application/json"
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "Media type(s) that is/are acceptable for the response. Default value for payment provider protocol is application/json"
      },
      "from": {
        "default": "",
        "type": "string",
        "description": ""
      },
      "to": {
        "default": "",
        "type": "string",
        "description": ""
      },
      "createdDateFrom": {
        "example": "",
        "type": "string",
        "description": ""
      },
      "createdDateTo": {
        "example": "",
        "type": "string",
        "description": ""
      },
      "value": {
        "default": "101.22",
        "type": "number",
        "description": "Invoice's value. It must be completed with a decimal value."
      },
      "status": {
        "default": "Paid",
        "type": "string",
        "description": "Invoice's status. It must be completed with \"Paid\", \"Cancelled\" or \"Open\" value."
      },
      "friendlyId": {
        "default": "insert identifier here",
        "type": "string",
        "description": "Invoice's identifier"
      },
      "creditAccountId": {
        "default": "B75F0",
        "type": "string",
        "description": "Credit account's identifier"
      }
    },
    "required": [
      "Content-Type",
      "Accept"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/creditcontrol/invoices{?from,to,createdDateFrom,createdDateTo,value,status,friendlyId,creditAccountId}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Paidinvoices"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**: ",
            [
              "**summary**",
              {
                "$ul": [
                  "**count**",
                  "**total**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
