{
  "name": "Getfeedorderstatus1",
  "description": "Retrieve items from [feed](https://developers.vtex.com/docs/guides/orders-feed) queue. \n\r\nThe event will be removed if the message `send retry` is equal to, or greater than the maximum retention period.\r\n\r\n> This API will return `404 Not Found` if there is no [Feed Configuration](https://developers.vtex.com/docs/guides/orders-feed) available for the given X-VTEX-API-AppKey.",
  "parameters": {
    "type": "object",
    "properties": {
      "maxlot": {
        "default": "{{maxLot}}",
        "type": "string",
        "description": "Lot quantity to retrieve. Maximum accepted value is 10."
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand"
      },
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent"
      }
    },
    "required": [
      "maxlot",
      "Accept",
      "Content-Type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/orders/feed{?maxlot}"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      },
      "Content-Type": {
        "$": "Content-Type"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Getfeedorderstatus list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**currentChange**",
            "**domain**",
            "**eventId**",
            "**handle**",
            "**lastChange**",
            "**lastState**",
            "**orderId**",
            "**state**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
