{
  "name": "GetGiftCardTransactions",
  "description": "Returns all transaction of a giftcard.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "Media type(s) that is/are acceptable for the response. Default value for payment provider protocol is application/json"
      },
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "The Media type of the body of the request. Default value for payment provider protocol is application/json"
      },
      "giftCardID": {
        "default": "2",
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "Accept",
      "Content-Type",
      "giftCardID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/giftcards/{giftCardID}/transactions"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      },
      "Content-Type": {
        "$": "Content-Type"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "response3 list"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_self**",
              {
                "$ul": [
                  "**href**"
                ]
              }
            ],
            "**id**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
