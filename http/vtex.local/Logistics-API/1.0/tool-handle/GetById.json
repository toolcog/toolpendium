{
  "name": "GetById",
  "description": "Lists your store's pickup points while searching by ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json; charset=utf-8",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand"
      },
      "pickupPointId": {
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "pickupPointId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/logistics/pvt/configuration/pickuppoints/{pickupPointId}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "GetById"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**address**",
              {
                "$ul": [
                  "**city**",
                  "**complement**",
                  [
                    "**country**",
                    {
                      "$ul": [
                        "**acronym**",
                        "**name**"
                      ]
                    }
                  ],
                  [
                    "**location**",
                    {
                      "$ul": [
                        "**latitude**",
                        "**longitude**"
                      ]
                    }
                  ],
                  "**neighborhood**",
                  "**number**",
                  "**postalCode**",
                  "**state**",
                  "**street**"
                ]
              }
            ],
            "**businessHours**: ",
            "**description**",
            "**formatted_address**",
            "**id**",
            "**instructions**",
            "**isActive**",
            "**name**",
            "**warehouseId**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
