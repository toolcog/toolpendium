{
  "name": "DockById",
  "description": "Informs a given dock's information, searching by dock ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json; charset=utf-8",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand"
      },
      "dockId": {
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "dockId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/logistics/pvt/configuration/docks/{dockId}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": ""
        },
        "**Key properties:**",
        {
          "$ul": [
            "**dockTimeFake**",
            "**freightTableIds**: ",
            "**id**",
            "**name**",
            [
              "**pickupStoreInfo**",
              {
                "$ul": [
                  "**additionalInfo**",
                  "**address**",
                  "**dockId**",
                  "**friendlyName**",
                  "**isPickupStore**",
                  "**storeId**"
                ]
              }
            ],
            "**priority**",
            "**salesChannel**",
            "**salesChannels**: ",
            "**timeFakeOverhead**",
            "**wmsEndPoint**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
