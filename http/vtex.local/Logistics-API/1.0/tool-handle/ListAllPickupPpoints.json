{
  "name": "ListAllPickupPpoints",
  "description": "Lists all of your store's pickup points.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json; charset=utf-8",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand"
      }
    },
    "required": [
      "Content-Type",
      "Accept"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/logistics/pvt/configuration/pickuppoints"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accountGroupId**: Account group ID.",
            "**accountOwnerId**: Account owner ID.",
            "**accountOwnerName**: Account owner name.",
            [
              "**address**",
              {
                "$ul": [
                  "**city**: City.",
                  "**complement**: Address complement.",
                  [
                    "**country**",
                    {
                      "$ul": [
                        "**acronym**: Three letter ISO code.",
                        "**name**: Country name."
                      ]
                    }
                  ],
                  [
                    "**location**: Geolocation properties.",
                    {
                      "$ul": [
                        "**latitude**: Latitude.",
                        "**longitude**: Longitude."
                      ]
                    }
                  ],
                  "**neighborhood**: Neighborhood.",
                  "**number**: Pickup point address number.",
                  "**postalCode**: Postal code.",
                  "**state**: State.",
                  "**street**: Street name."
                ]
              }
            ],
            "**businessHours**: Business hours information for each day of the week.",
            "**description**: Pickup point description.",
            "**distance**: Distance.",
            "**formatted_address**: Formated address.",
            "**id**: Pickup point ID.",
            "**instructions**: Instructions.",
            "**isActive**: Indicates whether pickup point is active.",
            "**isThirdPartyPickup**: Indicates whether pickup point is third party.",
            "**name**: Pickup point name.",
            "**originalId**: Original ID.",
            "**parentAccountName**: Parent account name.",
            "**pickupHolidays**: Holidays which apply for pickup point.",
            "**seller**: Seller corresponding to pickup point.",
            "**tagsLabel**: List of tags."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
