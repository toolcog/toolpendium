{
  "name": "AllDocks",
  "description": "Informs a list of all docks.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json; charset=utf-8",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand"
      }
    },
    "required": [
      "Content-Type",
      "Accept"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/logistics/pvt/configuration/docks"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**dockTimeFake**",
            "**freightTableIds**: ",
            "**id**",
            "**name**",
            [
              "**pickupStoreInfo**",
              {
                "$ul": [
                  "**additionalInfo**",
                  "**address**",
                  "**dockId**",
                  "**friendlyName**",
                  "**isPickupStore**",
                  "**storeId**"
                ]
              }
            ],
            "**priority**",
            "**salesChannel**",
            "**salesChannels**: ",
            "**timeFakeOverhead**",
            "**wmsEndPoint**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
