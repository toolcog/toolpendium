{
  "name": "GetSupplyLots",
  "description": "Returns a list of the supply lots of an SKU in a specific warehouse.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept": {
        "example": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand."
      },
      "Content-Type": {
        "example": "application/json; charset=utf-8",
        "type": "string",
        "description": "Type of the content being sent."
      },
      "skuId": {
        "type": "string",
        "description": "ID of the SKU."
      },
      "warehouseId": {
        "type": "string",
        "description": "ID of the warehouse where the SKU is located."
      }
    },
    "required": [
      "Accept",
      "Content-Type",
      "skuId",
      "warehouseId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/logistics/pvt/inventory/items/{skuId}/warehouses/{warehouseId}/supplyLots"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      },
      "Content-Type": {
        "$": "Content-Type"
      }
    }
  },
  "responses": {}
}
