{
  "name": "FreightValues",
  "description": "Lists freight values apointed to your store's carriers, searching by carrier ID and postal code (`cep`).",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json; charset=utf-8",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand"
      },
      "carrierId": {
        "example": "carrier-123",
        "type": "string",
        "description": "Carrier ID"
      },
      "cep": {
        "example": "12345000",
        "type": "string",
        "description": "Postal code."
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "carrierId",
      "cep"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/logistics/pvt/configuration/freights/{carrierId}/{cep}/values"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**absoluteMoneyCost**",
            "**country**",
            "**maxVolume**",
            "**minimumValueInsurance**",
            "**operationType**",
            "**polygon**",
            "**pricePercent**",
            "**pricePercentByWeight**",
            "**restrictedFreights**: ",
            "**timeCost**",
            "**weightEnd**",
            "**weightStart**",
            "**zipCodeEnd**",
            "**zipCodeStart**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
