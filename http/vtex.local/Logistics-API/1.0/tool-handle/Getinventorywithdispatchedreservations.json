{
  "name": "Getinventorywithdispatchedreservations",
  "description": "Lists inventory with dispatched reservations. When the number of active reservations is more than 2000 the return is an error with status code 400 (BadRequest) and the message: Too many active reservations.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json; charset=utf-8",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation Accept Header. Indicates the types of responses the client can understand"
      },
      "itemId": {
        "type": "string",
        "description": ""
      },
      "warehouseId": {
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "itemId",
      "warehouseId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/logistics/pvt/inventory/items/{itemId}/warehouses/{warehouseId}/dispatched"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**availableQuantity**: Available quantity.",
            "**dispatchedReservationsQuantity**: Dispatched reservations quantity.",
            "**isUnlimitedQuantity**: Indicates whether the SKU's availability is unlimited (\"infinite inventory\").",
            "**quantity**: SKU quantity.",
            "**skuId**: SKU ID.",
            "**totalReservedQuantity**: Total reserved quantity of the SKU.",
            "**warehouseId**: Warehouse ID."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
