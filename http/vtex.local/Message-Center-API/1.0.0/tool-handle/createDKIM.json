{
  "name": "createDKIM",
  "description": "Create DKIM keys for sender that was setup in VTEX mail servers",
  "parameters": {
    "type": "object",
    "properties": {
      "EmailProvider": {
        "default": "help@valdie.co",
        "type": "string",
        "description": "E-mail address for sender that was setup in VTEX mail servers"
      }
    },
    "required": [
      "EmailProvider"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://vtex.local/api/mail-service/pvt/providers/{EmailProvider}/dkim"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**dkimKeys**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status**",
            "**title**",
            "**traceId**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
