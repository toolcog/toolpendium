{
  "name": "Putversion",
  "description": "Updates document with version values.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/vnd.vtex.ds.v10+json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand "
      },
      "acronym": {
        "default": "{{acronym}}",
        "type": "string",
        "description": "Two letter word that identifies the data structure"
      },
      "id": {
        "default": "{{id}}",
        "type": "string",
        "description": "Id of the document"
      },
      "versionId": {
        "default": "{{versionId}}",
        "type": "string",
        "description": "Id of the version to update"
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "acronym",
      "id",
      "versionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{acronym}/documents/{id}/versions/{versionId}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Putversion"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Href**",
            "**Id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
