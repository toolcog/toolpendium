{
  "name": "Listversions",
  "description": "Allows to list the versions of a document.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/vnd.vtex.ds.v10+json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand "
      },
      "acronym": {
        "default": "{{acronym}}",
        "type": "string",
        "description": "Two letter word that identifies the data structure"
      },
      "id": {
        "default": "{{id}}",
        "type": "string",
        "description": "Id of the document"
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "acronym",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{acronym}/documents/{id}/versions"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Listversion list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**date**",
            "**id**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
