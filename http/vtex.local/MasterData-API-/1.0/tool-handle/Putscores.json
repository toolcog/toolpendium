{
  "name": "Putscores",
  "description": "It allows punctuate in more than one field and more than one key.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept": {
        "default": "application/vnd.vtex.ds.v10+json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand "
      },
      "acronym": {
        "default": "{{acronym}}",
        "type": "string",
        "description": "Two letter word that identifies the data structure"
      },
      "id": {
        "default": "{{id}}",
        "type": "string",
        "description": "Id of the document"
      },
      "body": {
        "description": "",
        "items": {
          "$ref": "#/$defs/PutscoresRequest"
        },
        "type": "array"
      }
    },
    "required": [
      "Accept",
      "acronym",
      "id",
      "body"
    ],
    "$defs": {
      "PutscoresRequest": {
        "example": {
          "field": "carttag",
          "key": "Payment",
          "point": 1,
          "until": "10m"
        },
        "properties": {
          "field": {
            "type": "string"
          },
          "key": {
            "type": "string"
          },
          "point": {
            "format": "int32",
            "type": "integer"
          },
          "until": {
            "type": "string"
          }
        },
        "required": [
          "field",
          "key",
          "point",
          "until"
        ],
        "title": "PutscoresRequest",
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{acronym}/documents/{id}/score"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
