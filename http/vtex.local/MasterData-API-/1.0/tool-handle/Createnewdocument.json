{
  "name": "Createnewdocument",
  "description": "Creates documents through a JSON object where the key is the name of the field.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept": {
        "default": "application/vnd.vtex.ds.v10+json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand "
      },
      "acronym": {
        "default": "{{acronym}}",
        "type": "string",
        "description": "Identifies the kind of data"
      },
      "body": {
        "type": "object",
        "description": ""
      }
    },
    "required": [
      "Accept",
      "acronym",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{acronym}/documents"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Createnewdocument"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Href**",
            "**Id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
