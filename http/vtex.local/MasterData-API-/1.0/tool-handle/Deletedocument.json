{
  "name": "Deletedocument",
  "description": "It allows to delete a document.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/vnd.vtex.ds.v10+json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand "
      },
      "acronym": {
        "default": "{{acronym}}",
        "type": "string",
        "description": "Two letter word that identifies the data structure"
      },
      "id": {
        "default": "{{id}}",
        "type": "string",
        "description": "Id of the document"
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "acronym",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{acronym}/documents/{id}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {}
}
