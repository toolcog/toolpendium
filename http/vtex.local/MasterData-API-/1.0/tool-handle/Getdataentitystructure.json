{
  "name": "Getdataentitystructure",
  "description": "Returns the data entity structure with its respective fields and data type.\r\n\r\n### Response status code\r\n\r\n1. Status Code `403`: Access not allowed\r\n2. Status Code `200`: Retrieves data entity structure\r\n\r\n> All headers listed below are required.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/vnd.vtex.ds.v10+json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand "
      },
      "acronym": {
        "default": "{{acronym}}",
        "type": "string",
        "description": "Identifies the kind of data"
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "acronym"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{acronym}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Getdataentityfields"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**acronym**",
            "**allowGetAll**",
            "**fields**: ",
            "**name**",
            "**primaryKeyType**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
