{
  "name": "Getversion",
  "description": "Returns the version of a document.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "Type of the content being sent"
      },
      "Accept": {
        "default": "application/vnd.vtex.ds.v10+json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand "
      },
      "acronym": {
        "default": "{{acronym}}",
        "type": "string",
        "description": "Two letter word that identifies the data structure"
      },
      "id": {
        "default": "{{id}}",
        "type": "string",
        "description": "Id of the document"
      },
      "versionId": {
        "default": "{{versionId}}",
        "type": "string",
        "description": "Id of the version to get"
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "acronym",
      "id",
      "versionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{acronym}/documents/{id}/versions/{versionId}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Getversion"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**author**",
            [
              "**document**",
              {
                "$ul": [
                  "**accountId**",
                  "**accountName**",
                  "**carttag**",
                  "**checkouttag**",
                  "**dataEntityId**",
                  [
                    "**departmentVisitedTag**",
                    {
                      "$ul": [
                        "**DisplayValue**",
                        [
                          "**Scores**",
                          {
                            "$ul": [
                              "**Ar e Ventilação**: "
                            ]
                          }
                        ]
                      ]
                    }
                  ],
                  "**email**",
                  "**followers**: ",
                  "**id**",
                  "**rclastsession**",
                  "**rclastsessiondate**"
                ]
              }
            ],
            "**id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
