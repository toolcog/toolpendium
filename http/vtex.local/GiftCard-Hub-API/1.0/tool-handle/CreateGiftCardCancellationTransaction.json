{
  "name": "CreateGiftCardCancellationTransaction",
  "description": "Creates a cancellation transaction to a giftcard.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept": {
        "default": "application/json",
        "type": "string",
        "description": "Media type(s) that is/are acceptable for the response. Default value for payment provider protocol is application/json."
      },
      "Content-Type": {
        "default": "application/json",
        "type": "string",
        "description": "The Media type of the body of the request. Default value for payment provider protocol is application/json"
      },
      "X-VTEX-API-AppKey": {
        "default": "{{X-VTEX-API-AppKey}}",
        "type": "string",
        "description": "VTEX API AppKey"
      },
      "X-VTEX-API-AppToken": {
        "default": "{{X-VTEX-API-AppToken}}",
        "type": "string",
        "description": "VTEX API AppToken"
      },
      "giftCardProviderID": {
        "default": "insert identifier here",
        "type": "string",
        "description": "Gift Card provider's ID."
      },
      "giftCardID": {
        "default": "6",
        "type": "string",
        "description": "Gift Card ID."
      },
      "tId": {
        "default": "b47690",
        "type": "string",
        "description": "Transaction ID generated by the provider. It is different from the `transactionID` generated by VTEX."
      }
    },
    "required": [
      "Accept",
      "Content-Type",
      "X-VTEX-API-AppKey",
      "X-VTEX-API-AppToken",
      "giftCardProviderID",
      "giftCardID",
      "tId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://vtex.local/giftcardproviders/{giftCardProviderID}/giftcards/{giftCardID}/transactions/{tId}/cancellations"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      },
      "Content-Type": {
        "$": "Content-Type"
      },
      "X-VTEX-API-AppKey": {
        "$": "X-VTEX-API-AppKey"
      },
      "X-VTEX-API-AppToken": {
        "$": "X-VTEX-API-AppToken"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
