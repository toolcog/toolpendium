{
  "name": "GetRolesbyUser",
  "description": "Gets roles of a particular user or application key.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "example": "application/json",
        "type": "string",
        "description": "The media type of the body of the request. Default value for license manager protocol is application/json"
      },
      "userId": {
        "type": "string",
        "description": "ID corresponding to the user"
      }
    },
    "required": [
      "Content-Type",
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/license-manager/users/{userId}/roles"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Array of role objects",
        "**Key properties:**",
        {
          "$ul": [
            "**id**: ID of role",
            "**name**: Name of role"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
