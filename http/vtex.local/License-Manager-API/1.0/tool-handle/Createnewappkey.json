{
  "name": "Createnewappkey",
  "description": "Creates a new pair of `appKey` and `appToken`.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/CreatenewappkeyRequest"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "CreatenewappkeyRequest": {
        "description": "Request body for creating new application key (/api/vlm/appkeys)",
        "properties": {
          "label": {
            "description": "Label for application key",
            "type": "string"
          }
        },
        "required": [
          "label"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://vtex.local/api/vlm/appkeys"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Response body of sucessful call to /api/vlm/appkeys",
        "**Key properties:**",
        {
          "$ul": [
            "**appKey**: AppKey (Access Key in the license manager UI)",
            "**appToken**: Secret of application key",
            "**createdIn**: Date of creation of application key",
            "**id**: ID of application key",
            "**isActive**: If the application key is active",
            "**label**: Label for application key"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
