{
  "name": "Getappkeysfromaccount",
  "description": "Gets all application keys from an account.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "example": "application/json",
        "type": "string",
        "description": "The media type of the body of the request. Default value for license manager protocol is application/json"
      }
    },
    "required": [
      "Content-Type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/vlm/appkeys"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Array of AppKeys object",
        "**Key properties:**",
        {
          "$ul": [
            "**appKey**: AppKey (Access Key in the license manager UI)",
            "**createdIn**: Date of creation of application key",
            "**id**: ID of application key",
            "**isActive**: If the application key is active",
            "**label**: Label for application key"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
