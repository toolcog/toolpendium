{
  "name": "Updateappkey",
  "description": "Activates or deactivates an `appKey` by its ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID from the appKey which will be updated"
      },
      "body": {
        "$ref": "#/components/schemas/UpdateappkeyRequest",
        "description": "Request body for updating AppKeys"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://vtex.local/api/vlm/appkeys/{id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Object representing VLMError",
        "**Key properties:**",
        {
          "$ul": [
            "**Detais**: Details of error",
            "**HttpStatusCode**: Status code of HTTP response",
            "**Message**: Error message",
            "**VLMErrorCode**: Error code for License Manager"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
