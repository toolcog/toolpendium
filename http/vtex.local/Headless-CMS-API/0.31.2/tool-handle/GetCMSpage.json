{
  "name": "GetCMSpage",
  "description": "Gets all data from a given page.",
  "parameters": {
    "type": "object",
    "properties": {
      "builderId": {
        "example": "faststore",
        "type": "string",
        "description": "Builder ID specified in the settings of the CMS app."
      },
      "content-type": {
        "example": "plp",
        "type": "string",
        "description": "Content Type ID defined in the FastStore project."
      },
      "document-id": {
        "example": "5af643b5-9a6d-48f2-9b34-919dd762c908",
        "type": "string",
        "description": "Document ID presented in the URL path of a CMS preview."
      },
      "versionId": {
        "example": "e7263fc8-bc68-4052-9e25-dd5a2572d3bb",
        "type": "string",
        "description": "Version ID presented in the URL path of a CMS preview."
      },
      "releaseId": {
        "example": "6196c277c6dce15f9709a2a7",
        "type": "string",
        "description": "Release ID presented in the URL path of a CMS preview."
      }
    },
    "required": [
      "builderId",
      "content-type",
      "document-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/_v/cms/api/{builderId}/{content-type}/{document-id}/{?versionId,releaseId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": ""
        },
        "Object containing the data related to a specific page.",
        "**Key properties:**",
        {
          "$ul": [
            "**id**: Document ID.",
            "**name**: Name of the page created in the CMS app.",
            "**sections**: Sections that compose the page.",
            "**status**: Current status of the page.",
            "**type**: Name of the Content Type defined in the FastStore project.",
            "**versionId**: Version ID.",
            "**versionStatus**: Version status"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
