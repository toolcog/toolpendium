{
  "name": "GetAllContentTypes",
  "description": "Gets data from all Content Types.",
  "parameters": {
    "type": "object",
    "properties": {
      "builderId": {
        "example": "faststore",
        "type": "string",
        "description": "Builder ID specified in the settings of the CMS app."
      }
    },
    "required": [
      "builderId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/_v/cms/api/{builderId}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": ""
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**contentTypes**: Array with data of each Content Type."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
