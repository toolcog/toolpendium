{
  "name": "Getdocument",
  "description": "Gets document by ID.\n\r\n\r> Assign the `_fields` parameter in the query string to retrieve the desired fields. If you want to return all the fields use `_fields=_all`.",
  "parameters": {
    "type": "object",
    "properties": {
      "dataEntityName": {
        "example": "Client",
        "type": "string",
        "description": "Name of the data entity. Defined by the api. Examples of native data entities you can use are `CL` for client profiles and `AD` for client addresses."
      },
      "Content-Type": {
        "example": "application/json",
        "type": "string",
        "description": "Type of the content being sent."
      },
      "Accept": {
        "example": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand."
      },
      "id": {
        "example": "Client-b818cbda-e489-11e6-94f4-0ac138d2d42e",
        "type": "string",
        "description": "ID of the Document."
      }
    },
    "required": [
      "dataEntityName",
      "Content-Type",
      "Accept",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{dataEntityName}/documents/{id}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Using_fields=_all"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accountId**",
            "**accountName**",
            "**dataEntityId**",
            "**id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
