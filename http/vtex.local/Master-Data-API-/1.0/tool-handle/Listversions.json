{
  "name": "Listversions",
  "description": "Allows to list the versions of a document.",
  "parameters": {
    "type": "object",
    "properties": {
      "dataEntityName": {
        "example": "Client",
        "type": "string",
        "description": "Name of the data entity. Defined by the api. Examples of native data entities you can use are `CL` for client profiles and `AD` for client addresses."
      },
      "Content-Type": {
        "example": "application/json",
        "type": "string",
        "description": "Type of the content being sent."
      },
      "Accept": {
        "example": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand."
      },
      "id": {
        "example": "Client-b818cbda-e489-11e6-94f4-0ac138d2d42e",
        "type": "string",
        "description": "ID of the Document."
      },
      "load": {
        "default": true,
        "type": "boolean",
        "description": "If true, return all the fields in each version of the document"
      },
      "fields": {
        "default": "id,dataEntityId,isNewsletterOptIn,createdBy",
        "type": "string",
        "description": "If `load` is true, the response will return only these specific fields"
      }
    },
    "required": [
      "dataEntityName",
      "Content-Type",
      "Accept",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{dataEntityName}/documents/{id}/versions{?load,fields}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Listversion list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**date**",
            "**document**",
            "**id**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
