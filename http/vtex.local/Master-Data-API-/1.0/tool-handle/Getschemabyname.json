{
  "name": "Getschemabyname",
  "description": "Returns an existing schema for a given data entity.",
  "parameters": {
    "type": "object",
    "properties": {
      "dataEntityName": {
        "example": "Client",
        "type": "string",
        "description": "Name of the data entity. Defined by the api. Examples of native data entities you can use are `CL` for client profiles and `AD` for client addresses."
      },
      "Content-Type": {
        "example": "application/json",
        "type": "string",
        "description": "Type of the content being sent."
      },
      "schemaName": {
        "example": "{{schema}}",
        "type": "string",
        "description": "Name of the schema."
      }
    },
    "required": [
      "dataEntityName",
      "Content-Type",
      "schemaName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/dataentities/{dataEntityName}/schemas/{schemaName}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      }
    }
  },
  "responses": {}
}
