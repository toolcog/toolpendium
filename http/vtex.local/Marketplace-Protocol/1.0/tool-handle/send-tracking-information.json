{
  "name": "send-tracking-information",
  "description": "This request is sent by the external seller to the VTEX marketplace to add tracking information to a given order's invoice, in case it is necessary to do so after the invoice has been sent.",
  "parameters": {
    "type": "object",
    "properties": {
      "marketplaceServicesEndpoint": {
        "example": "marketplaceservicesendpoint.myvtex.com",
        "type": "string",
        "description": "This is an endpoint sent from VTEX to the external seller in the [Order placement request](https://developers.vtex.com/vtex-rest-api/reference/external-seller#order-placement)."
      },
      "marketplaceOrderId": {
        "example": "1138342255777-01",
        "type": "string",
        "description": "Identifies the order in the marketplace."
      },
      "invoiceNumber": {
        "example": "NFe-00002",
        "type": "string",
        "description": "Invoice number."
      },
      "Accept": {
        "example": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand."
      },
      "Content-Type": {
        "example": "application/json",
        "type": "string",
        "description": "Describes the type of the content being sent."
      },
      "body": {
        "properties": {
          "courier": {
            "description": "Courier.",
            "example": "courier-example",
            "type": "string"
          },
          "dispatchedDate": {
            "description": "Date of order dispatch.",
            "example": "2021-06-09",
            "type": "string"
          },
          "trackingNumber": {
            "description": "Tracking number.",
            "example": "12345678abc",
            "type": "string"
          },
          "trackingUrl": {
            "description": "Tracking URL.",
            "example": "https://courier-example.com/tracking",
            "type": "string"
          }
        },
        "type": "object",
        "description": ""
      }
    },
    "required": [
      "marketplaceServicesEndpoint",
      "marketplaceOrderId",
      "invoiceNumber",
      "Accept",
      "Content-Type",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://vtex.local/{marketplaceServicesEndpoint}/pvt/orders/{marketplaceOrderId}/invoice/{invoiceNumber}"
    },
    "headers": {
      "Accept": {
        "$": "Accept"
      },
      "Content-Type": {
        "$": "Content-Type"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
