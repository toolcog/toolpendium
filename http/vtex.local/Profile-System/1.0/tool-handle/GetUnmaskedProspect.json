{
  "name": "GetUnmaskedProspect",
  "description": "Retrieves unmasked information of a specific prospect, by its `prospectId`.\n\r\n\r> Learn more about the [Profile System](https://developers.vtex.com/vtex-rest-api/docs/profile-system) and its other API endpoints.",
  "parameters": {
    "type": "object",
    "properties": {
      "Content-Type": {
        "example": "application/json",
        "type": "string",
        "description": "Type of the content being sent."
      },
      "Accept": {
        "example": "application/json",
        "type": "string",
        "description": "HTTP Client Negotiation _Accept_ Header. Indicates the types of responses the client can understand."
      },
      "prospectId": {
        "example": "51clk394-8534-447e-a0ca-1803p669c987",
        "type": "string",
        "description": "ID of the prospect as returned by the Create prospect endpoint's response, in the `id` field."
      },
      "reason": {
        "example": "data-validation",
        "type": "string",
        "description": "Reason for requesting unmasked data."
      }
    },
    "required": [
      "Content-Type",
      "Accept",
      "prospectId",
      "reason"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://vtex.local/api/storage/profile-system/prospects/{prospectId}/unmask{?reason}"
    },
    "headers": {
      "Content-Type": {
        "$": "Content-Type"
      },
      "Accept": {
        "$": "Accept"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
