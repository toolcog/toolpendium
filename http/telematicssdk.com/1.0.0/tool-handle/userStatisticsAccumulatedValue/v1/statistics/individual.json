                  "**InstanceId**",
                  "**MaxSpeedKmh**",
                  "**MaxSpeedMileh**",
                  "**MileageKm**",
                  "**MileageMile**",
                  "**NightDrivingTime**",
                  "**OtherTripsCount**",
                  "**PhoneUsageDistanceKm**",
                  "**PhoneUsageDistanceMile**",
                  "**PhoneUsageDurationMin**",
                  "**PhoneUsageOverSpeedDistanceKm**",
                  "**PhoneUsageOverSpeedDistanceMile**",
                  "**PhoneUsageOverSpeedDurationMin**",
                  "**RushHoursDrivingTime**",
                  "**TotalSpeedingKm**",
                  "**TotalSpeedingMile**",
                  "**TripsCount**"
