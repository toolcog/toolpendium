{
  "name": "tripsTripDetails",
  "description": "Trips - trip details",
  "parameters": {
    "type": "object",
    "properties": {
      "trackToken": {
        "example": "",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telematicssdk.com/mobilesdk/stage/track/get_track/v1{?trackToken}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**Result**",
              {
                "$ul": [
                  "**Code**",
                  [
                    "**Track**",
                    {
                      "$ul": [
                        "**AccelerationCount**",
                        "**AddressEnd**",
                        [
                          "**AddressFinishParts**",
                          {
                            "$ul": [
                              "**City**",
                              "**CountryCode**",
                              "**District**",
                              "**House**",
                              "**PostalCode**",
                              "**Street**"
                            ]
                          }
                        ],
                        "**AddressStart**",
                        [
                          "**AddressStartParts**",
                          {
                            "$ul": [
                              "**City**",
                              "**CountryCode**",
                              "**District**",
                              "**House**",
                              "**PostalCode**",
                              "**Street**"
                            ]
                          }
                        ],
                        "**BeaconId**",
                        "**CityFinish**",
                        "**CityStart**",
                        "**DecelerationCount**",
                        "**Distance**",
                        "**DrivingTips**",
                        "**Duration**",
                        "**EcoScore**",
                        "**EcoScoreBrakes**",
                        "**EcoScoreDepreciation**",
                        "**EcoScoreFuel**",
                        "**EcoScoreTyres**",
                        "**EndDate**",
                        "**HighOverSpeedMileage**",
                        "**MidOverSpeedMileage**",
                        "**OriginChanged**",
                        "**PhoneUsage**",
                        "**Points**",
                        "**Rating**",
                        "**Rating100**",
                        "**RatingAcceleration**",
                        "**RatingAcceleration100**",
                        "**RatingBraking**",
                        "**RatingBraking100**",
                        "**RatingCornering**",
                        "**RatingCornering100**",
                        "**RatingPhoneDistraction100**",
                        "**RatingPhoneUsage**",
                        "**RatingSpeeding**",
                        "**RatingSpeeding100**",
                        "**RatingTimeOfDay**",
                        "**ShareType**",
                        "**StartDate**",
                        "**Status**",
                        "**TrackOriginCode**"
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
