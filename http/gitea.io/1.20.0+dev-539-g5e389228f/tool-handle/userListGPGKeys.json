{
  "name": "userListGPGKeys",
  "description": "List the given user's GPG keys",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "username of user"
      },
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      },
      "limit": {
        "type": "integer",
        "description": "page size of results"
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/users/{username}/gpg_keys{?page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "GPGKey a user GPG key to sign commit and tag in repository",
        "**Key properties:**",
        {
          "$ul": [
            "**can_certify**",
            "**can_encrypt_comms**",
            "**can_encrypt_storage**",
            "**can_sign**",
            "**created_at**",
            "**emails**",
            "**expires_at**",
            "**id**",
            "**key_id**",
            "**primary_key_id**",
            "**public_key**",
            "**subkeys**",
            "**verified**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
