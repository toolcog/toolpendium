{
  "name": "repoDeleteBranchProtection",
  "description": "Delete a specific branch protection for the repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "name": {
        "type": "string",
        "description": "name of protected branch"
      }
    },
    "required": [
      "owner",
      "repo",
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/branch_protections/{name}"
    }
  },
  "responses": {}
}
