{
  "name": "orgGetTeam",
  "description": "Get a team",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the team to get"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/teams/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Team represents a team in an organization",
        "**Key properties:**",
        {
          "$ul": [
            "**can_create_org_repo**",
            "**description**",
            "**id**",
            "**includes_all_repositories**",
            "**name**",
            [
              "**organization**: Organization represents an organization",
              {
                "$ul": [
                  "**avatar_url**",
                  "**description**",
                  "**full_name**",
                  "**id**",
                  "**location**",
                  "**name**",
                  "**repo_admin_change_team_access**",
                  "**username**: deprecated",
                  "**visibility**",
                  "**website**"
                ]
              }
            ],
            "**permission**",
            "**units**",
            "**units_map**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
