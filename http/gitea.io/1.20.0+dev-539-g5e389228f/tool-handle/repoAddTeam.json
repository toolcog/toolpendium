{
  "name": "repoAddTeam",
  "description": "Add a team to a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "team": {
        "type": "string",
        "description": "team name"
      }
    },
    "required": [
      "owner",
      "repo",
      "team"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/teams/{team}"
    }
  },
  "responses": {}
}
