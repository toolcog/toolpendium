{
  "name": "issueCheckSubscription",
  "description": "Check if user is subscribed to an issue",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "index": {
        "format": "int64",
        "type": "integer",
        "description": "index of the issue"
      }
    },
    "required": [
      "owner",
      "repo",
      "index"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/issues/{index}/subscriptions/check"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "WatchInfo represents an API watch status of one repository",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**ignored**",
            "**reason**",
            "**repository_url**",
            "**subscribed**",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
