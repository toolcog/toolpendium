{
  "name": "teamSearch",
  "description": "Search for teams within an organization",
  "parameters": {
    "type": "object",
    "properties": {
      "org": {
        "type": "string",
        "description": "name of the organization"
      },
      "q": {
        "type": "string",
        "description": "keywords to search"
      },
      "include_desc": {
        "type": "boolean",
        "description": "include search within team description (defaults to true)"
      },
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      },
      "limit": {
        "type": "integer",
        "description": "page size of results"
      }
    },
    "required": [
      "org"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/orgs/{org}/teams/search{?q,include_desc,page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**ok**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
