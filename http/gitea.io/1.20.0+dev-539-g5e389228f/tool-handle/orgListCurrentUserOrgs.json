{
  "name": "orgListCurrentUserOrgs",
  "description": "List the current user's organizations",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      },
      "limit": {
        "type": "integer",
        "description": "page size of results"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/user/orgs{?page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Organization represents an organization",
        "**Key properties:**",
        {
          "$ul": [
            "**avatar_url**",
            "**description**",
            "**full_name**",
            "**id**",
            "**location**",
            "**name**",
            "**repo_admin_change_team_access**",
            "**username**: deprecated",
            "**visibility**",
            "**website**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
