{
  "name": "orgGet",
  "description": "Get an organization",
  "parameters": {
    "type": "object",
    "properties": {
      "org": {
        "type": "string",
        "description": "name of the organization to get"
      }
    },
    "required": [
      "org"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/orgs/{org}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Organization represents an organization",
        "**Key properties:**",
        {
          "$ul": [
            "**avatar_url**",
            "**description**",
            "**full_name**",
            "**id**",
            "**location**",
            "**name**",
            "**repo_admin_change_team_access**",
            "**username**: deprecated",
            "**visibility**",
            "**website**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
