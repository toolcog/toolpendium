{
  "name": "repoMirrorSync",
  "description": "Sync a mirrored repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo to sync"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo to sync"
      }
    },
    "required": [
      "owner",
      "repo"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/mirror-sync"
    }
  },
  "responses": {}
}
