{
  "name": "orgAddTeamRepository",
  "description": "Add a repository to a team",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the team"
      },
      "org": {
        "type": "string",
        "description": "organization that owns the repo to add"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo to add"
      }
    },
    "required": [
      "id",
      "org",
      "repo"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/api/v1/teams/{id}/repos/{org}/{repo}"
    }
  },
  "responses": {}
}
