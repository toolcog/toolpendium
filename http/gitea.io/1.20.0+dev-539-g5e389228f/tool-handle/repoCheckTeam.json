{
  "name": "repoCheckTeam",
  "description": "Check if a team is assigned to a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "team": {
        "type": "string",
        "description": "team name"
      }
    },
    "required": [
      "owner",
      "repo",
      "team"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/teams/{team}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Team represents a team in an organization",
        "**Key properties:**",
        {
          "$ul": [
            "**can_create_org_repo**",
            "**description**",
            "**id**",
            "**includes_all_repositories**",
            "**name**",
            [
              "**organization**: Organization represents an organization",
              {
                "$ul": [
                  "**avatar_url**",
                  "**description**",
                  "**full_name**",
                  "**id**",
                  "**location**",
                  "**name**",
                  "**repo_admin_change_team_access**",
                  "**username**: deprecated",
                  "**visibility**",
                  "**website**"
                ]
              }
            ],
            "**permission**",
            "**units**",
            "**units_map**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
