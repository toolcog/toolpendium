{
  "name": "repoCancelScheduledAutoMerge",
  "description": "Cancel the scheduled auto merge for the given pull request",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "index": {
        "format": "int64",
        "type": "integer",
        "description": "index of the pull request to merge"
      }
    },
    "required": [
      "owner",
      "repo",
      "index"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/pulls/{index}/merge"
    }
  },
  "responses": {}
}
