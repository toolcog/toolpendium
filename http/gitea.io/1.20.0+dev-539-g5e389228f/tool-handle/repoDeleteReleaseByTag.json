{
  "name": "repoDeleteReleaseByTag",
  "description": "Delete a release by tag name",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "tag": {
        "type": "string",
        "description": "tag name of the release to delete"
      }
    },
    "required": [
      "owner",
      "repo",
      "tag"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/releases/tags/{tag}"
    }
  },
  "responses": {}
}
