{
  "name": "repoDeletePushMirror",
  "description": "deletes a push mirror from a repository by remoteName",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "name": {
        "type": "string",
        "description": "remote name of the pushMirror"
      }
    },
    "required": [
      "owner",
      "repo",
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/push_mirrors/{name}"
    }
  },
  "responses": {}
}
