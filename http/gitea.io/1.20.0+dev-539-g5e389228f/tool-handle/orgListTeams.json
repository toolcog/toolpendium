{
  "name": "orgListTeams",
  "description": "List an organization's teams",
  "parameters": {
    "type": "object",
    "properties": {
      "org": {
        "type": "string",
        "description": "name of the organization"
      },
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      },
      "limit": {
        "type": "integer",
        "description": "page size of results"
      }
    },
    "required": [
      "org"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/orgs/{org}/teams{?page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Team represents a team in an organization",
        "**Key properties:**",
        {
          "$ul": [
            "**can_create_org_repo**",
            "**description**",
            "**id**",
            "**includes_all_repositories**",
            "**name**",
            [
              "**organization**: Organization represents an organization",
              {
                "$ul": [
                  "**avatar_url**",
                  "**description**",
                  "**full_name**",
                  "**id**",
                  "**location**",
                  "**name**",
                  "**repo_admin_change_team_access**",
                  "**username**: deprecated",
                  "**visibility**",
                  "**website**"
                ]
              }
            ],
            "**permission**",
            "**units**",
            "**units_map**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
