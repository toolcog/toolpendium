{
  "name": "adminAdoptRepository",
  "description": "Adopt unadopted files as a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      }
    },
    "required": [
      "owner",
      "repo"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/admin/unadopted/{owner}/{repo}"
    }
  },
  "responses": {}
}
