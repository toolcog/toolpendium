{
  "name": "orgAddTeamMember",
  "description": "Add a team member",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the team"
      },
      "username": {
        "type": "string",
        "description": "username of the user to add"
      }
    },
    "required": [
      "id",
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/api/v1/teams/{id}/members/{username}"
    }
  },
  "responses": {}
}
