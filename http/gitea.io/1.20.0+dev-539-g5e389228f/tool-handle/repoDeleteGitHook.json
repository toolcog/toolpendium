{
  "name": "repoDeleteGitHook",
  "description": "Delete a Git hook in a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "id": {
        "type": "string",
        "description": "id of the hook to get"
      }
    },
    "required": [
      "owner",
      "repo",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/hooks/git/{id}"
    }
  },
  "responses": {}
}
