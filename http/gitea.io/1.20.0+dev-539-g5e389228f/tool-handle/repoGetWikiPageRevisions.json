{
  "name": "repoGetWikiPageRevisions",
  "description": "Get revisions of a wiki page",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "pageName": {
        "type": "string",
        "description": "name of the page"
      },
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      }
    },
    "required": [
      "owner",
      "repo",
      "pageName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/wiki/revisions/{pageName}{?page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "WikiCommitList commit/revision list",
        "**Key properties:**",
        {
          "$ul": [
            "**commits**",
            "**count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
