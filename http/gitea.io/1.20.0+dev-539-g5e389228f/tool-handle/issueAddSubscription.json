{
  "name": "issueAddSubscription",
  "description": "Subscribe user to issue",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "index": {
        "format": "int64",
        "type": "integer",
        "description": "index of the issue"
      },
      "user": {
        "type": "string",
        "description": "user to subscribe"
      }
    },
    "required": [
      "owner",
      "repo",
      "index",
      "user"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/issues/{index}/subscriptions/{user}"
    }
  },
  "responses": {}
}
