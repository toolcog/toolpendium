{
  "name": "repoGetEditorConfig",
  "description": "Get the EditorConfig definitions of a file in a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "filepath": {
        "type": "string",
        "description": "filepath of file to get"
      },
      "ref": {
        "type": "string",
        "description": "The name of the commit/branch/tag. Default the repositoryâ€™s default branch (usually master)"
      }
    },
    "required": [
      "owner",
      "repo",
      "filepath"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/editorconfig/{filepath}{?ref}"
    }
  },
  "responses": {}
}
