{
  "name": "issueStartStopWatch",
  "description": "Start stopwatch on an issue.",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "index": {
        "format": "int64",
        "type": "integer",
        "description": "index of the issue to create the stopwatch on"
      }
    },
    "required": [
      "owner",
      "repo",
      "index"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/issues/{index}/stopwatch/start"
    }
  },
  "responses": {}
}
