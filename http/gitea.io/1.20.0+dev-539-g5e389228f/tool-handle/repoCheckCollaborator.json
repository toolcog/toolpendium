{
  "name": "repoCheckCollaborator",
  "description": "Check if a user is a collaborator of a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "collaborator": {
        "type": "string",
        "description": "username of the collaborator"
      }
    },
    "required": [
      "owner",
      "repo",
      "collaborator"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/collaborators/{collaborator}"
    }
  },
  "responses": {}
}
