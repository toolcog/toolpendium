{
  "name": "adminCronList",
  "description": "List cron tasks",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      },
      "limit": {
        "type": "integer",
        "description": "page size of results"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/admin/cron{?page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Cron represents a Cron task",
        "**Key properties:**",
        {
          "$ul": [
            "**exec_times**",
            "**name**",
            "**next**",
            "**prev**",
            "**schedule**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
