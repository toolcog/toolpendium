{
  "name": "repoGetWikiPage",
  "description": "Get a wiki page",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "pageName": {
        "type": "string",
        "description": "name of the page"
      }
    },
    "required": [
      "owner",
      "repo",
      "pageName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/wiki/page/{pageName}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "WikiPage a wiki page",
        "**Key properties:**",
        {
          "$ul": [
            "**commit_count**",
            "**content_base64**: Page content, base64 encoded",
            "**footer**",
            "**html_url**",
            [
              "**last_commit**: WikiCommit page commit/revision",
              {
                "$ul": [
                  [
                    "**author**",
                    {
                      "$ul": [
                        "**date**",
                        "**email**",
                        "**name**"
                      ]
                    }
                  ],
                  [
                    "**commiter**",
                    {
                      "$ul": [
                        "**date**",
                        "**email**",
                        "**name**"
                      ]
                    }
                  ],
                  "**message**",
                  "**sha**"
                ]
              }
            ],
            "**sidebar**",
            "**sub_url**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
