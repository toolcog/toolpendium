{
  "name": "repoGetPushMirrorByRemoteName",
  "description": "Get push mirror of the repository by remoteName",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "name": {
        "type": "string",
        "description": "remote name of push mirror"
      }
    },
    "required": [
      "owner",
      "repo",
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/push_mirrors/{name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "PushMirror represents information of a push mirror",
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**interval**",
            "**last_error**",
            "**last_update**",
            "**remote_address**",
            "**remote_name**",
            "**repo_name**",
            "**sync_on_commit**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
