{
  "name": "getGeneralUISettings",
  "description": "Get instance's global settings for ui",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/settings/ui"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "GeneralUISettings contains global ui settings exposed by API",
        "**Key properties:**",
        {
          "$ul": [
            "**allowed_reactions**",
            "**custom_emojis**",
            "**default_theme**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
