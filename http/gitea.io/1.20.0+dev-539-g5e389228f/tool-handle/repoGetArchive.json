{
  "name": "repoGetArchive",
  "description": "Get an archive of a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "archive": {
        "type": "string",
        "description": "the git reference for download with attached archive format (e.g. master.zip)"
      }
    },
    "required": [
      "owner",
      "repo",
      "archive"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/archive/{archive}"
    }
  },
  "responses": {}
}
