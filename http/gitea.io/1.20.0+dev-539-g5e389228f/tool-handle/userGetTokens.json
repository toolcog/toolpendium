{
  "name": "userGetTokens",
  "description": "List the authenticated user's access tokens",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "username of user"
      },
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      },
      "limit": {
        "type": "integer",
        "description": "page size of results"
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/users/{username}/tokens{?page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "AccessToken represents an API access token. list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**name**",
            "**scopes**",
            "**sha1**",
            "**token_last_eight**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
