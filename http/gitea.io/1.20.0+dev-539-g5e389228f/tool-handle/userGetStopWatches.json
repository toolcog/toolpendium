{
  "name": "userGetStopWatches",
  "description": "Get list of all existing stopwatches",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      },
      "limit": {
        "type": "integer",
        "description": "page size of results"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/user/stopwatches{?page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "StopWatch represent a running stopwatch",
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**duration**",
            "**issue_index**",
            "**issue_title**",
            "**repo_name**",
            "**repo_owner_name**",
            "**seconds**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
