{
  "name": "repoEditReleaseAttachment",
  "description": "Edit a release attachment",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the release"
      },
      "attachment_id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the attachment to edit"
      },
      "body": {
        "$ref": "#/$defs/EditAttachmentOptions"
      }
    },
    "required": [
      "owner",
      "repo",
      "id",
      "attachment_id"
    ],
    "$defs": {
      "EditAttachmentOptions": {
        "description": "EditAttachmentOptions options for editing attachments",
        "properties": {
          "name": {
            "type": "string",
            "x-go-name": "Name"
          }
        },
        "type": "object",
        "x-go-package": "code.gitea.io/gitea/modules/structs"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/releases/{id}/assets/{attachment_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Attachment a generic attachment",
        "**Key properties:**",
        {
          "$ul": [
            "**browser_download_url**",
            "**created_at**",
            "**download_count**",
            "**id**",
            "**name**",
            "**size**",
            "**uuid**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
