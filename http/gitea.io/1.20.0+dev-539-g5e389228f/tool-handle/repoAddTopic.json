{
  "name": "repoAddTopic",
  "description": "Add a topic to a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "topic": {
        "type": "string",
        "description": "name of the topic to add"
      }
    },
    "required": [
      "owner",
      "repo",
      "topic"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/topics/{topic}"
    }
  },
  "responses": {}
}
