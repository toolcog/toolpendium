{
  "name": "repoDeletePullReview",
  "description": "Delete a specific review from a pull request",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "index": {
        "format": "int64",
        "type": "integer",
        "description": "index of the pull request"
      },
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the review"
      }
    },
    "required": [
      "owner",
      "repo",
      "index",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/pulls/{index}/reviews/{id}"
    }
  },
  "responses": {}
}
