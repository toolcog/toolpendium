{
  "name": "issueDeleteIssueAttachment",
  "description": "Delete an issue attachment",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "index": {
        "format": "int64",
        "type": "integer",
        "description": "index of the issue"
      },
      "attachment_id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the attachment to delete"
      }
    },
    "required": [
      "owner",
      "repo",
      "index",
      "attachment_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/issues/{index}/assets/{attachment_id}"
    }
  },
  "responses": {}
}
