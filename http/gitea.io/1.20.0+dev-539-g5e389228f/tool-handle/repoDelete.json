{
  "name": "repoDelete",
  "description": "Delete a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo to delete"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo to delete"
      }
    },
    "required": [
      "owner",
      "repo"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}"
    }
  },
  "responses": {}
}
