{
  "name": "orgListHooks",
  "description": "List an organization's webhooks",
  "parameters": {
    "type": "object",
    "properties": {
      "org": {
        "type": "string",
        "description": "name of the organization"
      },
      "page": {
        "type": "integer",
        "description": "page number of results to return (1-based)"
      },
      "limit": {
        "type": "integer",
        "description": "page size of results"
      }
    },
    "required": [
      "org"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/orgs/{org}/hooks{?page,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Hook a hook is a web hook when one repository changed",
        "**Key properties:**",
        {
          "$ul": [
            "**active**",
            "**authorization_header**",
            "**config**",
            "**created_at**",
            "**events**",
            "**id**",
            "**type**",
            "**updated_at**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
