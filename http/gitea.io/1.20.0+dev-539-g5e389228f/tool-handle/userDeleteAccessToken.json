{
  "name": "userDeleteAccessToken",
  "description": "delete an access token",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "username of user"
      },
      "token": {
        "type": "string",
        "description": "token to be deleted, identified by ID and if not available by name"
      }
    },
    "required": [
      "username",
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/users/{username}/tokens/{token}"
    }
  },
  "responses": {}
}
