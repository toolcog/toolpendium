{
  "name": "repoDeleteReleaseAttachment",
  "description": "Delete a release attachment",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the release"
      },
      "attachment_id": {
        "format": "int64",
        "type": "integer",
        "description": "id of the attachment to delete"
      }
    },
    "required": [
      "owner",
      "repo",
      "id",
      "attachment_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/releases/{id}/assets/{attachment_id}"
    }
  },
  "responses": {}
}
