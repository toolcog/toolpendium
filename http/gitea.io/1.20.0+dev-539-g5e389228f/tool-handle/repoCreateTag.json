{
  "name": "repoCreateTag",
  "description": "Create a new git tag in a repository",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "body": {
        "$ref": "#/$defs/CreateTagOption"
      }
    },
    "required": [
      "owner",
      "repo"
    ],
    "$defs": {
      "CreateTagOption": {
        "description": "CreateTagOption options when creating a tag",
        "properties": {
          "message": {
            "type": "string",
            "x-go-name": "Message"
          },
          "tag_name": {
            "type": "string",
            "x-go-name": "TagName"
          },
          "target": {
            "type": "string",
            "x-go-name": "Target"
          }
        },
        "required": [
          "tag_name"
        ],
        "type": "object",
        "x-go-package": "code.gitea.io/gitea/modules/structs"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/tags"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Tag represents a repository tag",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**commit**",
              {
                "$ul": [
                  "**created**",
                  "**sha**",
                  "**url**"
                ]
              }
            ],
            "**id**",
            "**message**",
            "**name**",
            "**tarball_url**",
            "**zipball_url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
