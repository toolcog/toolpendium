{
  "name": "repoDeleteWikiPage",
  "description": "Delete a wiki page",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "pageName": {
        "type": "string",
        "description": "name of the page"
      }
    },
    "required": [
      "owner",
      "repo",
      "pageName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/wiki/page/{pageName}"
    }
  },
  "responses": {}
}
