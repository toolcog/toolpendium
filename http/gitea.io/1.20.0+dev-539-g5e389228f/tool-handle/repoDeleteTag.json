{
  "name": "repoDeleteTag",
  "description": "Delete a repository's tag by name",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "tag": {
        "type": "string",
        "description": "name of tag to delete"
      }
    },
    "required": [
      "owner",
      "repo",
      "tag"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/tags/{tag}"
    }
  },
  "responses": {}
}
