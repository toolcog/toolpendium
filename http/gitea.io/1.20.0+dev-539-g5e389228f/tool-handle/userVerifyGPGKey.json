{
  "name": "userVerifyGPGKey",
  "description": "Verify a GPG key",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/user/gpg_key_verify"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "GPGKey a user GPG key to sign commit and tag in repository",
        "**Key properties:**",
        {
          "$ul": [
            "**can_certify**",
            "**can_encrypt_comms**",
            "**can_encrypt_storage**",
            "**can_sign**",
            "**created_at**",
            "**emails**",
            "**expires_at**",
            "**id**",
            "**key_id**",
            "**primary_key_id**",
            "**public_key**",
            "**subkeys**",
            "**verified**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
