{
  "name": "repoUpdatePullRequest",
  "description": "Merge PR's baseBranch into headBranch",
  "parameters": {
    "type": "object",
    "properties": {
      "owner": {
        "type": "string",
        "description": "owner of the repo"
      },
      "repo": {
        "type": "string",
        "description": "name of the repo"
      },
      "index": {
        "format": "int64",
        "type": "integer",
        "description": "index of the pull request to get"
      },
      "style": {
        "enum": [
          "merge",
          "rebase"
        ],
        "type": "string",
        "description": "how to update pull request"
      }
    },
    "required": [
      "owner",
      "repo",
      "index"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/api/v1/repos/{owner}/{repo}/pulls/{index}/update{?style}"
    }
  },
  "responses": {}
}
