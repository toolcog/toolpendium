{
  "name": "userCheckFollowing",
  "description": "Check if one user is following another user",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "username of following user"
      },
      "target": {
        "type": "string",
        "description": "username of followed user"
      }
    },
    "required": [
      "username",
      "target"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/users/{username}/following/{target}"
    }
  },
  "responses": {}
}
