{
  "name": "By_Category",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "category": {
        "type": "string",
        "description": "the code of category"
      },
      "subcategory": {
        "type": "string",
        "description": "the code of subcategory"
      },
      "amount": {
        "default": 25,
        "type": "number",
        "description": "the maximum number of icons which you'd like to receive"
      },
      "offset": {
        "type": "number",
        "description": "the offset from the first received result"
      },
      "platform": {
        "enum": [
          "ios7",
          "win8",
          "win10",
          "android",
          "androidL",
          "color",
          "office"
        ],
        "type": "string",
        "description": "the style of the icons"
      },
      "language": {
        "enum": [
          "en-US",
          "fr-FR",
          "de-DE",
          "it-IT",
          "pt-BR",
          "pl-PL",
          "ru-RU",
          "es-ES"
        ],
        "type": "string",
        "description": "the language code to get localized result"
      }
    },
    "required": [
      "category",
      "subcategory",
      "amount",
      "offset",
      "platform",
      "language"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.icons8.com/api/iconsets/v3/category?category={category}&subcategory={subcategory}&amount={amount}&offset={offset}&platform={platform}&language={language}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**parameters**: copy of the input parameters from the request",
              {
                "$ul": [
                  "**amount**",
                  "**category**",
                  "**language**",
                  "**offset**",
                  "**platform**",
                  "**subcategory**"
                ]
              }
            ],
            [
              "**result**: the result of operation",
              {
                "$ul": [
                  "**category**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
