{
  "name": "addPacerCredential",
  "description": "Register PACER credentials with UniCourt.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/PacerCredentialRequest"
      }
    },
    "$defs": {
      "PacerCredentialRequest": {
        "properties": {
          "defaultPacerClientCode": {
            "description": "Pacer Client Code.",
            "example": "Test UniCourt API",
            "maxLength": 32,
            "minLength": 0,
            "nullable": true,
            "type": "string"
          },
          "pacerUserId": {
            "description": "Pacer User Id.",
            "example": "URKYwer3tyh5r56gq2",
            "maxLength": 40,
            "minLength": 6,
            "nullable": false,
            "type": "string"
          },
          "password": {
            "description": "Password.",
            "example": "your password",
            "maxLength": 45,
            "minLength": 8,
            "nullable": false,
            "type": "string"
          }
        },
        "required": [
          "pacerUserId",
          "password"
        ],
        "type": "object",
        "x-unicourt-container": false
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
