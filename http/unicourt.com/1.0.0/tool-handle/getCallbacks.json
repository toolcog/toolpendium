{
  "name": "getCallbacks",
  "description": "Get list of callback types with count for a requested Date.",
  "parameters": {
    "type": "object",
    "properties": {
      "date": {
        "format": "date-time",
        "maxLength": 25,
        "minLength": 25,
        "nullable": false,
        "type": "string",
        "description": "Date for which fetch the callback type list. By default, the date will be set to current date."
      },
      "status": {
        "enum": [
          "IN_PROGRESS",
          "COMPLETE",
          "FAILURE"
        ],
        "maxLength": 11,
        "minLength": 7,
        "nullable": false,
        "type": "string",
        "description": "Status of the callbacks. Default status will fetch all callbacks."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://enterpriseapi.unicourt.com//callbacks{?date,status}"
    }
  },
