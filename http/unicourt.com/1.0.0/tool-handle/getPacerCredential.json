    "type": "object",
    "properties": {
      "pageNumber": {
        "example": 1,
        "minimum": 1,
        "type": "integer",
        "description": "The page number of the PACER credentials to be retrieved.<br>\n  - Minimum: 1\n"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://enterpriseapi.unicourt.com//pacerCredential{?pageNumber}"
    }
  },
