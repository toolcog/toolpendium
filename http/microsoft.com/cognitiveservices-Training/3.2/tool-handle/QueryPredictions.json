{
  "name": "QueryPredictions",
  "description": "Get images that were sent to your prediction endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id."
      },
      "body": {
        "$ref": "#/components/schemas/PredictionQueryToken",
        "description": "Parameters used to query the predictions. Limited to combining 2 tags."
      }
    },
    "required": [
      "projectId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v3.2/training/projects/{projectId}/predictions/query"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Query result of the prediction images that were sent to your prediction endpoint.",
        "**Key properties:**",
        {
          "$ul": [
            "**results**: Result of an prediction request.",
            [
              "**token**",
              {
                "$ul": [
                  "**application**",
                  "**continuation**",
                  "**endTime**",
                  "**iterationId**",
                  "**maxCount**",
                  "**orderBy**",
                  "**session**",
                  "**startTime**",
                  "**tags**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The error code.",
            "**message**: A message explaining the error reported by the service."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
