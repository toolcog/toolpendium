{
  "name": "QuickTestImage",
  "description": "Quick test an image.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id."
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "Optional. Specifies the id of a particular iteration to evaluate against.\r\n            The default iteration for the project will be used when not specified."
      },
      "store": {
        "default": true,
        "type": "boolean",
        "description": "Optional. Specifies whether or not to store the result of this prediction. The default is true, to store."
      },
      "body": {
        "properties": {
          "imageData": {
            "description": "Binary image data. Supported formats are JPEG, GIF, PNG, and BMP. Supports images up to 6MB.",
            "format": "binary",
            "type": "string"
          }
        },
        "required": [
          "imageData"
        ],
        "type": "object"
      }
    },
    "required": [
      "projectId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v3.2/training/projects/{projectId}/quicktest/image{?iterationId,store}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Result of an image prediction request.",
        "**Key properties:**",
        {
          "$ul": [
            "**created**: Date this prediction was created.",
            "**id**: Prediction Id.",
            "**iteration**: Iteration Id.",
            "**predictions**: List of predictions.",
            "**project**: Project Id."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The error code.",
            "**message**: A message explaining the error reported by the service."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
