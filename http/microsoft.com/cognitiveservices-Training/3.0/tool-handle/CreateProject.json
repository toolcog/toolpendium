{
  "name": "CreateProject",
  "description": "Create a project.",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "type": "string",
        "description": "Name of the project."
      },
      "description": {
        "type": "string",
        "description": "The description of the project."
      },
      "domainId": {
        "format": "uuid",
        "type": "string",
        "description": "The id of the domain to use for this project. Defaults to General."
      },
      "classificationType": {
        "enum": [
          "Multiclass",
          "Multilabel"
        ],
        "type": "string",
        "description": "The type of classifier to create for this project."
      },
      "targetExportPlatforms": {
        "items": {
          "enum": [
            "CoreML",
            "TensorFlow",
            "DockerFile",
            "ONNX",
            "VAIDK"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "List of platforms the trained model is intending exporting to."
      },
      "Training-Key": {
        "type": "string",
        "description": "API key."
      }
    },
    "required": [
      "name",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v3.0/training/projects{?name,description,domainId,classificationType,targetExportPlatforms}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents a project.",
        "**Key properties:**",
        {
          "$ul": [
            "**created**: Gets the date this project was created.",
            "**description**: Gets or sets the description of the project.",
            "**drModeEnabled**: Gets if the DR mode is on.",
            "**id**: Gets the project id.",
            "**lastModified**: Gets the date this project was last modified.",
            "**name**: Gets or sets the name of the project.",
            [
              "**settings**: Represents settings associated with a project.",
              {
                "$ul": [
                  "**classificationType**: Gets or sets the classification type of the project.",
                  "**domainId**: Gets or sets the id of the Domain to use with this project.",
                  "**targetExportPlatforms**: A list of ExportPlatform that the trained model should be able to support."
                ]
              }
            ],
            "**thumbnailUri**: Gets the thumbnail url representing the image."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The error code.",
            "**message**: A message explaining the error reported by the service."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
