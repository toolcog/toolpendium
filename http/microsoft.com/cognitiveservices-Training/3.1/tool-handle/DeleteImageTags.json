{
  "name": "DeleteImageTags",
  "description": "Remove a set of tags from a set of images.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id."
      },
      "imageIds": {
        "items": {
          "format": "uuid",
          "nullable": false,
          "type": "string"
        },
        "maxItems": 64,
        "minItems": 0,
        "type": "array",
        "description": "Image ids. Limited to 64 images."
      },
      "tagIds": {
        "items": {
          "format": "uuid",
          "nullable": false,
          "type": "string"
        },
        "maxItems": 20,
        "minItems": 0,
        "type": "array",
        "description": "Tags to be deleted from the specified images. Limited to 20 tags."
      },
      "Training-Key": {
        "type": "string",
        "description": "API key."
      }
    },
    "required": [
      "projectId",
      "imageIds",
      "tagIds",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v3.1/training/projects/{projectId}/images/tags{?imageIds,tagIds}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
