{
  "name": "DeleteImages",
  "description": "Delete images from the set of training images.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id."
      },
      "imageIds": {
        "items": {
          "format": "uuid",
          "nullable": false,
          "type": "string"
        },
        "maxItems": 256,
        "minItems": 0,
        "type": "array",
        "description": "Ids of the images to be deleted. Limited to 256 images per batch."
      },
      "allImages": {
        "type": "boolean",
        "description": "Flag to specify delete all images, specify this flag or a list of images. Using this flag will return a 202 response to indicate the images are being deleted."
      },
      "allIterations": {
        "type": "boolean",
        "description": "Removes these images from all iterations, not just the current workspace. Using this flag will return a 202 response to indicate the images are being deleted."
      },
      "Training-Key": {
        "type": "string",
        "description": "API key."
      }
    },
    "required": [
      "projectId",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v3.1/training/projects/{projectId}/images{?imageIds,allImages,allIterations}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The error code.",
            "**message**: A message explaining the error reported by the service."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
