{
  "name": "ExportIteration",
  "description": "Export a trained iteration.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id."
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "The iteration id."
      },
      "platform": {
        "enum": [
          "CoreML",
          "TensorFlow",
          "DockerFile",
          "ONNX",
          "VAIDK"
        ],
        "type": "string",
        "description": "The target platform."
      },
      "flavor": {
        "enum": [
          "Linux",
          "Windows",
          "ONNX10",
          "ONNX12",
          "ARM",
          "TensorFlowNormal",
          "TensorFlowLite"
        ],
        "type": "string",
        "description": "The flavor of the target platform."
      },
      "Training-Key": {
        "type": "string",
        "description": "API key."
      }
    },
    "required": [
      "projectId",
      "iterationId",
      "platform",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v3.1/training/projects/{projectId}/iterations/{iterationId}/export{?platform,flavor}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**downloadUri**: URI used to download the model.",
            "**flavor**: Flavor of the export.",
            "**newerVersionAvailable**: Indicates an updated version of the export package is available and should be re-exported for the latest changes.",
            "**platform**: Platform of the export.",
            "**status**: Status of the export."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The error code.",
            "**message**: A message explaining the error reported by the service."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
