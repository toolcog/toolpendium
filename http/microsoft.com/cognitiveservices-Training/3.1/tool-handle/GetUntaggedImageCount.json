{
  "name": "GetUntaggedImageCount",
  "description": "This API returns the images which have no tags for a given project and optionally an iteration. If no iteration is specified the\r\ncurrent workspace is used.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id."
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "The iteration id. Defaults to workspace."
      },
      "Training-Key": {
        "type": "string",
        "description": "API key."
      }
    },
    "required": [
      "projectId",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v3.1/training/projects/{projectId}/images/untagged/count{?iterationId}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The error code.",
            "**message**: A message explaining the error reported by the service."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
