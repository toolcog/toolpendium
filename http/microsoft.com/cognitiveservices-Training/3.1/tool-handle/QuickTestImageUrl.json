{
  "name": "QuickTestImageUrl",
  "description": "Quick test an image url.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project to evaluate against."
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "Optional. Specifies the id of a particular iteration to evaluate against.\r\n            The default iteration for the project will be used when not specified."
      },
      "store": {
        "default": true,
        "type": "boolean",
        "description": "Optional. Specifies whether or not to store the result of this prediction. The default is true, to store."
      },
      "Training-Key": {
        "type": "string",
        "description": "API key."
      },
      "body": {
        "$ref": "#/components/schemas/ImageUrl",
        "description": "An ImageUrl that contains the url of the image to be evaluated."
      }
    },
    "required": [
      "projectId",
      "Training-Key",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v3.1/training/projects/{projectId}/quicktest/url{?iterationId,store}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Result of an image prediction request.",
        "**Key properties:**",
        {
          "$ul": [
            "**created**: Date this prediction was created.",
            "**id**: Prediction Id.",
            "**iteration**: Iteration Id.",
            "**predictions**: List of predictions.",
            "**project**: Project Id."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The error code.",
            "**message**: A message explaining the error reported by the service."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
