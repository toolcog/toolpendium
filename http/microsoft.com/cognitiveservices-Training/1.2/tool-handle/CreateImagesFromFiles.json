{
  "name": "CreateImagesFromFiles",
  "description": "Add the provided batch of images to the set of training images",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "Training-Key": {
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/ImageFileCreateBatch",
        "description": "The batch of image files to add. Limited to 64 images and 20 tags per batch"
      }
    },
    "required": [
      "projectId",
      "Training-Key",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v1.2/Training/projects/{projectId}/images/files"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Images**",
            "**IsBatchSuccessful**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
