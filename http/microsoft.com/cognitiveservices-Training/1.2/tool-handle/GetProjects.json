{
  "name": "GetProjects",
  "description": "Get your projects",
  "parameters": {
    "type": "object",
    "properties": {
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v1.2/Training/projects"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Represents a project",
        "**Key properties:**",
        {
          "$ul": [
            "**Created**: Gets the date this project was created",
            "**CurrentIterationId**: Gets the current iteration id",
            "**Description**: Gets or sets the description of the project",
            "**Id**: Gets the project id",
            "**LastModified**: Gets the date this project was last modified",
            "**Name**: Gets or sets the name of the project",
            [
              "**Settings**: Represents settings associated with a project",
              {
                "$ul": [
                  "**DomainId**: Gets or sets the id of the Domain to use with this project"
                ]
              }
            ],
            "**ThumbnailUri**: Gets the thumbnail url representing the image"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
