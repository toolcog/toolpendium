{
  "name": "DeletePrediction",
  "description": "Delete a set of predicted images and their associated prediction results",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "ids": {
        "items": {
          "nullable": false,
          "type": "string"
        },
        "type": "array",
        "description": "The prediction ids. Limited to 64"
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "projectId",
      "ids",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.0/Training/projects/{projectId}/predictions{?ids}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {}
}
