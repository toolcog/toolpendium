{
  "name": "GetTags",
  "description": "Get the tags for a given project and iteration",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "The iteration id. Defaults to workspace"
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "projectId",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.0/Training/projects/{projectId}/tags{?iterationId}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Represents a Tag",
        "**Key properties:**",
        {
          "$ul": [
            "**description**: Gets or sets the description of the tag",
            "**id**: Gets the Tag ID",
            "**imageCount**: Gets the number of images with this tag",
            "**name**: Gets or sets the name of the tag"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
