{
  "name": "GetDomain",
  "description": "Get information about a specific domain",
  "parameters": {
    "type": "object",
    "properties": {
      "domainId": {
        "format": "uuid",
        "type": "string",
        "description": "The id of the domain to get information about"
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "domainId",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.0/Training/domains/{domainId}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**enabled**",
            "**exportable**",
            "**id**",
            "**name**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
