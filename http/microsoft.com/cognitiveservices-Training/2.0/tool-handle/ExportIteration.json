{
  "name": "ExportIteration",
  "description": "Export a trained iteration",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "The iteration id"
      },
      "platform": {
        "type": "string",
        "description": "The target platform (coreml or tensorflow)"
      },
      "flavor": {
        "type": "string",
        "description": "The flavor of the target platform (Windows, Linux, ARM, or GPU)"
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "projectId",
      "iterationId",
      "platform",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.0/Training/projects/{projectId}/iterations/{iterationId}/export{?platform,flavor}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**downloadUri**",
            "**flavor**",
            "**platform**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
