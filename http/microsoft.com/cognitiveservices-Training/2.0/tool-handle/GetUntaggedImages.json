{
  "name": "GetUntaggedImages",
  "description": "This API supports batching and range selection. By default it will only return first 50 images matching images.\r\nUse the {take} and {skip} parameters to control how many images to return in a given batch.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "The iteration id. Defaults to workspace"
      },
      "orderBy": {
        "enum": [
          "Newest",
          "Oldest"
        ],
        "type": "string",
        "description": "The ordering. Defaults to newest"
      },
      "take": {
        "default": 50,
        "format": "int32",
        "type": "integer",
        "description": "Maximum number of images to return. Defaults to 50, limited to 256"
      },
      "skip": {
        "default": 0,
        "format": "int32",
        "type": "integer",
        "description": "Number of images to skip before beginning the image batch. Defaults to 0"
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "projectId",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.0/Training/projects/{projectId}/images/untagged{?iterationId,orderBy,take,skip}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Image model to be sent as JSON",
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**height**",
            "**id**",
            "**imageUri**",
            "**regions**",
            "**tags**",
            "**thumbnailUri**",
            "**width**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
