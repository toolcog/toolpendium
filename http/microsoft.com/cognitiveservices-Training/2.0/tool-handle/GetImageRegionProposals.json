{
  "name": "GetImageRegionProposals",
  "description": "This API will get region proposals for an image along with confidences for the region. It returns an empty array if no proposals are found.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "imageId": {
        "format": "uuid",
        "type": "string",
        "description": "The image id"
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "projectId",
      "imageId",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.0/Training/{projectId}/images/{imageId}/regionproposals"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**imageId**",
            "**projectId**",
            "**proposals**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
