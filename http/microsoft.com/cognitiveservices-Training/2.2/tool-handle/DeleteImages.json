{
  "name": "DeleteImages",
  "description": "Delete images from the set of training images.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id."
      },
      "imageIds": {
        "items": {
          "nullable": false,
          "type": "string"
        },
        "type": "array",
        "description": "Ids of the images to be deleted. Limited to 256 images per batch."
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "projectId",
      "imageIds",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.2/Training/projects/{projectId}/images{?imageIds}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {}
}
