{
  "name": "DeleteImageRegions",
  "description": "Delete a set of image regions",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "regionIds": {
        "items": {
          "nullable": false,
          "type": "string"
        },
        "type": "array",
        "description": "Regions to delete. Limited to 64"
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "projectId",
      "regionIds",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.1/Training/projects/{projectId}/images/regions{?regionIds}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {}
}
