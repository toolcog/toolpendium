{
  "name": "DeleteImageTags",
  "description": "Remove a set of tags from a set of images",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "imageIds": {
        "items": {
          "nullable": false,
          "type": "string"
        },
        "type": "array",
        "description": "Image ids. Limited to 64 images"
      },
      "tagIds": {
        "items": {
          "nullable": false,
          "type": "string"
        },
        "type": "array",
        "description": "Tags to be deleted from the specified images. Limited to 20 tags"
      },
      "Training-Key": {
        "type": "string"
      }
    },
    "required": [
      "projectId",
      "imageIds",
      "tagIds",
      "Training-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.1/Training/projects/{projectId}/images/tags{?imageIds,tagIds}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    }
  },
  "responses": {}
}
