{
  "name": "QuickTestImage",
  "description": "Quick test an image",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "Optional. Specifies the id of a particular iteration to evaluate against.\r\n            The default iteration for the project will be used when not specified."
      },
      "Training-Key": {
        "type": "string"
      },
      "body": {
        "properties": {
          "imageData": {
            "format": "binary",
            "type": "string"
          }
        },
        "required": [
          "imageData"
        ],
        "type": "object"
      }
    },
    "required": [
      "projectId",
      "Training-Key",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.1/Training/projects/{projectId}/quicktest/image{?iterationId}"
    },
    "headers": {
      "Training-Key": {
        "$": "Training-Key"
      }
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**id**",
            "**iteration**",
            "**predictions**",
            "**project**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
