        "description": "Expand related entities"
      },
      "drive-id": {
        "type": "string",
        "description": "key: id of drive"
      },
      "driveItem-id": {
        "type": "string",
        "description": "key: id of driveItem"
      }
    },
    "required": [
      "drive-id",
      "driveItem-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://graph.microsoft.com/v1.0/drives/{drive-id}/items/{driveItem-id}/thumbnails{?$top,$skip,$search,$filter,$count,$orderby,$select,$expand}"
