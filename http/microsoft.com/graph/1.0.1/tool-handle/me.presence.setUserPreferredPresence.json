    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "activity": {
            "type": "string"
          },
          "availability": {
            "type": "string"
          },
          "expirationDuration": {
            "format": "duration",
            "nullable": true,
            "pattern": "^-?P([0-9]+D)?(T([0-9]+H)?([0-9]+M)?([0-9]+([.][0-9]+)?S)?)?$",
            "type": "string"
          }
        },
        "type": "object",
        "description": "Action parameters"
      }
    },
