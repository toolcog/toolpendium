        },
        "type": "array",
        "uniqueItems": true,
        "description": "Order items by property values"
      },
      "$select": {
        "items": {
          "enum": [
            "id",
            "assignedTo",
            "classification",
            "comments",
            "createdDateTime",
            "customTags",
            "determination",
            "displayName",
            "incidentWebUrl",
            "lastUpdateDateTime",
            "redirectIncidentId",
            "severity",
            "status",
            "tenantId",
            "alerts"
          ],
          "type": "string"
        },
        "type": "array",
        "uniqueItems": true,
        "description": "Select properties to be returned"
      },
      "$expand": {
