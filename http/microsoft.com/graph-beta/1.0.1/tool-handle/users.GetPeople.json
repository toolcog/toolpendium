        },
        "type": "array",
        "uniqueItems": true,
        "description": "Select properties to be returned"
      },
      "user-id": {
        "type": "string",
        "description": "key: id of user"
      },
      "person-id": {
        "type": "string",
        "description": "key: id of person"
      }
    },
    "required": [
      "user-id",
      "person-id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://graph.microsoft.com/beta/users/{user-id}/people/{person-id}{?$select}"
    }
  },
