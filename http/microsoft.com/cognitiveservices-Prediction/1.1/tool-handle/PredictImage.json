{
  "name": "PredictImage",
  "description": "Predict an image and saves the result",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "Optional. Specifies the id of a particular iteration to evaluate against.\r\n            The default iteration for the project will be used when not specified"
      },
      "application": {
        "type": "string",
        "description": "Optional. Specifies the name of application using the endpoint"
      },
      "Prediction-Key": {
        "type": "string"
      },
      "body": {
        "properties": {
          "imageData": {
            "format": "binary",
            "type": "string"
          }
        },
        "required": [
          "imageData"
        ],
        "type": "object"
      }
    },
    "required": [
      "projectId",
      "Prediction-Key",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v1.1/Prediction/{projectId}/image{?iterationId,application}"
    },
    "headers": {
      "Prediction-Key": {
        "$": "Prediction-Key"
      }
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "result of an image prediction request",
        "**Key properties:**",
        {
          "$ul": [
            "**Created**",
            "**Id**",
            "**Iteration**",
            "**Predictions**",
            "**Project**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
