{
  "name": "PredictImageUrlWithNoStore",
  "description": "Predict an image url without saving the result",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "uuid",
        "type": "string",
        "description": "The project id"
      },
      "iterationId": {
        "format": "uuid",
        "type": "string",
        "description": "Optional. Specifies the id of a particular iteration to evaluate against.\r\n            The default iteration for the project will be used when not specified"
      },
      "application": {
        "type": "string",
        "description": "Optional. Specifies the name of application using the endpoint"
      },
      "Prediction-Key": {
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/ImageUrl",
        "description": "An {Iris.Web.Api.Models.ImageUrl} that contains the url of the image to be evaluated"
      }
    },
    "required": [
      "projectId",
      "Prediction-Key",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://southcentralus.api.cognitive.microsoft.com/customvision/v2.0/Prediction/{projectId}/url/nostore{?iterationId,application}"
    },
    "headers": {
      "Prediction-Key": {
        "$": "Prediction-Key"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**id**",
            "**iteration**",
            "**predictions**",
            "**project**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
