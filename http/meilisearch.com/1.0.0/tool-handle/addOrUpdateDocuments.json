{
  "name": "addOrUpdateDocuments",
  "description": "Add or update documents",
  "parameters": {
    "type": "object",
    "properties": {
      "primaryKey": {
        "example": "id",
        "type": "string"
      },
      "csvDelimiter": {
        "example": ";",
        "type": "string"
      },
      "body": {
        "example": [
          {
            "author": "J. Austen",
            "date": "1813",
            "id": 2
          }
        ],
        "items": {
          "properties": {
            "author": {
              "example": "J. Austen",
              "type": "string"
            },
            "date": {
              "example": "1813",
              "type": "string"
            },
            "id": {
              "example": 2,
              "type": "number"
            }
          },
          "type": "object"
        },
        "type": "array"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://localhost:7700//indexes/books/documents{?primaryKey,csvDelimiter}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
