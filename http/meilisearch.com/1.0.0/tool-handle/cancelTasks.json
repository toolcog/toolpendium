{
  "name": "cancelTasks",
  "description": "Cancel tasks",
  "parameters": {
    "type": "object",
    "properties": {
      "uids": {
        "type": "string"
      },
      "indexUids": {
        "example": "books",
        "type": "string"
      },
      "types": {
        "example": "documentAdditionOrUpdate",
        "type": "string"
      },
      "statuses": {
        "example": "failed",
        "type": "string"
      },
      "beforeEnqueuedAt": {
        "type": "string"
      },
      "afterEnqueuedAt": {
        "type": "string"
      },
      "beforeStartedAt": {
        "type": "string"
      },
      "afterStartedAt": {
        "type": "string"
      },
      "beforeFinishedAt": {
        "type": "string"
      },
      "afterFinishedAt": {
        "type": "string"
      },
      "canceledBy": {
        "type": "string"
      },
      "limit": {
        "example": "2",
        "type": "string"
      },
      "from": {
        "example": "10",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://localhost:7700//tasks/cancel{?uids,indexUids,types,statuses,beforeEnqueuedAt,afterEnqueuedAt,beforeStartedAt,afterStartedAt,beforeFinishedAt,afterFinishedAt,canceledBy,limit,from}"
    }
  },
  "responses": {}
}
