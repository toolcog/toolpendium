{
  "name": "validateUrl",
  "description": "",
  "parameters": {
    "type": "object",
    "properties": {
      "url": {
        "format": "uri",
        "type": "string",
        "description": "The URL to retrieve the OpenAPI 3.0.x definition from"
      }
    },
    "required": [
      "url"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://mermade.org.uk/api/v1/validate{?url}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**openapi**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
