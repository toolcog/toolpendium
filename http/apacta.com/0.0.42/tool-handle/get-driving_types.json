{
  "name": "get-driving_types",
  "description": "List the driving types of the company",
  "parameters": {
    "type": "object",
    "properties": {
      "q": {
        "type": "string"
      },
      "sort": {
        "type": "string"
      },
      "direction": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.apacta.com/api/v1/driving_types{?q,sort,direction}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**pagination**",
              {
                "$ul": [
                  "**count**",
                  "**current_page**",
                  "**has_next_page**",
                  "**has_prev_page**",
                  "**limit**",
                  "**page_count**"
                ]
              }
            ],
            "**success** (default: true)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
