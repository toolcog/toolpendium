{
  "name": "getFinancialStatistics",
  "description": "Get general statistics",
  "parameters": {
    "type": "object",
    "properties": {
      "date_from": {
        "format": "date",
        "type": "string"
      },
      "date_to": {
        "format": "date",
        "type": "string"
      },
      "project_id": {
        "format": "uuid",
        "type": "string"
      },
      "project_status_ids[]": {
        "format": "uuid",
        "type": "string"
      },
      "only_not_invoiced": {
        "type": "boolean"
      },
      "details": {
        "type": "boolean"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.apacta.com/api/v1/financial_statistics{?date_from,date_to,project_id,project_status_ids[],only_not_invoiced,details}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**invoicedAmount**",
                  "**invoicedWorkingHours**",
                  "**notInvoicedAmount**",
                  "**notInvoicedWorkingHours**",
                  "**productsCosts**",
                  "**productsSales**",
                  "**rentalsCosts**",
                  "**rentalsSales**",
                  "**totalCosts**",
                  "**totalSales**",
                  "**workTimeCosts**",
                  "**workTimeSales**"
                ]
              }
            ],
            "**success** (default: true)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
