{
  "name": "getFinancialStatisticsOverview",
  "description": "Get statistics overview",
  "parameters": {
    "type": "object",
    "properties": {
      "date_from": {
        "format": "date",
        "type": "string"
      },
      "date_to": {
        "format": "date",
        "type": "string"
      },
      "project_id": {
        "format": "uuid",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.apacta.com/api/v1/financial_statistics/overview{?date_from,date_to,project_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**expensesSalesPrice**",
                  "**invoicedAmount**",
                  "**margin**",
                  "**materialRentalsCostPrice**",
                  "**productsCostPrice**",
                  "**totalWorkingHours**"
                ]
              }
            ],
            "**success** (default: true)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
