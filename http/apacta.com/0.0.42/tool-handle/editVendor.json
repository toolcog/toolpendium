{
  "name": "editVendor",
  "description": "Edit a vendor",
  "parameters": {
    "type": "object",
    "properties": {
      "vendor_id": {
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "properties": {
          "country_id": {
            "format": "uuid",
            "type": "string"
          },
          "cvr": {
            "maxLength": 12,
            "type": "string"
          },
          "email": {
            "maxLength": 255,
            "type": "string"
          },
          "identifier": {
            "maxLength": 255,
            "type": "string"
          },
          "is_custom": {
            "type": "boolean"
          },
          "name": {
            "maxLength": 255,
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "vendor_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://app.apacta.com/api/v1/vendors/{vendor_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**success**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
