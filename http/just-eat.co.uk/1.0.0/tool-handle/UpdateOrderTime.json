{
  "name": "UpdateOrderTime",
  "description": "Update the restaurant's delivery and collection lead times",
  "parameters": {
    "type": "object",
    "properties": {
      "tenant": {
        "type": "string",
        "description": "A valid country code, e.g. \"uk\"."
      },
      "restaurantId": {
        "type": "string",
        "description": "The restaurant Id."
      },
      "dayOfWeek": {
        "enum": [
          "Sunday",
          "Monday",
          "Tuesday",
          "Wednesday",
          "Thursday",
          "Friday",
          "Saturday"
        ],
        "type": "string",
        "description": "Day of week for order time"
      },
      "serviceType": {
        "enum": [
          "Delivery",
          "Collection"
        ],
        "type": "string",
        "description": "Service type of the order time"
      },
      "Authorization": {
        "type": "string",
        "description": "OAuth2 token issued for logged in restaurant OR API token for third party, in the format `Bearer {api_key}`"
      },
      "body": {
        "$ref": "#/$defs/updateOrderTimeRequest"
      }
    },
    "required": [
      "tenant",
      "restaurantId",
      "dayOfWeek",
      "serviceType",
      "Authorization",
      "body"
    ],
    "$defs": {
      "updateOrderTimeRequest": {
        "description": "Request object for order time update",
        "example": {
          "lowerBoundMinutes": 35,
          "upperBoundMinutes": 50
        },
        "properties": {
          "lowerBoundMinutes": {
            "description": "Order time lower bound value, in minutes.",
            "format": "int32",
            "type": "integer"
          },
          "upperBoundMinutes": {
            "description": "Order time upper bound value, in minutes.",
            "format": "int32",
            "type": "integer"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://uk.api.just-eat.io/restaurants/{tenant}/{restaurantId}/ordertimes/{dayOfWeek}/{serviceType}"
    },
    "headers": {
      "Authorization": {
        "$": "Authorization"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
