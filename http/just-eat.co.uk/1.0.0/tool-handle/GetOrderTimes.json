{
  "name": "GetOrderTimes",
  "description": "Get list of the delivery and collection lead times supplied by the restaurant",
  "parameters": {
    "type": "object",
    "properties": {
      "tenant": {
        "type": "string",
        "description": "A valid country code, e.g. \"uk\"."
      },
      "restaurantId": {
        "type": "string",
        "description": "The restaurant Id."
      },
      "Authorization": {
        "type": "string",
        "description": "OAuth2 token issued for logged in restaurant"
      }
    },
    "required": [
      "tenant",
      "restaurantId",
      "Authorization"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://uk.api.just-eat.io/restaurants/{tenant}/{restaurantId}/ordertimes"
    },
    "headers": {
      "Authorization": {
        "$": "Authorization"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "The object representing order time entry",
        "**Key properties:**",
        {
          "$ul": [
            "**dayOfWeek**: Day of week for order time",
            "**lowerBoundMinutes**: Order time lower bound value, in minutes.",
            "**serviceType**: Service type of the order time",
            "**upperBoundMinutes**: Order time upper bound value, in minutes."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A HTTP 4xx error response",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**fault**",
              {
                "$ul": [
                  "**errors**",
                  "**faultId**: A value that helps identify this response back to logs, so we can easily find this specific fault",
                  "**traceId**: A value that helps identify the trace back to logs, so that we can easily find what was happening on our system when the fault was generated"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A HTTP 4xx error response",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**fault**",
              {
                "$ul": [
                  "**errors**",
                  "**faultId**: A value that helps identify this response back to logs, so we can easily find this specific fault",
                  "**traceId**: A value that helps identify the trace back to logs, so that we can easily find what was happening on our system when the fault was generated"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A HTTP 500 error response",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**fault**",
              {
                "$ul": [
                  "**errors**",
                  "**faultId**: A value that helps identify this response back to logs, so we can easily find this specific fault",
                  "**traceId**: A value that helps identify the trace back to logs, so that we can easily find what was happening on our system when the fault was generated"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
