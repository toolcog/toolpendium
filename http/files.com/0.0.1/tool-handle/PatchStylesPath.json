{
  "name": "PatchStylesPath",
  "description": "Update Style",
  "parameters": {
    "type": "object",
    "properties": {
      "path": {
        "type": "string",
        "description": "Style path."
      },
      "body": {
        "properties": {
          "file": {
            "description": "Logo for custom branding.",
            "format": "binary",
            "type": "string",
            "x-ms-summary": "Logo for custom branding."
          }
        },
        "required": [
          "file"
        ],
        "type": "object"
      }
    },
    "required": [
      "path",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//app.files.com/api/rest/v1/styles/{path}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Show Style",
        "**Key properties:**",
        {
          "$ul": [
            "**id**: Style ID",
            [
              "**logo**",
              {
                "$ul": [
                  "**name**: Image name",
                  "**uri**: Image URI"
                ]
              }
            ],
            "**path**: Folder path",
            [
              "**thumbnail**",
              {
                "$ul": [
                  "**name**: Image name",
                  "**uri**: Image URI"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
