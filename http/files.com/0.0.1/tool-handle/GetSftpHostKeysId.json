{
  "name": "GetSftpHostKeysId",
  "description": "Show Sftp Host Key",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int32",
        "type": "integer",
        "description": "Sftp Host Key ID."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//app.files.com/api/rest/v1/sftp_host_keys/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Create Sftp Host Key",
        "**Key properties:**",
        {
          "$ul": [
            "**fingerprint_md5**: MD5 Fingerpint of the public key",
            "**fingerprint_sha256**: SHA256 Fingerpint of the public key",
            "**id**: Sftp Host Key ID",
            "**name**: The friendly name of this SFTP Host Key."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
