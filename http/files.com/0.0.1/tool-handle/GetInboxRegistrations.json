{
  "name": "GetInboxRegistrations",
  "description": "List Inbox Registrations",
  "parameters": {
    "type": "object",
    "properties": {
      "cursor": {
        "type": "string",
        "description": "Used for pagination.  When a list request has more records available, cursors are provided in the response headers `X-Files-Cursor-Next` and `X-Files-Cursor-Prev`.  Send one of those cursor value here to resume an existing list from the next available record.  Note: many of our SDKs have iterator methods that will automatically handle cursor-based pagination."
      },
      "per_page": {
        "format": "int32",
        "type": "integer",
        "description": "Number of records to show per page.  (Max: 10,000, 1,000 or less is recommended)."
      },
      "folder_behavior_id": {
        "format": "int32",
        "type": "integer",
        "description": "ID of the associated Inbox."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//app.files.com/api/rest/v1/inbox_registrations{?cursor,per_page,folder_behavior_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "List Inbox Registrations",
        "**Key properties:**",
        {
          "$ul": [
            "**clickwrap_body**: Clickwrap text that was shown to the registrant",
            "**code**: Registration cookie code",
            "**company**: Registrant company name",
            "**created_at**: Registration creation date/time",
            "**email**: Registrant email address",
            "**form_field_data**: Data for form field set with form field ids as keys and user data as values",
            "**form_field_set_id**: Id of associated form field set",
            "**inbox_id**: Id of associated inbox",
            "**inbox_recipient_id**: Id of associated inbox recipient",
            "**inbox_title**: Title of associated inbox",
            "**name**: Registrant name"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
