{
  "name": "PatchPublicKeysId",
  "description": "Update Public Key",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int32",
        "type": "integer",
        "description": "Public Key ID."
      },
      "body": {
        "properties": {
          "title": {
            "description": "Internal reference for key.",
            "example": "My Main Key",
            "type": "string",
            "x-ms-summary": "Internal reference for key."
          }
        },
        "required": [
          "title"
        ],
        "type": "object"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "//app.files.com/api/rest/v1/public_keys/{id}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "List Public Keys",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**: Public key created at date/time",
            "**fingerprint**: Public key fingerprint",
            "**id**: Public key ID",
            "**title**: Public key title"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
