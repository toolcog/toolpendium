{
  "name": "apiStatus",
  "description": "Prints the current status of the classifiers. A classifier name in apiStatus corresponds to a service name in apiServices.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://v2.namsor.com/NamSorAPIv2/api2/json/apiStatus"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**classifiers**: The list of classifiers and versions.",
            [
              "**softwareVersion**: The software version.",
              {
                "$ul": [
                  "**softwareNameAndVersion**: The software version",
                  "**softwareVersion**: The software version major minor build"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
