{
  "name": "genderBatch",
  "description": "Infer the likely gender of up to 100 names, detecting automatically the cultural context.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/BatchFirstLastNameIn",
        "description": "A list of personal names"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://v2.namsor.com/NamSorAPIv2/api2/json/genderBatch"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents the output of inferring the LIKELY gender from a list of personal names.",
        "**Key properties:**",
        {
          "$ul": [
            "**personalNames**: Classified genderized names"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
