{
  "name": "chineseNameGenderCandidates",
  "description": "Identify Chinese name candidates, based on the romanized name ex. Wang Xiaoming - having a known gender ('male' or 'female')",
  "parameters": {
    "type": "object",
    "properties": {
      "chineseSurnameLatin": {
        "type": "string"
      },
      "chineseGivenNameLatin": {
        "type": "string"
      },
      "knownGender": {
        "type": "string"
      }
    },
    "required": [
      "chineseSurnameLatin",
      "chineseGivenNameLatin",
      "knownGender"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://v2.namsor.com/NamSorAPIv2/api2/json/chineseNameGenderCandidates/{chineseSurnameLatin}/{chineseGivenNameLatin}/{knownGender}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Classified matched names",
        "**Key properties:**",
        {
          "$ul": [
            "**firstName**: The first name (also known as given name)",
            "**id**",
            "**lastName**: The last name (also known as family name, or surname)",
            "**matchCandidates**: The ordered list of name matching candidates",
            "**orderOption**: The option for ordering",
            "**script**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
