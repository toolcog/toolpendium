{
  "name": "nameType",
  "description": "Infer the likely type of a proper noun (personal name, brand name, place name etc.)",
  "parameters": {
    "type": "object",
    "properties": {
      "properNoun": {
        "type": "string"
      }
    },
    "required": [
      "properNoun"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://v2.namsor.com/NamSorAPIv2/api2/json/nameType/{properNoun}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Classified typed proper names",
        "**Key properties:**",
        {
          "$ul": [
            "**commonType**: The most likely common name type",
            "**commonTypeAlt**: Best alternative for : The most likely common name type",
            "**id**",
            "**name**: The input name",
            "**score**: Higher score is better, but score is not normalized. Use calibratedProbability if available. ",
            "**script**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
