{
  "name": "japaneseNameKanjiCandidates",
  "description": "Identify japanese name candidates in KANJI, based on the romanized name ex. Yamamoto Sanae - and a known gender.",
  "parameters": {
    "type": "object",
    "properties": {
      "japaneseSurnameLatin": {
        "type": "string"
      },
      "japaneseGivenNameLatin": {
        "type": "string"
      },
      "knownGender": {
        "type": "string"
      }
    },
    "required": [
      "japaneseSurnameLatin",
      "japaneseGivenNameLatin",
      "knownGender"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://v2.namsor.com/NamSorAPIv2/api2/json/japaneseNameKanjiCandidates/{japaneseSurnameLatin}/{japaneseGivenNameLatin}/{knownGender}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Classified matched names",
        "**Key properties:**",
        {
          "$ul": [
            "**firstName**: The first name (also known as given name)",
            "**id**",
            "**lastName**: The last name (also known as family name, or surname)",
            "**matchCandidates**: The ordered list of name matching candidates",
            "**orderOption**: The option for ordering",
            "**script**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
