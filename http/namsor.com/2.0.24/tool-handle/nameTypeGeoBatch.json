{
  "name": "nameTypeGeoBatch",
  "description": "Infer the likely common type of up to 100 proper nouns (personal name, brand name, place name etc.)",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/BatchNameGeoIn",
        "description": "A list of proper names"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://v2.namsor.com/NamSorAPIv2/api2/json/nameTypeGeoBatch"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents the output of inferring the common type (anthroponym, toponym, brand, etc.) from a list of proper names.",
        "**Key properties:**",
        {
          "$ul": [
            "**properNouns**: Classified typed proper names"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
