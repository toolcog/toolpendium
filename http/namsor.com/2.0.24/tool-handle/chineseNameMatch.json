{
  "name": "chineseNameMatch",
  "description": "Return a score for matching Chinese name ex. 王晓明 with a romanized name ex. Wang Xiaoming",
  "parameters": {
    "type": "object",
    "properties": {
      "chineseSurnameLatin": {
        "type": "string"
      },
      "chineseGivenNameLatin": {
        "type": "string"
      },
      "chineseName": {
        "type": "string"
      }
    },
    "required": [
      "chineseSurnameLatin",
      "chineseGivenNameLatin",
      "chineseName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://v2.namsor.com/NamSorAPIv2/api2/json/chineseNameMatch/{chineseSurnameLatin}/{chineseGivenNameLatin}/{chineseName}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Classified matched names",
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**matchStatus**: The name matching status.",
            "**score**",
            "**script**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
