{
  "name": "get_search_domain_item",
  "description": "Domains Database Search",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "API key"
      },
      "date": {
        "type": "string",
        "description": "Request date"
      },
      "page": {
        "type": "string",
        "description": "Search page to request"
      },
      "limit": {
        "default": 50,
        "type": "integer",
        "description": "Results per page"
      },
      "domain": {
        "type": "string",
        "description": "Domain includes"
      },
      "zone": {
        "type": "string",
        "description": "In Zone"
      },
      "country": {
        "type": "string",
        "description": "Hosting Country"
      },
      "isDead": {
        "type": "boolean",
        "description": "Dead or Not, default not"
      },
      "A": {
        "type": "string",
        "description": "A record includes"
      },
      "NS": {
        "type": "string",
        "description": "NS record includes"
      },
      "CNAME": {
        "type": "string",
        "description": "CNAME record includes"
      },
      "MX": {
        "type": "string",
        "description": "MX record includes"
      },
      "TXT": {
        "type": "string",
        "description": "TXT record includes"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v1/domains/search{?api_key,date,page,limit,domain,zone,country,isDead,A,NS,CNAME,MX,TXT}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**next_page**: Next page id",
            "**time**: Time took",
            "**total**: Total days reported",
            "**domains**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
