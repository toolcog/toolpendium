{
  "name": "get_api_info_item",
  "parameters": {
    "type": "object",
    "properties": {
      "api_key": {
        "type": "string",
        "description": "API key"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/v1/info/api{?api_key}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**customer_email**: State",
            "**customer_name**: State",
            "**expires**: State",
            "**item_id**: State",
            "**item_name**: State",
            "**license**: State",
            "**license_limit**: State",
            "**price_id**: State",
            "**success**: State"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
