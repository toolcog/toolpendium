{
  "name": "getEndpointSummary",
  "description": "Returns endpoint summary",
  "parameters": {
    "type": "object",
    "properties": {
      "db": {
        "default": "nanoreg1",
        "enum": [
          "calibrate",
          "enanomapper",
          "enpra",
          "marina",
          "nanogenotox",
          "nanoinformatix",
          "nanoreg1",
          "nanoreg2",
          "nanotest"
        ],
        "type": "string",
        "description": "Database ID"
      },
      "top": {
        "enum": [
          "P-CHEM",
          "ECOTOX",
          "ENV FATE",
          "TOX",
          "EXPOSURE"
        ],
        "type": "string",
        "description": "Top endpoint category"
      },
      "category": {
        "type": "string",
        "description": "Endpoint category (The value in the protocol.category.code field)"
      }
    },
    "required": [
      "db"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ideaconsult.net/nanoreg1/enm/{db}/query/study{?top,category}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**facet**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
