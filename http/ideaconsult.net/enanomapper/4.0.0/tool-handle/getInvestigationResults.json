{
  "name": "getInvestigationResults",
  "description": "Multiple studies in tabular form",
  "parameters": {
    "type": "object",
    "properties": {
      "db": {
        "default": "nanoreg1",
        "enum": [
          "calibrate",
          "enanomapper",
          "enpra",
          "marina",
          "nanogenotox",
          "nanoinformatix",
          "nanoreg1",
          "nanoreg2",
          "nanotest"
        ],
        "type": "string",
        "description": "Database ID"
      },
      "type": {
        "enum": [
          "byinvestigation",
          "byassay",
          "bysubstance",
          "byprovider",
          "bycitation",
          "bystudytype",
          "bystructure_inchikey",
          "bystructure_smiles",
          "bystructure_name",
          "bysubstance_name",
          "bysubstance_type"
        ],
        "type": "string",
        "description": "query type"
      },
      "search": {
        "type": "string",
        "description": "Search parameter, UUID of the investigation or a substance"
      },
      "inchikey": {
        "type": "string",
        "description": "Search parameter, InChI key(s) of the substance component(s), comma delimited"
      },
      "id": {
        "type": "string",
        "description": "Search parameter, chemical structure or substance identifier(s), comma delimited"
      },
      "page": {
        "type": "integer",
        "description": "Starting page"
      },
      "pagesize": {
        "type": "integer",
        "description": "Page size"
      }
    },
    "required": [
      "db",
      "type",
      "search"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ideaconsult.net/enanomapper/enm/{db}/investigation{?type,search,inchikey,id,page,pagesize}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**_childDocuments_**",
            "**assay**",
            "**document_uuid**",
            "**effectendpoint**",
            "**endpoint**",
            "**endpointcategory**",
            "**err**",
            "**errQualifier**",
            "**guidance**",
            "**investigation**",
            "**loQualifier**",
            "**loValue**",
            "**name**",
            "**owner_name**",
            "**publicname**",
            "**reference**",
            "**reference_owner**",
            "**reference_year**",
            "**resulttype**",
            "**s_uuid**",
            "**studyResultType**",
            "**substanceType**",
            "**textValue**",
            "**topcategory**",
            "**type_s**",
            "**unit**",
            "**upQualifier**",
            "**upValue**",
            "**updated**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
