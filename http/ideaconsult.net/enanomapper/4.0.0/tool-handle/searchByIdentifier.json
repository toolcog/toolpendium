{
  "name": "searchByIdentifier",
  "description": "Returns compounds found",
  "parameters": {
    "type": "object",
    "properties": {
      "db": {
        "default": "nanoreg1",
        "enum": [
          "calibrate",
          "enanomapper",
          "enpra",
          "marina",
          "nanogenotox",
          "nanoinformatix",
          "nanoreg1",
          "nanoreg2",
          "nanotest"
        ],
        "type": "string",
        "description": "Database ID"
      },
      "term": {
        "enum": [
          "search",
          "url",
          "inchikey"
        ],
        "type": "string",
        "description": "search term type"
      },
      "representation": {
        "enum": [
          "all",
          "smiles",
          "reach",
          "stdinchi",
          "stdinchikey",
          "names",
          "iupac_name",
          "synonym",
          "cas",
          "einecs"
        ],
        "type": "string"
      },
      "search": {
        "type": "string",
        "description": "Compound identifier (SMILES, InChI, name, registry identifiers)"
      },
      "b64search": {
        "type": "string",
        "description": "Base64 encoded mol file; if included, will be used instead of the 'search' parameter"
      },
      "casesens": {
        "type": "boolean",
        "description": "Case sensitive search if yes"
      },
      "bundle_uri": {
        "type": "string",
        "description": "Bundle URI"
      },
      "sameas": {
        "type": "string",
        "description": "Ontology URI to define groups of columns"
      },
      "page": {
        "type": "integer",
        "description": "Starting page"
      },
      "pagesize": {
        "type": "integer",
        "description": "Page size"
      }
    },
    "required": [
      "db",
      "term",
      "representation"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ideaconsult.net/enanomapper/enm/{db}/query/compound/{term}/{representation}{?search,b64search,casesens,bundle_uri,sameas,page,pagesize}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**dataEntry**",
            "**feature**",
            "**model_uri**",
            "**query**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
