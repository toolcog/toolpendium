{
  "name": "getSubstanceComposition",
  "description": "Returns substance composition",
  "parameters": {
    "type": "object",
    "properties": {
      "db": {
        "default": "nanoreg1",
        "enum": [
          "calibrate",
          "enanomapper",
          "enpra",
          "marina",
          "nanogenotox",
          "nanoinformatix",
          "nanoreg1",
          "nanoreg2",
          "nanotest"
        ],
        "type": "string",
        "description": "Database ID"
      },
      "uuid": {
        "type": "string",
        "description": "Substance UUID"
      },
      "all": {
        "type": "boolean",
        "description": "true (Show all compositions) false (do not show hidden compositions)"
      },
      "page": {
        "type": "integer",
        "description": "Starting page"
      },
      "pagesize": {
        "type": "integer",
        "description": "Page size"
      }
    },
    "required": [
      "db",
      "uuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.ideaconsult.net/enanomapper/enm/{db}/substance/{uuid}/composition{?all,page,pagesize}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**composition**",
            "**feature**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
