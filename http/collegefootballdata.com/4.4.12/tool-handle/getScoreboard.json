{
  "name": "getScoreboard",
  "description": "Get live game results",
  "parameters": {
    "type": "object",
    "properties": {
      "classification": {
        "type": "string",
        "description": "Classification filter (fbs, fcs, ii, or iii). Defaults to fbs."
      },
      "conference": {
        "type": "string",
        "description": "Conference abbreviation filter."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/scoreboard{?classification,conference}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**awayTeam**",
              {
                "$ul": [
                  "**classification**",
                  "**conference**",
                  "**id**",
                  "**name**",
                  "**points**"
                ]
              }
            ],
            [
              "**betting**",
              {
                "$ul": [
                  "**awayMoneyline**",
                  "**homeMoneyline**",
                  "**overUnder**",
                  "**spread**"
                ]
              }
            ],
            "**clock**",
            "**conferenceGame**",
            [
              "**homeTeam**",
              {
                "$ul": [
                  "**classification**",
                  "**conference**",
                  "**id**",
                  "**name**",
                  "**points**"
                ]
              }
            ],
            "**id**",
            "**neutralSite**",
            "**period**",
            "**possession**",
            "**situation**",
            "**startDate**",
            "**startTimeTBD**",
            "**status**",
            "**tv**",
            [
              "**venue**",
              {
                "$ul": [
                  "**city**",
                  "**name**",
                  "**state**"
                ]
              }
            ],
            [
              "**weather**",
              {
                "$ul": [
                  "**description**",
                  "**temperature**",
                  "**windDirection**",
                  "**windSpeed**"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
