{
  "name": "getRecruitingGroups",
  "description": "Gets a list of aggregated statistics by team and position grouping",
  "parameters": {
    "type": "object",
    "properties": {
      "startYear": {
        "minimum": 2000,
        "type": "integer",
        "description": "Starting year"
      },
      "endYear": {
        "minimum": 2000,
        "type": "integer",
        "description": "Ending year"
      },
      "team": {
        "type": "string",
        "description": "Team filter"
      },
      "conference": {
        "type": "string",
        "description": "conference filter"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/recruiting/groups{?startYear,endYear,team,conference}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**averageRating**",
            "**averageStars**",
            "**commits**",
            "**conference**",
            "**positionGroup**",
            "**team**",
            "**totalRating**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
