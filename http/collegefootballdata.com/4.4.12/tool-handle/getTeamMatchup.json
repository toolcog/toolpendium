{
  "name": "getTeamMatchup",
  "description": "Matchup history",
  "parameters": {
    "type": "object",
    "properties": {
      "team1": {
        "type": "string",
        "description": "First team"
      },
      "team2": {
        "type": "string",
        "description": "Second team"
      },
      "minYear": {
        "minimum": 1869,
        "type": "integer",
        "description": "Minimum year"
      },
      "maxYear": {
        "minimum": 1869,
        "type": "integer",
        "description": "Maximum year"
      }
    },
    "required": [
      "team1",
      "team2"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/teams/matchup{?team1,team2,minYear,maxYear}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**endYear**",
            "**games**",
            "**startYear**",
            "**team1**",
            "**team1Wins**",
            "**team2**",
            "**team2Wins**",
            "**ties**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
