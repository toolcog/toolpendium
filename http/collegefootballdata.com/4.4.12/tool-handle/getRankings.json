{
  "name": "getRankings",
  "description": "Poll rankings",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "minimum": 1936,
        "type": "integer",
        "description": "Year/season filter for games"
      },
      "week": {
        "maximum": 16,
        "minimum": 1,
        "type": "integer",
        "description": "Week filter"
      },
      "seasonType": {
        "default": "regular",
        "type": "string",
        "description": "Season type filter (regular or postseason)"
      }
    },
    "required": [
      "year"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/rankings{?year,week,seasonType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**polls**",
            "**season**",
            "**seasonType**",
            "**week**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
