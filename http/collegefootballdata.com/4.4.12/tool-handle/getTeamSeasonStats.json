{
  "name": "getTeamSeasonStats",
  "description": "Team season stats",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "minimum": 2001,
        "type": "integer",
        "description": "Year filter (required if no team specified)"
      },
      "team": {
        "type": "string",
        "description": "Team filter (required if no year specified)"
      },
      "conference": {
        "type": "string",
        "description": "Conference abbreviation filter"
      },
      "startWeek": {
        "maximum": 16,
        "minimum": 1,
        "type": "integer",
        "description": "Starting week filter"
      },
      "endWeek": {
        "maximum": 16,
        "minimum": 1,
        "type": "integer",
        "description": "Starting week filter"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/stats/season{?year,team,conference,startWeek,endWeek}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**conference**",
            "**season**",
            "**statName**",
            "**statValue**",
            "**team**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
