{
  "name": "getPredictedPoints",
  "description": "Predicted Points",
  "parameters": {
    "type": "object",
    "properties": {
      "down": {
        "maximum": 4,
        "minimum": 1,
        "type": "integer",
        "description": "Down filter"
      },
      "distance": {
        "maximum": 99,
        "minimum": 1,
        "type": "integer",
        "description": "Distance filter"
      }
    },
    "required": [
      "down",
      "distance"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/ppa/predicted{?down,distance}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**predictedPoints**",
            "**yardLine**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
