{
  "name": "getAdvancedBoxScore",
  "description": "Get advanced box score data",
  "parameters": {
    "type": "object",
    "properties": {
      "gameId": {
        "type": "integer",
        "description": "Game id parameters"
      }
    },
    "required": [
      "gameId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/game/box/advanced{?gameId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**players**",
              {
                "$ul": [
                  "**ppa**",
                  "**usage**"
                ]
              }
            ],
            [
              "**teams**",
              {
                "$ul": [
                  "**cumulativePpa**",
                  "**explosiveness**",
                  "**fieldPosition**",
                  "**havoc**",
                  "**ppa**",
                  "**rushing**",
                  "**scoringOpportunities**",
                  "**successRates**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
