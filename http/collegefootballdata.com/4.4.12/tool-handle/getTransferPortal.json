{
  "name": "getTransferPortal",
  "description": "Transfer portal by season",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "type": "integer",
        "description": "Year filter"
      }
    },
    "required": [
      "year"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/player/portal{?year}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**destination**",
            "**eligibility**",
            "**firstName**",
            "**lastName**",
            "**origin**",
            "**position**",
            "**rating**",
            "**season**",
            "**stars**",
            "**transferDate**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
