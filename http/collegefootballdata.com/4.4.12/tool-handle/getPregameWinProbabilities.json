{
  "name": "getPregameWinProbabilities",
  "description": "Pregame win probabilities",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "minimum": 2001,
        "type": "integer",
        "description": "Year filter"
      },
      "week": {
        "maximum": 16,
        "minimum": 1,
        "type": "integer",
        "description": "Week filter"
      },
      "team": {
        "type": "string",
        "description": "Team filter"
      },
      "seasonType": {
        "type": "string",
        "description": "regular or postseason"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/metrics/wp/pregame{?year,week,team,seasonType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**awayTeam**",
            "**gameId**",
            "**homeTeam**",
            "**homeWinProb**",
            "**season**",
            "**seasonType**",
            "**spread**",
            "**week**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
