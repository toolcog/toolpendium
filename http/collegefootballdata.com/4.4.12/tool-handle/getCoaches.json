{
  "name": "getCoaches",
  "description": "Coaching history",
  "parameters": {
    "type": "object",
    "properties": {
      "firstName": {
        "type": "string",
        "description": "First name filter"
      },
      "lastName": {
        "type": "string",
        "description": "Last name filter"
      },
      "team": {
        "type": "string",
        "description": "Team name filter"
      },
      "year": {
        "minimum": 1869,
        "type": "integer",
        "description": "Year filter"
      },
      "minYear": {
        "minimum": 1869,
        "type": "integer",
        "description": "Minimum year filter (inclusive)"
      },
      "maxYear": {
        "minimum": 1869,
        "type": "integer",
        "description": "Maximum year filter (inclusive)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/coaches{?firstName,lastName,team,year,minYear,maxYear}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**first_name**",
            "**hire_date**",
            "**last_name**",
            "**seasons**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
