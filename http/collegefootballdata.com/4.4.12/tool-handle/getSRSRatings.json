{
  "name": "getSRSRatings",
  "description": "SRS rating data (requires either a year or team specified)",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "minimum": 1897,
        "type": "integer",
        "description": "Season filter (required if team not specified)"
      },
      "team": {
        "type": "string",
        "description": "Team filter (required if year not specified)"
      },
      "conference": {
        "type": "string",
        "description": "Conference filter"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/ratings/srs{?year,team,conference}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**conference**",
            "**division**",
            "**ranking**",
            "**rating**",
            "**team**",
            "**year**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
