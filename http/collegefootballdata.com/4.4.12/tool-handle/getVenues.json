{
  "name": "getVenues",
  "description": "Venues",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/venues"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**capacity**",
            "**city**",
            "**country_code**",
            "**dome**",
            "**elevation**",
            "**grass**",
            "**id**",
            [
              "**location**",
              {
                "$ul": [
                  "**x**",
                  "**y**"
                ]
              }
            ],
            "**name**",
            "**state**",
            "**timezone**",
            "**year_constructed**",
            "**zip**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
