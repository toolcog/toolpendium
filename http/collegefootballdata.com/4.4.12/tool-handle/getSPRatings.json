{
  "name": "getSPRatings",
  "description": "SP+ rating data",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "minimum": 1970,
        "type": "integer",
        "description": "Season filter (required if team not specified)"
      },
      "team": {
        "type": "string",
        "description": "Team filter (required if year not specified)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/ratings/sp{?year,team}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**conference**",
            [
              "**defense**",
              {
                "$ul": [
                  "**explosiveness**",
                  [
                    "**havoc**",
                    {
                      "$ul": [
                        "**db**",
                        "**frontSeven**",
                        "**total**"
                      ]
                    }
                  ],
                  "**pasing**",
                  "**passingDowns**",
                  "**ranking**",
                  "**rating**",
                  "**rushing**",
                  "**standardDowns**",
                  "**success**"
                ]
              }
            ],
            [
              "**offense**",
              {
                "$ul": [
                  "**explosiveness**",
                  "**pace**",
                  "**passing**",
                  "**passingDowns**",
                  "**ranking**",
                  "**rating**",
                  "**runRate**",
                  "**rushing**",
                  "**standardDowns**",
                  "**success**"
                ]
              }
            ],
            "**ranking**",
            "**rating**",
            "**secondOrderWins**",
            "**sos**",
            [
              "**specialTeams**",
              {
                "$ul": [
                  "**rating**"
                ]
              }
            ],
            "**team**",
            "**year**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
