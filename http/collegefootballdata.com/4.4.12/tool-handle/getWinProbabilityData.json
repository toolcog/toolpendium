{
  "name": "getWinProbabilityData",
  "description": "Win probability data",
  "parameters": {
    "type": "object",
    "properties": {
      "gameId": {
        "type": "integer",
        "description": "Game id filter"
      }
    },
    "required": [
      "gameId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/metrics/wp{?gameId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**away**",
            "**awayId**",
            "**awayScore**",
            "**distance**",
            "**down**",
            "**gamesId**",
            "**home**",
            "**homeBall**",
            "**homeId**",
            "**homeScore**",
            "**homeWinProb**",
            "**playId**",
            "**playNumber**",
            "**playText**",
            "**spread**",
            "**timeRemaining**",
            "**yardLine**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
