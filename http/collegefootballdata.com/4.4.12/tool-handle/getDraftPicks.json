{
  "name": "getDraftPicks",
  "description": "List of NFL Draft picks",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "type": "integer",
        "description": "Year filter"
      },
      "nflTeam": {
        "type": "string",
        "description": "NFL team filter"
      },
      "college": {
        "type": "string",
        "description": "Player college filter"
      },
      "conference": {
        "type": "string",
        "description": "College confrence abbreviation filter"
      },
      "position": {
        "type": "string",
        "description": "NFL position filter"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/draft/picks{?year,nflTeam,college,conference,position}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**collegeAthleteId**",
            "**collegeConference**",
            "**collegeId**",
            "**collegeTeam**",
            "**height**",
            [
              "**hometownInfo**",
              {
                "$ul": [
                  "**city**",
                  "**country**",
                  "**countryFips**",
                  "**latitude**",
                  "**longitude**",
                  "**state**"
                ]
              }
            ],
            "**name**",
            "**nflAthleteId**",
            "**nflTeam**",
            "**overall**",
            "**pick**",
            "**position**",
            "**preDraftGrade**",
            "**preDraftPositionRanking**",
            "**preDraftRanking**",
            "**round**",
            "**weight**",
            "**year**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
