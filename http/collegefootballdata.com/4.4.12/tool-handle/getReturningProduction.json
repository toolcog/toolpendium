{
  "name": "getReturningProduction",
  "description": "Returning production metrics",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "minimum": 2014,
        "type": "integer",
        "description": "Year filter"
      },
      "team": {
        "type": "string",
        "description": "Team filter"
      },
      "conference": {
        "type": "string",
        "description": "Conference abbreviation filter"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/player/returning{?year,team,conference}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**conference**",
            "**passingUsage**",
            "**percentPPA**",
            "**percentPassingPPA**",
            "**percentReceivingPPA**",
            "**percentRushingPPA**",
            "**receivingUsage**",
            "**rushingUsage**",
            "**season**",
            "**team**",
            "**totalPPA**",
            "**totalPassingPPA**",
            "**totalReceivingPPA**",
            "**totalRushingPPA**",
            "**usage**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
