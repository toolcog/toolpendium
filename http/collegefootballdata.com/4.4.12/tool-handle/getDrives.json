{
  "name": "getDrives",
  "description": "Get game drives",
  "parameters": {
    "type": "object",
    "properties": {
      "seasonType": {
        "default": "regular",
        "type": "string",
        "description": "Season type filter"
      },
      "year": {
        "minimum": 2001,
        "type": "integer",
        "description": "Year filter"
      },
      "week": {
        "maximum": 16,
        "minimum": 1,
        "type": "integer",
        "description": "Week filter"
      },
      "team": {
        "type": "string",
        "description": "Team filter"
      },
      "offense": {
        "type": "string",
        "description": "Offensive team filter"
      },
      "defense": {
        "type": "string",
        "description": "Defensive team filter"
      },
      "conference": {
        "type": "string",
        "description": "Conference filter"
      },
      "offenseConference": {
        "type": "string",
        "description": "Offensive conference filter"
      },
      "defenseConference": {
        "type": "string",
        "description": "Defensive conference filter"
      },
      "classification": {
        "type": "string",
        "description": "Division classification filter (fbs/fcs/ii/iii)"
      }
    },
    "required": [
      "year"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/drives{?seasonType,year,week,team,offense,defense,conference,offenseConference,defenseConference,classification}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**defense**",
            "**defense_conference**",
            "**drive_number**",
            "**drive_result**",
            "**end_defense_score**",
            "**end_offense_score**",
            "**end_period**",
            [
              "**end_time**",
              {
                "$ul": [
                  "**minutes**",
                  "**seconds**"
                ]
              }
            ],
            "**end_yardline**",
            "**end_yards_to_goal**",
            "**game_id**",
            "**id**",
            "**is_home_offense**",
            "**offense**",
            "**offense_conference**",
            "**plays**",
            "**scoring**",
            "**start_defense_score**",
            "**start_offense_score**",
            "**start_period**",
            [
              "**start_time**",
              {
                "$ul": [
                  "**minutes**",
                  "**seconds**"
                ]
              }
            ],
            "**start_yardline**",
            "**start_yards_to_goal**",
            "**yards**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
