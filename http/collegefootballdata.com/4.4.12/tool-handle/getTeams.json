{
  "name": "getTeams",
  "description": "Get team information",
  "parameters": {
    "type": "object",
    "properties": {
      "conference": {
        "type": "string",
        "description": "Conference abbreviation filter"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/teams{?conference}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**abbreviation**",
            "**alt_color**",
            "**alt_name_1**",
            "**alt_name_2**",
            "**alt_name_3**",
            "**classification**",
            "**color**",
            "**conference**",
            "**division**",
            "**id**",
            [
              "**location**",
              {
                "$ul": [
                  "**capacity**",
                  "**city**",
                  "**country_code**",
                  "**dome**",
                  "**elevation**",
                  "**grass**",
                  "**latitude**",
                  "**longitude**",
                  "**name**",
                  "**state**",
                  "**timezone**",
                  "**venue_id**",
                  "**year_constructed**",
                  "**zip**"
                ]
              }
            ],
            "**logos**",
            "**mascot**",
            "**school**",
            "**twitter**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
