{
  "name": "getLines",
  "description": "Closing betting lines",
  "parameters": {
    "type": "object",
    "properties": {
      "gameId": {
        "type": "integer",
        "description": "Game id filter"
      },
      "year": {
        "minimum": 2013,
        "type": "integer",
        "description": "Year/season filter for games"
      },
      "week": {
        "maximum": 16,
        "minimum": 1,
        "type": "integer",
        "description": "Week filter"
      },
      "seasonType": {
        "default": "regular",
        "type": "string",
        "description": "Season type filter (regular or postseason)"
      },
      "team": {
        "type": "string",
        "description": "Team"
      },
      "home": {
        "type": "string",
        "description": "Home team filter"
      },
      "away": {
        "type": "string",
        "description": "Away team filter"
      },
      "conference": {
        "type": "string",
        "description": "Conference abbreviation filter"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.collegefootballdata.com/lines{?gameId,year,week,seasonType,team,home,away,conference}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**awayConference**",
            "**awayScore**",
            "**awayTeam**",
            "**homeConference**",
            "**homeScore**",
            "**homeTeam**",
            "**id**",
            "**lines**",
            "**season**",
            "**seasonType**",
            "**startDate**",
            "**week**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
