{
  "name": "getBasicMoonPhaseData",
  "description": "Get Basic Moon Phase Data",
  "parameters": {
    "type": "object",
    "properties": {
      "X-Rapidapi-Key": {
        "example": "{{Rapidapi-Key}}",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://moon-phase.p.rapidapi.com/basic"
    },
    "headers": {
      "X-Rapidapi-Key": {
        "$": "X-Rapidapi-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**days_until_next_full_moon**",
            "**days_until_next_new_moon**",
            "**phase_name**",
            "**stage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
