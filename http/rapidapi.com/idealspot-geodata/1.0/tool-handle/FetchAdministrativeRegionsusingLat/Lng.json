{
  "name": "FetchAdministrativeRegionsusingLat_Lng",
  "description": "For given latitude and longitude, find intersecting administrative regions. Region polygons are simplified for web mapping.",
  "parameters": {
    "type": "object",
    "properties": {
      "latitude": {
        "format": "double",
        "type": "number",
        "description": "(Required) Search coordinate latitude"
      },
      "longitude": {
        "format": "double",
        "type": "number",
        "description": "(Required) Search coordinate longitude"
      },
      "X-RapidAPI-Key": {
        "type": "string",
        "description": "(Required) Rapid API Key. See https://rapidapi.com/idealspot-inc-idealspot-inc-default/api/idealspot-geodata"
      },
      "X-RapidAPI-Host": {
        "type": "string",
        "description": ""
      }
    },
    "required": [
      "latitude",
      "longitude",
      "X-RapidAPI-Key",
      "X-RapidAPI-Host"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://idealspot-geodata.p.rapidapi.com/api/v1/geometries/regions/intersecting/{latitude}/{longitude}"
    },
    "headers": {
      "X-RapidAPI-Key": {
        "$": "X-RapidAPI-Key"
      },
      "X-RapidAPI-Host": {
        "$": "X-RapidAPI-Host"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "FetchAdministrativeRegionsusingLat/Lng"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
