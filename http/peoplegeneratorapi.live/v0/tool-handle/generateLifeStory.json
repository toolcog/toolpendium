{
  "name": "generateLifeStory",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://peoplegeneratorapi.live/api/lifestory/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**lifestory**",
            [
              "**person**",
              {
                "$ul": [
                  [
                    "**address**",
                    {
                      "$ul": [
                        "**city**",
                        "**country**",
                        "**countryCode**",
                        "**geonameId**",
                        "**ipAddress**",
                        "**phoneNumber**",
                        "**state**",
                        "**streetAddress**",
                        "**zip**"
                      ]
                    }
                  ],
                  "**age**",
                  "**bloodType**",
                  "**ccNumber**",
                  "**creditScore**",
                  "**doB**",
                  "**email**",
                  "**eyeColor**",
                  "**gender**",
                  "**gpa**",
                  "**hasChildren**",
                  "**hasDegree**",
                  "**height**",
                  "**incomeUSD**",
                  "**job**",
                  "**married**",
                  "**name**",
                  "**politicalLeaning**",
                  "**religion**",
                  "**username**",
                  "**weight**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
