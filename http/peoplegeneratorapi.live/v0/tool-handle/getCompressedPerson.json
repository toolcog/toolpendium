{
  "name": "getCompressedPerson",
  "parameters": {
    "type": "object",
    "properties": {
      "number": {
        "format": "int32",
        "type": "integer"
      }
    },
    "required": [
      "number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://peoplegeneratorapi.live/api/person/{number}/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
