{
  "name": "quittungComit",
  "description": "Uses collected fields or provided fields to create a final receipt (Strom-Quittung).\n",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "account": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.corrently.io/v2.0/quittung/commit"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
