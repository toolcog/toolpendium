{
  "name": "stromkontoChoices",
  "description": "Signable choices (contract changes) for customer.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "account": {
        "type": "string",
        "description": "Ethereum style address referencing a valid account alias (never use Stromkonto directly!)."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.corrently.io/v2.0/stromkonto/choices{?account}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**balance**: Sum owned - sum due by account",
            "**haben**: Sum owned by account",
            "**soll**: Sum due by account",
            "**txs**",
            "**variation**: Type of subbalance (sub account)"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
