{
  "name": "wimstatus",
  "description": "Access to status information of an existing metering change and allocation process.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "vid": {
        "type": "string",
        "description": "VID key of the process."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.corrently.io/v2.0/wim/status{?vid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**wim_started**: Starting time of process",
            "**wim_status**: Latest Status"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
