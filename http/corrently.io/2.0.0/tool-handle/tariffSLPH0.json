{
  "name": "tariffSLPH0",
  "description": "Provides pricing data for private households with standard load profiles (Standardlastprofil H0).\n",
  "parameters": {
    "type": "object",
    "properties": {
      "zipcode": {
        "type": "string",
        "description": "Zipcode (Postzleitzahl) of a city in Germany."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.corrently.io/v2.0/tariff/slph0{?zipcode}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**ap**: Energy price in cent per kwh. (Arbeitspreis) including all taxes and fees.",
            "**gp**: Base price in euro per montah (Grundpreis) including all taxes and fees."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
