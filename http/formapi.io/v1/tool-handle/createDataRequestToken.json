{
  "name": "createDataRequestToken",
  "description": "Creates a new data request token for form authentication",
  "parameters": {
    "type": "object",
    "properties": {
      "data_request_id": {
        "type": "string"
      }
    },
    "required": [
      "data_request_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/data_requests/{data_request_id}/tokens"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "create_submission_data_request_token_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**",
            "**status**",
            [
              "**token**",
              {
                "$ul": [
                  "**data_request_url**",
                  "**expires_at**",
                  "**id**",
                  "**secret**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
