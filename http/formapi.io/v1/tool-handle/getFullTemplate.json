{
  "name": "getFullTemplate",
  "description": "Fetch the full template attributes",
  "parameters": {
    "type": "object",
    "properties": {
      "template_id": {
        "type": "string"
      }
    },
    "required": [
      "template_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/templates/{template_id}?full=true"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "full_template"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**allow_additional_properties**",
            [
              "**defaults**",
              {
                "$ul": [
                  "**color**",
                  "**fontSize**",
                  "**typeface**"
                ]
              }
            ],
            "**demo**",
            "**description**",
            "**document_filename**",
            "**document_md5**",
            "**document_parse_error**",
            "**document_processed**",
            "**document_state**",
            "**document_url**",
            "**editable_submissions**",
            "**embed_domains**",
            "**encrypt_pdfs**",
            "**encrypt_pdfs_password**",
            "**expiration_interval**",
            "**expire_after**",
            "**expire_submissions**",
            "**field_order**",
            "**fields**",
            "**first_template**",
            "**footer_html**",
            "**header_html**",
            "**html**",
            "**id**",
            "**locked**",
            "**name**",
            "**page_count**",
            "**page_dimensions**",
            "**parent_folder_id**",
            "**path**",
            "**permanent_document_url**",
            "**public_submissions**",
            "**public_web_form**",
            "**redirect_url**",
            "**scss**",
            "**shared_field_data**",
            "**slack_webhook_url**",
            "**template_type**",
            "**webhook_url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
