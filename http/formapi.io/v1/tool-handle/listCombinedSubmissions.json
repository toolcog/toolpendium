{
  "name": "listCombinedSubmissions",
  "description": "Get a list of all combined submissions",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "minimum": 1,
        "type": "integer",
        "description": "Default: 1"
      },
      "per_page": {
        "maximum": 50,
        "minimum": 1,
        "type": "integer",
        "description": "Default: 50"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/combined_submissions{?page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "combined_submissions"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**actions**",
            "**download_url**",
            "**error_message**",
            "**expired**",
            "**expires_at**",
            "**expires_in**",
            "**id**",
            "**metadata**",
            "**password**",
            "**pdf_hash**",
            "**source_pdfs**",
            "**state**",
            "**submission_ids**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
