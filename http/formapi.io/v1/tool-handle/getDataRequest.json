{
  "name": "getDataRequest",
  "description": "Look up a submission data request",
  "parameters": {
    "type": "object",
    "properties": {
      "data_request_id": {
        "type": "string"
      }
    },
    "required": [
      "data_request_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/data_requests/{data_request_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "submission_data_request"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**auth_phone_number_hash**",
            "**auth_provider**",
            "**auth_second_factor_type**",
            "**auth_session_id_hash**",
            "**auth_session_started_at**",
            "**auth_type**",
            "**auth_user_id_hash**",
            "**auth_username_hash**",
            "**completed_at**",
            "**email**",
            "**fields**",
            "**id**",
            "**ip_address**",
            "**metadata**",
            "**name**",
            "**order**",
            "**sort_order**",
            "**state**",
            "**submission_id**",
            "**user_agent**",
            "**viewed_at**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
