{
  "name": "createFolder",
  "description": "Create a folder",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "example": {
          "folder": {
            "name": "Folder 3"
          }
        },
        "properties": {
          "folder": {
            "properties": {
              "name": {
                "type": "string"
              },
              "parent_folder_id": {
                "type": "string"
              }
            },
            "required": [
              "name"
            ],
            "type": "object"
          }
        },
        "required": [
          "folder"
        ],
        "title": "create_folder_data",
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/folders/"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "folder"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**name**",
            "**parent_folder_id**",
            "**path**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
