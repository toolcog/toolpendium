{
  "name": "renameFolder",
  "description": "Rename a folder",
  "parameters": {
    "type": "object",
    "properties": {
      "folder_id": {
        "type": "string"
      },
      "body": {
        "example": {
          "name": "Folder 3"
        },
        "properties": {
          "name": {
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "title": "rename_folder_data",
        "type": "object"
      }
    },
    "required": [
      "folder_id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/folders/{folder_id}/rename"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
