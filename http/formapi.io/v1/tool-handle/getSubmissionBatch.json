{
  "name": "getSubmissionBatch",
  "description": "Check the status of a submission batch job",
  "parameters": {
    "type": "object",
    "properties": {
      "submission_batch_id": {
        "type": "string"
      },
      "include_submissions": {
        "type": "boolean"
      }
    },
    "required": [
      "submission_batch_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/submissions/batches/{submission_batch_id}{?include_submissions}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "submission_batch"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**completion_percentage**",
            "**error_count**",
            "**id**",
            "**metadata**",
            "**pending_count**",
            "**processed_at**",
            "**state**",
            "**submissions**",
            "**total_count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
