{
  "name": "getPresignUrl",
  "description": "Get a presigned URL so that you can upload a file to our AWS S3 bucket",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/uploads/presign"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "upload_presign"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**fields**",
              {
                "$ul": [
                  "**key**",
                  "**policy**",
                  "**x-amz-algorithm**",
                  "**x-amz-credential**",
                  "**x-amz-date**",
                  "**x-amz-signature**"
                ]
              }
            ],
            "**headers**",
            "**method**",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
