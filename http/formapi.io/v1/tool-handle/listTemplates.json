{
  "name": "listTemplates",
  "description": "Get a list of all templates",
  "parameters": {
    "type": "object",
    "properties": {
      "query": {
        "type": "string",
        "description": "Search By Name"
      },
      "parent_folder_id": {
        "type": "string",
        "description": "Filter By Folder Id"
      },
      "page": {
        "minimum": 1,
        "type": "integer",
        "description": "Default: 1"
      },
      "per_page": {
        "maximum": 50,
        "minimum": 1,
        "type": "integer",
        "description": "Default: 50"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/templates{?query,parent_folder_id,page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "templates"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**allow_additional_properties**",
            "**description**",
            "**document_url**",
            "**editable_submissions**",
            "**expiration_interval**",
            "**expire_after**",
            "**expire_submissions**",
            "**id**",
            "**locked**",
            "**name**",
            "**page_dimensions**",
            "**parent_folder_id**",
            "**path**",
            "**permanent_document_url**",
            "**public_submissions**",
            "**public_web_form**",
            "**redirect_url**",
            "**slack_webhook_url**",
            "**template_type**",
            "**webhook_url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
