{
  "name": "listFolders",
  "description": "Get a list of all folders",
  "parameters": {
    "type": "object",
    "properties": {
      "parent_folder_id": {
        "type": "string",
        "description": "Filter By Folder Id"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.docspring.com/api/v1/folders/{?parent_folder_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "folders"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**name**",
            "**parent_folder_id**",
            "**path**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "authentication_error_response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**",
            "**status**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
