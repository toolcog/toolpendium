{
  "name": "getPermissions",
  "description": "View a list of permissions that your user account is authorized for, configured either by default, or by your account administator.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.motaword.com/permissions"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  [
                    "**paging**",
                    {
                      "$ul": [
                        "**count**",
                        [
                          "**links**",
                          {
                            "$ul": [
                              [
                                "**self**",
                                {
                                  "$ul": [
                                    "**href**"
                                  ]
                                }
                              ],
                              "**next**",
                              "**previous**"
                            ]
                          }
                        ],
                        "**page** (default: 1)",
                        "**per_page** (default: 10)",
                        "**total_count**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**permissions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
