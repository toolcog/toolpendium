                  "**pam_tqs**",
                  "**paypal_email**",
                  [
                    "**profile_survey**",
                    {
                      "$ul": [
                        "**current_services**",
                        "**daily_proofreading_capacity**",
                        "**daily_translation_capacity**",
                        "**dtp_software**",
                        "**experience**",
                        "**is_certified_translator**",
                        "**is_sworn_translator**",
                        "**memoq**",
