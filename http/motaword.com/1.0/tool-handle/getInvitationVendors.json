{
  "name": "getInvitationVendors",
  "description": "Get vendor list for compiled invitation needs",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "items": {
          "$ref": "#/$defs/FileNeedsVendor"
        },
        "type": "array"
      }
    },
    "$defs": {
      "FileNeedsVendor": {
        "properties": {
          "fileId": {
            "format": "int64",
            "type": "integer"
          },
          "guid": {
            "type": "string"
          },
          "projectId": {
            "format": "int64",
            "type": "integer"
          },
          "reason": {
            "type": "string"
          },
          "targetLanguage": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "taskType": {
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.motaword.com/invitation/vendors"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**vendors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
