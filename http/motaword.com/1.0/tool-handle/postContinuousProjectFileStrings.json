{
  "name": "postContinuousProjectFileStrings",
  "description": "Get a list of strings and its translations in the project.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "int64",
        "type": "integer",
        "description": "Project ID"
      },
      "body": {
        "$ref": "#/$defs/ContinuousProjectDocumentStringsBody"
      }
    },
    "required": [
      "projectId"
    ],
    "$defs": {
      "ContinuousProjectDocumentStringsBody": {
        "properties": {
          "documentName": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.motaword.com/continuous_projects/{projectId}/documents/strings"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  [
                    "**paging**",
                    {
                      "$ul": [
                        "**count**",
                        [
                          "**links**",
                          {
                            "$ul": [
                              [
                                "**self**",
                                {
                                  "$ul": [
                                    "**href**"
                                  ]
                                }
                              ],
                              "**next**",
                              "**previous**"
                            ]
                          }
                        ],
                        "**page** (default: 1)",
                        "**per_page** (default: 10)",
                        "**total_count**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**translations**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
