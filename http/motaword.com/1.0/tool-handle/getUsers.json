{
  "name": "getUsers",
  "description": "Get a list of platform users",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": 1,
        "format": "int64",
        "type": "integer"
      },
      "per_page": {
        "default": 10,
        "format": "int64",
        "type": "integer"
      },
      "user_type": {
        "default": "all",
        "enum": [
          "vendor",
          "all"
        ],
        "type": "string"
      },
      "search": {
        "type": "string"
      },
      "email": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.motaword.com/users{?page,per_page,user_type,search,email}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  [
                    "**paging**",
                    {
                      "$ul": [
                        "**count**",
                        [
                          "**links**",
                          {
                            "$ul": [
                              [
                                "**self**",
                                {
                                  "$ul": [
                                    "**href**"
                                  ]
                                }
                              ],
                              "**next**",
                              "**previous**"
                            ]
                          }
                        ],
                        "**page** (default: 1)",
                        "**per_page** (default: 10)",
                        "**total_count**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**users**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
