{
  "name": "useAsDraft",
  "description": "Use the translation of given source manual document as manual draft source for the given target document.",
  "parameters": {
    "type": "object",
    "properties": {
      "documentId": {
        "format": "int64",
        "type": "integer",
        "description": "Document ID"
      },
      "body": {
        "$ref": "#/$defs/UseAsDraftPayload"
      }
    },
    "required": [
      "documentId"
    ],
    "$defs": {
      "UseAsDraftPayload": {
        "properties": {
          "fromFileId": {
            "type": "number"
          },
          "fromManualTranslationFileId": {
            "type": "number"
          },
          "toManualTranslationFileId": {
            "type": "number"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.motaword.com/documents/{documentId}/use_as_draft"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status** (default: success)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
