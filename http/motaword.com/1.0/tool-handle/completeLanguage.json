{
  "name": "completeLanguage",
  "description": "Complete continuous project language",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "Continuous project ID"
      },
      "targetLanguage": {
        "type": "string",
        "description": "Target language that you want to complete"
      }
    },
    "required": [
      "id",
      "targetLanguage"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.motaword.com/continuous_projects/{id}/languages/{targetLanguage}/complete"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status** (default: success)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
