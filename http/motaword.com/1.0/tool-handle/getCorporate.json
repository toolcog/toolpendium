{
  "name": "getCorporate",
  "description": "View the details of the corporate account that your user account belongs to.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.motaword.com/corporate"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**billing**",
              {
                "$ul": [
                  "**city**",
                  "**country**",
                  "**phone**",
                  "**state**",
                  "**street**",
                  "**zip**",
                  "**name**: addressing name, such as company name. used in billing address. (default: )"
                ]
              }
            ],
            "**domain**",
            "**email**",
            "**id**",
            "**logo**",
            "**name**",
            "**web_site**: Deprecated, use `domain`."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
