{
  "name": "getCorporatesList",
  "description": "Get a list of corporate accounts",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.motaword.com/corporates/all"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**billing**",
              {
                "$ul": [
                  "**city**",
                  "**country**",
                  "**phone**",
                  "**state**",
                  "**street**",
                  "**zip**",
                  "**name**: addressing name, such as company name. used in billing address. (default: )"
                ]
              }
            ],
            "**domain**",
            "**email**",
            "**id**",
            "**logo**",
            "**name**",
            "**web_site**: Deprecated, use `domain`."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
