{
  "name": "getCommissionStats",
  "description": "Returns the total commissions stats.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.motaword.com/stats/commissions"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**balance**",
              {
                "$ul": [
                  "**amount**",
                  "**currency** (default: usd)"
                ]
              }
            ],
            [
              "**paid**",
              {
                "$ul": [
                  "**amount**",
                  "**currency** (default: usd)"
                ]
              }
            ],
            [
              "**quote_total**",
              {
                "$ul": [
                  "**amount**",
                  "**currency** (default: usd)"
                ]
              }
            ],
            [
              "**total**",
              {
                "$ul": [
                  "**amount**",
                  "**currency** (default: usd)"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**help**",
            "**http_code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
