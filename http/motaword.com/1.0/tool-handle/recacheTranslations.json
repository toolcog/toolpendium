{
  "name": "recacheTranslations",
  "description": "Recache translations for the continuous project.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "int64",
        "type": "integer",
        "description": "Project ID"
      },
      "locale": {
        "type": "string",
        "description": "Locale"
      },
      "file_id": {
        "format": "int64",
        "type": "integer",
        "description": "Continuous Project File ID"
      }
    },
    "required": [
      "projectId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.motaword.com/continuous_projects/{projectId}/strings/recache-tms{?locale,file_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status** (default: success)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
