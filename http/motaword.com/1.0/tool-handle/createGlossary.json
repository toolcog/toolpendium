{
  "name": "createGlossary",
  "description": "Upload a new glossary file to your project to be used during translation. Glossaries can be CSV or TBX files.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "int64",
        "type": "integer",
        "description": "Project ID"
      },
      "body": {
        "$ref": "#/$defs/GlossaryUploadRequest"
      }
    },
    "required": [
      "projectId"
    ],
    "$defs": {
      "GlossaryUploadRequest": {
        "properties": {
          "glossaries": {
            "description": "You can only add one glossary, even though the name suggests multiple glossaries. This may be updated in the future to support multiple glossaries.",
            "format": "binary",
            "type": "string"
          }
        },
        "required": [
          "glossaries"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.motaword.com/projects/{projectId}/glossaries"
    },
    "body": {
