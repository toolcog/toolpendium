{
  "name": "getProjectStrings",
  "description": "View the strings and their translations in your translation project, for all target languages. The list of translations is live if your project is not completed yet. If you need the translated version of your source document/file, then you need to use package and download endpoints.",
  "parameters": {
    "type": "object",
    "properties": {
      "projectId": {
        "format": "int64",
        "type": "integer",
        "description": "Project ID"
      }
    },
    "required": [
      "projectId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.motaword.com/projects/{projectId}/strings"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  [
                    "**paging**",
                    {
                      "$ul": [
                        "**count**",
                        [
                          "**links**",
                          {
                            "$ul": [
                              [
                                "**self**",
                                {
                                  "$ul": [
                                    "**href**"
                                  ]
                                }
                              ],
                              "**next**",
                              "**previous**"
                            ]
                          }
                        ],
                        "**page** (default: 1)",
                        "**per_page** (default: 10)",
                        "**total_count**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**translations**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
