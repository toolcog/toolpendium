{
  "name": "regeneratePreview",
  "description": "Regenerate preview and return preview URL for given file",
  "parameters": {
    "type": "object",
    "properties": {
      "documentId": {
        "format": "int64",
        "type": "integer",
        "description": "Document ID"
      }
    },
    "required": [
      "documentId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.motaword.com/documents/{documentId}/regenerate_preview"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**link**",
              {
                "$ul": [
                  "**href**"
                ]
              }
            ],
            "**source**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
