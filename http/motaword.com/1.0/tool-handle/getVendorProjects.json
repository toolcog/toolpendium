{
  "name": "getVendorProjects",
  "description": "Get a list of projects that are available to you to work on as a vendor. This is not a list of projects that you ordered as a customer.",
  "parameters": {
    "type": "object",
    "properties": {
      "joined": {
        "type": "boolean",
        "description": "Return only projects that this user has already joined"
      },
      "completed": {
        "type": "boolean",
        "description": "Return only projects that have been completed. When `true`, this makes `joined` true as well."
      },
      "page": {
        "default": 1,
        "format": "int64",
        "type": "integer"
      },
      "per_page": {
        "default": 10,
        "format": "int64",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.motaword.com/projects/vendor{?joined,completed,page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**meta**",
              {
                "$ul": [
                  [
                    "**paging**",
                    {
                      "$ul": [
                        "**count**",
                        [
                          "**links**",
                          {
                            "$ul": [
                              [
                                "**self**",
                                {
                                  "$ul": [
                                    "**href**"
                                  ]
                                }
                              ],
                              "**next**",
                              "**previous**"
                            ]
                          }
                        ],
                        "**page** (default: 1)",
                        "**per_page** (default: 10)",
                        "**total_count**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**projects**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**help**",
            "**http_code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
