{
  "name": "createWdrReport",
  "description": "Asynchronously create a report containing Wireless Detail Records (WDRs) for the SIM cards that consumed wireless data in the given time period.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/WdrReportRequest"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "WdrReportRequest": {
        "example": {
          "end_time": "2018-02-02T22:25:27.521Z",
          "start_time": "2018-02-02T22:25:27.521Z"
        },
        "properties": {
          "end_time": {
            "description": "ISO 8601 formatted date-time indicating the end time.",
            "example": "2018-02-02T22:25:27.521Z",
            "type": "string"
          },
          "start_time": {
            "description": "ISO 8601 formatted date-time indicating the start time.",
            "example": "2018-02-02T22:25:27.521Z",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.telnyx.com/v2/wireless/detail_records_reports"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**created_at**: ISO 8601 formatted date-time indicating when the resource was created.",
                  "**end_time**: ISO 8601 formatted date-time indicating the end time.",
                  "**id**: Identifies the resource.",
                  "**record_type**",
                  "**report_url**: The URL where the report content, when generated, will be published to.",
                  "**start_time**: ISO 8601 formatted date-time indicating the start time.",
                  "**status**: Indicates the status of the report, which is updated asynchronously.",
                  "**updated_at**: ISO 8601 formatted date-time indicating when the resource was updated."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
