{
  "name": "SIMCardNetworkPreferencesGet",
  "description": "It returns the network preferences currently applied in the SIM card.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "sim_card_id": {
        "example": "6a09cdc3-8948-47f0-aa62-74ac943d6c58",
        "format": "uuid",
        "type": "string",
        "description": "Identifies a SIM card."
      },
      "include_ota_updates": {
        "default": false,
        "example": true,
        "type": "boolean",
        "description": "It includes the associated OTA update objects in the response when present."
      }
    },
    "required": [
      "sim_card_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/sim_cards/{sim_card_id}/network_preferences{?include_ota_updates}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**created_at**: ISO 8601 formatted date-time indicating when the resource was created.",
                  "**mobile_operator_networks_preferences**: A list of mobile operator networks and the priority that should be applied when the SIM is connecting to the network.",
                  "**ota_updates**",
                  "**record_type**",
                  "**sim_card_id**",
                  "**updated_at**: ISO 8601 formatted date-time indicating when the resource was updated."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
