{
  "name": "getPaginatedMdrs",
  "description": "Fetch all Mdr records ",
  "parameters": {
    "type": "object",
    "properties": {
      "start_date": {
        "example": "2020-07-01T00:00:00-06:00",
        "format": "date-time",
        "type": "string",
        "description": "Start date"
      },
      "end_date": {
        "example": "2020-07-01T00:00:00-06:00",
        "format": "date-time",
        "type": "string",
        "description": "End date"
      },
      "id": {
        "description": "Message uuid",
        "example": "e093fbe0-5bde-11eb-ae93-0242ac130002",
        "type": "string"
      },
      "direction": {
        "$ref": "#/$defs/Direction"
      },
      "outbound_profile_id": {
        "description": "Configured profile id. New profiles can be created and configured on Telnyx portal",
        "example": "30ef55db-c4a2-4c4a-9804-a68077973d07",
        "type": "string"
      },
      "cld": {
        "description": "Destination number",
        "example": "+15551237654",
        "type": "string"
      },
      "cli": {
        "description": "Origination number",
        "example": "+15551237654",
        "type": "string"
      },
      "status": {
        "$ref": "#/$defs/MessageStatus"
      },
      "message_type": {
        "$ref": "#/$defs/MessageType"
      },
      "country_iso": {
        "description": "Destination number ISO country code",
        "example": "US",
        "type": "string"
      },
      "error": {
        "description": "Reported error code",
        "example": "40001",
        "type": "string"
      },
      "normalized_carrier": {
        "description": "Carrier",
        "example": "Verizon",
        "type": "string"
      },
      "tag": {
        "description": "Specified tag",
        "example": "Tag1",
        "type": "string"
      },
      "mcc": {
        "description": "Mobile country code",
        "example": "204",
        "type": "string"
      },
      "mnc": {
        "description": "Mobile network code",
        "example": "01",
        "type": "string"
      },
      "product": {
        "$ref": "#/$defs/Product"
      },
      "page[number]": {
        "default": 1,
        "format": "int32",
        "minimum": 1,
        "type": "integer",
        "description": "Page number"
      },
      "page[size]": {
        "default": 20,
        "format": "int32",
        "maximum": 50,
        "minimum": 1,
        "type": "integer",
        "description": "Size of the page"
      },
      "sort": {
        "default": "created_at",
        "description": "Field used to order the data. If no field is specified, default value is 'created_at'",
        "example": "created_at",
        "items": {
          "type": "string"
        },
        "type": "array"
      }
    },
    "required": [
      "start_date",
      "end_date"
    ],
    "$defs": {
      "Direction": {
        "enum": [
          "INBOUND",
          "OUTBOUND"
        ],
        "type": "string"
      },
      "MessageStatus": {
        "description": "Message status",
        "enum": [
          "GW_TIMEOUT",
          "DELIVERED",
          "DLR_UNCONFIRMED",
          "DLR_TIMEOUT",
          "RECEIVED",
          "GW_REJECT",
          "FAILED"
        ],
        "example": "DELIVERED",
        "type": "string"
      },
      "MessageType": {
        "default": "text",
        "description": "type of the message",
        "enum": [
          "audio",
          "contacts",
          "document",
          "hsm",
          "image",
          "location",
          "template",
          "text",
          "video",
          "voice",
          "unknown"
        ],
        "title": "Message Type",
        "type": "string"
      },
      "Product": {
        "description": "Used product",
        "enum": [
          "LONG_CODE",
          "SHORT_CODE",
          "SHORT_CODE_FTEU",
          "TOLL_FREE",
          "ALPHANUMERIC_ID",
          "RCS"
        ],
        "example": "LONG_CODE",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/message_detail_records{?start_date,end_date,id,direction,outbound_profile_id,cld,cli,status,message_type,country_iso,error,normalized_carrier,tag,mcc,mnc,product,page[number],page[size],sort}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**meta**",
              {
                "$ul": [
                  "**page_number**",
                  "**page_size**",
                  "**total_pages**",
                  "**total_results**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
