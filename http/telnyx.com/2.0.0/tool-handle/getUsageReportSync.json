{
  "name": "getUsageReportSync",
  "description": "Generate and fetch voice usage report synchronously. This endpoint will both generate and fetch the voice report over a specified time period. No polling is necessary but the response may take up to a couple of minutes. ",
  "parameters": {
    "type": "object",
    "properties": {
      "start_date": {
        "example": "2020-07-01T00:00:00-06:00",
        "format": "date-time",
        "type": "string"
      },
      "end_date": {
        "example": "2020-07-01T00:00:00-06:00",
        "format": "date-time",
        "type": "string"
      },
      "aggregation_type": {
        "enum": [
          "NO_AGGREGATION",
          "CONNECTION",
          "TAG",
          "BILLING_GROUP"
        ],
        "example": "no_aggregation",
        "type": "string"
      },
      "product_breakdown": {
        "enum": [
          "NO_BREAKDOWN",
          "DID_VS_TOLL_FREE",
          "COUNTRY",
          "DID_VS_TOLL_FREE_PER_COUNTRY"
        ],
        "example": "no_breakdown",
        "type": "string"
      },
      "connections": {
        "example": 1234567890123,
        "items": {
          "example": 1234567890123,
          "type": "number"
        },
        "type": "array"
      }
    },
    "required": [
      "aggregation_type",
      "product_breakdown"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/reports/cdr_usage_reports/sync{?start_date,end_date,aggregation_type,product_breakdown,connections}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**aggregation_type**",
                  [
                    "**connections**",
                    {
                      "$ul": [
                        "**empty**"
                      ]
                    }
                  ],
                  "**created_at**",
                  "**end_time**",
                  "**id**: Identifies the resource",
                  "**product_breakdown**",
                  "**record_type**",
                  "**report_url**",
                  "**result**",
                  "**start_time**",
                  "**status**",
                  "**updated_at**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
