{
  "name": "UpdateBulkTelephonyCredential",
  "description": "Update several credentials in bulk.",
  "parameters": {
    "type": "object",
    "properties": {
      "filter[tag]": {
        "type": "string",
        "description": "Filter by tag, required by bulk operations."
      },
      "body": {
        "$ref": "#/components/schemas/BulkCredentialRequest",
        "description": "Parameters to update credentials on bulk"
      }
    },
    "required": [
      "filter[tag]",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.telnyx.com/v2/actions/bulk/telephony_credentials{?filter[tag]}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Bulk Credential Response"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**credentials**: Amount of credentials affected"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
