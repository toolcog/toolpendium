{
  "name": "ViewRoomSession",
  "description": "View a room session.",
  "parameters": {
    "type": "object",
    "properties": {
      "room_session_id": {
        "example": "0ccc7b54-4df3-4bca-a65a-3da1ecc777f0",
        "format": "uuid",
        "type": "string",
        "description": "The unique identifier of a room session."
      },
      "include_participants": {
        "example": true,
        "type": "boolean",
        "description": "To decide if room participants should be included in the response."
      }
    },
    "required": [
      "room_session_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/room_sessions/{room_session_id}{?include_participants}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**active**: Shows if the room session is active or not.",
                  "**created_at**: ISO 8601 timestamp when the room session was created.",
                  "**ended_at**: ISO 8601 timestamp when the room session has ended.",
                  "**id**: Identifies the room session.",
                  "**participants**",
                  "**record_type**",
                  "**room_id**: Identify the room hosting that room session.",
                  "**updated_at**: ISO 8601 timestamp when the room session was updated."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**detail**",
            "**meta**",
            [
              "**source**",
              {
                "$ul": [
                  "**parameter**: Indicates which query parameter caused the error.",
                  "**pointer**: JSON pointer (RFC6901) to the offending entity."
                ]
              }
            ],
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
