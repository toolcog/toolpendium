{
  "name": "updateWhatsAppWebhook",
  "description": "Update webhook URL used for a given WhatsApp user.",
  "parameters": {
    "type": "object",
    "properties": {
      "whatsapp_user_id": {
        "type": "string",
        "description": "User's WhatsApp ID"
      },
      "body": {
        "$ref": "#/$defs/UpdateUserRequest"
      }
    },
    "required": [
      "whatsapp_user_id",
      "body"
    ],
    "$defs": {
      "UpdateUserRequest": {
        "properties": {
          "webhook_url": {
            "description": "The desired URL to set for your WhatsApp webhook endpoint.",
            "example": "https://mywebhook.com/example/endpoint",
            "title": "Webhook Url",
            "type": "string"
          }
        },
        "required": [
          "webhook_url"
        ],
        "title": "UpdateUserRequest",
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.telnyx.com/v2/whatsapp_users/{whatsapp_user_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
