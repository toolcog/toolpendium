{
  "name": "getPaginatedWdrs",
  "description": "Fetch all Wdr records ",
  "parameters": {
    "type": "object",
    "properties": {
      "start_date": {
        "example": "2021-05-01T00:00:00Z",
        "type": "string",
        "description": "Start date"
      },
      "end_date": {
        "example": "2021-06-01T00:00:00Z",
        "type": "string",
        "description": "End date"
      },
      "id": {
        "description": "WDR uuid",
        "example": "e093fbe0-5bde-11eb-ae93-0242ac130002",
        "type": "string"
      },
      "mcc": {
        "description": "Mobile country code",
        "example": "204",
        "type": "string"
      },
      "mnc": {
        "description": "Mobile network code",
        "example": "01",
        "type": "string"
      },
      "imsi": {
        "description": "International mobile subscriber identity",
        "example": "123456",
        "type": "string"
      },
      "sim_group_name": {
        "description": "Sim group name",
        "example": "sim name",
        "type": "string"
      },
      "sim_group_id": {
        "description": "Sim group unique identifier",
        "example": "f05a189f-7c46-4531-ac56-1460dc465a42",
        "type": "string"
      },
      "sim_card_id": {
        "description": "Sim card unique identifier",
        "example": "877f80a6-e5b2-4687-9a04-88076265720f",
        "type": "string"
      },
      "phone_number": {
        "description": "Phone number",
        "example": "+12345678910",
        "type": "string"
      },
      "page[number]": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "Page number"
      },
      "page[size]": {
        "default": 20,
        "format": "int32",
        "type": "integer",
        "description": "Size of the page"
      },
      "sort": {
        "default": "created_at",
        "description": "Field used to order the data. If no field is specified, default value is 'created_at'",
        "example": "created_at",
        "items": {
          "type": "string"
        },
        "type": "array"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/reports/wdrs{?start_date,end_date,id,mcc,mnc,imsi,sim_group_name,sim_group_id,sim_card_id,phone_number,page[number],page[size],sort}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**meta**",
              {
                "$ul": [
                  "**page_number**",
                  "**page_size**",
                  "**total_pages**",
                  "**total_results**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
