{
  "name": "getUsageAggregationsSync",
  "description": "Generate and fetch WhatsApp usage aggregations synchronously. This endpoint will both generate and fetch the WhatsApp aggregations over a specified time period. ",
  "parameters": {
    "type": "object",
    "properties": {
      "start_date": {
        "example": "2020-07-01T00:00:00-06:00",
        "format": "date-time",
        "type": "string"
      },
      "end_date": {
        "example": "2020-07-01T00:00:00-06:00",
        "format": "date-time",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/whatsapp_usage_aggregations{?start_date,end_date}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
