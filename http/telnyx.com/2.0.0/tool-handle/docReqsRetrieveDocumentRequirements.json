{
  "name": "docReqsRetrieveDocumentRequirements",
  "description": "Retrieve a document requirement record",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "Uniquely identifies the requirement_type record"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/requirements/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**action**: Indicates whether this requirement applies to ordering, porting, or both",
                  "**country_code**: The 2-character (ISO 3166-1 alpha-2) country code where this requirement applies",
                  "**created_at**: ISO 8601 formatted date-time indicating when the resource was created.",
                  "**id**: Identifies the associated document",
                  "**locality**: The locality where this requirement applies",
                  "**phone_number_type**: Indicates the phone_number_type this requirement applies to. Leave blank if this requirement applies to all number_types.",
                  "**record_type**: Identifies the type of the resource.",
                  "**requirements_types**: Lists the requirement types necessary to fulfill this requirement",
                  "**updated_at**: ISO 8601 formatted date-time indicating when the resource was last updated."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
