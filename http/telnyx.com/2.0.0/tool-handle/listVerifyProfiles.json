{
  "name": "listVerifyProfiles",
  "description": "Gets a paginated list of Verify profiles.",
  "parameters": {
    "type": "object",
    "properties": {
      "filter[name]": {
        "description": "Optional filter for profile names.",
        "title": "filter[name]",
        "type": "string"
      },
      "page[size]": {
        "default": 25,
        "title": "page[size]",
        "type": "integer"
      },
      "page[number]": {
        "default": 1,
        "title": "page[number]",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/verify_profiles{?filter[name],page[size],page[number]}"
    }
  },
  "responses": {
