          },
          "record_type": {
            "example": "number_order_phone_number",
            "readOnly": true,
            "type": "string"
          },
          "regulatory_group_id": {
            "example": "dc8e4d67-33a0-4cbb-af74-7b58f05bd494",
            "readOnly": true,
            "type": "string"
