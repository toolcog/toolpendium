{
  "name": "listAvailablePhoneNumbers",
  "description": "List available phone numbers",
  "parameters": {
    "type": "object",
    "properties": {
      "filter[phone_number][starts_with]": {
        "type": "string",
        "description": "Filter numbers starting with a pattern (meant to be used after `national_destination_code` filter has been set)."
      },
      "filter[phone_number][ends_with]": {
        "type": "string",
        "description": "Filter numbers ending with a pattern."
      },
      "filter[phone_number][contains]": {
        "type": "string",
        "description": "Filter numbers containing a pattern."
      },
      "filter[locality]": {
        "type": "string",
        "description": "Filter phone numbers by city."
      },
      "filter[administrative_area]": {
        "example": "IL",
        "type": "string",
        "description": "Filter phone numbers by US state/CA province."
      },
      "filter[country_code]": {
        "example": "US",
        "type": "string",
        "description": "Filter phone numbers by ISO alpha-2 country code."
      },
      "filter[national_destination_code]": {
        "type": "string",
        "description": "Filter by the national destination code of the number. This filter is only applicable to North American numbers."
      },
      "filter[rate_center]": {
        "example": "CHICAGO HEIGHTS",
        "type": "string",
        "description": "Filter phone numbers by NANP rate center. This filter is only applicable to North American numbers."
      },
      "filter[number_type]": {
        "enum": [
          "toll-free",
          "national",
          "mobile",
          "local"
        ],
        "example": "local",
        "type": "string",
        "description": "Filter phone numbers by number type."
      },
      "filter[features]": {
        "example": "voice,sms",
        "items": {
          "enum": [
            "sms",
            "mms",
            "voice",
            "fax",
            "emergency"
          ],
          "type": "string"
        },
        "type": "array",
        "description": "Filter if the phone number should be used for voice, fax, mms, sms, emergency."
      },
      "filter[limit]": {
        "example": 100,
        "type": "integer",
        "description": "Limits the number of results."
      },
      "filter[best_effort]": {
        "example": false,
        "type": "boolean",
        "description": "Filter to determine if best effort results should be included."
      },
      "filter[quickship]": {
        "example": true,
        "type": "boolean",
        "description": "Filter to exclude phone numbers that need additional time after to purchase to receive phone calls."
      },
      "filter[reservable]": {
        "example": true,
        "type": "boolean",
        "description": "Filter to exclude phone numbers that cannot be reserved before purchase."
      },
      "filter[exclude_held_numbers]": {
        "example": false,
        "type": "boolean",
        "description": "Filter to exclude phone numbers that are currently on hold for your account."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/available_phone_numbers{?filter[phone_number][starts_with],filter[phone_number][ends_with],filter[phone_number][contains],filter[locality],filter[administrative_area],filter[country_code],filter[national_destination_code],filter[rate_center],filter[number_type],filter[features],filter[limit],filter[best_effort],filter[quickship],filter[reservable],filter[exclude_held_numbers]}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List Available Phone Numbers Response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**meta**",
              {
                "$ul": [
                  "**best_effort_results**",
                  "**total_results**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
