{
  "name": "getUsageReport",
  "description": "Fetch a single messaging usage report by id",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/reports/mdr_usage_reports/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**aggregation_type**",
                  "**connections**",
                  "**created_at**",
                  "**end_date**",
                  "**id**: Identifies the resource",
                  "**profiles**",
                  "**record_type**",
                  "**report_url**",
                  "**result**",
                  "**start_date**",
                  "**status**",
                  "**updated_at**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
