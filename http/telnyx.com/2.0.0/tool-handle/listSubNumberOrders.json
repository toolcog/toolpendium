{
  "name": "listSubNumberOrders",
  "description": "Get a paginated list of sub number orders.",
  "parameters": {
    "type": "object",
    "properties": {
      "filter[user_id]": {
        "example": "d70873cd-7c98-401a-81b6-b1ae08246995",
        "format": "uuid",
        "type": "string",
        "description": "User ID of the user who owns the sub number order"
      },
      "filter[order_request_id]": {
        "example": "12ade33a-21c0-473b-b055-b3c836e1c293",
        "format": "uuid",
        "type": "string",
        "description": "ID of the number order the sub number order belongs to"
      },
      "filter[country_code]": {
        "example": "US",
        "type": "string",
        "description": "ISO alpha-2 country code."
      },
      "filter[phone_number_type]": {
        "example": "local",
        "type": "string",
        "description": "Phone Number Type"
      },
      "filter[phone_numbers_count]": {
        "example": 1,
        "type": "integer",
        "description": "Amount of numbers in the sub number order"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/sub_number_orders{?filter[user_id],filter[order_request_id],filter[country_code],filter[phone_number_type],filter[phone_numbers_count]}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List Sub Number Orders Response"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**meta**",
              {
                "$ul": [
                  "**page_number**",
                  "**page_size**",
                  "**total_pages**",
                  "**total_results**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
