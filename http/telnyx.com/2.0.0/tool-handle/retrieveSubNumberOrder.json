{
  "name": "retrieveSubNumberOrder",
  "description": "Get an existing sub number order.",
  "parameters": {
    "type": "object",
    "properties": {
      "sub_number_order_id": {
        "type": "string",
        "description": "The sub number order ID."
      },
      "filter[include_phone_numbers]": {
        "default": false,
        "type": "boolean",
        "description": "Include the first 50 phone number objects in the results"
      }
    },
    "required": [
      "sub_number_order_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/sub_number_orders/{sub_number_order_id}{?filter[include_phone_numbers]}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Sub Number Order Response"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**country_code**",
                  "**created_at**: An ISO 8901 datetime string denoting when the number order was created.",
                  "**id**",
                  "**order_request_id**",
                  "**phone_number_type**",
                  "**phone_numbers_count**: The count of phone numbers in the number order.",
                  "**record_type**",
                  "**regulatory_requirements**",
                  "**requirements_met**: True if all requirements are met for every phone number, false otherwise.",
                  "**updated_at**: An ISO 8901 datetime string for when the number order was updated.",
                  "**user_id**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
