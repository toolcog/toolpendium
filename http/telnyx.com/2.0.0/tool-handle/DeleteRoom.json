{
  "name": "DeleteRoom",
  "description": "Synchronously delete a Room. Participants from that room will be kicked out, they won't be able to join that room anymore, and you won't be charged anymore for that room.",
  "parameters": {
    "type": "object",
    "properties": {
      "room_id": {
        "example": "0ccc7b54-4df3-4bca-a65a-3da1ecc777f0",
        "format": "uuid",
        "type": "string",
        "description": "The unique identifier of a room."
      }
    },
    "required": [
      "room_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.telnyx.com/v2/rooms/{room_id}"
    }
  },
  "responses": {
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**detail**",
            "**meta**",
            [
              "**source**",
              {
                "$ul": [
                  "**parameter**: Indicates which query parameter caused the error.",
                  "**pointer**: JSON pointer (RFC6901) to the offending entity."
                ]
              }
            ],
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
