{
  "name": "retrieveCallQueue",
  "description": "Retrieve an existing call queue",
  "parameters": {
    "type": "object",
    "properties": {
      "queue_name": {
        "type": "string",
        "description": "Uniquely identifies the queue by name"
      }
    },
    "required": [
      "queue_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/queues/{queue_name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Queue Response"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**average_wait_time_secs**: The average time that the calls currently in the queue have spent waiting, given in seconds.",
                  "**created_at**: ISO 8601 formatted date of when the queue was created",
                  "**current_size**: The number of calls currently in the queue",
                  "**id**: Uniquely identifies the queue",
                  "**max_size**: The maximum number of calls allowed in the queue",
                  "**name**: Name of the queue",
                  "**record_type**",
                  "**updated_at**: ISO 8601 formatted date of when the queue was last updated"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
