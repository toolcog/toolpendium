{
  "name": "submitMdrRequest",
  "description": "Submit a request for new messaging detail report. Messaging detail report pulls all raw messaging data according to defined filters.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/MdrPostDetailReportRequest",
        "description": "Mdr detail request data"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.telnyx.com/v2/reports/batch_mdr_reports"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**connections**",
                  "**created_at**",
                  "**directions**",
                  "**end_date**",
                  "**filters**",
                  "**id**: Identifies the resource",
                  "**profiles**",
                  "**record_type**",
                  "**record_types**",
                  "**report_name**",
                  "**report_url**",
                  "**start_date**",
                  "**status**",
                  "**updated_at**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
