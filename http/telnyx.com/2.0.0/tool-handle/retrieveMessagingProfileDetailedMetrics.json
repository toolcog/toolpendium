{
  "name": "retrieveMessagingProfileDetailedMetrics",
  "description": "Retrieve messaging profile metrics",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "The id of the messaging profile to retrieve"
      },
      "time_frame": {
        "default": "24h",
        "enum": [
          "1h",
          "3h",
          "24h",
          "3d",
          "7d",
          "30d"
        ],
        "type": "string",
        "description": "The timeframe for which you'd like to retrieve metrics."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/messaging_profiles/{id}/metrics{?time_frame}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Retrieve Messaging Profile Metrics Response"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**detailed**",
                  [
                    "**overview**",
                    {
                      "$ul": [
                        [
                          "**inbound**",
                          {
                            "$ul": [
                              "**received**: The number of inbound messages received."
                            ]
                          }
                        ],
                        "**messaging_profile_id**: Identifies the type of resource.",
                        [
                          "**outbound**",
                          {
                            "$ul": [
                              "**delivered**: The number of outbound messages successfully delivered.",
                              "**error_ratio**: The ratio of messages sent that resulted in errors.",
                              "**sent**: The number of outbound messages sent."
                            ]
                          }
                        ],
                        "**phone_numbers**: The number of phone numbers associated with the messaging profile.",
                        "**record_type**: Identifies the type of the resource."
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
