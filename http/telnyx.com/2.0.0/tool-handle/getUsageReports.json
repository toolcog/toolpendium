{
  "name": "getUsageReports",
  "description": "Fetch all messaging usage reports. Usage reports are aggregated messaging data for specified time period and breakdown",
  "parameters": {
    "type": "object",
    "properties": {
      "page[number]": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "Page number"
      },
      "page[size]": {
        "default": 20,
        "format": "int32",
        "type": "integer",
        "description": "Size of the page"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/reports/mdr_usage_reports{?page[number],page[size]}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**meta**",
              {
                "$ul": [
                  "**page_number**",
                  "**page_size**",
                  "**total_pages**",
                  "**total_results**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
