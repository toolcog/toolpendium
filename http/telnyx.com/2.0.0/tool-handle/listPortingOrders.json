{
  "name": "listPortingOrders",
  "description": "Returns a list of your porting order.",
  "parameters": {
    "type": "object",
    "properties": {
      "page[number]": {
        "default": 1,
        "minimum": 1,
        "type": "integer",
        "description": "The page number to load."
      },
      "page[size]": {
        "default": 20,
        "maximum": 250,
        "minimum": 1,
        "type": "integer",
        "description": "The size of the page."
      },
      "include_phone_numbers": {
        "default": true,
        "type": "boolean",
        "description": "Include the first 50 phone number objects in the results"
      },
      "filter[status]": {
        "$ref": "#/components/schemas/PortingOrderStatus",
        "description": "Filter results by status"
      },
      "filter[customer_reference]": {
        "type": "string",
        "description": "Filter results by user reference"
      },
      "filter[phone_numbers][country_code]": {
        "type": "string",
        "description": "Filter results by country ISO 3166-1 alpha-2 code"
      },
      "filter[phone_numbers][carrier_name]": {
        "type": "string",
        "description": "Filter results by old service provider"
      },
      "filter[misc][type]": {
        "$ref": "#/components/schemas/PortingOrderType",
        "description": "Filter results by porting order type"
      },
      "filter[end_user][admin][entity_name]": {
        "type": "string",
        "description": "Filter results by person or company name"
      },
      "filter[end_user][admin][auth_person_name]": {
        "type": "string",
        "description": "Filter results by authorized person"
      },
      "filter[activation_settings][fast_port_eligible]": {
        "type": "boolean",
        "description": "Filter results by fast port eligible"
      },
      "filter[activation_settings][foc_datetime_requested][gt]": {
        "type": "string",
        "description": "Filter results by foc date later than this value"
      },
      "filter[activation_settings][foc_datetime_requested][lt]": {
        "type": "string",
        "description": "Filter results by foc date earlier than this value"
      },
      "sort": {
        "enum": [
          "created_at",
          "-created_at",
          "activation_settings.foc_datetime_requested",
          "-activation_settings.foc_datetime_requested"
        ],
        "example": "created_at",
        "type": "string",
        "description": "Specifies the sort order for results. If not given, results are sorted by created_at in descending order."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/porting_orders{?page[number],page[size],include_phone_numbers,filter[status],filter[customer_reference],filter[phone_numbers][country_code],filter[phone_numbers][carrier_name],filter[misc][type],filter[end_user][admin][entity_name],filter[end_user][admin][auth_person_name],filter[activation_settings][fast_port_eligible],filter[activation_settings][foc_datetime_requested][gt],filter[activation_settings][foc_datetime_requested][lt],sort}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            [
              "**meta**",
              {
                "$ul": [
                  "**page_number**",
                  "**page_size**",
                  "**total_pages**",
                  "**total_results**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
