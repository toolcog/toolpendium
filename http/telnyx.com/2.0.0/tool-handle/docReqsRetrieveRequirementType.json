{
  "name": "docReqsRetrieveRequirementType",
  "description": "Retrieve a requirement type by id",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "Uniquely identifies the requirement_type record"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.telnyx.com/v2/requirement_types/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  [
                    "**acceptance_criteria**: Specifies objective criteria for acceptance",
                    {
                      "$ul": [
                        "**locality_limit**: Specifies geography-based acceptance criteria",
                        "**time_limit**: Specifies time-based acceptance criteria"
                      ]
                    }
                  ],
                  "**created_at**: ISO 8601 formatted date-time indicating when the resource was created.",
                  "**description**: Describes the requirement type",
                  "**example**: Provides one or more examples of acceptable documents",
                  "**id**: Identifies the associated document",
                  "**name**: A short descriptive name for this requirement_type",
                  "**record_type**: Identifies the type of the resource",
                  "**type**: Defines the type of this requirement type",
                  "**updated_at**: ISO 8601 formatted date-time indicating when the resource was last updated."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
