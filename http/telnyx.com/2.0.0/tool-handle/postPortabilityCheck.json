{
  "name": "postPortabilityCheck",
  "description": "Runs a portability check, returning the results immediately.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "phone_numbers": {
            "description": "The list of +E.164 formatted phone numbers to check for portability",
            "example": [
              "+13035550000",
              "+13035550001",
              "+13035550002"
            ],
            "items": {
              "type": "string"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.telnyx.com/v2/portability_checks"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
