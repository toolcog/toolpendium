{
  "name": "getUrls",
  "description": "Executes a query and returns a paginated response",
  "parameters": {
    "type": "object",
    "properties": {
      "area": {
        "default": "current",
        "enum": [
          "current",
          "disappeared",
          "new",
          "search_engines_orphans"
        ],
        "type": "string",
        "description": "Analysis context to execute the query"
      },
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "Page Number"
      },
      "size": {
        "format": "int32",
        "type": "integer",
        "description": "Page Size"
      },
      "username": {
        "type": "string",
        "description": "User's identifier"
      },
      "project_slug": {
        "type": "string",
        "description": "Project's identifier"
      },
      "analysis_slug": {
        "type": "string",
        "description": "Analysis' identifier"
      },
      "body": {
        "$ref": "#/$defs/UrlsQuery"
      }
    },
    "required": [
      "username",
      "project_slug",
      "analysis_slug"
    ],
    "$defs": {
      "UrlsQuery": {
        "properties": {
          "fields": {
            "description": "",
            "items": {
              "type": "string"
            },
            "readOnly": false,
            "type": "array"
          },
          "filters": {
            "description": "",
            "readOnly": false,
            "type": "object"
          },
          "sort": {
            "description": "",
            "items": {
              "type": "object"
            },
            "readOnly": false,
            "type": "array"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.botify.com/v1/analyses/{username}/{project_slug}/{analysis_slug}/urls{?area,page,size}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**: ",
            "**next**: ",
            "**page**: ",
            "**previous**: ",
            "**results**: ",
            "**size**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "default payload for object for all HTTP codes that are not covered individually",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**error_code**: Error Code",
                  "**error_detail**: If available, detailed error",
                  "**message**: Error Message"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
