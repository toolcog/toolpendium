{
  "name": "getCrawlStatisticsByFrequency",
  "description": "Return crawl statistics grouped by time frequency (1 min, 5 mins or 60 min) for an analysis",
  "parameters": {
    "type": "object",
    "properties": {
      "limit": {
        "format": "int32",
        "type": "integer",
        "description": "max number of elements to retrieve"
      },
      "frequency": {
        "enum": [
          "1mn",
          "5mn",
          "60mn"
        ],
        "type": "string",
        "description": "Aggregation frequency"
      },
      "username": {
        "type": "string",
        "description": "User's identifier"
      },
      "project_slug": {
        "type": "string",
        "description": "Project's identifier"
      },
      "analysis_slug": {
        "type": "string",
        "description": "Analysis' identifier"
      }
    },
    "required": [
      "frequency",
      "username",
      "project_slug",
      "analysis_slug"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.botify.com/v1/analyses/{username}/{project_slug}/{analysis_slug}/crawl_statistics/time{?limit,frequency}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**avg_delay**: ",
            "**avg_size**: ",
            "**frequency**: ",
            "**http_code_2xx**: ",
            "**http_code_3xx**: ",
            "**http_code_4xx**: ",
            "**http_code_5xx**: ",
            "**http_code_xxx**: ",
            "**last_update_date**: ",
            "**new_urls**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "default payload for object for all HTTP codes that are not covered individually",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**error_code**: Error Code",
                  "**error_detail**: If available, detailed error",
                  "**message**: Error Message"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
