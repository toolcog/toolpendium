{
  "name": "getAnalysisSummary",
  "description": "Get an Analysis detail",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "User's identifier"
      },
      "project_slug": {
        "type": "string",
        "description": "Project's identifier"
      },
      "analysis_slug": {
        "type": "string",
        "description": "Analysis' identifier"
      }
    },
    "required": [
      "username",
      "project_slug",
      "analysis_slug"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.botify.com/v1/analyses/{username}/{project_slug}/{analysis_slug}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**config**: ",
            "**date_finished**: ",
            "**date_last_modified**: Date of last modified analysis",
            "**date_launched**: ",
            "**failures**: ",
            "**features**: ",
            "**id**: ",
            "**name**: ",
            "**red_button_domain**: ",
            "**slug**: ",
            "**url**: ",
            "**urls_done**: ",
            "**urls_in_queue**: ",
            [
              "**user**",
              {
                "$ul": [
                  "**company_name**: ",
                  "**date_joined**: ",
                  "**email**: ",
                  "**first_name**: ",
                  "**is_organization**: ",
                  "**last_name**: ",
                  "**login**: ",
                  "**status**: ",
                  "**url**: "
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "default payload for object for all HTTP codes that are not covered individually",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**error_code**: Error Code",
                  "**error_detail**: If available, detailed error",
                  "**message**: Error Message"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
