{
  "name": "getProjectUrlsAggs",
  "description": "Project Query aggregator. It accepts multiple queries that will be executed on all completed analyses in the project",
  "parameters": {
    "type": "object",
    "properties": {
      "area": {
        "default": "current",
        "enum": [
          "current",
          "disappeared",
          "new"
        ],
        "type": "string",
        "description": "Analysis context to execute the queries"
      },
      "last_analysis_slug": {
        "type": "string",
        "description": "Last analysis on the trend"
      },
      "nb_analyses": {
        "default": 20,
        "format": "int32",
        "maximum": 20,
        "minimum": 1,
        "type": "integer",
        "description": "Max number of analysis to return"
      },
      "username": {
        "type": "string",
        "description": "User's identifier"
      },
      "project_slug": {
        "type": "string",
        "description": "Project's identifier"
      },
      "body": {
        "$ref": "#/$defs/UrlsAggsQueries"
      }
    },
    "required": [
      "username",
      "project_slug"
    ],
    "$defs": {
      "UrlsAggsQueries": {
        "items": {
          "$ref": "#/$defs/UrlsAggsQuery"
        },
        "type": "array"
      },
      "UrlsAggsQuery": {
        "properties": {
          "aggs": {
            "description": "",
            "items": {
              "type": "object"
            },
            "readOnly": false,
            "type": "array"
          },
          "filters": {
            "description": "",
            "readOnly": false,
            "type": "object"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.botify.com/v1/projects/{username}/{project_slug}/urls/aggs{?area,last_analysis_slug,nb_analyses}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "default payload for object for all HTTP codes that are not covered individually",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**error_code**: Error Code",
                  "**error_detail**: If available, detailed error",
                  "**message**: Error Message"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
