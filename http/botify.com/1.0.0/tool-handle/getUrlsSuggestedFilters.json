{
  "name": "getUrlsSuggestedFilters",
  "description": "Return most frequent segments (= suggested patterns in the previous version) for a Botify Query.",
  "parameters": {
    "type": "object",
    "properties": {
      "area": {
        "default": "current",
        "enum": [
          "current",
          "new"
        ],
        "type": "string",
        "description": ""
      },
      "username": {
        "type": "string",
        "description": "User's identifier"
      },
      "project_slug": {
        "type": "string",
        "description": "Project's identifier"
      },
      "analysis_slug": {
        "type": "string",
        "description": "Analysis' identifier"
      },
      "body": {
        "$ref": "#/$defs/UrlsAggsQuery"
      }
    },
    "required": [
      "username",
      "project_slug",
      "analysis_slug"
    ],
    "$defs": {
      "UrlsAggsQuery": {
        "properties": {
          "aggs": {
            "description": "",
            "items": {
              "type": "object"
            },
            "readOnly": false,
            "type": "array"
          },
          "filters": {
            "description": "",
            "readOnly": false,
            "type": "object"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.botify.com/v1/analyses/{username}/{project_slug}/{analysis_slug}/urls/suggested_filters{?area}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**aggs**: ",
            "**filters**: "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "default payload for object for all HTTP codes that are not covered individually",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**error_code**: Error Code",
                  "**error_detail**: If available, detailed error",
                  "**message**: Error Message"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
