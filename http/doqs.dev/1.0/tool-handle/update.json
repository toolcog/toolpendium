{
  "name": "update",
  "description": "Update",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "title": "Id",
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UpdateTemplateRequest"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "UpdateTemplateRequest": {
        "properties": {
          "fields": {
            "items": {
              "discriminator": {
                "mapping": {
                  "check": "#/components/schemas/CheckBoxField",
                  "date": "#/components/schemas/DateField",
                  "image": "#/components/schemas/ImageField",
                  "text": "#/components/schemas/TextField"
                },
                "propertyName": "type"
              },
              "oneOf": [
                {
                  "$ref": "#/$defs/DateField"
                },
                {
                  "$ref": "#/$defs/TextField"
                },
                {
                  "$ref": "#/$defs/ImageField"
                },
                {
                  "$ref": "#/$defs/CheckBoxField"
                }
              ]
            },
            "title": "Fields",
            "type": "array"
          },
          "name": {
            "title": "Name",
            "type": "string"
          }
        },
        "required": [
          "fields"
        ],
        "title": "UpdateTemplateRequest",
        "type": "object"
      },
      "DateField": {
        "properties": {
          "bbox": {
            "$ref": "#/$defs/BoundingBox"
          },
          "cell_count": {
            "default": 0,
            "minimum": 0,
            "title": "Cell Count",
            "type": "integer"
          },
          "cell_offset": {
            "default": 0,
            "minimum": 0,
            "title": "Cell Offset",
            "type": "integer"
          },
          "cells": {
            "default": false,
            "title": "Cells",
            "type": "boolean"
          },
          "color": {
            "default": "black",
            "format": "color",
            "title": "Color",
            "type": "string"
          },
          "font": {
            "allOf": [
              {
                "$ref": "#/$defs/Font"
              }
            ],
            "default": "helvetica"
          },
          "font_size": {
            "default": 10,
            "exclusiveMinimum": true,
            "minimum": 0,
            "title": "Font Size",
            "type": "integer"
          },
          "format": {
            "title": "Format",
            "type": "string"
          },
          "h_align": {
            "allOf": [
              {
                "$ref": "#/$defs/HorizontalAlign"
              }
            ],
            "default": "L"
          },
          "locale": {
            "default": "en_US",
            "maxLength": 5,
            "minLength": 5,
            "title": "Locale",
            "type": "string"
          },
          "name": {
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "page": {
            "minimum": 0,
            "title": "Page",
            "type": "integer"
          },
          "required": {
            "default": true,
            "title": "Required",
            "type": "boolean"
          },
          "type": {
            "enum": [
              "date"
            ],
            "title": "Type",
            "type": "string"
          },
          "v_align": {
            "allOf": [
              {
                "$ref": "#/$defs/VerticalAlign"
              }
            ],
            "default": "B"
          }
        },
        "required": [
          "bbox",
          "name",
          "page",
          "type",
          "format"
        ],
        "title": "DateField",
        "type": "object"
      },
      "BoundingBox": {
        "properties": {
          "height": {
            "minimum": 1,
            "title": "Height",
            "type": "number"
          },
          "width": {
            "minimum": 1,
            "title": "Width",
            "type": "number"
          },
          "x": {
            "minimum": 0,
            "title": "X",
            "type": "number"
          },
          "y": {
            "minimum": 0,
            "title": "Y",
            "type": "number"
          }
        },
        "required": [
          "x",
          "y",
          "width",
          "height"
        ],
        "title": "BoundingBox",
        "type": "object"
      },
      "Font": {
        "description": "An enumeration.",
        "enum": [
          "courier",
          "helvetica",
          "times_roman"
        ],
        "title": "Font",
        "type": "string"
      },
      "HorizontalAlign": {
        "description": "An enumeration.",
        "enum": [
          "L",
          "C",
          "R"
        ],
        "title": "HorizontalAlign",
        "type": "string"
      },
      "VerticalAlign": {
        "description": "An enumeration.",
        "enum": [
          "T",
          "C",
          "B"
        ],
        "title": "VerticalAlign",
        "type": "string"
      },
      "TextField": {
        "properties": {
          "bbox": {
            "$ref": "#/$defs/BoundingBox"
          },
          "cell_count": {
            "default": 0,
            "minimum": 0,
            "title": "Cell Count",
            "type": "integer"
          },
          "cell_offset": {
            "default": 0,
            "minimum": 0,
            "title": "Cell Offset",
            "type": "integer"
          },
          "cells": {
            "default": false,
            "title": "Cells",
            "type": "boolean"
          },
          "color": {
            "default": "black",
            "format": "color",
            "title": "Color",
            "type": "string"
          },
          "font": {
            "allOf": [
              {
                "$ref": "#/$defs/Font"
              }
            ],
            "default": "helvetica"
          },
          "font_size": {
            "default": 10,
            "exclusiveMinimum": true,
            "minimum": 0,
            "title": "Font Size",
            "type": "integer"
          },
          "h_align": {
            "allOf": [
              {
                "$ref": "#/$defs/HorizontalAlign"
              }
            ],
            "default": "L"
          },
          "name": {
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "page": {
            "minimum": 0,
            "title": "Page",
            "type": "integer"
          },
          "required": {
            "default": true,
            "title": "Required",
            "type": "boolean"
          },
          "type": {
            "enum": [
              "text"
            ],
            "title": "Type",
            "type": "string"
          },
          "v_align": {
            "allOf": [
              {
                "$ref": "#/$defs/VerticalAlign"
              }
            ],
            "default": "B"
          }
        },
        "required": [
          "bbox",
          "name",
          "page",
          "type"
        ],
        "title": "TextField",
        "type": "object"
      },
      "ImageField": {
        "properties": {
          "anchor": {
            "allOf": [
              {
                "$ref": "#/$defs/ImageAnchor"
              }
            ],
            "default": "c"
          },
          "bbox": {
            "$ref": "#/$defs/BoundingBox"
          },
          "name": {
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "page": {
            "minimum": 0,
            "title": "Page",
            "type": "integer"
          },
          "required": {
            "default": true,
            "title": "Required",
            "type": "boolean"
          },
          "type": {
            "default": "image",
            "enum": [
              "image"
            ],
            "title": "Type",
            "type": "string"
          }
        },
        "required": [
          "bbox",
          "name",
          "page"
        ],
        "title": "ImageField",
        "type": "object"
      },
      "ImageAnchor": {
        "description": "An enumeration.",
        "enum": [
          "nw",
          "n",
          "ne",
          "w",
          "c",
          "e",
          "sw",
          "s"
        ],
        "title": "ImageAnchor",
        "type": "string"
      },
      "CheckBoxField": {
        "properties": {
          "bbox": {
            "$ref": "#/$defs/BoundingBox"
          },
          "eval": {
            "anyOf": [
              {
                "type": "boolean"
              },
              {
                "minLength": 1,
                "type": "string"
              }
            ],
            "default": true,
            "title": "Eval"
          },
          "name": {
            "minLength": 1,
            "title": "Name",
            "type": "string"
          },
          "page": {
            "minimum": 0,
            "title": "Page",
            "type": "integer"
          },
          "required": {
            "default": true,
            "title": "Required",
            "type": "boolean"
          },
          "type": {
            "enum": [
              "check"
            ],
            "title": "Type",
            "type": "string"
          }
        },
        "required": [
          "bbox",
          "name",
          "page",
          "type"
        ],
        "title": "CheckBoxField",
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.doqs.dev/v1/templates/{id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ResponseOk[Template]"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**results**",
              {
                "$ul": [
                  "**created_at**",
                  "**fields** (default: )",
                  "**id**",
                  "**name**",
                  "**pages**",
                  "**size_bytes**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "4XX": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ResponseError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "5XX": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "ResponseError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
