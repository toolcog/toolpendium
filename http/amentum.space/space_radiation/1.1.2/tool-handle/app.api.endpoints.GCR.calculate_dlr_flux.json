{
  "name": "app_api_endpoints_GCR_calculate_dlr_flux",
  "description": "for the given energy, atomic number, and date.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "example": 2017,
        "type": "integer",
        "description": "<br>"
      },
      "month": {
        "example": 1,
        "type": "integer",
        "description": "<br>"
      },
      "day": {
        "example": 1,
        "type": "integer",
        "description": "<br>"
      },
      "z": {
        "example": 6,
        "type": "number",
        "description": "<br>Particle atomic number"
      },
      "energy": {
        "example": 100,
        "type": "number",
        "description": "<br>Particle energy in MeV/n<br> Valid range: [0, 10<sup>6</sup>] MeV/n<br> \n"
      }
    },
    "required": [
      "year",
      "month",
      "day",
      "z",
      "energy"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/gcr/flux_dlr{?year,month,day,z,energy}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**flux**: Particle flux value and units.",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
