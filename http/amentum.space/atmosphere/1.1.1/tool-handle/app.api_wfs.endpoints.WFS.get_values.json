{
  "name": "app_api_wfs_endpoints_WFS_get_values",
  "description": "at a given position and time on 42-48 hour forecast horizon (10 minute resolution). NOTE: latitudes outside the interval (-90,90) are clipped to the endpoints; longitudes outside (0,360) are wrapped.   \n",
  "parameters": {
    "type": "object",
    "properties": {
      "latitude": {
        "example": 42,
        "type": "number",
        "description": "Latitude (deg) -90 to 90 deg"
      },
      "longitude": {
        "example": 42,
        "type": "number",
        "description": "Longitude (deg) 0 to 360 deg or -180 to 180 deg"
      },
      "altitude": {
        "example": 300,
        "type": "number",
        "description": "Altitude in (km)"
      },
      "year": {
        "example": 2020,
        "type": "integer",
        "description": "Year in YYYY format"
      },
      "month": {
        "example": 5,
        "type": "integer",
        "description": "Month in MM format"
      },
      "day": {
        "example": 23,
        "type": "integer",
        "description": "Day in DD format"
      },
      "hour": {
        "example": 15,
        "type": "integer",
        "description": "UTC Hour of the day in 24 hour format"
      },
      "minute": {
        "example": 10,
        "type": "integer",
        "description": "Minute of the given hour"
      }
    },
    "required": [
      "latitude",
      "longitude",
      "altitude",
      "year",
      "month",
      "day",
      "hour",
      "minute"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/wam-ipe{?latitude,longitude,altitude,year,month,day,hour,minute}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**N2_density**: Molecular nitrogen density in (/m3).",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**O2_density**: Moleculary oxygen density in (/m3).",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**O_density**: Oxygen atomic density in (/m3).",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**eastward_wind_neutral**: Easterly component of neutral wind velocity in (m/s).",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**northward_wind_neutral**: Northerly component of neutral wind velocity in (m/s).",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**point**: The given point in the request.",
              {
                "$ul": [
                  "**altitude**",
                  "**latitude**",
                  "**longitude**"
                ]
              }
            ],
            [
              "**temp_neutral**: Temperature of the atmosphere in (K).",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**total_mass_density**: Total mass density (kg/m3).",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**upward_wind_neutral**: Upward component of neutral wind velocity in (m/s).",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
