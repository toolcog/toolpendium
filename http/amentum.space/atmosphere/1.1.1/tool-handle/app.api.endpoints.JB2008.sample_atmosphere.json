{
  "name": "app_api_endpoints_JB2008_sample_atmosphere",
  "description": "under given conditions.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "year": {
        "example": 2020,
        "type": "integer",
        "description": "Year in YYYY format"
      },
      "month": {
        "example": 5,
        "type": "integer",
        "description": "Month in MM format"
      },
      "day": {
        "example": 23,
        "type": "integer",
        "description": "Day in DD format"
      },
      "altitude": {
        "example": 300,
        "type": "number",
        "description": "Altitude in (km)"
      },
      "geodetic_latitude": {
        "example": 42,
        "type": "number",
        "description": "GeodeticLatitude (deg) -90 to 90 deg"
      },
      "geodetic_longitude": {
        "example": 42,
        "type": "number",
        "description": "GeodeticLongitude (deg) 0 to 360 deg"
      },
      "utc": {
        "example": 2,
        "type": "number",
        "description": "Coordinated Universal Time (hrs)"
      }
    },
    "required": [
      "year",
      "month",
      "day",
      "altitude",
      "geodetic_latitude",
      "geodetic_longitude",
      "utc"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/jb2008{?year,month,day,altitude,geodetic_latitude,geodetic_longitude,utc}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**at_alt_temp**",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**exospheric_temp**",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ],
            [
              "**total_mass_density**",
              {
                "$ul": [
                  "**units**",
                  "**value**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
