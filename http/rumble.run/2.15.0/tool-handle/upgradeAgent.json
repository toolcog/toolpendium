{
  "name": "upgradeAgent",
  "description": "Force an agent to update and restart",
  "parameters": {
    "type": "object",
    "properties": {
      "agent_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the agent to update"
      }
    },
    "required": [
      "agent_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/org/agents/{agent_id}/update"
    }
  },
  "responses": {}
}
