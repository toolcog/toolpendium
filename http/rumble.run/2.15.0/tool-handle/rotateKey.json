{
  "name": "rotateKey",
  "description": "Rotate the API key secret and return the updated key",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/org/key/rotate"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**client_id**",
            "**comment**",
            "**counter**",
            "**created_at**",
            "**created_by**",
            "**id**",
            "**inactive**",
            "**last_used_at**",
            "**last_used_ip**",
            "**last_used_ua**",
            "**organization_id**",
            "**token**",
            "**type**",
            "**usage_limit**",
            "**usage_today**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
