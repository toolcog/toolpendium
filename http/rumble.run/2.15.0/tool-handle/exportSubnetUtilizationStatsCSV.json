{
  "name": "exportSubnetUtilizationStatsCSV",
  "description": "Subnet utilization statistics as as CSV",
  "parameters": {
    "type": "object",
    "properties": {
      "mask": {
        "type": "string",
        "description": "an optional subnet mask size (ex:24)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/org/services/subnet.stats.csv{?mask}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
