{
  "name": "getWirelessLAN",
  "description": "Get wireless LAN details",
  "parameters": {
    "type": "object",
    "properties": {
      "wireless_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the wireless LAN to retrieve"
      }
    },
    "required": [
      "wireless_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/org/wireless/{wireless_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**agent_name**",
            "**authentication**",
            "**bssid**",
            "**channels**",
            "**created_at**",
            "**data**",
            "**encryption**",
            "**essid**",
            "**family**",
            "**id**",
            "**interface**",
            "**last_agent_id**",
            "**last_seen**",
            "**last_task_id**",
            "**org_name**",
            "**organization_id**",
            "**signal**",
            "**site_id**",
            "**site_name**",
            "**type**",
            "**vendor**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
