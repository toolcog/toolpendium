{
  "name": "getAccountOrganizations",
  "description": "Get all organization details",
  "parameters": {
    "type": "object",
    "properties": {
      "search": {
        "type": "string",
        "description": "an optional search string for filtering results"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/orgs{?search}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**asset_count**",
            "**client_id**",
            "**created_at**",
            "**deactivated_at**",
            "**description**",
            "**download_token**",
            "**download_token_created_at**",
            "**expiration_assets_offline**",
            "**expiration_assets_stale**",
            "**expiration_scans**",
            "**export_token**",
            "**export_token_counter**",
            "**export_token_created_at**",
            "**export_token_last_used_at**",
            "**export_token_last_used_by**",
            "**id**",
            "**inactive**",
            "**name**",
            "**parent_id**",
            "**permanent**",
            "**project**",
            "**service_count**",
            "**service_count_arp**",
            "**service_count_icmp**",
            "**service_count_tcp**",
            "**service_count_udp**",
            "**updated_at**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
