{
  "name": "getAccountAgents",
  "description": "Get all agents across all organizations",
  "parameters": {
    "type": "object",
    "properties": {
      "search": {
        "type": "string",
        "description": "an optional search string for filtering results"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/agents{?search}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**arch**",
            "**client_id**",
            "**connected**",
            "**created_at**",
            "**deactivated_at**",
            "**external_ip**",
            "**host_id**",
            "**hub_id**",
            "**id**",
            "**inactive**",
            "**internal_ip**",
            "**last_checkin**",
            "**name**",
            "**organization_id**",
            "**os**",
            "**site_id**",
            "**system_info**",
            "**updated_at**",
            "**version**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
