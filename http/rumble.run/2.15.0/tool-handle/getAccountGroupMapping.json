{
  "name": "getAccountGroupMapping",
  "description": "Get SSO group mapping details",
  "parameters": {
    "type": "object",
    "properties": {
      "group_mapping_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the SSO group mapping"
      }
    },
    "required": [
      "group_mapping_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/sso/groups/{group_mapping_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**created_by_email**",
            "**description**",
            "**group_id**",
            "**group_name**",
            "**id**",
            "**sso_attribute**",
            "**sso_value**",
            "**updated_at**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
