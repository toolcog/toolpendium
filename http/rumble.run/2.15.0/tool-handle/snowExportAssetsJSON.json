{
  "name": "snowExportAssetsJSON",
  "description": "Exports the asset inventory as JSON",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/export/org/assets.servicenow.json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**addresses_extra**",
            "**addresses_scope**",
            "**alive**",
            "**asset_id**",
            "**comments**",
            "**detected_by**",
            "**domains**",
            "**first_discovered**",
            "**hw_product**",
            "**hw_vendor**",
            "**hw_version**",
            "**ip_address**",
            "**last_discovered**",
            "**last_updated**",
            "**lowest_rtt**",
            "**lowest_ttl**",
            "**mac_address**",
            "**mac_manufacturer**",
            "**mac_vendors**",
            "**macs**",
            "**name**",
            "**newest_mac_age**",
            "**organization**",
            "**os_product**",
            "**os_vendor**",
            "**os_version**",
            "**service_count**",
            "**service_count_arp**",
            "**service_count_icmp**",
            "**service_count_tcp**",
            "**service_count_udp**",
            "**site**",
            "**sys_class_name**",
            "**tags**",
            "**type**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
