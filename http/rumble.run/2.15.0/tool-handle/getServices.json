{
  "name": "getServices",
  "description": "Get all services",
  "parameters": {
    "type": "object",
    "properties": {
      "search": {
        "type": "string",
        "description": "an optional search string for filtering results"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/org/services{?search}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**addresses**",
            "**addresses_extra**",
            "**agent_name**",
            "**alive**",
            "**attributes**",
            "**comments**",
            "**created_at**",
            "**credentials**",
            "**detected_by**",
            "**domains**",
            "**first_seen**",
            "**hw**",
            "**id**",
            "**last_agent_id**",
            "**last_seen**",
            "**last_task_id**",
            "**lowest_rtt**",
            "**lowest_ttl**",
            "**mac_vendors**",
            "**macs**",
            "**names**",
            "**newest_mac**",
            "**newest_mac_age**",
            "**newest_mac_vendor**",
            "**org_name**",
            "**organization_id**",
            "**os**",
            "**os_version**",
            "**rtts**",
            "**service_address**",
            "**service_asset_id**",
            "**service_count**",
            "**service_count_arp**",
            "**service_count_icmp**",
            "**service_count_tcp**",
            "**service_count_udp**",
            "**service_created_at**",
            "**service_data**",
            "**service_id**",
            "**service_link**",
            "**service_port**",
            "**service_ports_products**",
            "**service_ports_protocols**",
            "**service_ports_tcp**",
            "**service_ports_udp**",
            "**service_protocol**",
            "**service_screenshot_link**",
            "**service_summary**",
            "**service_transport**",
            "**service_updated_at**",
            "**service_vhost**",
            "**services**",
            "**site_id**",
            "**site_name**",
            "**tags**",
            "**type**",
            "**updated_at**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
