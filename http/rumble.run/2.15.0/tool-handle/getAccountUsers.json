{
  "name": "getAccountUsers",
  "description": "Get all users",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/users"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**client_admin**",
            "**client_id**",
            "**created_at**",
            "**email**",
            "**first_name**",
            "**id**",
            "**invite_token_expiration**",
            "**last_login_at**",
            "**last_login_ip**",
            "**last_login_ua**",
            "**last_name**",
            "**login_failures**",
            "**org_default_role**",
            "**org_roles**",
            "**reset_token_expiration**",
            "**sso_only**",
            "**updated_at**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
