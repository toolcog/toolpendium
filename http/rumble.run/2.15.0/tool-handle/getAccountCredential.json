{
  "name": "getAccountCredential",
  "description": "Get credential details",
  "parameters": {
    "type": "object",
    "properties": {
      "credential_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the credential to retrieve"
      }
    },
    "required": [
      "credential_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/credentials/{credential_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**acl**",
            "**cidrs**",
            "**client_id**",
            "**created_at**",
            "**created_by_email**",
            "**created_by_id**",
            "**global**",
            "**id**",
            "**last_used_at**",
            "**last_used_by_id**",
            "**name**",
            "**type**: The service the credentials are for."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
