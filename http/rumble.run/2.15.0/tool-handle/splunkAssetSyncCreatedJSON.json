{
  "name": "splunkAssetSyncCreatedJSON",
  "description": "Exports the asset inventory in a sync-friendly manner using created_at as a checkpoint. Requires the Splunk entitlement.",
  "parameters": {
    "type": "object",
    "properties": {
      "search": {
        "type": "string",
        "description": "an optional search string for filtering results"
      },
      "fields": {
        "type": "string",
        "description": "an optional list of fields to export, comma-separated"
      },
      "since": {
        "example": 1576300370,
        "format": "int64",
        "type": "integer",
        "description": "an optional unix timestamp to use as a checkpoint"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/export/org/assets/sync/created/assets.json{?search,fields,since}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**assets**",
            "**since**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
