{
  "name": "exportEventsJSONL",
  "description": "System event log as JSON line-delimited",
  "parameters": {
    "type": "object",
    "properties": {
      "search": {
        "type": "string",
        "description": "an optional search string for filtering results"
      },
      "fields": {
        "type": "string",
        "description": "an optional list of fields to export, comma-separated"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/events.jsonl{?search,fields}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**action**",
            "**client_id**",
            "**created_at**",
            "**details**",
            "**id**",
            "**organization_id**",
            "**processed_at**",
            "**processor_id**",
            "**site_id**",
            "**source_id**",
            "**source_name**",
            "**source_type**",
            "**state**",
            "**success**",
            "**target_id**",
            "**target_name**",
            "**target_type**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
