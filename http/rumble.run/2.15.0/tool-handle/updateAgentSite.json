{
  "name": "updateAgentSite",
  "description": "Update the site associated with agent",
  "parameters": {
    "type": "object",
    "properties": {
      "agent_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the agent to update"
      },
      "body": {
        "$ref": "#/components/schemas/AgentSiteID",
        "description": "site_id to associate with the agent"
      }
    },
    "required": [
      "agent_id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/org/agents/{agent_id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**arch**",
            "**client_id**",
            "**connected**",
            "**created_at**",
            "**deactivated_at**",
            "**external_ip**",
            "**host_id**",
            "**hub_id**",
            "**id**",
            "**inactive**",
            "**internal_ip**",
            "**last_checkin**",
            "**name**",
            "**organization_id**",
            "**os**",
            "**site_id**",
            "**system_info**",
            "**updated_at**",
            "**version**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
