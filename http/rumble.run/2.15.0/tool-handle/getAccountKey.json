{
  "name": "getAccountKey",
  "description": "Get key details",
  "parameters": {
    "type": "object",
    "properties": {
      "key_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the key to retrieve"
      }
    },
    "required": [
      "key_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/keys/{key_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**client_id**",
            "**comment**",
            "**counter**",
            "**created_at**",
            "**created_by**",
            "**id**",
            "**inactive**",
            "**last_used_at**",
            "**last_used_ip**",
            "**last_used_ua**",
            "**organization_id**",
            "**token**",
            "**type**",
            "**usage_limit**",
            "**usage_today**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
