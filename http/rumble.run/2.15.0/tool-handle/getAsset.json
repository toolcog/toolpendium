{
  "name": "getAsset",
  "description": "Get asset details",
  "parameters": {
    "type": "object",
    "properties": {
      "asset_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the asset to retrieve"
      }
    },
    "required": [
      "asset_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/org/assets/{asset_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**addresses**",
            "**addresses_extra**",
            "**agent_name**",
            "**alive**",
            "**attributes**",
            "**comments**",
            "**created_at**",
            "**credentials**",
            "**detected_by**",
            "**domains**",
            "**first_seen**",
            "**hw**",
            "**id**",
            "**last_agent_id**",
            "**last_seen**",
            "**last_task_id**",
            "**lowest_rtt**",
            "**lowest_ttl**",
            "**mac_vendors**",
            "**macs**",
            "**names**",
            "**newest_mac**",
            "**newest_mac_age**",
            "**newest_mac_vendor**",
            "**org_name**",
            "**organization_id**",
            "**os**",
            "**os_version**",
            "**rtts**",
            "**service_count**",
            "**service_count_arp**",
            "**service_count_icmp**",
            "**service_count_tcp**",
            "**service_count_udp**",
            "**service_ports_products**",
            "**service_ports_protocols**",
            "**service_ports_tcp**",
            "**service_ports_udp**",
            "**services**",
            "**site_id**",
            "**site_name**",
            "**tags**",
            "**type**",
            "**updated_at**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
