{
  "name": "createAccountUserInvite",
  "description": "Create a new user account and send an email invite",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/UserInviteOptions",
        "description": "user invite parameters"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/users/invite"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**client_admin**",
            "**client_id**",
            "**created_at**",
            "**email**",
            "**first_name**",
            "**id**",
            "**invite_token_expiration**",
            "**last_login_at**",
            "**last_login_ip**",
            "**last_login_ua**",
            "**last_name**",
            "**login_failures**",
            "**org_default_role**",
            "**org_roles**",
            "**reset_token_expiration**",
            "**sso_only**",
            "**updated_at**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
