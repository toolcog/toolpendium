{
  "name": "resetAccountUserPassword",
  "description": "Sends the user a password reset email",
  "parameters": {
    "type": "object",
    "properties": {
      "user_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the user to retrieve"
      }
    },
    "required": [
      "user_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/users/{user_id}/resetPassword"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**client_admin**",
            "**client_id**",
            "**created_at**",
            "**email**",
            "**first_name**",
            "**id**",
            "**invite_token_expiration**",
            "**last_login_at**",
            "**last_login_ip**",
            "**last_login_ua**",
            "**last_name**",
            "**login_failures**",
            "**org_default_role**",
            "**org_roles**",
            "**reset_token_expiration**",
            "**sso_only**",
            "**updated_at**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
