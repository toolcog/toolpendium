{
  "name": "getAccountKeys",
  "description": "Get all active API keys",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/keys"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**client_id**",
            "**comment**",
            "**counter**",
            "**created_at**",
            "**created_by**",
            "**id**",
            "**inactive**",
            "**last_used_at**",
            "**last_used_ip**",
            "**last_used_ua**",
            "**organization_id**",
            "**token**",
            "**type**",
            "**usage_limit**",
            "**usage_today**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
