{
  "name": "getAccountLicense",
  "description": "Get license details",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/license"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**activated_at**",
            "**address_city**",
            "**address_country**",
            "**address_postal**",
            "**address_region**",
            "**address_street**",
            "**created_at**",
            "**id**",
            "**license_expiration**",
            "**license_live_asset_count**",
            "**license_max_assets**",
            "**license_project_asset_count**",
            "**license_type**",
            "**name**",
            "**partner**",
            "**phone**",
            "**settings**",
            "**sso_default_role**",
            "**sso_domain**",
            "**sso_login_issuer_url**",
            "**sso_login_login_url**",
            "**sso_login_logout_url**",
            "**sso_login_message**",
            "**sso_mode**",
            "**sso_type**",
            "**subscription_cancel_at**",
            "**subscription_canceled_at**",
            "**subscription_period_end**",
            "**subscription_period_start**",
            "**updated_at**",
            "**via_reseller**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
