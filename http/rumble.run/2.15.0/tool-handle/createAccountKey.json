{
  "name": "createAccountKey",
  "description": "Create a new key",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/APIKeyOptions",
        "description": "key parameters"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/account/keys"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**client_id**",
            "**comment**",
            "**counter**",
            "**created_at**",
            "**created_by**",
            "**id**",
            "**inactive**",
            "**last_used_at**",
            "**last_used_ip**",
            "**last_used_ua**",
            "**organization_id**",
            "**token**",
            "**type**",
            "**usage_limit**",
            "**usage_today**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
