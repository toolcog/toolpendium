{
  "name": "exportWirelessJSON",
  "description": "Wireless inventory as JSON",
  "parameters": {
    "type": "object",
    "properties": {
      "search": {
        "type": "string",
        "description": "an optional search string for filtering results"
      },
      "fields": {
        "type": "string",
        "description": "an optional list of fields to export, comma-separated"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/export/org/wireless.json{?search,fields}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**agent_name**",
            "**authentication**",
            "**bssid**",
            "**channels**",
            "**created_at**",
            "**data**",
            "**encryption**",
            "**essid**",
            "**family**",
            "**id**",
            "**interface**",
            "**last_agent_id**",
            "**last_seen**",
            "**last_task_id**",
            "**org_name**",
            "**organization_id**",
            "**signal**",
            "**site_id**",
            "**site_name**",
            "**type**",
            "**vendor**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
