{
  "name": "getTaskChangeReport",
  "description": "Returns a temporary task change report data url",
  "parameters": {
    "type": "object",
    "properties": {
      "task_id": {
        "format": "uuid",
        "type": "string",
        "description": "UUID of the task"
      }
    },
    "required": [
      "task_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://console.rumble.run/api/v1.0/org/tasks/{task_id}/changes"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A URL to a resource",
        "**Key properties:**",
        {
          "$ul": [
            "**id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
