{
  "name": "get_teams_teamIdOrSlug_matches",
  "description": "List matches for the given team",
  "parameters": {
    "type": "object",
    "properties": {
      "team_id_or_slug": {
        "$ref": "#/components/schemas/TeamIDOrSlug",
        "description": "A team ID or slug"
      },
      "filter": {
        "$ref": "#/components/schemas/filter_over_Matches",
        "description": "Options to filter results. String fields are case sensitive"
      },
      "search": {
        "$ref": "#/components/schemas/search_over_Matches",
        "description": "Options to search results"
      },
      "sort": {
        "$ref": "#/components/schemas/sort_over_Matches",
        "description": "Options to sort results"
      },
      "range": {
        "$ref": "#/components/schemas/range_over_Matches",
        "description": "Options to select results within ranges"
      },
      "page": {
        "oneOf": [
          {
            "default": 1,
            "minimum": 1,
            "type": "integer"
          },
          {
            "additionalProperties": false,
            "properties": {
              "number": {
                "default": 1,
                "minimum": 1,
                "type": "integer"
              },
              "size": {
                "default": 50,
                "maximum": 100,
                "minimum": 1,
                "type": "integer"
              }
            },
            "type": "object"
          }
        ],
        "description": "Pagination in the form of `page=2` or `page[size]=30&amp;page[number]=2`"
      },
      "per_page": {
        "default": 50,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Equivalent to `page[size]`"
      }
    },
    "required": [
      "team_id_or_slug"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.pandascore.co//teams/{team_id_or_slug}/matches{?filter,search,sort,range,page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**begin_at**",
            "**detailed_stats**: Whether the match offers full stats",
            "**draw**: Whether result of the match is a draw",
            "**end_at**",
            "**forfeit**: Whether match was forfeited",
            "**game_advantage**",
            "**games**",
            "**id**",
            [
              "**league**",
              {
                "$ul": [
                  "**id**",
                  "**image_url**",
                  "**modified_at**",
                  "**name**",
                  "**slug**",
                  "**url**"
                ]
              }
            ],
            "**league_id**",
            [
              "**live**",
              {
                "$ul": [
                  "**opens_at**",
                  "**supported**: Whether live is supported",
                  "**url**"
                ]
              }
            ],
            "**live_embed_url**",
            "**match_type**",
            "**modified_at**",
            "**name**",
            "**number_of_games**: Number of games",
            "**official_stream_url**",
            "**opponents**",
            "**original_scheduled_at**",
            "**rescheduled**",
            "**results**",
            "**scheduled_at**",
            [
              "**serie**",
              {
                "$ul": [
                  "**begin_at**",
                  "**description**",
                  "**end_at**",
                  "**full_name**",
                  "**id**",
                  "**league_id**",
                  "**modified_at**",
                  "**name**",
                  "**season**",
                  "**slug**",
                  "**tier**",
                  "**winner_id**",
                  "**winner_type**",
                  "**year**"
                ]
              }
            ],
            "**serie_id**",
            "**slug**",
            "**status**",
            [
              "**streams**",
              {
                "$ul": [
                  [
                    "**english**",
                    {
                      "$ul": [
                        "**embed_url**",
                        "**raw_url**"
                      ]
                    }
                  ],
                  [
                    "**official**",
                    {
                      "$ul": [
                        "**embed_url**",
                        "**raw_url**"
                      ]
                    }
                  ],
                  [
                    "**russian**",
                    {
                      "$ul": [
                        "**embed_url**",
                        "**raw_url**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**streams_list**",
            [
              "**tournament**",
              {
                "$ul": [
                  "**begin_at**",
                  "**end_at**",
                  "**id**",
                  "**league_id**",
                  "**live_supported**: Whether live is supported",
                  "**modified_at**",
                  "**name**",
                  "**prizepool**",
                  "**serie_id**",
                  "**slug**",
                  "**winner_id**",
                  "**winner_type**"
                ]
              }
            ],
            "**tournament_id**",
            "**videogame**",
            "**videogame_version**",
            "**winner**",
            "**winner_id**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
