{
  "name": "get_leagues",
  "description": "List leagues",
  "parameters": {
    "type": "object",
    "properties": {
      "search": {
        "$ref": "#/components/schemas/search_over_Leagues",
        "description": "Options to search results"
      },
      "sort": {
        "$ref": "#/components/schemas/sort_over_Leagues",
        "description": "Options to sort results"
      },
      "range": {
        "$ref": "#/components/schemas/range_over_Leagues",
        "description": "Options to select results within ranges"
      },
      "filter": {
        "$ref": "#/components/schemas/filter_over_Leagues",
        "description": "Options to filter results. String fields are case sensitive"
      },
      "page": {
        "oneOf": [
          {
            "default": 1,
            "minimum": 1,
            "type": "integer"
          },
          {
            "additionalProperties": false,
            "properties": {
              "number": {
                "default": 1,
                "minimum": 1,
                "type": "integer"
              },
              "size": {
                "default": 50,
                "maximum": 100,
                "minimum": 1,
                "type": "integer"
              }
            },
            "type": "object"
          }
        ],
        "description": "Pagination in the form of `page=2` or `page[size]=30&amp;page[number]=2`"
      },
      "per_page": {
        "default": 50,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Equivalent to `page[size]`"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.pandascore.co//leagues{?search,sort,range,filter,page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**image_url**",
            "**modified_at**",
            "**name**",
            "**series**",
            "**slug**",
            "**url**",
            "**videogame**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
