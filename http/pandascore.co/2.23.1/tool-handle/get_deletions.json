{
  "name": "get_deletions",
  "description": "Get the latest deleted documents",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "oneOf": [
          {
            "default": 1,
            "minimum": 1,
            "type": "integer"
          },
          {
            "additionalProperties": false,
            "properties": {
              "number": {
                "default": 1,
                "minimum": 1,
                "type": "integer"
              },
              "size": {
                "default": 50,
                "maximum": 100,
                "minimum": 1,
                "type": "integer"
              }
            },
            "type": "object"
          }
        ],
        "description": "Pagination in the form of `page=2` or `page[size]=30&amp;page[number]=2`"
      },
      "per_page": {
        "default": 50,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Equivalent to `page[size]`"
      },
      "type": {
        "$ref": "#/components/schemas/type_over_DeletionIncidents",
        "description": "Filter by result type(s)"
      },
      "since": {
        "$ref": "#/components/schemas/IncidentModifiedAt",
        "description": "Filter out older results"
      },
      "videogame": {
        "$ref": "#/components/schemas/videogame_over_DeletionIncidents",
        "description": "Filter by videogame(s)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.pandascore.co//deletions{?page,per_page,type,since,videogame}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**change_type**",
            "**id**: An incident ID",
            "**modified_at**",
            [
              "**object**",
              {
                "$ul": [
                  "**deleted_at**",
                  "**reason**",
                  "**videogame_id**: A videogame ID"
                ]
              }
            ],
            "**type**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
