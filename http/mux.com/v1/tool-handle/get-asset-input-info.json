{
  "name": "get-asset-input-info",
  "description": "Returns a list of the input objects that were used to create the asset along with any settings that were applied to each input.",
  "parameters": {
    "type": "object",
    "properties": {
      "ASSET_ID": {
        "type": "string",
        "description": "The asset ID."
      }
    },
    "required": [
      "ASSET_ID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mux.com/video/v1/assets/{ASSET_ID}/input-info"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
