{
  "name": "delete-space-broadcast",
  "description": "Deletes a single broadcast of a specific space. Broadcasts can only be deleted when `idle`.",
  "parameters": {
    "type": "object",
    "properties": {
      "SPACE_ID": {
        "type": "string",
        "description": "The space ID."
      },
      "BROADCAST_ID": {
        "type": "string",
        "description": "The broadcast ID."
      }
    },
    "required": [
      "SPACE_ID",
      "BROADCAST_ID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.mux.com/video/v1/spaces/{SPACE_ID}/broadcasts/{BROADCAST_ID}"
    }
  },
  "responses": {}
}
