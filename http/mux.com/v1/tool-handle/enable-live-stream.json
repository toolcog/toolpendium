{
  "name": "enable-live-stream",
  "description": "Enables a live stream, allowing it to accept an incoming RTMP stream.",
  "parameters": {
    "type": "object",
    "properties": {
      "LIVE_STREAM_ID": {
        "type": "string",
        "description": "The live stream ID"
      }
    },
    "required": [
      "LIVE_STREAM_ID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.mux.com/video/v1/live-streams/{LIVE_STREAM_ID}/enable"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
