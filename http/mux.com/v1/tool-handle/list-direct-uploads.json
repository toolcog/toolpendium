{
  "name": "list-direct-uploads",
  "description": "Lists direct uploads in the current environment.",
  "parameters": {
    "type": "object",
    "properties": {
      "limit": {
        "default": 25,
        "format": "int32",
        "type": "integer",
        "description": "Number of items to include in the response"
      },
      "page": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "Offset by this many pages, of the size of `limit`"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mux.com/video/v1/uploads{?limit,page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
