{
  "name": "get-asset-or-livestream-id",
  "description": "Retrieves the Identifier of the Asset or Live Stream associated with the Playback ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "PLAYBACK_ID": {
        "type": "string",
        "description": "The live stream's playback ID."
      }
    },
    "required": [
      "PLAYBACK_ID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mux.com/video/v1/playback-ids/{PLAYBACK_ID}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**id**: The Playback ID used to retrieve the corresponding asset or the live stream ID",
                  [
                    "**object**: Describes the Asset or LiveStream object associated with the playback ID.",
                    {
                      "$ul": [
                        "**id**: The identifier of the object.",
                        "**type**: Identifies the object type associated with the playback ID."
                      ]
                    }
                  ],
                  "**policy**: * `public` playback IDs are accessible by constructing an HLS URL like `https://stream.mux.com/${PLAYBACK_ID}`"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
