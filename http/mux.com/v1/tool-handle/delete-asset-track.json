{
  "name": "delete-asset-track",
  "description": "Removes a text track from an asset. Audio and video tracks on assets cannot be removed.",
  "parameters": {
    "type": "object",
    "properties": {
      "ASSET_ID": {
        "type": "string",
        "description": "The asset ID."
      },
      "TRACK_ID": {
        "type": "string",
        "description": "The track ID."
      }
    },
    "required": [
      "ASSET_ID",
      "TRACK_ID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.mux.com/video/v1/assets/{ASSET_ID}/tracks/{TRACK_ID}"
    }
  },
  "responses": {}
}
