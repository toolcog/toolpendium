{
  "name": "list-assets",
  "description": "List all Mux assets.",
  "parameters": {
    "type": "object",
    "properties": {
      "limit": {
        "default": 25,
        "format": "int32",
        "type": "integer",
        "description": "Number of items to include in the response"
      },
      "page": {
        "default": 1,
        "format": "int32",
        "type": "integer",
        "description": "Offset by this many pages, of the size of `limit`"
      },
      "live_stream_id": {
        "type": "string",
        "description": "Filter response to return all the assets for this live stream only"
      },
      "upload_id": {
        "type": "string",
        "description": "Filter response to return an asset created from this direct upload only"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.mux.com/video/v1/assets{?limit,page,live_stream_id,upload_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
