{
  "name": "Release_GetRelease",
  "description": "Gets a Release by ID. When successful, the response is the requested Release.\r\n            If unsuccessful, an appropriate ApiError is returned.",
  "parameters": {
    "type": "object",
    "properties": {
      "ReleaseId": {
        "format": "int32",
        "type": "integer",
        "description": "The ID of the Release to get."
      }
    },
    "required": [
      "ReleaseId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://secure.agco-ats.com/api/v2/Releases/{ReleaseId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Release class",
        "**Key properties:**",
        {
          "$ul": [
            "**BuildDate**: Build Date",
            "**BundleIDs**: IDs of AUC Bundles associated with this Release.",
            "**ReleaseDate**: Release Date",
            "**ReleaseID**: Release ID",
            "**ReleaseNumber**: Release Number",
            "**Visible**: Visible"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DeveloperMessage**",
            "**ErrorCode**",
            "**MoreInfo**",
            "**UserMessage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
