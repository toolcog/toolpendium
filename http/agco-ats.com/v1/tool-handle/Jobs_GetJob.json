{
  "name": "Jobs_GetJob",
  "description": "Gets a Job by ID. When successful, the response is the requested Job.\r\n            If unsuccessful, an appropriate ApiError is returned.",
  "parameters": {
    "type": "object",
    "properties": {
      "jobID": {
        "format": "int32",
        "type": "integer",
        "description": "The ID of the Job to get."
      },
      "isIncludeDeleted": {
        "type": "boolean",
        "description": "Does it include deleted job, or not"
      }
    },
    "required": [
      "jobID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://secure.agco-ats.com/api/v2/jobs/{jobID}{?isIncludeDeleted}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A DTO for an IJob",
        "**Key properties:**",
        {
          "$ul": [
            "**Activities**: The activities which are performed for the job",
            "**Deleted**: Indicates if the job has been deleted.",
            "**JobID**: The ID of the job",
            "**Name**: The name of the job",
            "**Parameters**: The parameters for the job"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DeveloperMessage**",
            "**ErrorCode**",
            "**MoreInfo**",
            "**UserMessage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
