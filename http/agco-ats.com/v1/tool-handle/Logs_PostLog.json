{
  "name": "Logs_PostLog",
  "description": "No Documentation Found.",
  "parameters": {
    "type": "object",
    "properties": {
      "Message": {
        "type": "string",
        "description": "Message to enter into the log"
      }
    },
    "required": [
      "Message"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://secure.agco-ats.com/api/v2/Logs{?Message}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DeveloperMessage**",
            "**ErrorCode**",
            "**MoreInfo**",
            "**UserMessage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
