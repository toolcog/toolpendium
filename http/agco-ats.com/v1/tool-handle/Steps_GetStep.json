{
  "name": "Steps_GetStep",
  "description": "Gets a Step by ID. When successful, the response is the requested Step.\r\n            If unsuccessful, an appropriate ApiError is returned.  Steps.Read permission is required.",
  "parameters": {
    "type": "object",
    "properties": {
      "stepID": {
        "format": "int32",
        "type": "integer",
        "description": "The ID of the Step to get."
      },
      "isIncludeDeleted": {
        "type": "boolean",
        "description": "Does it include deleted step, or not"
      }
    },
    "required": [
      "stepID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://secure.agco-ats.com/api/v2/steps/{stepID}{?isIncludeDeleted}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Step",
        "**Key properties:**",
        {
          "$ul": [
            "**ConfigRequired**: Indicates if the step requires configuration values to be provided by the build agent",
            "**Deleted**: Read Only.  Indicates if the record is deleted.",
            "**Description**: A description of the step to be presented to a user",
            "**ImplementationID**: The implementation ID used to lookup the step implementation when it is executed",
            "**Name**: The name of the step",
            "**Parameters**: The parameters for this step",
            "**StepID**: The ID of the step"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DeveloperMessage**",
            "**ErrorCode**",
            "**MoreInfo**",
            "**UserMessage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
