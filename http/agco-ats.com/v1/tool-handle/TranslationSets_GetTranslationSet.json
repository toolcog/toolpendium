{
  "name": "TranslationSets_GetTranslationSet",
  "description": "No Documentation Found.",
  "parameters": {
    "type": "object",
    "properties": {
      "ID": {
        "format": "int32",
        "type": "integer",
        "description": ""
      },
      "includeAttributes": {
        "type": "string",
        "description": "Names of Attributes to include when retrieving this Translation set. This should be a comma-separated list. If not provided, Attributes are not included. If '*', all Attributes are included."
      }
    },
    "required": [
      "ID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://secure.agco-ats.com/api/v2/TranslationSets/{ID}{?includeAttributes}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A set of strings submitted for translation",
        "**Key properties:**",
        {
          "$ul": [
            "**Attributes**: Attributes of the Translation Set",
            "**FileIDs**: IDs for files related to this translation set. For example, the original and processed files",
            "**Id**: The id of the TranslationSet.",
            "**InDate**: Read Only. The date the translation set was returned.",
            "**Notes**: Notes on the TranslationSet",
            "**OutDate**: Read Only. The date the translation set was sent out.",
            "**State**: An enum indicating the state of the translation set",
            "**TranslationRequestID**: Read Only. The Id of the TranslationRequest which generated this translation set."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**DeveloperMessage**",
            "**ErrorCode**",
            "**MoreInfo**",
            "**UserMessage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
