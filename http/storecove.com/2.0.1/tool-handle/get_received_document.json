{
  "name": "get_received_document",
  "description": "EXPERIMENTAL: use only for orders. Get a new ReceivedDocument.",
  "parameters": {
    "type": "object",
    "properties": {
      "guid": {
        "format": "uuid",
        "type": "string",
        "description": "The guid of the document that was received. This is the \"document_guid\" property of the \"received_document\" webhook."
      },
      "syntax": {
        "default": "json",
        "enum": [
          "json",
          "original"
        ],
        "type": "string",
        "description": "The syntax in which to receive the received document."
      },
      "format": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "guid",
      "syntax",
      "format"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.storecove.com/api/v2/received_documents/{guid}/{format}{?syntax}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**guid**: The GUID of the received document"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
