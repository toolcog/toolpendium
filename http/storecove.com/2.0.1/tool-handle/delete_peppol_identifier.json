{
  "name": "delete_peppol_identifier",
  "description": "Delete a PeppolIdentifier.",
  "parameters": {
    "type": "object",
    "properties": {
      "legal_entity_id": {
        "format": "int64",
        "type": "integer",
        "description": "The id of the LegalEntity this PeppolIdentifier belongs to"
      },
      "superscheme": {
        "type": "string",
        "description": "The superscheme of the identifier. Should always be \"iso6523-actorid-upis\"."
      },
      "scheme": {
        "type": "string",
        "description": "PEPPOL identifier scheme id, e.g. \"DE:VAT\". For a full list see <<_receiver_identifiers_list>>."
      },
      "identifier": {
        "type": "string",
        "description": "PEPPOL identifier"
      }
    },
    "required": [
      "legal_entity_id",
      "superscheme",
      "scheme",
      "identifier"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.storecove.com/api/v2/legal_entities/{legal_entity_id}/peppol_identifiers/{superscheme}/{scheme}/{identifier}"
    }
  },
  "responses": {}
}
