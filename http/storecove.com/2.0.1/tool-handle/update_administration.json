{
  "name": "update_administration",
  "description": "Deprecated. Update an Administration",
  "parameters": {
    "type": "object",
    "properties": {
      "legal_entity_id": {
        "format": "int64",
        "type": "integer",
        "description": "The id of the LegalEntity the Administration belongs to"
      },
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "The id of the Administration to be updated"
      },
      "body": {
        "$ref": "#/components/schemas/AdministrationUpdate",
        "description": "Administration to update"
      }
    },
    "required": [
      "legal_entity_id",
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.storecove.com/api/v2/legal_entities/{legal_entity_id}/administrations/{id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**email**: The email address to send the received document to",
            "**id**: The Storecove assigned id for the Administration.",
            "**legal_entity_id**: The LegalEntity the Administration belongs to.",
            "**package_version**: The version of the package.",
            "**packaging**: How to package the purchase invoice.",
            "**sender_email_identity_id**: The id of the SenderEmailIdentity. If not provided, the Storecove default sender will be used"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
