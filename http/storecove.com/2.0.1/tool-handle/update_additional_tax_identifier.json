{
  "name": "update_additional_tax_identifier",
  "description": "Update an AdditionalTaxIdentifier",
  "parameters": {
    "type": "object",
    "properties": {
      "legal_entity_id": {
        "format": "int64",
        "type": "integer",
        "description": "The id of the LegalEntity the AdditionalTaxIdentifier belongs to"
      },
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "The id of the AdditionalTaxIdentifier to be updated"
      },
      "body": {
        "$ref": "#/components/schemas/AdditionalTaxIdentifierUpdate",
        "description": "AdditionalTaxIdentifier to update"
      }
    },
    "required": [
      "legal_entity_id",
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.storecove.com/api/v2/legal_entities/{legal_entity_id}/additional_tax_identifiers/{id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**country**: The ISO3166 country code to use this identifier for in case of consumerTaxMode.",
            "**county**: The county/state inside the country code to use this identifier for in case of consumerTaxMode.",
            "**id**: The Storecove assigned id for the AdditionalTaxIdentifier.",
            "**identifier**: The identifier.",
            "**scheme**: The scheme of the identifier.",
            "**superscheme**: The superscheme of the identifier."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
