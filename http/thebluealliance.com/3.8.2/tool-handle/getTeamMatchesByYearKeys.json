{
  "name": "getTeamMatchesByYearKeys",
  "description": "Gets a list of match keys for matches for the given team and year.",
  "parameters": {
    "type": "object",
    "properties": {
      "If-None-Match": {
        "type": "string",
        "description": "Value of the `ETag` header in the most recently cached response by the client."
      },
      "team_key": {
        "type": "string",
        "description": "TBA Team Key, eg `frc254`"
      },
      "year": {
        "type": "integer",
        "description": "Competition Year (or Season). Must be 4 digits."
      }
    },
    "required": [
      "team_key",
      "year"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.thebluealliance.com/api/v3/team/{team_key}/matches/{year}/keys"
    },
    "headers": {
      "If-None-Match": {
        "$": "If-None-Match"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Array of Match Keys",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
