{
  "name": "getTeamsByYearKeys",
  "description": "Gets a list Team Keys that competed in the given year, paginated in groups of 500.",
  "parameters": {
    "type": "object",
    "properties": {
      "If-None-Match": {
        "type": "string",
        "description": "Value of the `ETag` header in the most recently cached response by the client."
      },
      "year": {
        "type": "integer",
        "description": "Competition Year (or Season). Must be 4 digits."
      },
      "page_num": {
        "type": "integer",
        "description": "Page number of results to return, zero-indexed"
      }
    },
    "required": [
      "year",
      "page_num"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://www.thebluealliance.com/api/v3/teams/{year}/{page_num}/keys"
    },
    "headers": {
      "If-None-Match": {
        "$": "If-None-Match"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Array of Team Keys",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
