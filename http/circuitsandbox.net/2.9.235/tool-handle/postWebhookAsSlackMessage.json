{
  "name": "postWebhookAsSlackMessage",
  "description": "Post text items to conversations via slack apps.",
  "parameters": {
    "type": "object",
    "properties": {
      "webhookId": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the webhook."
      },
      "body": {
        "$ref": "#/components/schemas/IncomingWebhookSlackMessage",
        "description": "Attached JSON object in slack format."
      }
    },
    "required": [
      "webhookId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/webhooks/incoming/{webhookId}"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
