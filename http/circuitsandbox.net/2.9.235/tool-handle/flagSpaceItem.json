{
  "name": "flagSpaceItem",
  "description": "flag a space item\nOauthScopes: WRITE_SPACE, UPDATE_SPACE_CONTENT",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "maxLength": 2048,
        "type": "string",
        "description": "the id of the item you want to flag"
      }
    },
    "required": [
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/flag/{itemId}"
    }
  },
  "responses": {}
}
