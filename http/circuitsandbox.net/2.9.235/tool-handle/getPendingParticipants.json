{
  "name": "getPendingParticipants",
  "description": "Get the pending participants of a space\nOauthScopes: WRITE_SPACE, MANAGE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the space."
      },
      "searchPointer": {
        "maxLength": 2048,
        "type": "string",
        "description": "The search pointer (leave empty initially)."
      },
      "numberOfResults": {
        "default": 25,
        "format": "int32",
        "maximum": 255,
        "minimum": 0,
        "type": "number",
        "description": "number of results to return, 25 by default."
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/participants/pending{?searchPointer,numberOfResults}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**hasMore**: are more results available, than start another search",
            "**participants**: list of participants",
            "**searchPointer**: pointer for stored search "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
