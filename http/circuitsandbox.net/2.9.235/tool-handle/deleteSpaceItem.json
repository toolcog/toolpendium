{
  "name": "deleteSpaceItem",
  "description": "deletes a space item\nOauthScopes: WRITE_SPACE, DELETE_SPACE_CONTENT",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "maxLength": 2048,
        "type": "string",
        "description": "the id of the spaceItem"
      }
    },
    "required": [
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/item/{itemId}"
    }
  },
  "responses": {}
}
