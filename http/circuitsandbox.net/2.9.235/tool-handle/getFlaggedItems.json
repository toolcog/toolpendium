{
  "name": "getFlaggedItems",
  "description": "Get flagged items\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "searchDirection": {
        "default": "BEFORE",
        "enum": [
          "BEFORE",
          "AFTER"
        ],
        "maxLength": 2048,
        "type": "string",
        "description": "before or after the time stamp"
      },
      "timestamp": {
        "format": "date-time",
        "type": "string",
        "description": "The timestamp according to which you want to retrieve the flagged items"
      },
      "searchPointer": {
        "maxLength": 2048,
        "type": "string",
        "description": "The searchpointer for the search (initially not set)."
      },
      "numberOfResults": {
        "default": 25,
        "format": "int32",
        "maximum": 255,
        "minimum": 0,
        "type": "number",
        "description": "The number of results you want to retrieve."
      }
    },
    "required": [
      "searchDirection",
      "timestamp"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/flagged{?searchDirection,timestamp,searchPointer,numberOfResults}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**flaggedItems**: A list of flagged items",
            "**hasMore**: More results available (true-use searchpointer for next search/false)",
            "**searchPointer**: The searchpointer"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
