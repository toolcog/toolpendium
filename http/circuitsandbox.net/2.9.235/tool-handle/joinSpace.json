{
  "name": "joinSpace",
  "description": "Join a space\nOauthScopes: WRITE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the space"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/join"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
