{
  "name": "startBasicSpacesSearch",
  "description": "starts a basic search in spaces\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "scope": {
        "enum": [
          "ALL",
          "SPACES",
          "TOPICBY",
          "FILES",
          "TAGS",
          "LABELS",
          "DATE"
        ],
        "maxLength": 2048,
        "type": "string",
        "description": "the scope of the search"
      },
      "searchTerm": {
        "maxLength": 2048,
        "type": "string",
        "description": "the term to search for"
      },
      "startTime": {
        "format": "date-time",
        "type": "string",
        "description": "the starttime"
      },
      "endTime": {
        "format": "date-time",
        "type": "string",
        "description": "the end time"
      },
      "prioritySpaces": {
        "items": {
          "maxLength": 2048,
          "type": "string"
        },
        "type": "array",
        "description": "list of prioritized spaces"
      }
    },
    "required": [
      "scope",
      "searchTerm"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/search/startBasic{?scope,searchTerm,startTime,endTime,prioritySpaces}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**spaces**: List of spaces",
            "**status**: The status of the search (NORESULT, ERROR, TIMEOUT, MORERESULT)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
