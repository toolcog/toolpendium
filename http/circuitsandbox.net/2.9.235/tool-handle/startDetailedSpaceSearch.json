{
  "name": "startDetailedSpaceSearch",
  "description": "starts a detailed search in a space\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "scope": {
        "enum": [
          "ALL",
          "SPACES",
          "TOPICBY",
          "FILES",
          "TAGS",
          "LABELS",
          "DATE"
        ],
        "maxLength": 2048,
        "type": "string",
        "description": "the scope of the search"
      },
      "searchTerm": {
        "maxLength": 2048,
        "type": "string",
        "description": "the term to search for"
      },
      "startTime": {
        "format": "date-time",
        "type": "string",
        "description": "the starttime"
      },
      "endTime": {
        "format": "date-time",
        "type": "string",
        "description": "the end time"
      },
      "spaceId": {
        "maxLength": 2048,
        "type": "string",
        "description": "missing documentation"
      },
      "searchId": {
        "maxLength": 2048,
        "type": "string",
        "description": "missing documentation"
      }
    },
    "required": [
      "scope",
      "searchTerm",
      "spaceId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/search/startDetailed{?scope,searchTerm,startTime,endTime,spaceId,searchId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**resList**: A list of v2restspacesearchresultdetailedback containing spaceId, item ids and file ids"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
