{
  "name": "assignLabels",
  "description": "Assign labels to space\nOauthScopes: WRITE_SPACE, ORGANIZE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the space."
      },
      "body": {
        "properties": {
          "labels": {
            "description": "The labels to assign to the space",
            "items": {
              "maxLength": 2048,
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "labels"
        ],
        "type": "object"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/labels/assign"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**labelIds**: A list of label ids."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
