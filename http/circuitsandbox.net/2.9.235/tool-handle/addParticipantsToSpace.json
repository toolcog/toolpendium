{
  "name": "addParticipantsToSpace",
  "description": "Add a participant to a space\nOauthScopes: WRITE_SPACE, MANAGE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the space"
      },
      "body": {
        "properties": {
          "role": {
            "default": "DEFAULT",
            "description": "The name of the role of the participant",
            "enum": [
              "DEFAULT",
              "MODERATOR",
              "AUTHOR",
              "PARTICIPANT",
              "READER"
            ],
            "maxLength": 2048,
            "type": "string"
          },
          "userId": {
            "description": "The user id of the participant",
            "items": {
              "maxLength": 2048,
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "userId",
          "role"
        ],
        "type": "object"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/participant"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
