{
  "name": "requestSpaceAcces",
  "description": "request access for a space\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "spaceId": {
        "maxLength": 2048,
        "type": "string",
        "description": "Id of the space"
      },
      "body": {
        "properties": {
          "reason": {
            "description": "Reason why the Access has been requested",
            "maxLength": 2048,
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "spaceId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{spaceId}/participant/request"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
