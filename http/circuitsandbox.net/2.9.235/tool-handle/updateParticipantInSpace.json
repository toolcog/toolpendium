{
  "name": "updateParticipantInSpace",
  "description": "Update participant in space\nOauthScopes: WRITE_SPACE, MANAGE_SPACE, ORGANIZE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "spaceId": {
        "maxLength": 2048,
        "type": "string",
        "description": "Id of the space"
      },
      "body": {
        "properties": {
          "role": {
            "description": "updated role of participant",
            "enum": [
              "MODERATOR",
              "AUTHOR",
              "PARTICIPANT",
              "READER"
            ],
            "maxLength": 2048,
            "type": "string"
          },
          "userId": {
            "description": "The id of the participant to update",
            "maxLength": 2048,
            "type": "string"
          }
        },
        "required": [
          "userId",
          "role"
        ],
        "type": "object"
      }
    },
    "required": [
      "spaceId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{spaceId}/participant"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
