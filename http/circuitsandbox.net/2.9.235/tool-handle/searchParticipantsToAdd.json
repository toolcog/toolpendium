{
  "name": "searchParticipantsToAdd",
  "description": "Finds participants to add to a space \nOauthScopes: WRITE_SPACE, MANAGE_SPACE, ORGANIZE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the space"
      },
      "query": {
        "maxLength": 2048,
        "type": "string",
        "description": "The query "
      }
    },
    "required": [
      "id",
      "query"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/searchParticipantsToAdd{?query}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**department**: The department of the participant",
            "**firstName**: The first name of the participant",
            "**isMember**: is the participant a member, true or false",
            "**jobTitle**: The job title of the participant",
            "**lastName**: The last name of the participant",
            "**smallImageUri**: The uri of the small picture of the participant",
            "**tenantId**: the tenant id of the participant",
            "**userId**: The id of the participant"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
