{
  "name": "addRecentSpaceSearch",
  "description": "Add recent search of a client to search controller.\nOauthScopes: WRITE_SPACE, MANAGE_SPACE, ORGANIZE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "endTime": {
            "description": "The end time.",
            "format": "date-time",
            "type": "string"
          },
          "scope": {
            "description": "The scope of the search.",
            "enum": [
              "ALL",
              "SPACES",
              "TOPICBY",
              "FILES",
              "TAGS",
              "LABELS",
              "DATE"
            ],
            "maxLength": 2048,
            "type": "string"
          },
          "searchTerm": {
            "description": "The term to search for.",
            "maxLength": 2048,
            "type": "string"
          },
          "startTime": {
            "description": "The start time.",
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "scope",
          "searchTerm"
        ],
        "type": "object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/search/add/recent"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
