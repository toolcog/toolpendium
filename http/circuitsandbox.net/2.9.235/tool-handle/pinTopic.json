{
  "name": "pinTopic",
  "description": "Pin a topic\nOauthScopes: WRITE_SPACE, MANAGE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "topicId": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the topic"
      },
      "body": {
        "properties": {
          "position": {
            "description": "The position to pin to",
            "format": "int32",
            "maximum": 255,
            "minimum": 0,
            "type": "number"
          }
        },
        "required": [
          "position"
        ],
        "type": "object"
      }
    },
    "required": [
      "topicId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{topicId}/pin"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
