{
  "name": "searchSpaceParticipants",
  "description": "Get the participants of a space\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the space."
      },
      "query": {
        "maxLength": 2048,
        "type": "string",
        "description": "The query to search with. If searchpointer/hasMotre is returned, refine query."
      }
    },
    "required": [
      "id",
      "query"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/searchSpaceParticipants{?query}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**hasMore**: are more results available, then start another search with finer query",
            "**participants**: list of participants",
            "**searchPointer**: pointer for stored search "
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
