{
  "name": "cancelSpaceSearch",
  "description": "Cancels a space search of a client.\nOauthScopes: WRITE_SPACE, MANAGE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "searchId": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the search to cancel"
      }
    },
    "required": [
      "searchId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/search/cancel/{searchId}"
    }
  },
  "responses": {}
}
