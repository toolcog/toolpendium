{
  "name": "updateReadTimestamp",
  "description": "Update read timestamp\nOauthScopes: READ_SPACE, WRITE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "Id of a space"
      },
      "body": {
        "properties": {
          "timestamp": {
            "description": "The new timestamp",
            "format": "date-time",
            "type": "string"
          }
        },
        "required": [
          "timestamp"
        ],
        "type": "object"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/updateTimestamp"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
