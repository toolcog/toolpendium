{
  "name": "getSpaces",
  "description": "Get the spaces\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "timestamp": {
        "format": "date-time",
        "type": "string",
        "description": "a beautiful timestamp"
      },
      "numberOfResults": {
        "format": "int32",
        "maximum": 255,
        "minimum": 0,
        "type": "number",
        "description": "the number of results you want"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces{?timestamp,numberOfResults}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
