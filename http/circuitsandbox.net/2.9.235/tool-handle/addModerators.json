{
  "name": "addModerators",
  "description": "Adds a list of moderators to a conversation\nOauthScopes: WRITE_CONVERSATIONS, MANAGE_CONVERSATIONS",
  "parameters": {
    "type": "object",
    "properties": {
      "convId": {
        "maxLength": 2048,
        "type": "string",
        "description": "The ID of the conversation to which the moderators are added"
      },
      "body": {
        "properties": {
          "moderators": {
            "description": "The list of moderator ids to add ",
            "items": {
              "maxLength": 6500,
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "moderators"
        ],
        "type": "object"
      }
    },
    "required": [
      "convId",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/conversations/{convId}/moderators"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
