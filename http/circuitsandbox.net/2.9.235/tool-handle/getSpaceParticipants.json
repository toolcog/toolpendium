{
  "name": "getSpaceParticipants",
  "description": "Get the participants of a space\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the space."
      },
      "sortBy": {
        "default": "NAME",
        "enum": [
          "DISPLAY_NAME",
          "NAME",
          "FIRST_NAME"
        ],
        "maxLength": 2048,
        "type": "string",
        "description": "sort the spaces by LAST_CONTENT, NAME, NUMBER_OF_USERS or CREATION_DATE"
      },
      "sortOrder": {
        "default": "ASCENDING",
        "enum": [
          "ASCENDING",
          "DESCENDING"
        ],
        "maxLength": 2048,
        "type": "string",
        "description": "ascending or descending"
      },
      "filterType": {
        "enum": [
          "NONE",
          "ACCESS_TYPE",
          "ROLE",
          "STATE"
        ],
        "maxLength": 2048,
        "type": "string",
        "description": "filtertype for participants (ACCESS_TYPE, ROLE or STATE)"
      },
      "filterValue": {
        "maxLength": 2048,
        "type": "string",
        "description": "value for the filter"
      },
      "query": {
        "maxLength": 2048,
        "type": "string",
        "description": "some sort of query"
      },
      "searchPointer": {
        "maxLength": 2048,
        "type": "string",
        "description": "The search pointer (leave empty initially)."
      },
      "numberOfResults": {
        "default": 25,
        "format": "int32",
        "maximum": 255,
        "minimum": 0,
        "type": "number",
        "description": "number of results to return, 25 by default."
      }
    },
    "required": [
      "id",
      "sortBy",
      "sortOrder",
      "filterType"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/participants{?sortBy,sortOrder,filterType,filterValue,query,searchPointer,numberOfResults}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**hasMore**: are more results available, than start another search",
            "**participants**: list of participants",
            "**searchPointer**: pointer for stored search "
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
