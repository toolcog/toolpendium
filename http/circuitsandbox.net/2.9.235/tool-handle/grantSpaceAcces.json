{
  "name": "grantSpaceAcces",
  "description": "grant access for a space\nOauthScopes: WRITE_SPACE, MANAGE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "spaceId": {
        "maxLength": 2048,
        "type": "string",
        "description": "Id of the space"
      },
      "participantId": {
        "maxLength": 2048,
        "type": "string",
        "description": "Id of the participant"
      }
    },
    "required": [
      "spaceId",
      "participantId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{spaceId}/participant/{participantId}/grant"
    }
  },
  "responses": {}
}
