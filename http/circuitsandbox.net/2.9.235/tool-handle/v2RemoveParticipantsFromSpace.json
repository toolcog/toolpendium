{
  "name": "v2RemoveParticipantsFromSpace",
  "description": "removes Participants from a space\nOauthScopes: WRITE_SPACE, MANAGE_SPACE, ORGANIZE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the space"
      },
      "body": {
        "properties": {
          "userIds": {
            "description": "The ids of the participants to remove ",
            "items": {
              "maxLength": 2048,
              "type": "string"
            },
            "type": "array"
          }
        },
        "required": [
          "userIds"
        ],
        "type": "object"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{id}/participant/remove"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
