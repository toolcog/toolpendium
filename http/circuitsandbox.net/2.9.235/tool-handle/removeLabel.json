{
  "name": "removeLabel",
  "description": "Remove a label from the list of user labels\nOauthScopes: WRITE_USER_PROFILE, ORGANIZE_CONVERSATIONS",
  "parameters": {
    "type": "object",
    "properties": {
      "labelId": {
        "maxLength": 2048,
        "type": "string",
        "description": "The label value to remove, either the unique ID or the label value"
      }
    },
    "required": [
      "labelId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/users/labels/{labelId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**labelId**: The unique ID of the label",
            "**value**: The clear text value"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
