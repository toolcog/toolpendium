{
  "name": "likeSpaceItem",
  "description": "Like a space item\nOauthScopes: WRITE_SPACE, UPDATE_SPACE_CONTENT",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the item you want to like"
      }
    },
    "required": [
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/like/{itemId}"
    }
  },
  "responses": {}
}
