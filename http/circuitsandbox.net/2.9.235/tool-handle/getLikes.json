{
  "name": "getLikes",
  "description": "Get the likes of an item\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "itemId": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the item to retrieve the likes from"
      },
      "searchPointer": {
        "maxLength": 2048,
        "type": "string",
        "description": "The searchpointer for the search (initially not set)."
      },
      "numberOfResults": {
        "default": 25,
        "format": "int32",
        "maximum": 255,
        "minimum": 0,
        "type": "number",
        "description": "The number of results you want to retrieve."
      }
    },
    "required": [
      "itemId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/likes/{itemId}{?searchPointer,numberOfResults}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**hasMore**: More results available (true-use searchpointer for next search/false)",
            "**participants**: A list of participants who like the topic",
            "**searchPointer**: The searchpointer"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
