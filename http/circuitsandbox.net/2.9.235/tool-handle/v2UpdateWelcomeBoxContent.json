{
  "name": "v2UpdateWelcomeBoxContent",
  "description": "Update content of the welcome box of a space\nOauthScopes: MANAGE_SPACE, WRITE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "spaceId": {
        "maxLength": 2048,
        "type": "string",
        "description": "Id of the space"
      },
      "content": {
        "maxLength": 2048,
        "type": "string",
        "description": "The new content"
      },
      "body": {
        "properties": {
          "displayWelcomeBox": {
            "default": false,
            "description": "True, false, default:false",
            "type": "boolean"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "spaceId",
      "content"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{spaceId}/welcomebox/{content}"
    },
    "body": {
      "$": "body",
      "encode": "urlencoded"
    }
  },
  "responses": {}
}
