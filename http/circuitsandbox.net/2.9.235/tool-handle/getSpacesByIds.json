{
  "name": "getSpacesByIds",
  "description": "Get the spaces by their ids\nOauthScopes: READ_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "ids": {
        "items": {
          "maxLength": 2048,
          "type": "string"
        },
        "type": "array",
        "description": "an array of ids"
      }
    },
    "required": [
      "ids"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/ids{?ids}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
