{
  "name": "searchUsersList",
  "description": "Search multiple users given by id or email address.\nOauthScopes: READ_USER",
  "parameters": {
    "type": "object",
    "properties": {
      "name": {
        "items": {
          "maxLength": 2048,
          "type": "string"
        },
        "type": "array",
        "description": "Multiple email addresses or UUIDs."
      },
      "returnFullUserInfo": {
        "default": false,
        "type": "boolean",
        "description": "Boolean, return full user info?"
      },
      "secondaryLookup": {
        "default": false,
        "type": "boolean",
        "description": "Boolean, lookup secondary email?"
      }
    },
    "required": [
      "name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/users/list{?name,returnFullUserInfo,secondaryLookup}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**avatar**: The URL of the small avatar picture.",
            "**avatarLarge**: The URL of the large avatar picture.",
            "**company**: The name of the user's company",
            "**department**: missing documentation",
            "**displayName**: The user's display name",
            "**emailAddress**: The primary email address of the user",
            "**emailAddresses**: additional email addresses of the user",
            "**firstName**: The first name of the user",
            "**jobTitle**: The job title of the user",
            "**lastName**: The last name of the user",
            "**locale**: The locale of the user. (E.g. en-US, de-DE)",
            "**phoneNumber**: provisioned phone number of the user",
            "**phoneNumbers**: additional phone numbers of the user",
            "**primaryTenantId**: The primary tenant id",
            "**secondaryEmailAddress**: The secondary email address",
            "**secondaryTenantId**: The secondary tenant id",
            "**userId**: The ID of the user",
            "**userState**: The state of the user, e.g. deleted",
            "**userType**: the type of the user"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
