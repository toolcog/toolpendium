{
  "name": "unpinTopic",
  "description": "Unpin a topic\nOauthScopes: WRITE_SPACE, MANAGE_SPACE",
  "parameters": {
    "type": "object",
    "properties": {
      "topicId": {
        "maxLength": 2048,
        "type": "string",
        "description": "The id of the topic to unpin"
      }
    },
    "required": [
      "topicId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://circuitsandbox.net/rest/v2/spaces/{topicId}/unpin"
    }
  },
  "responses": {}
}
