{
  "name": "getMatomoKTokenValidation",
  "description": "Validate a keycloak token",
  "parameters": {
    "type": "object",
    "properties": {
      "keycloakToken": {
        "type": "string",
        "description": "Environment variable injected on the app with 'KEYCLOAK_TOKEN' name"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.clever-cloud.com/v2/v4/addon-providers/addon-matomo/token/validate{?keycloakToken}"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
