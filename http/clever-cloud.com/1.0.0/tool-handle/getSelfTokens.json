{
  "name": "getSelfTokens",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.clever-cloud.com/v2/self/tokens"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**consumer**",
              {
                "$ul": [
                  "**baseUrl**",
                  "**description**",
                  "**key**",
                  "**name**",
                  "**picture**",
                  [
                    "**rights**",
                    {
                      "$ul": [
                        "**access_bills** (default: false)",
                        "**access_consumption_statistics** (default: false)",
                        "**access_credit_count** (default: false)",
                        "**access_organisations** (default: false)",
                        "**access_organisations_bills** (default: false)",
                        "**access_organisations_consumption_statistics** (default: false)",
                        "**access_organisations_credit_count** (default: false)",
                        "**access_personal_information** (default: false)",
                        "**almighty** (default: false)",
                        "**manage_applications** (default: false)",
                        "**manage_organisations** (default: false)",
                        "**manage_organisations_applications** (default: false)",
                        "**manage_organisations_members** (default: false)",
                        "**manage_organisations_services** (default: false)",
                        "**manage_personal_information** (default: false)",
                        "**manage_services** (default: false)",
                        "**manage_ssh_keys** (default: false)"
                      ]
                    }
                  ],
                  "**url**"
                ]
              }
            ],
            "**creationDate**",
            "**lastUtilisation**",
            [
              "**rights**",
              {
                "$ul": [
                  "**description**",
                  "**name**"
                ]
              }
            ],
            "**token**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
