{
  "name": "updateConfigProviderEnv",
  "description": "Update provider's addon environment",
  "parameters": {
    "type": "object",
    "properties": {
      "configurationProviderId": {
        "type": "string",
        "description": "Automatically added"
      },
      "body": {
        "items": {
          "$ref": "#/$defs/WannabeEnvVar"
        },
        "type": "array"
      }
    },
    "required": [
      "configurationProviderId",
      "body"
    ],
    "$defs": {
      "WannabeEnvVar": {}
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.clever-cloud.com/v2/v4/addon-providers/config-provider/addons/{configurationProviderId}/env"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
