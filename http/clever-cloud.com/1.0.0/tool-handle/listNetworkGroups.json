{
  "name": "listNetworkGroups",
  "description": "Lists Network Groups from an owner.",
  "parameters": {
    "type": "object",
    "properties": {
      "ownerId": {
        "type": "string",
        "description": "Automatically added"
      }
    },
    "required": [
      "ownerId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.clever-cloud.com/v2/v4/networkgroups/organisations/{ownerId}/networkgroups"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    }
  }
}
