{
  "name": "getSummary",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.clever-cloud.com/v2/summary"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**organisations**",
            [
              "**user**",
              {
                "$ul": [
                  "**addons**",
                  "**admin** (default: false)",
                  "**applications**",
                  "**avatar**",
                  "**consumers**",
                  "**id**",
                  "**lang**",
                  "**name**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
