{
  "name": "untagServiceInventory",
  "description": "Untags a ServiceInventory object",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "pattern": "^\\d+$",
        "type": "string",
        "description": "ID of the resource"
      },
      "body": {
        "items": {
          "$ref": "#/$defs/Tag"
        },
        "type": "array",
        "description": "Tag attributes to removed"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "Tag": {
        "additionalProperties": false,
        "properties": {
          "tag": {
            "example": "/namespace/architecture=x86_64",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://cloud.redhat.com/%2Fapi%2Fcatalog-inventory%2Fv1.0/service_inventories/{id}/untag"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
