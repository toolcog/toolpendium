{
  "name": "add_video_embed_preset",
  "description": "Add an embed preset to a video",
  "parameters": {
    "type": "object",
    "properties": {
      "preset_id": {
        "example": 12345,
        "type": "number",
        "description": "The ID of the preset."
      },
      "video_id": {
        "example": 258684937,
        "type": "number",
        "description": "The ID of the video."
      }
    },
    "required": [
      "preset_id",
      "video_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.vimeo.com/videos/{video_id}/presets/{preset_id}"
    }
  },
  "responses": {}
}
