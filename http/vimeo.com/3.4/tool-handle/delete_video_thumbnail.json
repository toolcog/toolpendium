{
  "name": "delete_video_thumbnail",
  "description": "Delete a video thumbnail",
  "parameters": {
    "type": "object",
    "properties": {
      "picture_id": {
        "example": 12345,
        "type": "number",
        "description": "The ID of the picture."
      },
      "video_id": {
        "example": 258684937,
        "type": "number",
        "description": "The ID of the video."
      }
    },
    "required": [
      "picture_id",
      "video_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.vimeo.com/videos/{video_id}/pictures/{picture_id}"
    }
  },
  "responses": {}
}
