{
  "name": "check_if_user_subscribed_to_category",
  "description": "Check if a user follows a category",
  "parameters": {
    "type": "object",
    "properties": {
      "category": {
        "example": "animation",
        "type": "string",
        "description": "The name of the category."
      },
      "user_id": {
        "example": 152184,
        "type": "number",
        "description": "The ID of the user."
      }
    },
    "required": [
      "category",
      "user_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vimeo.com/users/{user_id}/categories/{category}"
    }
  },
  "responses": {}
}
