{
  "name": "get_video_privacy_domains",
  "description": "Get all the domains on which a video can be embedded",
  "parameters": {
    "type": "object",
    "properties": {
      "video_id": {
        "example": 258684937,
        "type": "number",
        "description": "The ID of the video."
      },
      "page": {
        "example": 1,
        "type": "number",
        "description": "The page number of the results to show."
      },
      "per_page": {
        "example": 10,
        "type": "number",
        "description": "The number of items to show on each page of results, up to a maximum of 100."
      }
    },
    "required": [
      "video_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vimeo.com/videos/{video_id}/privacy/domains{?page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Domain list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**allow_hd**: Whether to permit HD embeds on this domain.",
            "**domain**: The domain name.",
            "**uri**: The URI of the domain."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
