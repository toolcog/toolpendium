{
  "name": "add_video_privacy_domain",
  "description": "If domain privacy is enabled for this video, this method permits the video to be embedded on the specified domain.",
  "parameters": {
    "type": "object",
    "properties": {
      "domain": {
        "example": "example.com",
        "type": "string",
        "description": "The domain name."
      },
      "video_id": {
        "example": 258684937,
        "type": "number",
        "description": "The ID of the video."
      }
    },
    "required": [
      "domain",
      "video_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.vimeo.com/videos/{video_id}/privacy/domains/{domain}"
    }
  },
  "responses": {
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
