{
  "name": "delete_vod_genre",
  "description": "Remove a genre from an On Demand page",
  "parameters": {
    "type": "object",
    "properties": {
      "genre_id": {
        "example": "animation",
        "type": "string",
        "description": "The ID of the genre."
      },
      "ondemand_id": {
        "example": 61326,
        "type": "number",
        "description": "The ID of the On Demand."
      }
    },
    "required": [
      "genre_id",
      "ondemand_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.vimeo.com/ondemand/pages/{ondemand_id}/genres/{genre_id}"
    }
  },
