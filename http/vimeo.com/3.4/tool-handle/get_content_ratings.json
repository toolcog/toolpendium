{
  "name": "get_content_ratings",
  "description": "Get all content ratings",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vimeo.com/contentratings"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Content Rating list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The code that uniquely identifies this content rating:",
            "**name**: The description of this content rating.",
            "**uri**: The canonical relative URI of the content rating."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
