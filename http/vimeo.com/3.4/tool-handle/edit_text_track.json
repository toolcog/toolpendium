{
  "name": "edit_text_track",
  "description": "Edit a text track",
  "parameters": {
    "type": "object",
    "properties": {
      "texttrack_id": {
        "example": 12345,
        "type": "number",
        "description": "The ID of the text track."
      },
      "video_id": {
        "example": 258684937,
        "type": "number",
        "description": "The ID of the video."
      }
    },
    "required": [
      "texttrack_id",
      "video_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.vimeo.com/videos/{video_id}/texttracks/{texttrack_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Text Track"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**: Whether this text track is active.",
            "**hls_link**: The read-only URL of the text track file, intended for use with HLS playback.",
            "**hls_link_expires_time**: The time in ISO 8601 format when the read-only HLS playback text track file expires.",
            "**language**: The language code for this text track. To see a full list, request",
            "**link**: The read-only URL of the text track file. You can upload to this link when you create it for the first time.",
            "**link_expires_time**: The time in ISO 8601 format when the text track link expires.",
            "**name**: The descriptive name of this text track.",
            "**type**: The type of the text track:",
            "**uri**: The relative URI of the text track."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
