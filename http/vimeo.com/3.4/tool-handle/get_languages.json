{
  "name": "get_languages",
  "description": "Get all languages",
  "parameters": {
    "type": "object",
    "properties": {
      "filter": {
        "enum": [
          "texttracks"
        ],
        "type": "string",
        "description": "The attribute by which to filter the results.\n\nOption descriptions:\n * `texttracks` - Only return text track supported languages\n"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vimeo.com/languages{?filter}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Language list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The code that represents this language.",
            "**name**: The name of the language."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
