{
  "name": "replace_videos_in_album",
  "description": "This method replaces all the existing videos in an album with one or more videos.",
  "parameters": {
    "type": "object",
    "properties": {
      "album_id": {
        "example": 3706071,
        "type": "number",
        "description": "The ID of the album."
      },
      "user_id": {
        "example": 152184,
        "type": "number",
        "description": "The ID of the user."
      },
      "body": {
        "properties": {
          "videos": {
            "description": "A comma-separated list of video URIs.",
            "example": "/videos/258684937,/videos/273576296",
            "type": "string"
          }
        },
        "required": [
          "videos"
        ],
        "type": "object"
      }
    },
    "required": [
      "album_id",
      "user_id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.vimeo.com/users/{user_id}/albums/{album_id}/videos"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
