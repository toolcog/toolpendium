{
  "name": "add_videos_to_project",
  "description": "This method adds multiple videos to the specified project.",
  "parameters": {
    "type": "object",
    "properties": {
      "project_id": {
        "example": 12345,
        "type": "number",
        "description": "The ID of the project."
      },
      "user_id": {
        "example": 152184,
        "type": "number",
        "description": "The ID of the user."
      },
      "uris": {
        "example": "/videos/258684937,/videos/273576296",
        "type": "string",
        "description": "A comma-separated list of video URIs to add."
      }
    },
    "required": [
      "project_id",
      "user_id",
      "uris"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.vimeo.com/users/{user_id}/projects/{project_id}/videos{?uris}"
    }
  },
