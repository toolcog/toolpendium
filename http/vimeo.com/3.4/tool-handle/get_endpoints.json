{
  "name": "get_endpoints",
  "description": "Get an API specification",
  "parameters": {
    "type": "object",
    "properties": {
      "openapi": {
        "example": "true",
        "type": "boolean",
        "description": "Return an OpenAPI specification."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vimeo.com/{?openapi}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Endpoint"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**methods**: All HTTP methods permitted on this endpoint.",
            "**path**: The path section of the URL, which, when appended to the API host `https:///api.vimeo.com`, builds a full API endpoint."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
