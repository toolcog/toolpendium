{
  "name": "add_videos_to_channel",
  "description": "Add a list of videos to a channel",
  "parameters": {
    "type": "object",
    "properties": {
      "channel_id": {
        "example": 927,
        "type": "number",
        "description": "The ID of the channel."
      },
      "body": {
        "properties": {
          "video_uri": {
            "description": "The URI of a video to add.",
            "example": "/videos/258684937",
            "type": "string"
          }
        },
        "required": [
          "video_uri"
        ],
        "type": "object"
      }
    },
    "required": [
      "channel_id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.vimeo.com/channels/{channel_id}/videos"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
