{
  "name": "subscribe_to_category",
  "description": "Subscribe a user to a single category",
  "parameters": {
    "type": "object",
    "properties": {
      "category": {
        "example": 0,
        "type": "number",
        "description": "The name of the category."
      },
      "user_id": {
        "example": 152184,
        "type": "number",
        "description": "The ID of the user."
      }
    },
    "required": [
      "category",
      "user_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.vimeo.com/users/{user_id}/categories/{category}"
    }
  },
  "responses": {}
}
