{
  "name": "add_video_to_watch_later",
  "description": "Add a video to a user's Watch Later queue",
  "parameters": {
    "type": "object",
    "properties": {
      "user_id": {
        "example": 152184,
        "type": "number",
        "description": "The ID of the user."
      },
      "video_id": {
        "example": 258684937,
        "type": "number",
        "description": "The ID of the video."
      }
    },
    "required": [
      "user_id",
      "video_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://api.vimeo.com/users/{user_id}/watchlater/{video_id}"
    }
  },
  "responses": {}
}
