{
  "name": "get_album_logos",
  "description": "Get all the custom logos of an album",
  "parameters": {
    "type": "object",
    "properties": {
      "album_id": {
        "example": 3706071,
        "type": "number",
        "description": "The ID of the album."
      },
      "user_id": {
        "example": 152184,
        "type": "number",
        "description": "The ID of the user."
      },
      "page": {
        "example": 1,
        "type": "number",
        "description": "The page number of the results to show."
      },
      "per_page": {
        "example": 10,
        "type": "number",
        "description": "The number of items to show on each page of results, up to a maximum of 100."
      }
    },
    "required": [
      "album_id",
      "user_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vimeo.com/users/{user_id}/albums/{album_id}/logos{?page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Picture list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**: Whether this picture is the active picture for its parent resource.",
            "**link**: The upload URL for the picture. This field appears when you create the picture resource for the first time.",
            "**resource_key**: The picture's resource key string.",
            "**sizes**: An array containing reference information about all available image files.",
            "**type**: The type of the picture:",
            "**uri**: The picture's URI."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Legacy Error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**error**: User-friendly error message"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
