{
  "name": "get_cc_licenses",
  "description": "Get all Creative Commons licenses",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.vimeo.com/creativecommons"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Creative Commons list"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The code that uniquely identifies this Creative Commons license:",
            "**name**: The description of this Creative Commons license.",
            "**uri**: The canonical relative URI of the Creative Commons license."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
