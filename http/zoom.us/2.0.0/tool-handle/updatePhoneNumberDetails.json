{
  "name": "updatePhoneNumberDetails",
  "description": "Update phone number details.\n\n**Scopes:**\n* `phone:write` `phone:write:admin`\n* `phone:master`\n\n**Prerequisite:**\n* Paid account\n\n**[Rate Limit Label](https://marketplace.zoom.us/docs/api-reference/rate-limits#rate-limits):** `Light`",
  "parameters": {
    "type": "object",
    "properties": {
      "numberId": {
        "type": "string",
        "description": "Phone number ID."
      },
      "body": {
        "properties": {
          "capability": {
            "description": "Phone number capability. Values: `outgoing` or `incoming`. Add one or both.",
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "display_name": {
            "description": "Phone number display name.",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "numberId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.zoom.us/v2/phone/numbers/{numberId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "204": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
