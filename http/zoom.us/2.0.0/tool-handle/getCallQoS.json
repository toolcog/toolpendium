{
  "name": "getCallQoS",
  "description": "Get call quality of service(QoS) data for a call made or received by a Zoom phone user in the account. \n\n**Prerequisites:**\n* Business, or Education account\n* Zoom Phone license <br><br>\n**Scopes:** `phone:read:admin`, `phone:write:admin`<br>\n**Rate Limit Label:** `Light`",
  "parameters": {
    "type": "object",
    "properties": {
      "callId": {
        "type": "string",
        "description": "Unique identifier of the call."
      }
    },
    "required": [
      "callId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.zoom.us/v2/phone/metrics/call_logs/{callId}/qos"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**call_id**: Unique identifier of the phone call.",
            [
              "**callee_qos**",
              {
                "$ul": [
                  "**receiving**: QoS received by the callee.",
                  "**sending**: QoS sent by the callee."
                ]
              }
            ],
            [
              "**caller_qos**: Quality of Service object that represents the call quality data of the caller.",
              {
                "$ul": [
                  "**receiving**: QoS received by the caller.",
                  "**sending**: QoS sent by the caller."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
