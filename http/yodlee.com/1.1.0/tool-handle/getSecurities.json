{
  "name": "getSecurities",
  "description": "The get security details service is used to get all the security information for the holdings<br>",
  "parameters": {
    "type": "object",
    "properties": {
      "holdingId": {
        "type": "string",
        "description": "Comma separated holdingId"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//holdings/securities{?holdingId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "HoldingSecuritiesResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**holding**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "YodleeError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errorCode**",
            "**errorMessage**",
            "**referenceCode**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
