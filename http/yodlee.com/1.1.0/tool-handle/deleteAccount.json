{
  "name": "deleteAccount",
  "description": "The delete account service allows an account to be deleted.<br>This service does not return a response. The HTTP response code is 204 (Success with no content).<br>",
  "parameters": {
    "type": "object",
    "properties": {
      "accountId": {
        "format": "int64",
        "type": "integer",
        "description": "accountId"
      }
    },
    "required": [
      "accountId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "//accounts/{accountId}"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "YodleeError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errorCode**",
            "**errorMessage**",
            "**referenceCode**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
