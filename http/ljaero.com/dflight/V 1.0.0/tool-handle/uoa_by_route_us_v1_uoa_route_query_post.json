{
  "name": "uoa_by_route_us_v1_uoa_route_query_post",
  "description": "Retrieve UAS Operating Areas (UOAs) found along your route. Request body parameters are:\n* route:  [GeoJSON Linestring](https://www.rfc-editor.org/rfc/rfc7946.html#appendix-A) defining the route. Max allowed length is 50 km.",
  "parameters": {
    "type": "object",
    "properties": {
      "x-api-key": {
        "title": "X-Api-Key",
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/UOAsByRoute"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "UOAsByRoute": {
        "description": "Geometry-defining arguments for along-route requests",
        "example": {
          "route": {
            "coordinates": [
              [
                -89.428019,
                38.693705
              ],
              [
                -89.256689,
                38.604771
              ],
              [
                -89.331396,
                38.388092
              ]
            ],
            "type": "LineString"
          }
        },
        "properties": {
          "route": {
            "title": "Route",
            "type": "object"
          }
        },
        "required": [
          "route"
        ],
        "title": "UOAsByRoute",
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://dflight-api.ljaero.com//us/v1/uoa/route-query"
    },
    "headers": {
      "x-api-key": {
        "$": "x-api-key"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "UOAsRouteResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**found**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "HTTPValidationError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
