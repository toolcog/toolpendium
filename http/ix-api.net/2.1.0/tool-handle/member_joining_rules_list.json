{
  "name": "member_joining_rules_list",
  "description": "Get a list of joining rules",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "id1,id2,id3",
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Filter by id"
      },
      "network_service": {
        "type": "string",
        "description": "Filter by network_service"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v2/member-joining-rules{?id,network_service}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "MemberJoiningRule list"
        },
        "Polymorphic Member Joining Rule",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "401": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "403": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
