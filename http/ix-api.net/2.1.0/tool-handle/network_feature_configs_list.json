{
  "name": "network_feature_configs_list",
  "description": "Get all network feature configs.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "id1,id2,id3",
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Filter by id"
      },
      "state": {
        "type": "string",
        "description": "Filter by state"
      },
      "state__is_not": {
        "type": "string",
        "description": "Filter by state__is_not"
      },
      "managing_account": {
        "type": "string",
        "description": "Filter by managing_account"
      },
      "consuming_account": {
        "type": "string",
        "description": "Filter by consuming_account"
      },
      "external_ref": {
        "type": "string",
        "description": "Filter by external_ref"
      },
      "type": {
        "enum": [
          "route_server"
        ],
        "type": "string",
        "description": "Filter by type"
      },
      "service_config": {
        "type": "string",
        "description": "Filter by service_config"
      },
      "network_feature": {
        "type": "string",
        "description": "Filter by network_feature"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v2/network-feature-configs{?id,state,state__is_not,managing_account,consuming_account,external_ref,type,service_config,network_feature}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "NetworkFeatureConfig list"
        },
        "Polymorphic Network Feature Config",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "401": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "403": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
