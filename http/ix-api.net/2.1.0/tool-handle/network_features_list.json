{
  "name": "network_features_list",
  "description": "List available network features.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "id1,id2,id3",
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Filter by id"
      },
      "type": {
        "enum": [
          "route_server"
        ],
        "type": "string",
        "description": "Filter by type"
      },
      "required": {
        "type": "string",
        "description": "Filter by required"
      },
      "network_service": {
        "type": "string",
        "description": "Filter by network_service"
      },
      "name": {
        "type": "string",
        "description": "Filter by name"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v2/network-features{?id,type,required,network_service,name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "NetworkFeature list"
        },
        "Polymorphic Network Feature",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "401": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "403": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
