{
  "name": "network_services_list",
  "description": "List available `NetworkService`s.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "id1,id2,id3",
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Filter by id"
      },
      "state": {
        "type": "string",
        "description": "Filter by state"
      },
      "state__is_not": {
        "type": "string",
        "description": "Filter by state__is_not"
      },
      "managing_account": {
        "type": "string",
        "description": "Filter by managing_account"
      },
      "consuming_account": {
        "type": "string",
        "description": "Filter by consuming_account"
      },
      "external_ref": {
        "type": "string",
        "description": "Filter by external_ref"
      },
      "type": {
        "enum": [
          "exchange_lan",
          "p2p_vc",
          "p2mp_vc",
          "mp2mp_vc",
          "cloud_vc"
        ],
        "type": "string",
        "description": "Filter by type"
      },
      "pop": {
        "type": "string",
        "description": "Filter by pop"
      },
      "product_offering": {
        "type": "string",
        "description": "Filter by product_offering"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v2/network-services{?id,state,state__is_not,managing_account,consuming_account,external_ref,type,pop,product_offering}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "NetworkService list"
        },
        "Polymorphic Network Services",
        {
          "$each": "$.body.*",
          "$as": "item",
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.item"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "401": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "403": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
