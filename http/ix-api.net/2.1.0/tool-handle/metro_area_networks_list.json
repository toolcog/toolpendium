{
  "name": "metro_area_networks_list",
  "description": "List all MetroAreaNetworks",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "example": "id1,id2,id3",
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Filter by id"
      },
      "name": {
        "type": "string",
        "description": "Filter by name"
      },
      "metro_area": {
        "type": "string",
        "description": "Filter by metro_area"
      },
      "service_provider": {
        "type": "string",
        "description": "Filter by service_provider"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v2/metro-area-networks{?id,name,metro_area,service_provider}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "PartialMetroAreaNetwork list"
        },
        "MetroAreaNetwork",
        "**Key properties:**",
        {
          "$ul": [
            "**id**",
            "**metro_area**: The id of the metro area.",
            "**name**: The name of the metro area network.",
            "**pops**: List of pops in the metro area network.",
            "**service_provider**: The service provider is operating the network."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "401": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "403": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
