{
  "name": "getUserMe",
  "description": "This endpoint allows the client to retrieve information about the authenticated user",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.to/api/api/users/me"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "The representation of a user",
        "**Key properties:**",
        {
          "$ul": [
            "**github_username**",
            "**id**",
            "**joined_at**",
            "**location**",
            "**name**",
            "**profile_image**",
            "**summary**",
            "**twitter_username**",
            "**type_of**",
            "**username**",
            "**website_url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
