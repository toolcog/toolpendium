{
  "name": "getOrgUsers",
  "description": "This endpoint allows the client to retrieve a list of users belonging to the organization\n\nIt supports pagination, each page will contain `30` users by default.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string"
      },
      "page": {
        "default": 1,
        "format": "int32",
        "minimum": 1,
        "type": "integer",
        "description": "Pagination page"
      },
      "per_page": {
        "default": 30,
        "format": "int32",
        "maximum": 1000,
        "minimum": 1,
        "type": "integer",
        "description": "Page size (the number of items to return per page). The default maximum value can be overridden by \"API_PER_PAGE_MAX\" environment variable."
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.to/api/api/organizations/{username}/users{?page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "The representation of a user",
        "**Key properties:**",
        {
          "$ul": [
            "**github_username**",
            "**id**",
            "**joined_at**",
            "**location**",
            "**name**",
            "**profile_image**",
            "**summary**",
            "**twitter_username**",
            "**type_of**",
            "**username**",
            "**website_url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
