{
  "name": "postAdminUsersCreate",
  "description": "This endpoint allows the client to trigger an invitation to the provided email address.\n\n        It requires a token from a user with `super_admin` privileges.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/UserInviteParam"
      }
    },
    "$defs": {
      "UserInviteParam": {
        "description": "User invite parameters",
        "properties": {
          "email": {
            "type": "string"
          },
          "name": {
            "nullable": true,
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://dev.to/api/api/admin/users"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
