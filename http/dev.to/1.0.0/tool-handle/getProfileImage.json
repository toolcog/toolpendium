{
  "name": "getProfileImage",
  "description": "This endpoint allows the client to retrieve a user or organization profile image information by its\n        corresponding username.",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string",
        "description": "The parameter is the username of the user or the username of the organization."
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dev.to/api/api/profile_images/{username}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "A profile image object",
        "**Key properties:**",
        {
          "$ul": [
            "**image_of**: Determines the type of the profile image owner (user or organization)",
            "**profile_image**: Profile image (640x640)",
            "**profile_image_90**: Profile image (90x90)",
            "**type_of**: Return profile_image"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
