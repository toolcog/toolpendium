{
  "name": "LandlordController_GetInvoice",
  "description": "Get an invoice pdf belonging to the landlord.",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "token": {
        "type": "string",
        "description": "The login token returned from the /session POST call"
      },
      "invoiceID": {
        "type": "string",
        "description": "The invoice ID to load."
      }
    },
    "required": [
      "shortName",
      "token",
      "invoiceID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://live-api.letmc.com/v2/customer/{shortName}/landlord/invoice{?token,invoiceID}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
