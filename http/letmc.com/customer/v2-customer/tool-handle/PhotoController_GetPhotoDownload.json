{
  "name": "PhotoController_GetPhotoDownload",
  "description": "Downloads the photo of a property given the photo ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "token": {
        "type": "string",
        "description": "The login token returned from the /session POST call"
      },
      "photoID": {
        "type": "string",
        "description": "The unique ID of the photo on the property"
      },
      "width": {
        "format": "int32",
        "type": "integer",
        "description": "An optional parameter specifying the image width"
      },
      "height": {
        "format": "int32",
        "type": "integer",
        "description": "An optional parameter specifying the image height"
      }
    },
    "required": [
      "shortName",
      "token",
      "photoID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://live-api.letmc.com/v2/customer/{shortName}/photo/download{?token,photoID,width,height}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
