{
  "name": "LandlordController_GetLandlordCrmEntries",
  "description": "Retrieve landlord's CRM ID",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "token": {
        "type": "string",
        "description": "The login token returned from the /session POST call"
      }
    },
    "required": [
      "shortName",
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://live-api.letmc.com/v2/customer/{shortName}/landlord/landlordcrmentries{?token}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Class for Landlord CRM Entries",
        "**Key properties:**",
        {
          "$ul": [
            "**BranchID**",
            "**GlobalReference**",
            "**Name**",
            "**OID**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
