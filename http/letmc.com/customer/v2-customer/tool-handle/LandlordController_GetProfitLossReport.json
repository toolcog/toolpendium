{
  "name": "LandlordController_GetProfitLossReport",
  "description": "Generate a Profit and Loss Report",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "token": {
        "type": "string",
        "description": "The login token returned from the /session POST call"
      }
    },
    "required": [
      "shortName",
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://live-api.letmc.com/v2/customer/{shortName}/landlord/profitloss{?token}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Class to represent proffit loss report.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**DirectCosts**: Class for a group of entries.",
              {
                "$ul": [
                  "**Rows**: Rows."
                ]
              }
            ],
            [
              "**GrossProfitLoss**: Class for a group of entries.",
              {
                "$ul": [
                  "**Rows**: Rows."
                ]
              }
            ],
            [
              "**Income**: Class for a group of entries.",
              {
                "$ul": [
                  "**Rows**: Rows."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
