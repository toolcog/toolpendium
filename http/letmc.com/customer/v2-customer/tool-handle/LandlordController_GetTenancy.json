{
  "name": "LandlordController_GetTenancy",
  "description": "Get tenancy details.",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "token": {
        "type": "string",
        "description": "The login token returned from the /session POST call"
      },
      "tenancyID": {
        "type": "string",
        "description": "The Tenancy ID"
      }
    },
    "required": [
      "shortName",
      "token",
      "tenancyID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://live-api.letmc.com/v2/customer/{shortName}/landlord/tenancy{?token,tenancyID}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Landlord Tenancy Model.",
        "**Key properties:**",
        {
          "$ul": [
            "**ActualEndDate**: Actual End Date",
            "**Beds**: Beds",
            "**Bond**: Bond",
            "**BranchID**: The tenancy branch ID:-",
            "**Certificates**: Maintenance Certificates.",
            "**Documents**: Tenancy documents:-",
            "**FixedDate**: Fixed Date",
            "**GlobalReference**: User Friendly ID",
            "**ID**: ID",
            "**Inspections**: Inspections",
            "**ManagedRent**: State of the tenancy",
            "**Preferences**: Maintenance Preferences.",
            "**PreviousRentAmount**: Previous Rent Amount",
            "**PropertyAddress**: Display Property Address",
            "**Rent**: Rent",
            "**RentAmount**: Rent Amount",
            "**StartDate**: Start Date",
            "**TenancyProperty**: Tenancy Property",
            "**TenancyState**: State of the tenancy",
            "**Tenants**: Tenants"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
