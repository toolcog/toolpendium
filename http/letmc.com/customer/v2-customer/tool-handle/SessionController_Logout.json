{
  "name": "SessionController_Logout",
  "description": "Logout a customer previously logged in via the Login endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "token": {
        "type": "string",
        "description": "The login token returned from the /session POST call"
      }
    },
    "required": [
      "shortName",
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://live-api.letmc.com/v2/customer/{shortName}/session{?token}"
    }
  },
  "responses": {}
}
