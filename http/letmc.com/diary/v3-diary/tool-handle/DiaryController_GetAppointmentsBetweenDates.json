{
  "name": "DiaryController_GetAppointmentsBetweenDates",
  "description": "A collection of diary appointments linked to a company filtered between specific dates and by appointment type",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "branchID": {
        "type": "string",
        "description": "The unique ID of the Branch"
      },
      "startDate": {
        "format": "date-time",
        "type": "string",
        "description": "The search from date"
      },
      "endDate": {
        "format": "date-time",
        "type": "string",
        "description": "The search to date"
      },
      "appointmentTypesToSearch": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "The appointment IDs to search for"
      },
      "offset": {
        "format": "int32",
        "type": "integer",
        "description": "The index of the first item to return"
      },
      "count": {
        "format": "int32",
        "type": "integer",
        "description": "The maximum number of items to return (up to 1000 per request)"
      }
    },
    "required": [
      "shortName",
      "branchID",
      "startDate",
      "endDate",
      "appointmentTypesToSearch",
      "offset",
      "count"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://live-api.letmc.com/v3/diary/{shortName}/appointmentsbetweendates{?branchID,startDate,endDate,appointmentTypesToSearch,offset,count}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Holds results from a paged query returning DiaryAppointmentModel values",
        "**Key properties:**",
        {
          "$ul": [
            "**Count**: The total number of results available for all pages",
            "**Data**: The resulting data returned from the paged query range"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
