{
  "name": "DiaryController_DeleteAppointment",
  "description": "Delete an existing appointment using its unique identifier",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "appointmentID": {
        "type": "string",
        "description": "The unique appointment id"
      }
    },
    "required": [
      "shortName",
      "appointmentID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://live-api.letmc.com/v3/diary/{shortName}/appointment{?appointmentID}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
