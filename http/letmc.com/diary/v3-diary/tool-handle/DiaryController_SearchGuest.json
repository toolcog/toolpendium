{
  "name": "DiaryController_SearchGuest",
  "description": "Match Guest Parameters with existing applicants",
  "parameters": {
    "type": "object",
    "properties": {
      "shortname": {
        "type": "string",
        "description": ""
      },
      "branchID": {
        "type": "string",
        "description": ""
      },
      "forename": {
        "type": "string",
        "description": ""
      },
      "emailaddress": {
        "type": "string",
        "description": ""
      },
      "surname": {
        "type": "string",
        "description": ""
      },
      "offset": {
        "format": "int32",
        "type": "integer",
        "description": ""
      },
      "count": {
        "format": "int32",
        "type": "integer",
        "description": ""
      }
    },
    "required": [
      "shortname",
      "branchID",
      "forename",
      "emailaddress",
      "surname",
      "offset",
      "count"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://live-api.letmc.com/v3/diary/{shortname}/{branchID}/guest/search{?forename,emailaddress,surname,offset,count}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Contacts Person Model Results:-",
        "**Key properties:**",
        {
          "$ul": [
            "**Count**: Results count:-",
            "**Data**: Results data:-",
            "**Links**: Results links:-"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
