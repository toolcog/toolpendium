{
  "name": "DiaryController_AddFeedback",
  "description": "Submit appointment feedback",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "body": {
        "$ref": "#/components/schemas/FeedbackSubmissionModel",
        "description": "Feedback submission model"
      }
    },
    "required": [
      "shortName",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://live-api.letmc.com/v3/diary/{shortName}/appointment/feedback"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
