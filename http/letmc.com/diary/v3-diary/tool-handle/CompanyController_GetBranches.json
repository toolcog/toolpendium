{
  "name": "CompanyController_GetBranches",
  "description": "All branches defined for a company",
  "parameters": {
    "type": "object",
    "properties": {
      "shortName": {
        "type": "string",
        "description": "The unique client short-name"
      },
      "offset": {
        "format": "int32",
        "type": "integer",
        "description": "The index of the first item to return"
      },
      "count": {
        "format": "int32",
        "type": "integer",
        "description": "The maximum number of items to return (up to 1000 per request)"
      }
    },
    "required": [
      "shortName",
      "offset",
      "count"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://live-api.letmc.com/v3/diary/{shortName}/company/branches{?offset,count}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Holds results from a paged query returning BranchModel values",
        "**Key properties:**",
        {
          "$ul": [
            "**Count**: The total number of results available for all pages",
            "**Data**: The resulting data returned from the paged query range"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
