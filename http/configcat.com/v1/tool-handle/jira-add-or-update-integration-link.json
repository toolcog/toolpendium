{
  "name": "jira-add-or-update-integration-link",
  "parameters": {
    "type": "object",
    "properties": {
      "environmentId": {
        "format": "uuid",
        "type": "string",
        "description": "The identifier of the Environment."
      },
      "settingId": {
        "format": "int32",
        "type": "integer",
        "description": "The id of the Setting."
      },
      "key": {
        "type": "string",
        "description": "The key of the integration link."
      },
      "body": {
        "$ref": "#/$defs/AddOrUpdateJiraIntegrationLinkModel"
      }
    },
    "required": [
      "environmentId",
      "settingId",
      "key"
    ],
    "$defs": {
      "AddOrUpdateJiraIntegrationLinkModel": {
        "additionalProperties": false,
        "properties": {
          "clientKey": {
            "maxLength": 255,
            "minLength": 0,
            "type": "string"
          },
          "description": {
            "maxLength": 1000,
            "minLength": 0,
            "nullable": true,
            "type": "string"
          },
          "jiraJwtToken": {
            "maxLength": 15000,
            "minLength": 0,
            "type": "string"
          },
          "url": {
            "maxLength": 1000,
            "minLength": 0,
            "nullable": true,
            "type": "string"
          }
        },
        "required": [
          "clientKey",
          "jiraJwtToken"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.configcat.com/v1/jira/environments/{environmentId}/settings/{settingId}/integrationLinks/{key}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**description**",
            "**integrationLinkType**",
            "**key**",
            "**url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
