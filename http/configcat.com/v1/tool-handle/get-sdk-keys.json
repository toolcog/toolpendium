{
  "name": "get-sdk-keys",
  "description": "This endpoint returns the SDK Key for your Config in a specified Environment.",
  "parameters": {
    "type": "object",
    "properties": {
      "configId": {
        "format": "uuid",
        "type": "string",
        "description": "The identifier of the Config."
      },
      "environmentId": {
        "format": "uuid",
        "type": "string",
        "description": "The identifier of the Environment."
      }
    },
    "required": [
      "configId",
      "environmentId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.configcat.com/v1/configs/{configId}/environments/{environmentId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**primary**",
            "**secondary**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
