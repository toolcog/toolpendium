{
  "name": "invite-member",
  "description": "This endpoint invites a Member into the given Product identified by the `productId` parameter.",
  "parameters": {
    "type": "object",
    "properties": {
      "productId": {
        "format": "uuid",
        "type": "string",
        "description": "The identifier of the Product."
      },
      "body": {
        "$ref": "#/$defs/InviteMembersRequest"
      }
    },
    "required": [
      "productId",
      "body"
    ],
    "$defs": {
      "InviteMembersRequest": {
        "additionalProperties": false,
        "properties": {
          "emails": {
            "items": {
              "type": "string"
            },
            "type": "array"
          },
          "permissionGroupId": {
            "format": "int64",
            "type": "integer"
          }
        },
        "required": [
          "emails",
          "permissionGroupId"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.configcat.com/v1/products/{productId}/members/invite"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
