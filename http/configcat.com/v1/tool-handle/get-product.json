{
  "name": "get-product",
  "description": "This endpoint returns the metadata of a Product \nidentified by the `productId`.",
  "parameters": {
    "type": "object",
    "properties": {
      "productId": {
        "format": "uuid",
        "type": "string",
        "description": "The identifier of the Product."
      }
    },
    "required": [
      "productId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.configcat.com/v1/products/{productId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_embedded**",
              {
                "$ul": [
                  [
                    "**organization**",
                    {
                      "$ul": [
                        [
                          "**_links**",
                          {
                            "$ul": [
                              "**members**",
                              "**products**"
                            ]
                          }
                        ],
                        "**name**",
                        "**organizationId**"
                      ]
                    }
                  ]
                ]
              }
            ],
            [
              "**_links**",
              {
                "$ul": [
                  "**configs**",
                  "**environments**",
                  "**members**",
                  "**permission-groups**",
                  "**segments**",
                  "**self**",
                  "**tags**"
                ]
              }
            ],
            "**description**",
            "**name**",
            "**order**",
            "**productId**",
            "**reasonRequired**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
