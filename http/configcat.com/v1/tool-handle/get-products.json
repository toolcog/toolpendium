{
  "name": "get-products",
  "description": "This endpoint returns the list of the Products that belongs to the user.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.configcat.com/v1/products"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_embedded**",
              {
                "$ul": [
                  [
                    "**organization**",
                    {
                      "$ul": [
                        [
                          "**_links**",
                          {
                            "$ul": [
                              "**members**",
                              "**products**"
                            ]
                          }
                        ],
                        "**name**",
                        "**organizationId**"
                      ]
                    }
                  ]
                ]
              }
            ],
            [
              "**_links**",
              {
                "$ul": [
                  "**configs**",
                  "**environments**",
                  "**members**",
                  "**permission-groups**",
                  "**segments**",
                  "**self**",
                  "**tags**"
                ]
              }
            ],
            "**description**",
            "**name**",
            "**order**",
            "**productId**",
            "**reasonRequired**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
