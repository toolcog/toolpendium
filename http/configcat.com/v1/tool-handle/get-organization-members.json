{
  "name": "get-organization-members",
  "description": "This endpoint returns the list of Members that belongs \nto the given Organization, identified by the `organizationId` parameter.",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationId": {
        "format": "uuid",
        "type": "string",
        "description": "The identifier of the Organization."
      }
    },
    "required": [
      "organizationId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.configcat.com/v1/organizations/{organizationId}/members"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**email**",
            "**fullName**",
            "**userId**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
