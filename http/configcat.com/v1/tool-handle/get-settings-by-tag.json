{
  "name": "get-settings-by-tag",
  "description": "This endpoint returns the list of the Settings that \nhas the specified Tag, identified by the `tagId` parameter.",
  "parameters": {
    "type": "object",
    "properties": {
      "tagId": {
        "format": "int64",
        "type": "integer",
        "description": "The identifier of the Tag."
      }
    },
    "required": [
      "tagId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.configcat.com/v1/tags/{tagId}/settings"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**_embedded**",
              {
                "$ul": [
                  "**tags**"
                ]
              }
            ],
            [
              "**_links**",
              {
                "$ul": [
                  "**self**"
                ]
              }
            ],
            "**configId**",
            "**configName**",
            "**hint**",
            "**key**",
            "**name**",
            "**order**",
            "**settingId**",
            "**settingType**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
