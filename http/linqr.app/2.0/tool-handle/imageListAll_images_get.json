{
  "name": "imageListAll_images_get",
  "description": "This endpoint allows you to list images hosted in the LinQR storage. If there are no images hosted, an empty array is returned.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://run.byvalue.org/images"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Response Imagelistall Images Get"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**: The creation time of the image.",
            "**id**: Image identifier.",
            "**size**: Size of the source file in bytes",
            "**source**: Source file name."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
