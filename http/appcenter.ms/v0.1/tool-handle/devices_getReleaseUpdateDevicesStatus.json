{
  "name": "devices_getReleaseUpdateDevicesStatus",
  "description": "Returns the resign status to the caller",
  "parameters": {
    "type": "object",
    "properties": {
      "release_id": {
        "type": "string",
        "description": "The ID of the release."
      },
      "resign_id": {
        "type": "string",
        "description": "The ID of the resign operation."
      },
      "include_provisioning_profile": {
        "type": "boolean",
        "description": "A boolean value that indicates if the provisioning profile should be return in addition to the status. When set to true, the provisioning profile will be returned only when status is 'complete' or 'preparing_for_testers'."
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "release_id",
      "resign_id",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/releases/{release_id}/update_devices/{resign_id}{?include_provisioning_profile}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The status of the resign operation.",
        "**Key properties:**",
        {
          "$ul": [
            "**error_code**: Error code for any error that occured during the resigning operation.",
            "**error_message**: Error message for any error that occured during the resigning operation.",
            "**status**: The status of the resign"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
