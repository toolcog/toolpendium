{
  "name": "codePushAcquisition_updateCheck",
  "description": "Check for updates",
  "parameters": {
    "type": "object",
    "properties": {
      "deployment_key": {
        "type": "string"
      },
      "app_version": {
        "type": "string"
      },
      "package_hash": {
        "type": "string"
      },
      "label": {
        "type": "string"
      },
      "client_unique_id": {
        "type": "string"
      },
      "is_companion": {
        "type": "boolean"
      },
      "previous_label_or_app_version": {
        "type": "string"
      },
      "previous_deployment_key": {
        "type": "string"
      }
    },
    "required": [
      "deployment_key",
      "app_version"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/public/codepush/update_check{?deployment_key,app_version,package_hash,label,client_unique_id,is_companion,previous_label_or_app_version,previous_deployment_key}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**update_info**",
              {
                "$ul": [
                  "**description**",
                  "**is_disabled**",
                  "**is_mandatory**",
                  "**rollout**",
                  "**target_binary_range**",
                  "**download_url**",
                  "**is_available**",
                  "**label**",
                  "**package_hash**",
                  "**package_size**",
                  "**should_run_binary_version**",
                  "**update_app_version**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
