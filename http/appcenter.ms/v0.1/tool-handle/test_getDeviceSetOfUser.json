{
  "name": "test_getDeviceSetOfUser",
  "description": "Gets a device set belonging to the user",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "The UUID of the device set"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "id",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/user/device_sets/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Device Set"
        },
        "The name and devices of the device set",
        "**Key properties:**",
        {
          "$ul": [
            "**deviceConfigurations**",
            "**id**: Identifier of the device set",
            "**manufacturerCount**: The number of manufacturers in the device set's device selection",
            "**name**: Name of the device set",
            "**osVersionCount**: The number of os versions in the device set's device selection",
            [
              "**owner**: The owner of a device set",
              {
                "$ul": [
                  "**displayName**: Display name of the account",
                  "**id**: Account ID",
                  "**name**: Name of the account",
                  "**type**: Type of account"
                ]
              }
            ],
            "**slug**: Slug of the device set"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
