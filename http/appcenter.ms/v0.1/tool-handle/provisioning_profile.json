{
  "name": "provisioning_profile",
  "description": "Return information about the provisioning profile. Only available for iOS.",
  "parameters": {
    "type": "object",
    "properties": {
      "release_id": {
        "type": "integer",
        "description": "The release_id"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "release_id",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/releases/{release_id}/provisioning_profile"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A response containing information about an iOS provisioning profile.",
        "**Key properties:**",
        {
          "$ul": [
            "**appex_profiles**: Array of provisioning profiles for any app extensions",
            "**provisioning_bundle_id**: The bundle identifier associated with the profile.",
            "**provisioning_profile_name**: The name of the provisioning profile.",
            "**provisioning_profile_type**",
            "**team_identifier**: The team identifier.",
            "**udids**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
