{
  "name": "builds_listToolsets",
  "description": "Returns available toolsets for application",
  "parameters": {
    "type": "object",
    "properties": {
      "tools": {
        "enum": [
          "xamarin",
          "xcode",
          "node"
        ],
        "type": "string",
        "description": "Toolset name"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/toolsets{?tools}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Set of toolsets available for app",
        "**Key properties:**",
        {
          "$ul": [
            "**node**: A list of Node versions",
            "**xamarin**: A list of Xamarin SDK bundles",
            "**xcode**: A list of Xcode versions"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**id**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
