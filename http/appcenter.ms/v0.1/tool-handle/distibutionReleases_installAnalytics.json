{
  "name": "distibutionReleases_installAnalytics",
  "description": "Notify download(s) for the provided distribution release(s).",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the app owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the app"
      },
      "body": {
        "properties": {
          "releases": {
            "items": {
              "properties": {
                "distribution_group_id": {
                  "description": "distribution group id",
                  "type": "string"
                },
                "release_id": {
                  "description": "release id",
                  "type": "integer"
                },
                "user_id": {
                  "description": "user id",
                  "format": "uuid",
                  "type": "string"
                }
              },
              "required": [
                "release_id",
                "distribution_group_id",
                "user_id"
              ],
              "type": "object"
            },
            "type": "array"
          }
        },
        "description": "The install analytics request payload"
      }
    },
    "required": [
      "owner_name",
      "app_name",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/public/apps/{owner_name}/{app_name}/install_analytics"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
