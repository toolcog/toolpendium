{
  "name": "azureSubscription_listForOrg",
  "description": "Returns a list of azure subscriptions for the organization",
  "parameters": {
    "type": "object",
    "properties": {
      "org_name": {
        "type": "string",
        "description": "The organization's name"
      }
    },
    "required": [
      "org_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/orgs/{org_name}/azure_subscriptions"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**is_billable**: If the subscription can be used for billing",
            "**is_billing**: If the subscription is used for billing",
            "**is_microsoft_internal**: If the subscription is internal Microsoft subscription",
            "**subscription_id**: The azure subscription id",
            "**subscription_name**: The name of the azure subscription",
            "**tenant_id**: The tenant id of the azure subscription belongs to"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
