{
  "name": "builds_get",
  "description": "Returns the build detail for the given build ID",
  "parameters": {
    "type": "object",
    "properties": {
      "build_id": {
        "exclusiveMinimum": true,
        "minimum": 0,
        "type": "integer",
        "description": "The build ID"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "build_id",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/builds/{build_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**buildNumber**: The build number",
            "**finishTime**: The time the build was finished",
            "**id**: The build ID",
            "**lastChangedDate**: The time the build status was last changed",
            "**queueTime**: The time the build was queued",
            "**result**: The build result",
            "**sourceBranch**: The source branch name",
            "**sourceVersion**: The source SHA",
            "**startTime**: The time the build was started",
            "**status**: The build status"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
