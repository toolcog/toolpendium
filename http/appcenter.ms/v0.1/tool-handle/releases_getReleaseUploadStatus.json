{
  "name": "releases_getReleaseUploadStatus",
  "description": "Get the current status of the release upload.",
  "parameters": {
    "type": "object",
    "properties": {
      "upload_id": {
        "format": "uuid",
        "type": "string",
        "description": "The ID of the release upload"
