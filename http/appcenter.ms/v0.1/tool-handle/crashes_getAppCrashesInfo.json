{
  "name": "crashes_getAppCrashesInfo",
  "description": "Gets whether the application has any crashes.",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/crashes_info"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**features**",
              {
                "$ul": [
                  "**crash_download_raw**: App supports download of raw crashes",
                  "**crashgroup_analytics_crashfreeusers**: App supports the 'crash free user' metric",
                  "**crashgroup_analytics_impactedusers**: App supports the 'impacted users' metric",
                  "**crashgroup_modify_annotation**: App supports modification of crashgroup annotation",
                  "**crashgroup_modify_status**: App supports modification of crashgroup status",
                  "**search**: App supports search API"
                ]
              }
            ],
            "**has_crashes**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
