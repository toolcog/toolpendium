{
  "name": "appleMapping_get",
  "description": "Get mapping of apple app to an existing app in apple store.",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/apple_mapping"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Apple Mapping Request Type",
        "**Key properties:**",
        {
          "$ul": [
            "**app_id**: ID of the apple application in Mobile Center",
            "**apple_id**: ID of the apple application in apple store",
            "**service_connection_id**: Id for the shared service connection. In case of Apple AppStore, this connection will be used to create and connect to the Apple AppStore in Mobile Center.",
            "**team_identifier**: ID of the Team associated with the app in apple store"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
