{
  "name": "crashGroups_list",
  "description": "Gets a list of crash groups and whether the list contains all available groups.",
  "parameters": {
    "type": "object",
    "properties": {
      "last_occurrence_from": {
        "format": "date-time",
        "type": "string",
        "description": "Earliest date when the last time a crash occured in a crash group"
      },
      "last_occurrence_to": {
        "format": "date-time",
        "type": "string",
        "description": "Latest date when the last time a crash occured in a crash group"
      },
      "app_version": {
        "type": "string",
        "description": "version"
      },
      "group_type": {
        "enum": [
          "GroupType1",
          "GroupType2"
        ],
        "type": "string"
      },
      "group_status": {
        "enum": [
          "open",
          "closed",
          "ignored"
        ],
        "type": "string"
      },
      "group_text_search": {
        "type": "string",
        "description": "A freetext search that matches in crash, crash types, crash stack_traces and crash user"
      },
      "$orderby": {
        "default": "last_occurrence desc",
        "enum": [
          "last_occurrence asc",
          "last_occurrence desc",
          "count asc",
          "count desc",
          "display_id asc",
          "display_id desc",
          "impacted_users asc",
          "impacted_users desc"
        ],
        "type": "string",
        "description": "the OData-like $orderby argument"
      },
      "continuation_token": {
        "type": "string",
        "description": "Cassandra request continuation token. The token is used for pagination."
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/crash_groups{?last_occurrence_from,last_occurrence_to,app_version,group_type,group_status,group_text_search,$orderby,continuation_token}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**continuation_token**: Cassandra request continuation token. The token is used for pagination.",
            "**crash_groups**",
            "**limited_result_set**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
