{
  "name": "stores_delete",
  "description": "delete the store based on specific store name.",
  "parameters": {
    "type": "object",
    "properties": {
      "store_name": {
        "type": "string",
        "description": "The name of the store"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      },
      "body": {
        "type": "string"
      }
    },
    "required": [
      "store_name",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/distribution_stores/{store_name}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
