{
  "name": "releases_getPublicGroupsForReleaseByHash",
  "description": "Get all public distribution groups that a given release has been distributed to",
  "parameters": {
    "type": "object",
    "properties": {
      "app_secret": {
        "type": "string",
        "description": "The secret of the target application"
      },
      "release_hash": {
        "type": "string",
        "description": "The hash of the release"
      }
    },
    "required": [
      "app_secret",
      "release_hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/public/sdk/apps/{app_secret}/releases/{release_hash}/public_distribution_groups"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**id**: The id of the distribution group"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.id"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
