{
  "name": "Analytics_EventPropertyCounts",
  "description": "Event properties value counts during the time range in descending order.",
  "parameters": {
    "type": "object",
    "properties": {
      "event_name": {
        "format": "string",
        "type": "string",
        "description": "The id of the event."
      },
      "event_property_name": {
        "format": "string",
        "type": "string",
        "description": "The id of the event property."
      },
      "start": {
        "format": "date-time",
        "type": "string",
        "description": "Start date time in data in ISO 8601 date time format."
      },
      "end": {
        "format": "date-time",
        "type": "string",
        "description": "Last date time in data in ISO 8601 date time format."
      },
      "versions": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "To select specific application versions"
      },
      "$top": {
        "default": 10,
        "format": "int64",
        "minimum": 0,
        "type": "integer",
        "description": "The number of property values to return. Set to 0 in order to fetch all results available."
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "event_name",
      "event_property_name",
      "start",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/analytics/events/{event_name}/properties/{event_property_name}/counts{?start,end,versions,$top}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Event property value counts during the time range in descending order.",
        "**Key properties:**",
        {
          "$ul": [
            "**total**: The total property value counts.",
            "**values**: The event property values."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Error",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**: The status code return by the API. It can be 400 or 403 or 500.",
                  "**message**: The reason for the request failed."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
