{
  "name": "codePushDeployments_list",
  "description": "Gets a list of CodePush deployments for the given app",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/deployments"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**key**",
            [
              "**latest_release**",
              {
                "$ul": [
                  "**description**",
                  "**is_disabled**",
                  "**is_mandatory**",
                  "**rollout**",
                  "**target_binary_range**",
                  "**blob_url**",
                  "**diff_package_map**",
                  "**label**",
                  "**original_deployment**: Set on 'Promote'",
                  "**original_label**: Set on 'Promote' and 'Rollback'",
                  "**package_hash**",
                  "**release_method**: The release method is unknown if unspecified",
                  "**released_by**",
                  "**size**",
                  "**upload_time**"
                ]
              }
            ],
            "**name**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
