{
  "name": "symbols_getStatus",
  "description": "Returns a particular symbol by id (uuid) for the provided application",
  "parameters": {
    "type": "object",
    "properties": {
      "symbol_id": {
        "type": "string",
        "description": "The ID of the symbol (uuid of the symbol)"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "symbol_id",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/symbols/{symbol_id}/status"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A response containing information pertaining to a symbol status",
        "**Key properties:**",
        {
          "$ul": [
            "**app_id**: The application that this symbol belongs to",
            "**status**: Whether the symbol is ignored.",
            "**symbol_id**: The unique id for this symbol (uuid)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
