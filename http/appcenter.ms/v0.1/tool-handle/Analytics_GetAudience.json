{
  "name": "Analytics_GetAudience",
  "description": "Gets audience definition.",
  "parameters": {
    "type": "object",
    "properties": {
      "audience_name": {
        "format": "string",
        "maxLength": 64,
        "pattern": "[^/]+",
        "type": "string",
        "description": "The name of the audience"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "audience_name",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/analytics/audiences/{audience_name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Audience definition.",
        "**Key properties:**",
        {
          "$ul": [
            "**definition**: Audience definition in OData format.",
            "**description**: Audience description.",
            "**estimated_count**: Estimated audience size.",
            "**name**: Audience name.",
            "**state**: Audience state."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
