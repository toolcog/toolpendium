{
  "name": "test_createTestSeries",
  "description": "Creates new test series for an application",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      },
      "body": {
        "properties": {
          "name": {
            "description": "Name of the new test series",
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "title": "Name of the test series",
        "type": "object"
      }
    },
    "required": [
      "owner_name",
      "app_name",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/test_series"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Test Series"
        },
        "Summary of a single test series",
        "**Key properties:**",
        {
          "$ul": [
            "**mostRecentActivity**: Date of the latest test run that used this test series",
            "**name**: Name of the test series",
            "**slug**: Unique, human-readable identifier of the test series",
            "**testRuns**: Most recent test runs"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Test Cloud Error Details"
        },
        "Details of a failed operation",
        "**Key properties:**",
        {
          "$ul": [
            "**message**: Human-readable message that describes the error",
            "**status**: Status of the operation"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
