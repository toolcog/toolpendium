{
  "name": "Analytics_CrashFreeDevicePercentages",
  "description": "Percentage of crash-free device by day in the time range based on the selected versions. Api will return -1 if crash devices is greater than active devices.",
  "parameters": {
    "type": "object",
    "properties": {
      "start": {
        "format": "date-time",
        "type": "string",
        "description": "Start date time in data in ISO 8601 date time format."
      },
      "end": {
        "format": "date-time",
        "type": "string",
        "description": "Last date time in data in ISO 8601 date time format."
      },
      "version": {
        "format": "string",
        "type": "string"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "start",
      "version",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/analytics/crashfree_device_percentages{?start,end,version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**average_percentage**: Average percentage.",
            "**daily_percentages**: The crash-free percentage per day."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Error",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**: The status code return by the API. It can be 400 or 403 or 500.",
                  "**message**: The reason for the request failed."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
