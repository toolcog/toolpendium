{
  "name": "Analytics_OperatingSystemCounts",
  "description": "OSes in the time range.",
  "parameters": {
    "type": "object",
    "properties": {
      "start": {
        "format": "date-time",
        "type": "string",
        "description": "Start date time in data in ISO 8601 date time format."
      },
      "end": {
        "format": "date-time",
        "type": "string",
        "description": "Last date time in data in ISO 8601 date time format."
      },
      "$top": {
        "default": 30,
        "format": "int64",
        "maximum": 2000,
        "minimum": 0,
        "type": "integer",
        "description": "The maximum number of results to return. (0 will fetch all results)"
      },
      "versions": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "To select specific application versions"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "start",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/analytics/oses{?start,end,$top,versions}"
    }
  },
