{
  "name": "builds_create",
  "description": "Create a build",
  "parameters": {
    "type": "object",
    "properties": {
      "branch": {
        "type": "string",
        "description": "The branch name"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      },
      "body": {
        "properties": {
          "debug": {
            "description": "Run build in debug mode",
            "type": "boolean"
          },
          "sourceVersion": {
            "description": "Version to build which represents the full Git commit reference",
            "type": "string"
          }
        },
        "type": "object",
        "description": "Parameters of the build"
      }
    },
    "required": [
      "branch",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/branches/{branch}/builds"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**buildNumber**: The build number",
            "**finishTime**: The time the build was finished",
            "**id**: The build ID",
            "**lastChangedDate**: The time the build status was last changed",
            "**queueTime**: The time the build was queued",
            "**result**: The build result",
            "**sourceBranch**: The source branch name",
            "**sourceVersion**: The source SHA",
            "**startTime**: The time the build was started",
            "**status**: The build status"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
