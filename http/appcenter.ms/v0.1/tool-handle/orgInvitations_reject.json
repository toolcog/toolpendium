{
  "name": "orgInvitations_reject",
  "description": "Rejects a pending organization invitation",
  "parameters": {
    "type": "object",
    "properties": {
      "invitation_token": {
        "type": "string",
        "description": "The app invitation token that was sent to the user"
      },
      "body": {
        "type": "object",
        "description": "allow empty body for custom http-client lib"
      }
    },
    "required": [
      "invitation_token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/user/invitations/orgs/{invitation_token}/reject"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
