{
  "name": "Errors_ErrorAttachments",
  "description": "List error attachments.",
  "parameters": {
    "type": "object",
    "properties": {
      "errorId": {
        "format": "string",
        "type": "string",
        "description": "The id of the error"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "errorId",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/errors/{errorId}/attachments"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**appId**",
            "**attachmentId**",
            "**blobLocation**",
            "**contentType**",
            "**crashId**",
            "**createdTime**",
            "**fileName**",
            "**size**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
