{
  "name": "Analytics_TestAudience",
  "description": "Tests audience definition.",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      },
      "body": {
        "description": "Audience definition",
        "properties": {
          "custom_properties": {
            "additionalProperties": {
              "enum": [
                "string",
                "number",
                "boolean",
                "date_time"
              ],
              "type": "string",
              "x-ms-enum": {
                "modelAsString": false,
                "name": "device_property_type"
              }
            },
            "description": "Custom properties used in the definition.",
            "type": "object"
          },
          "definition": {
            "description": "Audience definition in OData format.",
            "maxLength": 2000,
            "type": "string"
          },
          "description": {
            "description": "Audience description.",
            "maxLength": 128,
            "type": "string"
          },
          "enabled": {
            "default": true,
            "type": "boolean"
          }
        },
        "required": [
          "definition"
        ],
        "type": "object"
      }
    },
    "required": [
      "owner_name",
      "app_name",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/analytics/audiences/definition/test"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Audience test result.",
        "**Key properties:**",
        {
          "$ul": [
            "**custom_properties**: Custom properties used in the definition.",
            "**definition**: Audience definition in OData format.",
            "**estimated_count**: Estimated audience size.",
            "**estimated_total_count**: Estimated total audience size."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
