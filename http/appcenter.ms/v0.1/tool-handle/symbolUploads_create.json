{
  "name": "symbolUploads_create",
  "description": "Begins the symbol upload process for a new set of symbols for the specified application",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      },
      "body": {
        "description": "The symbol information",
        "properties": {
          "build": {
            "description": "The build number. Optional for Apple. Required for Android.",
            "type": "string"
          },
          "client_callback": {
            "description": "The callback URL that the client can optionally provide to get status updates for the current symbol upload",
            "type": "string"
          },
          "file_name": {
            "description": "The file name for the symbol upload",
            "type": "string"
          },
          "symbol_type": {
            "description": "The type of the symbol for the current symbol upload",
            "enum": [
              "Apple",
              "JavaScript",
              "Breakpad",
              "AndroidProguard",
              "UWP"
            ],
            "type": "string"
          },
          "version": {
            "description": "The version number. Optional for Apple. Required for Android.",
            "type": "string"
          }
        },
        "required": [
          "symbol_type"
        ]
      }
    },
    "required": [
      "owner_name",
      "app_name",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/symbol_uploads"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A response containing information pertaining to starting a symbol upload process",
        "**Key properties:**",
        {
          "$ul": [
            "**expiration_date**: Describes how long the upload_url is valid",
            "**symbol_upload_id**: The id for the current upload",
            "**upload_url**: The URL where the client needs to upload the symbol blob to"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
