{
  "name": "Analytics_CrashGroupModelCounts",
  "description": "Top models of the selected crash group with selected version. Available for UWP apps only.",
  "parameters": {
    "type": "object",
    "properties": {
      "crash_group_id": {
        "format": "string",
        "type": "string",
        "description": "The id of the crash group."
      },
      "version": {
        "format": "string",
        "type": "string"
      },
      "$top": {
        "default": 30,
        "format": "int64",
        "maximum": 2000,
        "minimum": 0,
        "type": "integer",
        "description": "The maximum number of results to return. (0 will fetch all results)"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "crash_group_id",
      "version",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/analytics/crash_groups/{crash_group_id}/models{?version,$top}"
    }
  },
  "responses": {
