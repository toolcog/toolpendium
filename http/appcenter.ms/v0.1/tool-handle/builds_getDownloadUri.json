{
  "name": "builds_getDownloadUri",
  "description": "Gets the download URI",
  "parameters": {
    "type": "object",
    "properties": {
      "build_id": {
        "exclusiveMinimum": true,
        "minimum": 0,
        "type": "integer",
        "description": "The build ID"
      },
      "download_type": {
        "enum": [
          "build",
          "symbols",
          "logs",
          "mapping",
          "bundle"
        ],
        "type": "string",
        "description": "The download type"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "build_id",
      "download_type",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/builds/{build_id}/downloads/{download_type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A download reference",
        "**Key properties:**",
        {
          "$ul": [
            "**uri**: Download URI"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
