{
  "name": "Errors_CountsPerDay",
  "description": "Count of crashes or errors by day in the time range based the selected versions. If SingleErrorTypeParameter is not provided, defaults to handlederror.",
  "parameters": {
    "type": "object",
    "properties": {
      "version": {
        "format": "string",
        "type": "string"
      },
      "start": {
        "format": "date-time",
        "type": "string",
        "description": "Start date time in data in ISO 8601 date time format"
      },
      "end": {
        "format": "date-time",
        "type": "string",
        "description": "Last date time in data in ISO 8601 date time format"
      },
      "app_build": {
        "format": "string",
        "type": "string",
        "description": "app build"
      },
      "errorType": {
        "enum": [
          "unhandledError",
          "handledError"
        ],
        "type": "string",
        "description": "Type of error (handled vs unhandled), excluding All"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "start",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/errors/errorCountsPerDay{?version,start,end,app_build,errorType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**: total error count",
            "**errors**: the total error count for day"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
