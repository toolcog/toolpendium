{
  "name": "test_getTestReport",
  "description": "Returns a single test report",
  "parameters": {
    "type": "object",
    "properties": {
      "test_run_id": {
        "format": "uuid",
        "type": "string",
        "description": "The ID of the test run"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "test_run_id",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/test_runs/{test_run_id}/report"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**app_upload_id**",
            "**date**",
            "**date_finished**",
            "**device_logs**",
            "**errorMessage**",
            "**features**",
            "**finished_device_snapshots**",
            "**id**",
            "**platform**",
            "**revision**",
            "**schema_version**",
            "**snapshot_fatal_errors**",
            [
              "**stats**",
              {
                "$ul": [
                  "**artifacts**",
                  "**devices**",
                  "**devices_failed**",
                  "**devices_finished**",
                  "**devices_not_runned**",
                  "**devices_skipped**",
                  "**failed**",
                  "**filesize**",
                  "**os**",
                  "**passed**",
                  "**skipped**",
                  "**step_count**",
                  "**total**",
                  "**totalDeviceMinutes**"
                ]
              }
            ],
            "**testType**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
