{
  "name": "devices_listCsvFormat",
  "description": "Returns all devices associated with the given distribution group.",
  "parameters": {
    "type": "object",
    "properties": {
      "distribution_group_name": {
        "type": "string",
        "description": "The name of the distribution group."
      },
      "unprovisioned_only": {
        "default": false,
        "type": "boolean",
        "description": "when true, filters out provisioned devices"
      },
      "udids": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "multiple UDIDs which should be part of the resulting CSV."
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "distribution_group_name",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/distribution_groups/{distribution_group_name}/devices/download_devices_list{?unprovisioned_only,udids}"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
