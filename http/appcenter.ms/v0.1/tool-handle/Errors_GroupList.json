{
  "name": "Errors_GroupList",
  "description": "List of error groups",
  "parameters": {
    "type": "object",
    "properties": {
      "version": {
        "format": "string",
        "type": "string"
      },
      "app_build": {
        "format": "string",
        "type": "string",
        "description": "app build"
      },
      "groupState": {
        "format": "string",
        "type": "string"
      },
      "start": {
        "format": "date-time",
        "type": "string",
        "description": "Start date time in data in ISO 8601 date time format"
      },
      "end": {
        "format": "date-time",
        "type": "string",
        "description": "Last date time in data in ISO 8601 date time format"
      },
      "$orderby": {
        "default": "count desc",
        "type": "string",
        "description": "controls the sorting order and sorting based on which column"
      },
      "$top": {
        "default": 30,
        "format": "int64",
        "maximum": 1000,
        "minimum": 0,
        "type": "integer",
        "description": "The maximum number of results to return. (0 will fetch all results till the max number.)"
      },
      "errorType": {
        "enum": [
          "all",
          "unhandledError",
          "handledError"
        ],
        "type": "string",
        "description": "Type of error (handled vs unhandled), including All"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "start",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/errors/errorGroups{?version,app_build,groupState,start,end,$orderby,$top,errorType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errorGroups**",
            "**nextLink**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
