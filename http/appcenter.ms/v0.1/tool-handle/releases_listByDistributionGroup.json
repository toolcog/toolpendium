{
  "name": "releases_listByDistributionGroup",
  "description": "Return basic information about distributed releases in a given distribution group.",
  "parameters": {
    "type": "object",
    "properties": {
      "distribution_group_name": {
        "type": "string",
        "description": "The name of the distribution group."
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "distribution_group_name",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/distribution_groups/{distribution_group_name}/releases"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Response for getting a list of releases in a distribution group",
        "**Key properties:**",
        {
          "$ul": [
            "**enabled**: This value determines the whether a release currently is enabled or disabled.",
            "**id**: ID identifying this unique release.",
            "**is_external_build**: This value determines if a release is external or not.",
            "**mandatory_update**: A boolean which determines whether the release is a mandatory update or not.",
            "**origin**: The release's origin",
            "**short_version**: The release's short version.<br>",
            "**uploaded_at**: UTC time in ISO 8601 format of the uploaded time.",
            "**version**: The release's version.<br>"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
