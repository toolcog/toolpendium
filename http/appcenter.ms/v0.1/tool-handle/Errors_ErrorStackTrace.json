{
  "name": "Errors_ErrorStackTrace",
  "description": "Error Stacktrace details.",
  "parameters": {
    "type": "object",
    "properties": {
      "errorGroupId": {
        "format": "string",
        "type": "string",
        "description": "The id of the error group"
      },
      "errorId": {
        "format": "string",
        "type": "string",
        "description": "The id of the error"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "errorGroupId",
      "errorId",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/errors/errorGroups/{errorGroupId}/errors/{errorId}/stacktrace"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "a stacktrace in a processed and prettyfied way",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**exception**: a exception",
              {
                "$ul": [
                  "**frames**: frames of the excetpion",
                  "**inner_exceptions**",
                  "**platform**: SDK/Platform this thread is beeing generated from",
                  "**reason**: Reason of the exception",
                  "**relevant**: relevant exception (crashed)",
                  "**type**: Type of the exception (NSSomethingException, NullPointerException)"
                ]
              }
            ],
            "**reason**",
            "**threads**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
