{
  "name": "symbols_ignore",
  "description": "Marks a symbol by id (uuid) as ignored",
  "parameters": {
    "type": "object",
    "properties": {
      "symbol_id": {
        "type": "string",
        "description": "The ID of the symbol (uuid of the symbol)"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "symbol_id",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/symbols/{symbol_id}/ignore"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**alternate_symbol_ids**: The other symbols in the same file",
            "**app_id**: The application that this symbol belongs to",
            "**build**: The build number. Optional for Apple. Required for Android.",
            "**origin**: The origin of the symbol file",
            "**platform**: The platform that this symbol is associated with",
            "**status**: Whether the symbol is ignored.",
            "**symbol_id**: The unique id for this symbol (uuid)",
            "**symbol_upload_id**: The id of the symbol upload this symbol belongs to.",
            "**type**: The type of the symbol for the current symbol upload",
            "**url**: The path name of the symbol file in blob storage",
            "**version**: The version number. Optional for Apple. Required for Android."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
