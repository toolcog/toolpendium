{
  "name": "storeReleases_list",
  "description": "Return all releases published  in a store",
  "parameters": {
    "type": "object",
    "properties": {
      "store_name": {
        "type": "string",
        "description": "The name of the store"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "store_name",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/distribution_stores/{store_name}/releases"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Basic information on a release",
        "**Key properties:**",
        {
          "$ul": [
            "**destination_type**: Destination for this release.",
            "**distribution_stores**: a list of distribution stores that are associated with this release.",
            "**id**: ID identifying this unique release.",
            "**short_version**: The release's short version.",
            "**uploaded_at**: UTC time in ISO 8601 format of the uploaded time.",
            "**version**: The release's version."
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
