{
  "name": "Errors_ListSessionLogs",
  "description": "Get session logs by error ID",
  "parameters": {
    "type": "object",
    "properties": {
      "errorId": {
        "format": "string",
        "type": "string",
        "description": "The id of the error"
      },
      "date": {
        "format": "date-time",
        "type": "string",
        "description": "Date of data requested"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "errorId",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/errors/{errorId}/sessionLogs{?date}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**exceeded_max_limit**: indicates if the number of available logs are more than the max allowed return limit(100).",
            "**last_received_log_timestamp**: the timestamp of the last log received. This value can be used as the start time parameter in the consecutive API call.",
            "**logs**: the list of logs"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
