{
  "name": "Errors_ErrorGroupsSearch",
  "description": "Error groups list based on search parameters",
  "parameters": {
    "type": "object",
    "properties": {
      "filter": {
        "format": "string",
        "type": "string",
        "description": "A filter as specified in OData notation"
      },
      "q": {
        "format": "string",
        "type": "string",
        "description": "A query string"
      },
      "order": {
        "default": "desc",
        "enum": [
          "desc",
          "asc"
        ],
        "type": "string",
        "description": "It controls the order of sorting"
      },
      "sort": {
        "default": "matchingReportsCount",
        "enum": [
          "matchingReportsCount",
          "exceptionClassName",
          "exceptionMessage",
          "exceptionMethod",
          "lastOccurrence"
        ],
        "type": "string",
        "description": "It controls the sort based on specified field"
      },
      "$top": {
        "default": 100,
        "format": "int64",
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "The maximum number of results to return"
      },
      "$skip": {
        "default": 0,
        "format": "int64",
        "minimum": 0,
        "type": "integer",
        "description": "The offset (starting at 0) of the first result to return. This parameter along with limit is used to perform pagination."
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/errors/errorGroups/search{?filter,q,order,sort,$top,$skip}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errorGroups**",
            "**hasMoreResults**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
