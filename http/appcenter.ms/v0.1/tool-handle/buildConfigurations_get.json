{
  "name": "buildConfigurations_get",
  "description": "Gets the build configuration in Azure pipeline YAML format",
  "parameters": {
    "type": "object",
    "properties": {
      "branch": {
        "type": "string",
        "description": "The branch name"
      },
      "format": {
        "default": "yaml",
        "enum": [
          "yaml",
          "json"
        ],
        "type": "string",
        "description": "Configuration format"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "branch",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/branches/{branch}/export_config{?format}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "The build configuration in Azure pipeline YAML format",
        "**Key properties:**",
        {
          "$ul": [
            "**yaml**: Azure Pipelines YAML file"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**id**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
