{
  "name": "distributionGroups_detailsForOrg",
  "description": "Returns a list of distribution groups with details for an organization",
  "parameters": {
    "type": "object",
    "properties": {
      "org_name": {
        "type": "string",
        "description": "The organization's name"
      },
      "apps_limit": {
        "type": "number",
        "description": "The max number of apps to include in the response"
      }
    },
    "required": [
      "org_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/orgs/{org_name}/distribution_groups_details{?apps_limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**display_name**: The name of the distribution group",
            "**id**: The unique ID of the distribution group",
            "**is_public**: Whether the distribution group is public",
            "**name**: The name of the distribution group used in URLs",
            "**origin**: The creation origin of this distribution group",
            "**apps**: The apps associated with the distribution group",
            "**total_apps_count**: The count of apps associated with this distribution group",
            "**total_users_count**: The count of users in the distribution group"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
