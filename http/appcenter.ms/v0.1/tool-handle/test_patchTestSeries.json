{
  "name": "test_patchTestSeries",
  "description": "Updates name and slug of a test series",
  "parameters": {
    "type": "object",
    "properties": {
      "test_series_slug": {
        "type": "string",
        "description": "The slug of the test series"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      },
      "body": {
        "properties": {
          "name": {
            "description": "Name of the new test series",
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "title": "Name of the test series",
        "type": "object"
      }
    },
    "required": [
      "test_series_slug",
      "owner_name",
      "app_name",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/test_series/{test_series_slug}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Test Series"
        },
        "Summary of a single test series",
        "**Key properties:**",
        {
          "$ul": [
            "**mostRecentActivity**: Date of the latest test run that used this test series",
            "**name**: Name of the test series",
            "**slug**: Unique, human-readable identifier of the test series",
            "**testRuns**: Most recent test runs"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
