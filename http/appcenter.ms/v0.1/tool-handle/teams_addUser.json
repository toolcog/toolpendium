{
  "name": "teams_addUser",
  "description": "Adds a new user to a team that is owned by an organization",
  "parameters": {
    "type": "object",
    "properties": {
      "org_name": {
        "type": "string",
        "description": "The organization's name"
      },
      "team_name": {
        "type": "string",
        "description": "The team's name"
      },
      "body": {
        "properties": {
          "user_email": {
            "description": "The user's email address",
            "type": "string"
          }
        },
        "required": [
          "user_email"
        ],
        "description": "The email of the user to add to the team"
      }
    },
    "required": [
      "org_name",
      "team_name",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/orgs/{org_name}/teams/{team_name}/users"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**display_name**: The full name of the user. Might for example be first and last name",
            "**email**: The email address of the user",
            "**name**: The unique name that is used to identify the user.",
            "**role**: The role of the user has within the team"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
