{
  "name": "builds_listByBranch",
  "description": "Returns the list of builds for the branch",
  "parameters": {
    "type": "object",
    "properties": {
      "branch": {
        "type": "string",
        "description": "The branch name"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "branch",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/branches/{branch}/builds"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "A list of builds",
        "**Key properties:**",
        {
          "$ul": [
            "**buildNumber**: The build number",
            "**finishTime**: The time the build was finished",
            "**id**: The build ID",
            "**lastChangedDate**: The time the build status was last changed",
            "**queueTime**: The time the build was queued",
            "**result**: The build result",
            "**sourceBranch**: The source branch name",
            "**sourceVersion**: The source SHA",
            "**startTime**: The time the build was started",
            "**status**: The build status"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
