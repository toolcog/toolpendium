{
  "name": "missingSymbolGroups_list",
  "description": "Gets top N (ordered by crash count) of crash groups by missing symbol",
  "parameters": {
    "type": "object",
    "properties": {
      "top": {
        "type": "integer",
        "description": "top N elements"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "top",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/diagnostics/symbol_groups{?top}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "grouped by missing symbols crashes response object",
        "**Key properties:**",
        {
          "$ul": [
            "**groups**: list of crash groups formed by missing symbols combination",
            "**total_crash_count**: total number of crashes for all the groups"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "failure response object",
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
