{
  "name": "distributionGroups_addUser",
  "description": "Adds the members to the specified distribution group",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      },
      "distribution_group_name": {
        "type": "string",
        "description": "The name of the distribution group"
      },
      "body": {
        "properties": {
          "user_emails": {
            "description": "The list of emails of the users",
            "items": {
              "type": "string"
            },
            "minItems": 1,
            "type": "array"
          }
        },
        "description": "The list of members to add"
      }
    },
    "required": [
      "owner_name",
      "app_name",
      "distribution_group_name",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/distribution_groups/{distribution_group_name}/members"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: The code of the result",
            "**invite_pending**: Whether the has accepted the invite. Available when an invite is pending, and the value will be \"true\".",
            "**message**: The message of the result",
            "**status**: The status code of the result",
            "**user_email**: The email of the user"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
