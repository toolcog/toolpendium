{
  "name": "distributionGroups_resendSharedInvite",
  "description": "Resend shared distribution group invite notification to previously invited testers",
  "parameters": {
    "type": "object",
    "properties": {
      "org_name": {
        "type": "string",
        "description": "The organization's name"
      },
      "distribution_group_name": {
        "type": "string",
        "description": "The name of the distribution group"
      },
      "body": {
        "properties": {
          "user_emails": {
            "description": "The list of emails of the users",
            "items": {
              "type": "string"
            },
            "minItems": 1,
            "type": "array"
          }
        },
        "description": "The list of members to add"
      }
    },
    "required": [
      "org_name",
      "distribution_group_name",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/orgs/{org_name}/distribution_groups/{distribution_group_name}/resend_invite"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**",
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
