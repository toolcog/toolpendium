{
  "name": "builds_webhook",
  "description": "Public webhook sink",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/public/hooks"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**id**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
