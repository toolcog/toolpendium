{
  "name": "Analytics_Events",
  "description": "Count of active events in the time range ordered by event.",
  "parameters": {
    "type": "object",
    "properties": {
      "start": {
        "format": "date-time",
        "type": "string",
        "description": "Start date time in data in ISO 8601 date time format."
      },
      "end": {
        "format": "date-time",
        "type": "string",
        "description": "Last date time in data in ISO 8601 date time format."
      },
      "versions": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "To select specific application versions"
      },
      "event_name": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "To select the specific events."
      },
      "$top": {
        "default": 30,
        "format": "int64",
        "maximum": 2000,
        "minimum": 0,
        "type": "integer",
        "description": "The maximum number of results to return. (0 will fetch all results)"
      },
      "$skip": {
        "default": 0,
        "format": "int64",
        "minimum": 0,
        "type": "integer",
        "description": "The offset (starting at 0) of the first result to return. This parameter along with limit is used to perform pagination."
      },
      "$inlinecount": {
        "default": "none",
        "enum": [
          "allpages",
          "none"
        ],
        "type": "string",
        "description": "Controls whether or not to include a count of all the items across all pages."
      },
      "$orderby": {
        "default": "count desc",
        "type": "string",
        "description": "controls the sorting order and sorting based on which column"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "start",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/analytics/events{?start,end,versions,event_name,$top,$skip,$inlinecount,$orderby}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**events**",
            "**total**: The total count of events.",
            "**total_devices**: The active device over this period."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Error",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**code**: The status code return by the API. It can be 400 or 403 or 500.",
                  "**message**: The reason for the request failed."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
