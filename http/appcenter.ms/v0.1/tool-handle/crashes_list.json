{
  "name": "crashes_list",
  "description": "Gets all crashes of a group.",
  "parameters": {
    "type": "object",
    "properties": {
      "crash_group_id": {
        "type": "string",
        "description": "id of a specific group"
      },
      "include_report": {
        "default": false,
        "type": "boolean",
        "description": "true if the crash should include the raw crash report. Default is false"
      },
      "include_log": {
        "default": false,
        "type": "boolean",
        "description": "true if the crash should include the custom log report. Default is false"
      },
      "date_from": {
        "format": "date-time",
        "type": "string",
        "description": ""
      },
      "date_to": {
        "format": "date-time",
        "type": "string",
        "description": ""
      },
      "app_version": {
        "type": "string",
        "description": "version"
      },
      "error_type": {
        "enum": [
          "CrashingErrors",
          "HandledErrors"
        ],
        "type": "string"
      },
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "crash_group_id",
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/crash_groups/{crash_group_id}/crashes{?include_report,include_log,date_from,date_to,app_version,error_type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**build**: ",
            "**crash_id**: ",
            [
              "**details**",
              {
                "$ul": [
                  "**app_start_timestamp**: Application launch timestamp (example: 1985-04-12T23:20:50.52Z).",
                  "**carrier_country**: Carrier country code (for mobile devices).",
                  "**carrier_name**: Carrier name (for mobile devices).",
                  "**locale**: Language code (example: en_US).",
                  "**os_build**: OS build code (example: LMY47X).",
                  "**rooted**: Whether the device where the crash occurred is rooted or jailbroken",
                  "**screen_size**: Screen size of the device in pixels (example: 640x480)."
                ]
              }
            ],
            "**device**: ",
            "**device_name**: ",
            "**display_id**",
            "**new_crash_group_id**: ",
            "**new_crash_id**: ",
            "**os_type**: ",
            "**os_version**: ",
            [
              "**stacktrace**: a stacktrace in a processed and prettyfied way",
              {
                "$ul": [
                  [
                    "**exception**: a exception",
                    {
                      "$ul": [
                        "**frames**: frames of the excetpion",
                        "**inner_exceptions**",
                        "**platform**: SDK/Platform this thread is beeing generated from",
                        "**reason**: Reason of the exception",
                        "**relevant**: relevant exception (crashed)",
                        "**type**: Type of the exception (NSSomethingException, NullPointerException)"
                      ]
                    }
                  ],
                  "**reason**",
                  "**threads**",
                  "**title**"
                ]
              }
            ],
            "**timestamp**",
            "**user_email**: ",
            "**user_name**: ",
            "**version**: "
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
