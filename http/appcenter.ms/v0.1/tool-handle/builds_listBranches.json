{
  "name": "builds_listBranches",
  "description": "Returns the list of Git branches for this application",
  "parameters": {
    "type": "object",
    "properties": {
      "owner_name": {
        "type": "string",
        "description": "The name of the owner"
      },
      "app_name": {
        "type": "string",
        "description": "The name of the application"
      }
    },
    "required": [
      "owner_name",
      "app_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.appcenter.ms/v0.1/apps/{owner_name}/{app_name}/branches"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "A collection of branch status",
        "**Key properties:**",
        {
          "$ul": [
            "**configured**",
            [
              "**lastBuild**",
              {
                "$ul": [
                  "**buildNumber**: The build number",
                  "**finishTime**: The time the build was finished",
                  "**id**: The build ID",
                  "**lastChangedDate**: The time the build status was last changed",
                  "**queueTime**: The time the build was queued",
                  "**result**: The build result",
                  "**sourceBranch**: The source branch name",
                  "**sourceVersion**: The source SHA",
                  "**startTime**: The time the build was started",
                  "**status**: The build status"
                ]
              }
            ]
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**id**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
