{
  "name": "getSnapshotsUsingGET_1",
  "description": "getSnapshots",
  "parameters": {
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "description": "apiKey"
      },
      "pageSize": {
        "default": 20,
        "format": "int32",
        "type": "integer",
        "description": "pageSize"
      }
    },
    "required": [
      "apiKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//config2.gamesparks.net/restv2/game/{apiKey}/admin/snapshots{?pageSize}"
    }
  },
