{
  "name": "getAnalyticsDataUsingGET",
  "description": "Returns the results of executed query defined by the parameters passed in",
  "parameters": {
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "description": "apiKey"
      },
      "stage": {
        "enum": [
          "LIVE",
          "PREVIEW"
        ],
        "type": "string",
        "description": "stage"
      },
      "dataType": {
        "enum": [
          "activeDevices",
          "activeLocations",
          "activeUsers",
          "averageBandwidthPerUser",
          "averageDauOverMau",
          "averageJsExecutionTime",
          "averageRequestsPerUser",
          "averageResponseTime",
          "averageResponseTimePerType",
          "scriptLogLevelsCount",
          "sessionAnalytic",
          "storagePerUser",
          "customAnalyticTotal",
          "customAnalyticUser",
          "timedAnalyticTotal",
          "sessionAnalyticTotal",
          "connectedUsers"
        ],
        "type": "string",
        "description": "dataType"
      },
      "precision": {
        "enum": [
          "HOURLY",
          "DAILY",
          "MONTHLY"
        ],
        "type": "string",
        "description": "precision"
      },
      "startDate": {
        "format": "date",
        "type": "string",
        "description": "yyyy-MM-dd"
      },
      "endDate": {
        "format": "date",
        "type": "string",
        "description": "yyyy-MM-dd"
      },
      "keys": {
        "type": "string",
        "description": "the keys to select. For example \"ReturningUsers\", \"NewUsers\", etc"
      }
    },
    "required": [
      "apiKey",
      "stage",
      "dataType",
      "precision",
      "startDate",
      "endDate"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//config2.gamesparks.net/restv2/game/{apiKey}/admin/analytics{?stage,dataType,precision,startDate,endDate,keys}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**_id**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
