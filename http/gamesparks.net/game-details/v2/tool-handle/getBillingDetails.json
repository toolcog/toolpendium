{
  "name": "getBillingDetails",
  "description": "Retrieves the Billing Details.",
  "parameters": {
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "description": "apiKey"
      }
    },
    "required": [
      "apiKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//config2.gamesparks.net/restv2/game/{apiKey}/admin/billingDetails"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**building**",
            "**city**",
            "**companyName**",
            "**country**",
            "**email1**",
            "**email2**",
            "**email3**",
            "**firstName1**",
            "**firstName2**",
            "**firstName3**",
            "**lastName1**",
            "**lastName2**",
            "**lastName3**",
            "**postcode**",
            "**state**",
            "**street**",
            "**taxNumber**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
