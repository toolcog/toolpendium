{
  "name": "getExperimentsUsingGET",
  "description": "getExperiments",
  "parameters": {
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "description": "apiKey"
      }
    },
    "required": [
      "apiKey"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "//config2.gamesparks.net/restv2/game/{apiKey}/manage/experiments"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**",
            "**changedFieldsAndInitialValues**",
            "**complete**",
            [
              "**config**",
              {
                "$ul": [
                  "**playerMongoQuery**",
                  "**playerQuery**",
                  "**variants**"
                ]
              }
            ],
            "**endDate**",
            "**id**",
            "**measurements**",
            "**measurementsEsQuery**",
            "**name**",
            "**percentHash**",
            "**publishedStages**",
            "**startDate**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
