{
  "name": "updateCredentialSecretUsingPOST",
  "description": "Resets the secret of a credential",
  "parameters": {
    "type": "object",
    "properties": {
      "apiKey": {
        "type": "string",
        "description": "apiKey"
      },
      "credentialName": {
        "type": "string",
        "description": "credentialName"
      }
    },
    "required": [
      "apiKey",
      "credentialName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "//config2.gamesparks.net/restv2/game/{apiKey}/config/~credentials/{credentialName}/resetSecret"
    }
  },
  "responses": {
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
