{
  "name": "Reports_Index",
  "description": "Get's the report.",
  "parameters": {
    "type": "object",
    "properties": {
      "report_type": {
        "type": "string",
        "description": "Report Type Id (i.e Daily, Monthly, Annual)"
      },
      "sites": {
        "type": "string",
        "description": "Comma separated list of site Ids."
      },
      "start_date": {
        "type": "string",
        "description": "The start date of the report in the format ddmmyyyy (i.e 31012016)"
      },
      "end_date": {
        "type": "string",
        "description": "The end date of the report in the format ddmmyyyy (i.e 31012016)"
      },
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "The page offset to return."
      },
      "page_size": {
        "format": "int32",
        "type": "integer",
        "description": "The number of rows to return."
      },
      "reportSubTypeId": {
        "format": "int32",
        "type": "integer"
      },
      "version": {
        "type": "string"
      }
    },
    "required": [
      "report_type",
      "sites",
      "start_date",
      "end_date",
      "page",
      "page_size",
      "version"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://webtris.highwaysengland.co.uk/api/v{version}/reports/{report_type}{?sites,start_date,end_date,page,page_size,reportSubTypeId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
