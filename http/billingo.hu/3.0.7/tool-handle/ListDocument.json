{
  "name": "ListDocument",
  "description": "Returns a list of your documents. The documents are returned sorted by creation date, with the most recent documents appearing first.",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": "1",
        "type": "integer"
      },
      "per_page": {
        "default": 25,
        "maximum": 100,
        "minimum": 1,
        "type": "integer"
      },
      "block_id": {
        "type": "integer",
        "description": "Filter documents by the identifier of your DocumentBlock."
      },
      "partner_id": {
        "type": "integer",
        "description": "Filter documents by the identifier of your Partner."
      },
      "payment_method": {
        "$ref": "#/components/schemas/PaymentMethod",
        "description": "Filter documents by PaymentMethod value."
      },
      "payment_status": {
        "$ref": "#/components/schemas/PaymentStatus",
        "description": "Filter documents by PaymentStatus value."
      },
      "start_date": {
        "format": "date",
        "type": "string",
        "description": "Filter documents by date."
      },
      "end_date": {
        "format": "date",
        "type": "string",
        "description": "Filter documents by date."
      },
      "start_number": {
        "type": "integer",
        "description": "Starting number of the document, should not contain year or any other formatting. Required if `start_year` given"
      },
      "end_number": {
        "type": "integer",
        "description": "Ending number of the document, should not contain year or any other formatting. Required if `end_year` given"
      },
      "start_year": {
        "type": "integer",
        "description": "Year for `start_number` parameter. Required if `start_number` given."
      },
      "end_year": {
        "type": "integer",
        "description": "Year for `end_number` parameter. Required if `end_number` given."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.billingo.hu/v3/documents{?page,per_page,block_id,partner_id,payment_method,payment_status,start_date,end_date,start_number,end_number,start_year,end_year}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "A object with a data property that contains an array of up to limit documents. Each entry in the array is a separate document object. If no more documents are available, the resulting array will be empty.",
        "**Key properties:**",
        {
          "$ul": [
            "**current_page**",
            "**data**",
            "**last_page**",
            "**next_page_url**",
            "**per_page**",
            "**prev_page_url**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**message**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**errors**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "500": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**error**",
              {
                "$ul": [
                  "**message**",
                  "**trace_id**: You should send us this ID to investigate the issue."
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
