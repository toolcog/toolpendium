{
  "name": "retrieveFile",
  "description": "Returns information about a specific file.",
  "parameters": {
    "type": "object",
    "properties": {
      "file_id": {
        "type": "string",
        "description": "The ID of the file to use for this request"
      }
    },
    "required": [
      "file_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.openai.com/v1/files/{file_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "OpenAIFile"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**bytes**",
            "**created_at**",
            "**filename**",
            "**id**",
            "**object**",
            "**purpose**",
            "**status**",
            "**status_details**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
