{
  "name": "retrieveEngine",
  "description": "Retrieves a model instance, providing basic information about it such as the owner and availability.",
  "parameters": {
    "type": "object",
    "properties": {
      "engine_id": {
        "example": "davinci",
        "type": "string",
        "description": "The ID of the engine to use for this request\n"
      }
    },
    "required": [
      "engine_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.openai.com/v1/engines/{engine_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Engine"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**id**",
            "**object**",
            "**ready**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
