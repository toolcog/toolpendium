{
  "name": "deleteModel",
  "description": "Delete a fine-tuned model. You must have the Owner role in your organization.",
  "parameters": {
    "type": "object",
    "properties": {
      "model": {
        "example": "curie:ft-acmeco-2021-03-03-21-44-20",
        "type": "string",
        "description": "The model to delete"
      }
    },
    "required": [
      "model"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.openai.com/v1/models/{model}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**deleted**",
            "**id**",
            "**object**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
