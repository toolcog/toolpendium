{
  "name": "retrieveModel",
  "description": "Retrieves a model instance, providing basic information about the model such as the owner and permissioning.",
  "parameters": {
    "type": "object",
    "properties": {
      "model": {
        "example": "text-davinci-001",
        "type": "string",
        "description": "The ID of the model to use for this request"
      }
    },
    "required": [
      "model"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.openai.com/v1/models/{model}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Model"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**id**",
            "**object**",
            "**owned_by**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
