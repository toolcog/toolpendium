{
  "name": "getUserExtensions",
  "description": "This endpoint allows you to get list of your extensions including extensions from the watchlist.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://extpose.com/api/v1/user-extensions"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**avg_rating**",
            "**extension_id**",
            "**install_count**",
            "**prev_avg_rating**",
            "**prev_install_count**",
            "**prev_rating_count**",
            "**rating_count**",
            "**update_ts**",
            "**url**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
