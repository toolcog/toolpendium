{
  "name": "getWeatherDisplay",
  "description": "Get data from the weather display software<br/>None",
  "parameters": {
    "type": "object",
    "properties": {
      "stationName": {
        "default": "cnarenal",
        "type": "string",
        "description": "currently: 'cnarenal'|'campastilla' | 'cncg'"
      },
      "period": {
        "default": "latestdata",
        "type": "string",
        "description": "Period of time to get the data latestdata|latesthour|latestday|dailylog"
      }
    },
    "required": [
      "stationName",
      "period"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.oceandrivers.com//v1.0/getWeatherDisplay/{stationName}/{?period}"
    }
  },
  "responses": {}
}
