{
  "name": "getAemetStation",
  "description": "Get data from the aemet stations<br/>None",
  "parameters": {
    "type": "object",
    "properties": {
      "stationName": {
        "default": "aeropuertopalma",
        "type": "string",
        "description": "station name currently: aeropuertopalma | caboblanco "
      },
      "period": {
        "default": "lastdata",
        "type": "string",
        "description": "Period of time to get the data. Options: lastdata lastday"
      }
    },
    "required": [
      "stationName",
      "period"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.oceandrivers.com//v1.0/getAemetStation/{stationName}/{period}/"
    }
  },
  "responses": {}
}
