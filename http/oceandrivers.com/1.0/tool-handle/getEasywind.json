{
  "name": "getEasywind",
  "description": "Get data from the easywind weather stations<br/>None",
  "parameters": {
    "type": "object",
    "properties": {
      "easywindId": {
        "default": "EW013",
        "type": "string",
        "description": "currently: 'EW013'|'EW008'"
      },
      "period": {
        "default": "latestdata",
        "type": "string",
        "description": "Period of time to get the data latestdata|latesthour|latestday"
      }
    },
    "required": [
      "easywindId",
      "period"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.oceandrivers.com//v1.0/getEasyWind/{easywindId}/{?period}"
    }
  },
  "responses": {}
}
