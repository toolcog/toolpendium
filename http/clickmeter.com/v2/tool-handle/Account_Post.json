{
  "name": "Account_Post",
  "description": "Update current account data",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/Api.Core.Dto.Accounting.User"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "Api.Core.Dto.Accounting.User": {
        "properties": {
          "boGoVal": {
            "type": "string"
          },
          "bonusClicks": {
            "format": "int64",
            "type": "integer"
          },
          "companyName": {
            "type": "string"
          },
          "companyRole": {
            "type": "string"
          },
          "email": {
            "type": "string"
          },
          "firstName": {
            "type": "string"
          },
          "lastName": {
            "type": "string"
          },
          "phone": {
            "type": "string"
          },
          "redirectOnly": {
            "type": "boolean"
          },
          "registrationDate": {
            "description": " (A date in \"YmdHis\" format)",
            "example": "20120203120530",
            "type": "string"
          },
          "timeframeMinDate": {
            "description": " (A date in \"YmdHis\" format)",
            "example": "20120203120530",
            "type": "string"
          },
          "timezone": {
            "format": "int32",
            "type": "integer"
          },
          "timezonename": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/account"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**boGoVal**",
            "**bonusClicks**",
            "**companyName**",
            "**companyRole**",
            "**email**",
            "**firstName**",
            "**lastName**",
            "**phone**",
            "**redirectOnly**",
            "**registrationDate**:  (A date in \"YmdHis\" format)",
            "**timeframeMinDate**:  (A date in \"YmdHis\" format)",
            "**timezone**",
            "**timezonename**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
