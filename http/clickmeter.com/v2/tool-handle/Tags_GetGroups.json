{
  "name": "Tags_GetGroups",
  "description": "List of all the groups associated to the user filtered by this tag.",
  "parameters": {
    "type": "object",
    "properties": {
      "tagId": {
        "format": "int64",
        "type": "integer",
        "description": "Id of the tag."
      },
      "offset": {
        "default": 0,
        "format": "int32",
        "minLength": 0,
        "type": "integer",
        "description": "Where to start when retrieving elements. Default is 0 if not specified."
      },
      "limit": {
        "default": 20,
        "format": "int32",
        "maxLength": 0,
        "minLength": 0,
        "type": "integer",
        "description": "Maximum elements to retrieve. Default to 20 if not specified."
      },
      "status": {
        "enum": [
          "deleted",
          "active"
        ],
        "type": "string",
        "description": "Status of the datapoint"
      },
      "textSearch": {
        "type": "string",
        "description": "Filter fields by this pattern"
      },
      "createdAfter": {
        "type": "string",
        "description": "Exclude groups created before this date (YYYYMMDD)"
      },
      "createdBefore": {
        "type": "string",
        "description": "Exclude groups created after this date (YYYYMMDD)"
      }
    },
    "required": [
      "tagId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/tags/{tagId}/groups{?offset,limit,status,textSearch,createdAfter,createdBefore}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entities**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
