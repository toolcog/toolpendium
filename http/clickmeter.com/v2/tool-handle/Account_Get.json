{
  "name": "Account_Get",
  "description": "Retrieve current account data",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/account"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**boGoVal**",
            "**bonusClicks**",
            "**companyName**",
            "**companyRole**",
            "**email**",
            "**firstName**",
            "**lastName**",
            "**phone**",
            "**redirectOnly**",
            "**registrationDate**:  (A date in \"YmdHis\" format)",
            "**timeframeMinDate**:  (A date in \"YmdHis\" format)",
            "**timezone**",
            "**timezonename**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
