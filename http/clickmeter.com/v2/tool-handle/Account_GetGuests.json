{
  "name": "Account_GetGuests",
  "description": "Retrieve list of a guest",
  "parameters": {
    "type": "object",
    "properties": {
      "offset": {
        "format": "int32",
        "minLength": 0,
        "type": "integer",
        "description": "Offset where to start from"
      },
      "limit": {
        "format": "int32",
        "maxLength": 0,
        "minLength": 0,
        "type": "integer",
        "description": "Limit results to this number"
      },
      "sortBy": {
        "type": "string",
        "description": "Field to sort by"
      },
      "sortDirection": {
        "enum": [
          "asc",
          "desc"
        ],
        "type": "string",
        "description": "Direction of sort \"asc\" or \"desc\""
      },
      "textSearch": {
        "type": "string",
        "description": "Filter fields by this pattern"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/account/guests{?offset,limit,sortBy,sortDirection,textSearch}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entities**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
