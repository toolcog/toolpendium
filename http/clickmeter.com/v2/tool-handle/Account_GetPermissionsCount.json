{
  "name": "Account_GetPermissionsCount",
  "description": "Retrieve count of the permissions for a guest",
  "parameters": {
    "type": "object",
    "properties": {
      "guestId": {
        "format": "int64",
        "type": "integer",
        "description": "Id of the guest"
      },
      "entityType": {
        "enum": [
          "datapoint",
          "group"
        ],
        "type": "string",
        "description": "Can be \"datapoint\" or \"group\""
      },
      "type": {
        "enum": [
          "r",
          "w"
        ],
        "type": "string",
        "description": "Can be \"w\" or \"r\""
      },
      "entityId": {
        "format": "int64",
        "type": "integer",
        "description": "Optional id of the datapoint/group entity to filter by"
      }
    },
    "required": [
      "guestId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/account/guests/{guestId}/permissions/count{?entityType,type,entityId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
