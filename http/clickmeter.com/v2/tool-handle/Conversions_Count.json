{
  "name": "Conversions_Count",
  "description": "Retrieve a count of conversions",
  "parameters": {
    "type": "object",
    "properties": {
      "status": {
        "enum": [
          "deleted",
          "active"
        ],
        "type": "string",
        "description": "Status of conversion (\"deleted\"/\"active\")"
      },
      "textSearch": {
        "type": "string",
        "description": "Filter fields by this pattern"
      },
      "createdAfter": {
        "type": "string",
        "description": "Exclude conversions created before this date (YYYYMMDD)"
      },
      "createdBefore": {
        "type": "string",
        "description": "Exclude conversions created after this date (YYYYMMDD)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/conversions/count{?status,textSearch,createdAfter,createdBefore}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
