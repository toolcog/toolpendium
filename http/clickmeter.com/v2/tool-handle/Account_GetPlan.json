{
  "name": "Account_GetPlan",
  "description": "Retrieve current account plan",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/account/plan"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**allowedPersonalDomains**",
            "**allowedPersonalUrls**",
            "**billingPeriodEnd**:  (A date in \"YmdHis\" format)",
            "**billingPeriodStart**:  (A date in \"YmdHis\" format)",
            "**bonusMonthlyEvents**",
            "**maximumDatapoints**",
            "**maximumGuests**",
            "**monthlyEvents**",
            "**name**",
            "**price**",
            "**profileId**",
            "**recurring**",
            "**recurringPeriod**",
            "**usedDatapoints**",
            "**usedMonthlyEvents**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
