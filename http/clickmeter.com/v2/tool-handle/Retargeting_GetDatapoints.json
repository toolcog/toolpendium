{
  "name": "Retargeting_GetDatapoints",
  "description": "List of all the datapoints associated to the retargeting script.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "Id of the retargeting script"
      },
      "offset": {
        "default": 0,
        "format": "int32",
        "minLength": 0,
        "type": "integer",
        "description": "Where to start when retrieving elements. Default is 0 if not specified."
      },
      "limit": {
        "default": 20,
        "format": "int32",
        "maxLength": 0,
        "minLength": 0,
        "type": "integer",
        "description": "Maximum elements to retrieve. Default to 20 if not specified."
      },
      "status": {
        "enum": [
          "deleted",
          "active",
          "paused",
          "spam"
        ],
        "type": "string",
        "description": "Status of the datapoint"
      },
      "tags": {
        "type": "string",
        "description": "A comma separated list of tags you want to filter with."
      },
      "textSearch": {
        "type": "string",
        "description": "Filter fields by this pattern"
      },
      "onlyFavorites": {
        "type": "boolean",
        "description": "Filter fields by favourite status"
      },
      "sortBy": {
        "type": "string",
        "description": "Field to sort by"
      },
      "sortDirection": {
        "enum": [
          "asc",
          "desc"
        ],
        "type": "string",
        "description": "Direction of sort \"asc\" or \"desc\""
      },
      "createdAfter": {
        "type": "string",
        "description": "Exclude datapoints created before this date (YYYYMMDD)"
      },
      "createdBefore": {
        "type": "string",
        "description": "Exclude datapoints created after this date (YYYYMMDD)"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/retargeting/{id}/datapoints{?offset,limit,status,tags,textSearch,onlyFavorites,sortBy,sortDirection,createdAfter,createdBefore}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entities**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
