{
  "name": "Tags_Get",
  "description": "List of all the groups associated to the user filtered by this tag.",
  "parameters": {
    "type": "object",
    "properties": {
      "offset": {
        "default": 0,
        "format": "int32",
        "minLength": 0,
        "type": "integer",
        "description": "Where to start when retrieving elements. Default is 0 if not specified."
      },
      "limit": {
        "default": 20,
        "format": "int32",
        "maxLength": 0,
        "minLength": 0,
        "type": "integer",
        "description": "Maximum elements to retrieve. Default to 20 if not specified."
      },
      "name": {
        "type": "string",
        "description": "Name of the tag"
      },
      "datapoints": {
        "type": "string",
        "description": "Comma separated list of datapoints id to filter by"
      },
      "groups": {
        "type": "string",
        "description": "Comma separated list of groups id to filter by"
      },
      "type": {
        "enum": [
          "tp",
          "tl",
          "dp",
          "gr"
        ],
        "type": "string",
        "description": "Type of entity related to the tag"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/tags{?offset,limit,name,datapoints,groups,type}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entities**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
