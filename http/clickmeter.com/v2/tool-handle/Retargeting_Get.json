{
  "name": "Retargeting_Get",
  "description": "List of all the retargeting scripts associated to the user",
  "parameters": {
    "type": "object",
    "properties": {
      "offset": {
        "default": 0,
        "format": "int32",
        "minLength": 0,
        "type": "integer",
        "description": "Where to start when retrieving elements. Default is 0 if not specified."
      },
      "limit": {
        "default": 20,
        "format": "int32",
        "maxLength": 0,
        "minLength": 0,
        "type": "integer",
        "description": "Maximum elements to retrieve. Default to 20 if not specified."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/retargeting{?offset,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entities**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
