{
  "name": "Account_GetDomainWhitelist",
  "description": "Retrieve list of a domains allowed to redirect in DDU mode",
  "parameters": {
    "type": "object",
    "properties": {
      "offset": {
        "format": "int32",
        "minLength": 0,
        "type": "integer",
        "description": "Offset where to start from"
      },
      "limit": {
        "format": "int32",
        "maxLength": 0,
        "minLength": 0,
        "type": "integer",
        "description": "Limit results to this number"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/account/domainwhitelist{?offset,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**entities**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
