{
  "name": "Domains_Count",
  "description": "Retrieve count of domains",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "default": "system",
        "enum": [
          "system",
          "go",
          "personal",
          "dedicated"
        ],
        "type": "string",
        "description": "Type of domain (\"system\"/\"go\"/\"personal\"/\"dedicated\"). If not specified default is \"system\""
      },
      "name": {
        "type": "string",
        "description": "Filter domains with this anmen"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/domains/count{?type,name}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
