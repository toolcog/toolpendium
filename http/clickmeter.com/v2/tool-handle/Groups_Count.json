{
  "name": "Groups_Count",
  "description": "Count the groups associated to the user.",
  "parameters": {
    "type": "object",
    "properties": {
      "status": {
        "enum": [
          "deleted",
          "active"
        ],
        "type": "string",
        "description": "Status of the datapoint"
      },
      "tags": {
        "type": "string",
        "description": "A comma separated list of tags you want to filter with."
      },
      "textSearch": {
        "type": "string",
        "description": "Filter fields by this pattern"
      },
      "createdAfter": {
        "type": "string",
        "description": "Exclude groups created before this date (YYYYMMDD)"
      },
      "createdBefore": {
        "type": "string",
        "description": "Exclude groups created after this date (YYYYMMDD)"
      },
      "write": {
        "type": "boolean",
        "description": "Write permission"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://apiv2.clickmeter.com:80/groups/count{?status,tags,textSearch,createdAfter,createdBefore,write}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
