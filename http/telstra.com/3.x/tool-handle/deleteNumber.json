{
  "name": "deleteNumber",
  "description": "Use **virtual-number** to remove a Virtual Number that's been assigned to your account.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "Telstra-api-version": {
        "example": "3.1.0",
        "type": "string"
      },
