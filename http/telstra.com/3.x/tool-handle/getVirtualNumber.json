{
  "name": "getVirtualNumber",
  "description": "Fetch the tags, replyCallbackUrl and lastUse date for a Virtual Number.",
  "parameters": {
    "type": "object",
    "properties": {
      "Telstra-api-version": {
        "example": "3.1.0",
        "type": "string"
      },
      "Content-Language": {
        "example": "en-au",
        "type": "string"
      },
      "Authorization": {
        "example": "Bearer <access_token>",
        "type": "string"
      },
      "Accept": {
        "example": "application/json",
        "type": "string"
      },
      "Accept-Charset": {
        "example": "utf-8",
        "type": "string"
      },
      "Content-Type": {
        "example": "application/json",
        "type": "string"
      },
      "virtual-number": {
        "example": "0412345678",
        "maxLength": 10,
        "minLength": 10,
        "type": "string",
        "description": "Write the Virtual Number here, using national format (e.g. 0412345678).\n"
      }
    },
    "required": [
      "Content-Language",
      "Authorization",
      "Accept",
      "Accept-Charset",
      "Content-Type",
      "virtual-number"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://products.api.telstra.com/messaging/v3/virtual-numbers/{virtual-number}"
    },
    "headers": {
      "Telstra-api-version": {
        "$": "Telstra-api-version"
      },
      "Content-Language": {
        "$": "Content-Language"
      },
      "Authorization": {
        "$": "Authorization"
      },
      "Accept": {
        "$": "Accept"
      },
      "Accept-Charset": {
        "$": "Accept-Charset"
      },
      "Content-Type": {
        "$": "Content-Type"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**lastUse**: The last time the Virtual Number was used to send a message.",
            "**replyCallbackUrl**: The URL that replies to the Virtual Number will be posted to.",
            "**tags**: Any customisable tags assigned to the Virtual Number.",
            "**virtualNumber**: The Virtual Number assigned to your account."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
