{
  "name": "importLibraryV2",
  "description": "Import library items",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationUuid": {
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/BulkImportRequest"
      }
    },
    "required": [
      "organizationUuid",
      "body"
    ],
    "$defs": {
      "BulkImportRequest": {
        "properties": {
          "products": {
            "items": {
              "$ref": "#/$defs/FullProductUpdateRequest"
            },
            "type": "array"
          }
        },
        "required": [
          "products"
        ],
        "type": "object"
      },
      "FullProductUpdateRequest": {
        "properties": {
          "categories": {
            "items": {
              "type": "string"
            },
            "maxItems": 1,
            "minItems": 0,
            "type": "array",
            "uniqueItems": true
          },
          "category": {
            "$ref": "#/$defs/CategoryDTO"
          },
          "description": {
            "maxLength": 1024,
            "minLength": 0,
            "type": "string"
          },
          "externalReference": {
            "type": "string"
          },
          "imageLookupKeys": {
            "items": {
              "type": "string"
            },
            "type": "array",
            "uniqueItems": true
          },
          "metadata": {
            "$ref": "#/$defs/MetadataDTO"
          },
          "name": {
            "maxLength": 256,
            "minLength": 1,
            "type": "string"
          },
          "online": {
            "$ref": "#/$defs/OnlineProperties"
          },
          "presentation": {
            "$ref": "#/$defs/PresentationDTO"
          },
          "taxCode": {
            "type": "string"
          },
          "taxExempt": {
            "type": "boolean"
          },
          "taxRates": {
            "items": {
              "format": "uuid",
              "type": "string"
            },
            "type": "array",
            "uniqueItems": true
          },
          "unitName": {
            "type": "string"
          },
          "uuid": {
            "format": "uuid",
            "type": "string"
          },
          "variantOptionDefinitions": {
            "$ref": "#/$defs/VariantOptionDefinitions"
          },
          "variants": {
            "items": {
              "$ref": "#/$defs/VariantDTO"
            },
            "type": "array"
          },
          "vatPercentage": {
            "maximum": 100,
            "minimum": 0,
            "type": "number"
          }
        },
        "required": [
          "name",
          "uuid"
        ],
        "type": "object"
      },
      "CategoryDTO": {
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "uuid": {
            "format": "uuid",
            "type": "string"
          }
        },
        "required": [
          "name",
          "uuid"
        ],
        "type": "object"
      },
      "MetadataDTO": {
        "properties": {
          "inPos": {
            "type": "boolean"
          },
          "source": {
            "$ref": "#/$defs/SourceDTO"
          }
        },
        "required": [
          "inPos"
        ],
        "type": "object"
      },
      "SourceDTO": {
        "properties": {
          "external": {
            "type": "boolean"
          },
          "name": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "OnlineProperties": {
        "properties": {
          "description": {
            "maxLength": 3500,
            "minLength": 0,
            "type": "string"
          },
          "presentation": {
            "$ref": "#/$defs/Presentation"
          },
          "seo": {
            "$ref": "#/$defs/SearchEngineOptimization"
          },
          "shipping": {
            "$ref": "#/$defs/Shipping"
          },
          "status": {
            "enum": [
              "ACTIVE",
              "HIDDEN"
            ],
            "type": "string"
          },
          "title": {
            "maxLength": 256,
            "minLength": 0,
            "type": "string"
          }
        },
        "required": [
          "status"
        ],
        "type": "object"
      },
      "Presentation": {
        "properties": {
          "additionalImageUrls": {
            "items": {
              "format": "url",
              "type": "string"
            },
            "maxItems": 25,
            "minItems": 0,
            "type": "array"
          },
          "displayImageUrl": {
            "format": "url",
            "type": "string"
          },
          "mediaUrls": {
            "items": {
              "format": "url",
              "type": "string"
            },
            "maxItems": 8,
            "minItems": 0,
            "type": "array"
          }
        },
        "type": "object"
      },
      "SearchEngineOptimization": {
        "properties": {
          "metaDescription": {
            "maxLength": 320,
            "minLength": 0,
            "type": "string"
          },
          "slug": {
            "maxLength": 256,
            "minLength": 1,
            "type": "string"
          },
          "title": {
            "maxLength": 256,
            "minLength": 0,
            "type": "string"
          }
        },
        "type": "object"
      },
      "Shipping": {
        "properties": {
          "shippingPricingModel": {
            "enum": [
              "FREE",
              "STANDARD"
            ],
            "type": "string"
          },
          "weight": {
            "$ref": "#/$defs/Weight"
          },
          "weightInGrams": {
            "format": "int64",
            "maximum": 2147483647,
            "minimum": 0,
            "type": "integer"
          }
        },
        "type": "object"
      },
      "Weight": {
        "properties": {
          "unit": {
            "enum": [
              "kg",
              "g",
              "oz",
              "lb"
            ],
            "type": "string"
          },
          "weight": {
            "exclusiveMinimum": false,
            "minimum": 0,
            "type": "number"
          }
        },
        "required": [
          "unit",
          "weight"
        ],
        "type": "object"
      },
      "PresentationDTO": {
        "properties": {
          "backgroundColor": {
            "type": "string"
          },
          "imageUrl": {
            "type": "string"
          },
          "textColor": {
            "type": "string"
          }
        },
        "type": "object"
      },
      "VariantOptionDefinitions": {
        "properties": {
          "definitions": {
            "items": {
              "$ref": "#/$defs/VariantOptionDefinition"
            },
            "maxItems": 3,
            "minItems": 1,
            "type": "array"
          }
        },
        "required": [
          "definitions"
        ],
        "type": "object"
      },
      "VariantOptionDefinition": {
        "properties": {
          "name": {
            "maxLength": 30,
            "minLength": 1,
            "type": "string"
          },
          "properties": {
            "items": {
              "$ref": "#/$defs/VariantOptionProperties"
            },
            "maxItems": 99,
            "minItems": 1,
            "type": "array"
          }
        },
        "required": [
          "name",
          "properties"
        ],
        "type": "object"
      },
      "VariantOptionProperties": {
        "properties": {
          "imageUrl": {
            "maxLength": 256,
            "minLength": 1,
            "type": "string"
          },
          "value": {
            "maxLength": 256,
            "minLength": 1,
            "type": "string"
          }
        },
        "required": [
          "value"
        ],
        "type": "object"
      },
      "VariantDTO": {
        "properties": {
          "barcode": {
            "maxLength": 128,
            "minLength": 0,
            "type": "string"
          },
          "costPrice": {
            "$ref": "#/$defs/Price"
          },
          "description": {
            "maxLength": 1024,
            "minLength": 0,
            "type": "string"
          },
          "name": {
            "maxLength": 256,
            "minLength": 0,
            "type": "string"
          },
          "options": {
            "items": {
              "$ref": "#/$defs/VariantOption"
            },
            "maxItems": 99,
            "minItems": 1,
            "type": "array"
          },
          "presentation": {
            "$ref": "#/$defs/PresentationDTO"
          },
          "price": {
            "$ref": "#/$defs/Price"
          },
          "sku": {
            "maxLength": 64,
            "minLength": 0,
            "type": "string"
          },
          "uuid": {
            "format": "uuid",
            "type": "string"
          },
          "vatPercentage": {
            "deprecated": true,
            "maximum": 100,
            "minimum": 0,
            "type": "number"
          }
        },
        "required": [
          "uuid"
        ],
        "type": "object"
      },
      "Price": {
