  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://products.izettle.com/organizations/{organizationUuid}/import/v2"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
