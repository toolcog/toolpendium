{
  "name": "renameCategory",
  "description": "Rename a category",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationUuid": {
        "format": "uuid",
        "type": "string"
      },
      "categoryUuid": {
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/RenameCategoryRequest"
      }
    },
    "required": [
      "organizationUuid",
      "categoryUuid",
      "body"
    ],
    "$defs": {
      "RenameCategoryRequest": {
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 1,
            "pattern": "\\S|\\S.*\\S",
            "type": "string"
          }
        },
        "required": [
          "name"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://products.izettle.com/organizations/{organizationUuid}/categories/v2/{categoryUuid}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
