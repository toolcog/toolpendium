{
  "name": "getTaxRate",
  "description": "Get a single tax rate",
  "parameters": {
    "type": "object",
    "properties": {
      "taxRateUuid": {
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "taxRateUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://products.izettle.com/v1/taxes/{taxRateUuid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**default**",
            "**label**",
            "**percentage**",
            "**uuid**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
