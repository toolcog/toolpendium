{
  "name": "getProduct",
  "description": "Get the full product with the provided UUID. The method supports conditional GET through providing a HttpHeaders.IF_NONE_MATCH header. If the conditional prerequisite is fullfilled, the full product is returned, otherwise a 304 not modified will be returned with an empty body.",
  "parameters": {
    "type": "object",
    "properties": {
      "If-None-Match": {
        "type": "string"
      },
      "organizationUuid": {
        "format": "uuid",
        "type": "string"
      },
      "productUuid": {
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "organizationUuid",
      "productUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://products.izettle.com/organizations/{organizationUuid}/products/{productUuid}"
    },
    "headers": {
      "If-None-Match": {
        "$": "If-None-Match"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**categories**",
            [
              "**category**",
              {
                "$ul": [
                  "**name**",
                  "**uuid**"
                ]
              }
            ],
            "**created**",
            "**description**",
            "**etag**",
            "**externalReference**",
            "**imageLookupKeys**",
            [
              "**metadata**",
              {
                "$ul": [
                  "**inPos**",
                  [
                    "**source**",
                    {
                      "$ul": [
                        "**external**",
                        "**name**"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**name**",
            [
              "**online**",
              {
                "$ul": [
                  "**description**",
                  [
                    "**presentation**",
                    {
                      "$ul": [
                        "**additionalImageUrls**",
                        "**displayImageUrl**",
                        "**mediaUrls**"
                      ]
                    }
                  ],
                  [
                    "**seo**",
                    {
                      "$ul": [
                        "**metaDescription**",
                        "**slug**",
                        "**title**"
                      ]
                    }
                  ],
                  [
                    "**shipping**",
                    {
                      "$ul": [
                        "**shippingPricingModel**",
                        [
                          "**weight**",
                          {
                            "$ul": [
                              "**unit**",
                              "**weight**"
                            ]
                          }
                        ],
                        "**weightInGrams**"
                      ]
                    }
                  ],
                  "**status**",
                  "**title**"
                ]
              }
            ],
            [
              "**presentation**",
              {
                "$ul": [
                  "**backgroundColor**",
                  "**imageUrl**",
                  "**textColor**"
                ]
              }
            ],
            "**taxCode**",
            "**taxExempt**",
            "**taxRates**",
            "**unitName**",
            "**updated**",
            "**updatedBy**",
            "**uuid**",
            [
              "**variantOptionDefinitions**",
              {
                "$ul": [
                  "**definitions**"
                ]
              }
            ],
            "**variants**",
            "**vatPercentage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
