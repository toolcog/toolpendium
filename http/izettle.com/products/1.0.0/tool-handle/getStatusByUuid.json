{
  "name": "getStatusByUuid",
  "description": "Get status for an import",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationUuid": {
        "format": "uuid",
        "type": "string"
      },
      "importUuid": {
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "organizationUuid",
      "importUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://products.izettle.com/organizations/{organizationUuid}/import/status/{importUuid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**created**",
            "**errorMessage**",
            "**finished**",
            "**items**",
            "**state**",
            "**uuid**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
