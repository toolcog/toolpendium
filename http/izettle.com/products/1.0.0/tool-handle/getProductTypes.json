{
  "name": "getProductTypes",
  "description": "Retrieve all categories",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationUuid": {
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "organizationUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://products.izettle.com/organizations/{organizationUuid}/categories/v2"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**categories**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
