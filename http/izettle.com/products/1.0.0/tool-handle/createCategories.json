{
  "name": "createCategories",
  "description": "Create a new category",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationUuid": {
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/CreateCategoriesRequest"
      }
    },
    "required": [
      "organizationUuid",
      "body"
    ],
    "$defs": {
      "CreateCategoriesRequest": {
        "properties": {
          "categories": {
            "items": {
              "$ref": "#/$defs/CategoryDTO"
            },
            "type": "array"
          }
        },
        "required": [
          "categories"
        ],
        "type": "object"
      },
      "CategoryDTO": {
        "properties": {
          "name": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "uuid": {
            "format": "uuid",
            "type": "string"
          }
        },
        "required": [
          "name",
          "uuid"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://products.izettle.com/organizations/{organizationUuid}/categories/v2"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
