{
  "name": "countAllProducts",
  "description": "Retrieve the count of existing products",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationUuid": {
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "organizationUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://products.izettle.com/organizations/{organizationUuid}/products/v2/count"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**productCount**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
