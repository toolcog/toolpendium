{
  "name": "createProductSlug",
  "description": "Creates a unique slug (identifier) for a product. The slug is used to create a product URL",
  "parameters": {
    "type": "object",
    "properties": {
      "organizationUuid": {
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "$ref": "#/$defs/CreateSlugRequest"
      }
    },
    "required": [
      "organizationUuid",
      "body"
    ],
    "$defs": {
      "CreateSlugRequest": {
        "properties": {
          "productName": {
            "maxLength": 256,
            "minLength": 1,
            "type": "string"
          }
        },
        "required": [
          "productName"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://products.izettle.com/organizations/{organizationUuid}/products/online/slug"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**productName**",
            "**slug**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
