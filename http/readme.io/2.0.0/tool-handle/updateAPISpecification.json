{
  "name": "updateAPISpecification",
  "description": "Update an API specification in ReadMe",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID of the API specification. The unique ID for each API can be found by navigating to your **API Definitions** page."
      },
      "body": {
        "properties": {
          "spec": {
            "description": "OpenAPI/Swagger file",
            "format": "binary",
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://dash.readme.io/api/v1/api-specification/{id}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {}
}
