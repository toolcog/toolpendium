{
  "name": "updateChangelog",
  "description": "Update a changelog with this slug",
  "parameters": {
    "type": "object",
    "properties": {
      "slug": {
        "type": "string",
        "description": "Slug of changelog"
      },
      "body": {
        "$ref": "#/components/schemas/Changelog",
        "description": "Changelog object"
      }
    },
    "required": [
      "slug",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://dash.readme.io/api/v1/changelogs/{slug}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
