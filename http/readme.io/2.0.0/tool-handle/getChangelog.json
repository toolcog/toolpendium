{
  "name": "getChangelog",
  "description": "Returns the changelog with this slug",
  "parameters": {
    "type": "object",
    "properties": {
      "slug": {
        "type": "string",
        "description": "Slug of changelog"
      }
    },
    "required": [
      "slug"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://dash.readme.io/api/v1/changelogs/{slug}"
    }
  },
  "responses": {}
}
