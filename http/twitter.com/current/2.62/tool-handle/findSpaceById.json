{
  "name": "findSpaceById",
  "description": "Returns a variety of information about the Space specified by the requested ID",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "The ID of the Space to be retrieved.",
        "example": "1SLjjRYNejbKM",
        "pattern": "^[a-zA-Z0-9]{1,13}$",
        "type": "string"
      },
      "space.fields": {
        "description": "A comma separated list of Space fields to display.",
        "example": [
          "created_at",
          "creator_id",
          "ended_at",
          "host_ids",
          "id",
          "invited_user_ids",
          "is_ticketed",
          "lang",
          "participant_count",
          "scheduled_start",
          "speaker_ids",
          "started_at",
          "state",
          "subscriber_count",
          "title",
          "topic_ids",
          "updated_at"
        ],
        "items": {
          "enum": [
            "created_at",
            "creator_id",
            "ended_at",
            "host_ids",
            "id",
            "invited_user_ids",
            "is_ticketed",
            "lang",
            "participant_count",
            "scheduled_start",
            "speaker_ids",
            "started_at",
            "state",
            "subscriber_count",
            "title",
            "topic_ids",
            "updated_at"
          ],
          "type": "string"
        },
        "minItems": 1,
        "type": "array",
        "uniqueItems": true
      },
      "expansions": {
        "description": "A comma separated list of fields to expand.",
        "example": [
          "creator_id",
          "host_ids",
          "invited_user_ids",
          "speaker_ids",
          "topic_ids"
        ],
        "items": {
          "enum": [
            "creator_id",
            "host_ids",
            "invited_user_ids",
            "speaker_ids",
            "topic_ids"
          ],
          "type": "string"
        },
        "minItems": 1,
        "type": "array",
        "uniqueItems": true
      },
      "user.fields": {
        "description": "A comma separated list of User fields to display.",
        "example": [
          "created_at",
          "description",
          "entities",
          "id",
          "location",
          "name",
          "pinned_tweet_id",
          "profile_image_url",
          "protected",
          "public_metrics",
          "url",
          "username",
          "verified",
          "verified_type",
          "withheld"
        ],
        "items": {
          "enum": [
            "created_at",
            "description",
            "entities",
            "id",
            "location",
            "name",
            "pinned_tweet_id",
            "profile_image_url",
            "protected",
            "public_metrics",
            "url",
            "username",
            "verified",
            "verified_type",
            "withheld"
          ],
          "type": "string"
        },
        "minItems": 1,
        "type": "array",
        "uniqueItems": true
      },
      "topic.fields": {
        "description": "A comma separated list of Topic fields to display.",
        "example": [
          "description",
          "id",
          "name"
        ],
        "items": {
          "enum": [
            "description",
            "id",
            "name"
          ],
          "type": "string"
        },
        "minItems": 1,
        "type": "array",
        "uniqueItems": true
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.twitter.com/2/spaces/{id}{?space.fields,expansions,user.fields,topic.fields}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**: ",
              {
                "$ul": [
                  "**created_at**: Creation time of the Space.",
                  "**creator_id**: Unique identifier of this User. This is returned as a string in order to avoid complications with languages and tools that cannot handle large integers.",
                  "**ended_at**: End time of the Space.",
                  "**host_ids**: The user ids for the hosts of the Space.",
                  "**id**: The unique identifier of this Space.",
                  "**invited_user_ids**: An array of user ids for people who were invited to a Space.",
                  "**is_ticketed**: Denotes if the Space is a ticketed Space.",
                  "**lang**: The language of the Space.",
                  "**participant_count**: The number of participants in a Space.",
                  "**scheduled_start**: A date time stamp for when a Space is scheduled to begin.",
                  "**speaker_ids**: An array of user ids for people who were speakers in a Space.",
                  "**started_at**: When the Space was started as a date string.",
                  "**state**: The current state of the Space.",
                  "**subscriber_count**: The number of people who have either purchased a ticket or set a reminder for this Space.",
                  "**title**: The title of the Space.",
                  "**topics**: The topics of a Space, as selected by its creator.",
                  "**updated_at**: When the Space was last updated."
                ]
              }
            ],
            "**errors**",
            [
              "**includes**",
              {
                "$ul": [
                  "**media**",
                  "**places**",
                  "**polls**",
                  "**topics**",
                  "**tweets**",
                  "**users**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
