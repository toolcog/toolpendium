{
  "name": "usersIdUnretweets",
  "description": "Causes the User (in the path) to unretweet the specified Tweet. The User must match the User context authorizing the request",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "$ref": "#/components/schemas/UserIdMatchesAuthenticatedUser",
        "description": "The ID of the authenticated source User that is requesting to retweet the Tweet."
      },
      "source_tweet_id": {
        "$ref": "#/components/schemas/TweetId",
        "description": "The ID of the Tweet that the User is requesting to unretweet."
      }
    },
    "required": [
      "id",
      "source_tweet_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://api.twitter.com/2/users/{id}/retweets/{source_tweet_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**data**",
              {
                "$ul": [
                  "**retweeted**"
                ]
              }
            ],
            "**errors**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**",
            "**message**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
