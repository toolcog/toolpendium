{
  "name": "listUserFollow",
  "description": "Causes a User to follow a List.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "$ref": "#/components/schemas/UserIdMatchesAuthenticatedUser",
        "description": "The ID of the authenticated source User that will follow the List."
      },
      "body": {
        "$ref": "#/$defs/ListFollowedRequest"
      }
    },
    "required": [
      "id"
    ],
    "$defs": {
      "ListFollowedRequest": {
        "properties": {
          "list_id": {
            "$ref": "#/$defs/ListId"
          }
        },
        "required": [
          "list_id"
        ],
        "type": "object"
      },
      "ListId": {
        "description": "The unique identifier of this List.",
        "example": "1146654567674912769",
        "pattern": "^[0-9]{1,19}$",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.twitter.com/2/users/{id}/followed_lists"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
