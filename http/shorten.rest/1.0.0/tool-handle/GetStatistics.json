{
  "name": "GetStatistics",
  "description": "Retrieve the raw click statistics for your account. Clicks are retrieved by creation date in descending order.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/components/schemas/ClicksFilterModel",
        "description": "Filter"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.shorten.rest/clicks/pg"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**accountId**",
            "**alias**",
            "**aliasId**",
            "**browser**",
            "**country**",
            "**createdAt**",
            "**destination**",
            "**domain**",
            "**id**",
            "**os**",
            "**referrer**",
            "**userAgent**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
