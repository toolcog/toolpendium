{
  "name": "GetAlias",
  "description": "Get detailed information for a single alias by providing its alias and domain name",
  "parameters": {
    "type": "object",
    "properties": {
      "domainName": {
        "default": "short.fyi",
        "type": "string",
        "description": "domain which alias belongs to (string without `http/https` or `/`)"
      },
      "aliasName": {
        "type": "string",
        "description": "alias value (without `/` at the beginning)"
      }
    },
    "required": [
      "aliasName"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.shorten.rest/aliases{?domainName,aliasName}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**createdAt**",
            "**destinations**",
            "**domainName**",
            "**metatags**",
            "**name**",
            "**snippets**",
            "**updatedAt**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
