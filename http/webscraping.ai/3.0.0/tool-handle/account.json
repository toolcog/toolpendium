{
  "name": "account",
  "description": "Always returns JSON",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.webscraping.ai/account"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Account limits info"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**remaining_api_calls**: Remaining API calls quota",
            "**remaining_concurrency**: Remaining concurrent requests",
            "**resets_at**: Next billing cycle start time (UNIX timestamp)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Generic error"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**: Error description"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
