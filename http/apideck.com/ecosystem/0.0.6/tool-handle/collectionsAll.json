{
  "name": "collectionsAll",
  "description": "List collections",
  "parameters": {
    "type": "object",
    "properties": {
      "ecosystem_id": {
        "type": "string"
      },
      "cursor": {
        "nullable": true,
        "type": "string",
        "description": "Cursor to start from. You can find cursors for next/previous pages in the meta.cursors property of the response."
      },
      "limit": {
        "default": 50,
        "maximum": 200,
        "minimum": 1,
        "type": "integer",
        "description": "Number of records to return"
      }
    },
    "required": [
      "ecosystem_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.apideck.com/ecosystems/{ecosystem_id}/collections{?cursor,limit}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**status**: HTTP Response Status",
            "**status_code**: HTTP Response Status Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
