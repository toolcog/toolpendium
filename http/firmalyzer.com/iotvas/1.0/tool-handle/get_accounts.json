{
  "name": "get_accounts",
  "description": "Get default accounts and password hashes of a firmware",
  "parameters": {
    "type": "object",
    "properties": {
      "firmware_hash": {
        "description": "SHA2 hash of device firmware",
        "example": "af88b1aaac0b222df8539f3ae1479b5c8eaeae41f1776b5dd2fa805cb33a1175",
        "title": "Firmware Hash",
        "type": "string"
      }
    },
    "required": [
      "firmware_hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/firmware/{firmware_hash}/accounts"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Response Firmware Accounts Firmware  Firmware Hash  Accounts Get"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**gid**",
            "**hash_algorithm**",
            "**home_dir**",
            "**name**",
            "**pwd_hash**",
            "**shell**",
            "**uid**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "HTTPValidationError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
