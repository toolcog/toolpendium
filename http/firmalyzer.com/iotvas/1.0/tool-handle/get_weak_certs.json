{
  "name": "get_weak_certs",
  "description": "Get certificates with weak fingerprinting algorithms that are mebedded in a device firmware",
  "parameters": {
    "type": "object",
    "properties": {
      "firmware_hash": {
        "description": "SHA2 hash of device firmware",
        "example": "52841661d61e00649451cc471e9b56d169df8041926b1252bb3fd0710c27b12c",
        "title": "Firmware Hash",
        "type": "string"
      }
    },
    "required": [
      "firmware_hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/firmware/{firmware_hash}/weak-certs"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Response Firmware Weak Certs Firmware  Firmware Hash  Weak Certs Get"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**file_hash**",
            "**file_name**",
            "**sign_algorithm**",
            "**subject_name**",
            "**thumb_print**",
            "**valid_from**",
            "**valid_to**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "HTTPValidationError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
