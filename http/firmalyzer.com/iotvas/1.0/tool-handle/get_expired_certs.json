{
  "name": "get_expired_certs",
  "description": "Get expired digital certificates embedded in a device firmware",
  "parameters": {
    "type": "object",
    "properties": {
      "firmware_hash": {
        "description": "SHA2 hash of device firmware",
        "example": "ac7c090c34338ea6a3b335004755e24578e7e4eee739c5c33736f0822b64907e",
        "title": "Firmware Hash",
        "type": "string"
      }
    },
    "required": [
      "firmware_hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/firmware/{firmware_hash}/expired-certs"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Response Firmware Expired Certs Firmware  Firmware Hash  Expired Certs Get"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**file_hash**",
            "**file_name**",
            [
              "**public_key**",
              {
                "$ul": [
                  "**algorithm**",
                  "**bits**"
                ]
              }
            ],
            "**subject_name**",
            "**thumb_print**",
            "**valid_from**",
            "**valid_to**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "HTTPValidationError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
