{
  "name": "get_config_issues",
  "description": "Get default OS configuration issues of a device firmware",
  "parameters": {
    "type": "object",
    "properties": {
      "firmware_hash": {
        "description": "SHA2 hash of device firmware",
        "example": "aa96e4d41a4b0ceb3f1ae4d94f3cb445621b9501e3a9c69e6b9eb37c5888a03c",
        "title": "Firmware Hash",
        "type": "string"
      }
    },
    "required": [
      "firmware_hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/firmware/{firmware_hash}/config-issues"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Response Firmware Config Issues Firmware  Firmware Hash  Config Issues Get"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**config_file**",
            "**issues**",
            "**service_name**",
            "**suggestions**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "HTTPValidationError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
