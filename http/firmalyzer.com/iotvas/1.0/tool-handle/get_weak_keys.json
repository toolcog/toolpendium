{
  "name": "get_weak_keys",
  "description": "Get weak crypto keys with short length",
  "parameters": {
    "type": "object",
    "properties": {
      "firmware_hash": {
        "description": "SHA2 hash of device firmware",
        "example": "852031776c09f8152c90496f2c3fac85b46a938d20612d7fc03eea8aab46f23e",
        "title": "Firmware Hash",
        "type": "string"
      }
    },
    "required": [
      "firmware_hash"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/v1/firmware/{firmware_hash}/weak-keys"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Response Firmware Weak Keys Firmware  Firmware Hash  Weak Keys Get"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**algorithm**",
            "**bits**",
            "**file_hash**",
            "**file_name**",
            "**pem_type**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "HTTPValidationError"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
