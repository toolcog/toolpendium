{
  "name": "getPodcastAudience",
  "description": "Fetch audience demographics for a podcast - 1) directly measured on the Listen Notes platform; 2) only supports audience breakdown by regions for now; 3) not every podcast has data.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-ListenAPI-Key": {
        "type": "string",
        "description": "Get API Key on listennotes.com/api"
      },
      "id": {
        "type": "string",
        "description": "Podcast id."
      }
    },
    "required": [
      "X-ListenAPI-Key",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://listen-api.listennotes.com/api/v2/podcasts/{id}/audience"
    },
    "headers": {
      "X-ListenAPI-Key": {
        "$": "X-ListenAPI-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**by_regions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
