{
  "name": "getTrendingSearches",
  "description": "Fetch up to 10 most recent trending search terms on the Listen Notes platform.",
  "parameters": {
    "type": "object",
    "properties": {
      "X-ListenAPI-Key": {
        "type": "string",
        "description": "Get API Key on listennotes.com/api"
      }
    },
    "required": [
      "X-ListenAPI-Key"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://listen-api.listennotes.com/api/v2/trending_searches"
    },
    "headers": {
      "X-ListenAPI-Key": {
        "$": "X-ListenAPI-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**terms**: Trending search terms"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
