{
  "name": "getPodcastsByDomainName",
  "description": "Fetch podcasts by a publisher's domain name, e.g., nytimes.com, wondery.com, npr.org...\nEach request will return up to 10 podcasts. You can use the `page` parameter to paginate.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "X-ListenAPI-Key": {
        "type": "string",
        "description": "Get API Key on listennotes.com/api"
      },
      "domain_name": {
        "type": "string",
        "description": "A publisher's domain name, e.g., nytimes.com, wondery.com, npr.org..."
      },
      "page": {
        "default": 1,
        "examples": [
          1
        ],
        "type": "integer",
        "description": "Page number of the podcasts from this domain name"
      }
    },
    "required": [
      "X-ListenAPI-Key",
      "domain_name"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://listen-api.listennotes.com/api/v2/podcasts/domains/{domain_name}{?page}"
    },
    "headers": {
      "X-ListenAPI-Key": {
        "$": "X-ListenAPI-Key"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**has_next**",
            "**has_previous**",
            "**next_page_number**",
            "**page_number**",
            "**podcasts**",
            "**previous_page_number**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
