{
  "name": "idGetSinglePerformanceProfile",
  "description": "Returns single performance profile with the given id",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "id for a specific"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/user/performance/profiles/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "PerformanceProfile represents the performance profile that needs\nto be saved",
        "**Key properties:**",
        {
          "$ul": [
            "**concurrent_request**",
            "**content_type**",
            "**created_at**: +protobuf.options.marshal=false",
            "**duration**",
            "**endpoints**",
            "**id**",
            "**last_run**: +protobuf.options.marshal=false",
            "**load_generators**",
            "**name**",
            "**qps**",
            "**request_body**",
            "**request_cookies**",
            "**request_headers**",
            "**schedule**",
            "**service_mesh**",
            "**total_results**",
            "**updated_at**: +protobuf.options.marshal=false"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
