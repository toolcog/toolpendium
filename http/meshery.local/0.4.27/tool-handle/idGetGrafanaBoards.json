{
  "name": "idGetGrafanaBoards",
  "description": "Used for fetching Grafana boards and panels",
  "parameters": {
    "type": "object",
    "properties": {
      "dashboardSearch": {
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/telemetry/metrics/grafana/boards{?dashboardSearch}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "GrafanaBoard represents a Grafana board",
        "**Key properties:**",
        {
          "$ul": [
            "**org_id**",
            "**panels**",
            "**slug**",
            "**template_vars**",
            "**title**",
            "**uid**",
            "**uri**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.title"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
