{
  "name": "idGetFilterFile",
  "description": "Returns all the Meshery Filters saved by the current user",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/filter"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "FiltersAPIResponse response retruned by filterfile endpoint on meshery server",
        "**Key properties:**",
        {
          "$ul": [
            "**filters**",
            "**page**",
            "**page_size**",
            "**total_count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
