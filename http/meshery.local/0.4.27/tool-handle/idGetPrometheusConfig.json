{
  "name": "idGetPrometheusConfig",
  "description": "Used for fetching Prometheus configuration",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/telemetry/metrics/config"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Prometheus represents the prometheus session config",
        "**Key properties:**",
        {
          "$ul": [
            "**prometheusURL**",
            "**selectedPrometheusBoardsConfigs**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
