{
  "name": "idGetPatternFiles",
  "description": "Returns the list of all the patterns saved by the current user\nThis will return all the patterns with their details",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/pattern"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "PatternsAPIResponse response retruned by patternfile endpoint on meshery server",
        "**Key properties:**",
        {
          "$ul": [
            "**page**",
            "**page_size**",
            "**patterns**",
            "**total_count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
