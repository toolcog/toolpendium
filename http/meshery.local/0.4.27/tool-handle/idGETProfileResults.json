{
  "name": "idGETProfileResults",
  "description": "Fetchs pages of results from Remote Provider for the given id",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "id for a specific"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/user/performance/profiles/{id}/results"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "PerformanceResultsAPIResponse response retruned by performance endpoint on meshery server",
        "**Key properties:**",
        {
          "$ul": [
            "**page**",
            "**page_size**",
            "**results**",
            "**total_count**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
