{
  "name": "idGetSystemVersion",
  "description": "Returns the running Meshery version",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/system/version"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Version defines the Json payload structure for version api\\"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**build**",
            "**commitsha**",
            "**latest**",
            "**outdated**",
            "**release_channel**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
