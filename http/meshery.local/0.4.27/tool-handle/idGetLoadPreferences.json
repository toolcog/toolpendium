{
  "name": "idGetLoadPreferences",
  "description": "Used for fetching load test preferences",
  "parameters": {
    "type": "object",
    "properties": {
      "uuid": {
        "format": "uuid",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/user/prefs/perf{?uuid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**clients**: Single or distributed load generators",
            "**duration**: Length of time the endpoint will be under load",
            "**id**",
            "**labels**",
            "**name**",
            "**smp_version**: Spec version"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
