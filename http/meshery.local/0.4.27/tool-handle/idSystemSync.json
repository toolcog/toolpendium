{
  "name": "idSystemSync",
  "description": "Used to send session data to the UI for initial sync",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://meshery.local/api/system/sync"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Preference represents the data stored in session / local DB",
        "**Key properties:**",
        {
          "$ul": [
            "**anonymousPerfResults**",
            "**anonymousUsageStats**",
            [
              "**grafana**: Grafana represents the Grafana session config",
              {
                "$ul": [
                  "**grafanaAPIKey**",
                  "**grafanaURL**",
                  "**selectedBoardsConfigs**: GrafanaBoardSearch string          `json:\"grafanaBoardSearch,omitempty\"`"
                ]
              }
            ],
            [
              "**k8sConfig**: K8SConfig represents all the k8s session config",
              {
                "$ul": [
                  "**clusterConfigured**",
                  "**config**",
                  "**configuredServer**",
                  "**contextName**",
                  "**inClusterConfig**",
                  "**k8sfile**",
                  "**nodes**",
                  "**server_version**"
                ]
              }
            ],
            [
              "**loadTestPrefs**: LoadTestPreferences represents the load test preferences",
              {
                "$ul": [
                  "**c**",
                  "**gen**",
                  "**qps**",
                  "**t**"
                ]
              }
            ],
            "**meshAdapters**",
            [
              "**prometheus**: Prometheus represents the prometheus session config",
              {
                "$ul": [
                  "**prometheusURL**",
                  "**selectedPrometheusBoardsConfigs**"
                ]
              }
            ],
            "**updated_at**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
