{
  "name": "idPostPrometheusBoardImport",
  "description": "Used for importing Grafana board for Prometheus",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://meshery.local/api/telemetry/metrics/board_import"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "GrafanaBoard represents a Grafana board",
        "**Key properties:**",
        {
          "$ul": [
            "**org_id**",
            "**panels**",
            "**slug**",
            "**template_vars**",
            "**title**",
            "**uid**",
            "**uri**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
