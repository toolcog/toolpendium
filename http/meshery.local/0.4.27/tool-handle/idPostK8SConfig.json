{
  "name": "idPostK8SConfig",
  "description": "Used to add kubernetes config to System",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://meshery.local/api/system/kubernetes"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "K8SConfig represents all the k8s session config",
        "**Key properties:**",
        {
          "$ul": [
            "**clusterConfigured**",
            "**config**",
            "**configuredServer**",
            "**contextName**",
            "**inClusterConfig**",
            "**k8sfile**",
            "**nodes**",
            "**server_version**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
