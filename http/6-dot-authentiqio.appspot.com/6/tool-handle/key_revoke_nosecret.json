{
  "name": "key_revoke_nosecret",
  "description": "Revoke an Authentiq ID using email & phone.\n\nIf called with `email` and `phone` only, a verification code \nwill be sent by email. Do a second call adding `code` to \ncomplete the revocation.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "email": {
        "type": "string",
        "description": "primary email associated to Key (ID)"
      },
      "phone": {
        "type": "string",
        "description": "primary phone number, international representation"
      },
      "code": {
        "type": "string",
        "description": "verification code sent by email"
      }
    },
    "required": [
      "email",
      "phone"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "https://6-dot-authentiqio.appspot.com/key{?email,phone,code}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**status**: pending or done"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**error**",
            "**title**",
            "**type**: unique uri for this error"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
