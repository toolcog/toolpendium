{
  "name": "getVideoBlocks",
  "description": "List video blocks",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "enum": [
          1,
          2
        ],
        "type": "integer",
        "description": "list only blocks that match this type:\n- `1`: manual block\n- `2`: automatic block that needs review\n"
      },
      "search": {
        "type": "string",
        "description": "plain search that will match with video titles, and more"
      },
      "start": {
        "minimum": 0,
        "type": "integer",
        "description": "Offset used to paginate results"
      },
      "count": {
        "default": 15,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Number of items to return"
      },
      "sort": {
        "enum": [
          "-id",
          "name",
          "-duration",
          "-views",
          "-likes",
          "-dislikes",
          "-uuid",
          "-createdAt"
        ],
        "type": "string",
        "description": "Sort blocklists by criteria"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://peertube2.cpy.re/api/v1/videos/blacklist{?type,search,start,count,sort}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
