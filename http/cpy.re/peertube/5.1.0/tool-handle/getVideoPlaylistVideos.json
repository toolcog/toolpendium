{
  "name": "getVideoPlaylistVideos",
  "description": "List videos of a playlist",
  "parameters": {
    "type": "object",
    "properties": {
      "playlistId": {
        "$ref": "#/components/schemas/VideoPlaylist/properties/id",
        "description": "Playlist id"
      },
      "start": {
        "minimum": 0,
        "type": "integer",
        "description": "Offset used to paginate results"
      },
      "count": {
        "default": 15,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Number of items to return"
      }
    },
    "required": [
      "playlistId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://peertube2.cpy.re/api/v1/video-playlists/{playlistId}/videos{?start,count}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
