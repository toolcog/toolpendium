{
  "name": "reorderVideoPlaylist",
  "description": "Reorder a playlist",
  "parameters": {
    "type": "object",
    "properties": {
      "playlistId": {
        "$ref": "#/components/schemas/VideoPlaylist/properties/id",
        "description": "Playlist id"
      },
      "body": {
        "properties": {
          "insertAfterPosition": {
            "description": "New position for the block to reorder, to add the block before the first element",
            "minimum": 0,
            "type": "integer"
          },
          "reorderLength": {
            "description": "How many element from `startPosition` to reorder",
            "minimum": 1,
            "type": "integer"
          },
          "startPosition": {
            "description": "Start position of the element to reorder",
            "minimum": 1,
            "type": "integer"
          }
        },
        "required": [
          "startPosition",
          "insertAfterPosition"
        ],
        "type": "object"
      }
    },
    "required": [
      "playlistId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://peertube2.cpy.re/api/v1/video-playlists/{playlistId}/videos/reorder"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
