{
  "name": "putVideoPlaylistVideo",
  "description": "Update a playlist element",
  "parameters": {
    "type": "object",
    "properties": {
      "playlistId": {
        "$ref": "#/components/schemas/VideoPlaylist/properties/id",
        "description": "Playlist id"
      },
      "playlistElementId": {
        "$ref": "#/components/schemas/id",
        "description": "Playlist element id"
      },
      "body": {
        "properties": {
          "startTimestamp": {
            "description": "Start the video at this specific timestamp",
            "format": "seconds",
            "type": "integer"
          },
          "stopTimestamp": {
            "description": "Stop the video at this specific timestamp",
            "format": "seconds",
            "type": "integer"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "playlistId",
      "playlistElementId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://peertube2.cpy.re/api/v1/video-playlists/{playlistId}/videos/{playlistElementId}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
