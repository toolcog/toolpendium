{
  "name": "updatePlugin",
  "description": "Update a plugin",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "oneOf": [
          {
            "additionalProperties": false,
            "properties": {
              "npmName": {
                "example": "peertube-plugin-auth-ldap",
                "type": "string"
              }
            },
            "required": [
              "npmName"
            ],
            "type": "object"
          },
          {
            "additionalProperties": false,
            "properties": {
              "path": {
                "type": "string"
              }
            },
            "required": [
              "path"
            ],
            "type": "object"
          }
        ]
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://peertube2.cpy.re/api/v1/plugins/update"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
