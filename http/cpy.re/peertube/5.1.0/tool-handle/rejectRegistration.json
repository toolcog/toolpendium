{
  "name": "rejectRegistration",
  "description": "Reject registration",
  "parameters": {
    "type": "object",
    "properties": {
      "registrationId": {
        "$ref": "#/components/schemas/id",
        "description": "Registration ID"
      },
      "body": {
        "$ref": "#/$defs/UserRegistrationAcceptOrReject"
      }
    },
    "required": [
      "registrationId"
    ],
    "$defs": {
      "UserRegistrationAcceptOrReject": {
        "properties": {
          "moderationResponse": {
            "description": "Moderation response to send to the user",
            "type": "string"
          },
          "preventEmailDelivery": {
            "description": "Set it to true if you don't want PeerTube to send an email to the user",
            "type": "boolean"
          }
        },
        "required": [
          "moderationResponse"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://peertube2.cpy.re/api/v1/users/registrations/{registrationId}/reject"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
