{
  "name": "confirmTwoFactorRequest",
  "description": "Confirm a two factor authentication request",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "$ref": "#/components/schemas/id",
        "description": "Entity id"
      },
      "body": {
        "properties": {
          "otpToken": {
            "description": "OTP token generated by the app",
            "type": "string"
          },
          "requestToken": {
            "description": "Token to identify the two factor request",
            "type": "string"
          }
        },
        "required": [
          "requestToken",
          "otpToken"
        ],
        "type": "object"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://peertube2.cpy.re/api/v1/users/{id}/two-factor/confirm-request"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
