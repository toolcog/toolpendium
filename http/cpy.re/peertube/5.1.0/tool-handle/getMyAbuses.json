{
  "name": "getMyAbuses",
  "description": "List my abuses",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer",
        "description": "only list the report with this id"
      },
      "state": {
        "$ref": "#/$defs/AbuseStateSet"
      },
      "sort": {
        "enum": [
          "-id",
          "-createdAt",
          "-state"
        ],
        "type": "string",
        "description": "Sort abuses by criteria"
      },
      "start": {
        "minimum": 0,
        "type": "integer",
        "description": "Offset used to paginate results"
      },
      "count": {
        "default": 15,
        "maximum": 100,
        "minimum": 1,
        "type": "integer",
        "description": "Number of items to return"
      }
    },
    "$defs": {
      "AbuseStateSet": {
        "description": "The abuse state (Pending = `1`, Rejected = `2`, Accepted = `3`)",
        "enum": [
          1,
          2,
          3
        ],
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://peertube2.cpy.re/api/v1/users/me/abuses{?id,state,sort,start,count}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**data**",
            "**total**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
