{
  "name": "getSubmissionHistory",
  "description": "For teachers only. Returns a detailed history of the submission. This currently includes state and grade histories.\n",
  "parameters": {
    "type": "object",
    "properties": {
      "class": {
        "type": "string",
        "description": "Unique identifier of the class"
      },
      "assignment": {
        "type": "string",
        "description": "Unique identifier of the assignment"
      },
      "submission": {
        "type": "string",
        "description": "Unique identifier of the submission"
      }
    },
    "required": [
      "class",
      "assignment",
      "submission"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.flat.io/v2/classes/{class}/assignments/{assignment}/submissions/{submission}/history"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "History item of the submission",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**attachment**",
              {
                "$ul": [
                  "**revision**: The revision identifier that changed",
                  "**score**: The score identifier that changed"
                ]
              }
            ],
            "**date**: The date when the submission was changed",
            "**draftGrade**: The numerator of the grade at this time in the submission grade history",
            "**grade**: The numerator of the grade at this time in the submission grade history",
            "**maxPoints**: The denominator of the grade at this time in the submission grade history",
            "**state**: State of the submission",
            "**users**: The user(s) unique identifier(s) who made the change"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "An API Error response",
        "**Key properties:**",
        {
          "$ul": [
            "**code**: A corresponding code for this error",
            "**id**: An unique error identifier generated for the request",
            "**message**: A printable message for this message",
            "**param**: The related parameter that caused the error"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "An API Error response",
        "**Key properties:**",
        {
          "$ul": [
            "**code**: A corresponding code for this error",
            "**id**: An unique error identifier generated for the request",
            "**message**: A printable message for this message",
            "**param**: The related parameter that caused the error"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "An API Error response",
        "**Key properties:**",
        {
          "$ul": [
            "**code**: A corresponding code for this error",
            "**id**: An unique error identifier generated for the request",
            "**message**: A printable message for this message",
            "**param**: The related parameter that caused the error"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
