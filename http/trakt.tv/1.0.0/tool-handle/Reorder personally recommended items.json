{
  "name": "Reorder_personally_recommended_items",
  "description": "#### &#128274; OAuth Required\n\nReorder all items on a user's personal recommendations by sending the updated `rank` of list item ids. Use the [**/sync/recommendations**](#reference/sync/get-personal-recommendations) method to get all list item ids.",
  "parameters": {
    "type": "object",
    "properties": {
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      },
      "body": {
        "example": {
          "rank": [
            923,
            324,
            98768,
            456456,
            345,
            12,
            990
          ]
        },
        "properties": {
          "rank": {
            "items": {
              "type": "number"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.trakt.tv/sync/recommendations/reorder"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
