{
  "name": "Like_a_list",
  "description": "#### &#128274; OAuth Required\n\nVotes help determine popular lists. Only one like is allowed per list per user.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "User slug"
      },
      "list_id": {
        "type": "string",
        "description": "Trakt ID or Trakt slug"
      },
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      }
    },
    "required": [
      "id",
      "list_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.trakt.tv/users/{id}/lists/{list_id}/like"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    }
  },
  "responses": {}
}
