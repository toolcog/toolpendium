{
  "name": "Like_a_comment",
  "description": "#### &#128274; OAuth Required\n\nVotes help determine popular comments. Only one like is allowed per comment per user.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "A specific comment ID."
      },
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.trakt.tv/comments/{id}/like"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    }
  },
  "responses": {}
}
