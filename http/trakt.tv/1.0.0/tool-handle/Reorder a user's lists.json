{
  "name": "Reorder_a_user_s_lists",
  "description": "#### &#128274; OAuth Required\n\nReorder all lists by sending the updated `rank` of list ids. Use the [**/users/:id/lists**](#reference/users/lists) method to get all list ids.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "User slug"
      },
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      },
      "body": {
        "example": {
          "rank": [
            823,
            224,
            88768,
            356456,
            245,
            2,
            890
          ]
        },
        "properties": {
          "rank": {
            "items": {
              "type": "number"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.trakt.tv/users/{id}/lists/reorder"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
