{
  "name": "Get_episode_stats",
  "description": "Returns lots of episode stats.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "Trakt ID, Trakt slug, or IMDB ID"
      },
      "season": {
        "type": "integer",
        "description": "season number"
      },
      "episode": {
        "type": "integer",
        "description": "episode number"
      },
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      }
    },
    "required": [
      "id",
      "season",
      "episode"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.trakt.tv/shows/{id}/seasons/{season}/episodes/{episode}/stats"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    }
  },
  "responses": {}
}
