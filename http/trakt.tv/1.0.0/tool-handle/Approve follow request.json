{
  "name": "Approve_follow_request",
  "description": "#### &#128274; OAuth Required\n\nApprove a follower using the `id` of the request. If the `id` is not found, was already approved, or was already denied, a `404` error will be returned.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "ID of the follower request."
      },
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.trakt.tv/users/requests/{id}"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    }
  },
  "responses": {}
}
