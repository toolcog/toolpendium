{
  "name": "Get_countries",
  "description": "Get a list of all countries, including names and codes.",
  "parameters": {
    "type": "object",
    "properties": {
      "type": {
        "enum": [
          "movies",
          "shows"
        ],
        "type": "string",
        "description": ""
      },
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      }
    },
    "required": [
      "type"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.trakt.tv/countries/{type}"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    }
  },
  "responses": {}
}
