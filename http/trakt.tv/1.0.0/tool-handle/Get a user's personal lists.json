{
  "name": "Get_a_user_s_personal_lists",
  "description": "#### &#128275; OAuth Optional &#128513; Emojis\n\nReturns all personal lists for a user. Use the [**/users/:id/lists/:list_id/items**](#reference/users/list-items) method to get the actual items a specific list contains.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "User slug"
      },
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.trakt.tv/users/{id}/lists"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    }
  },
  "responses": {}
}
