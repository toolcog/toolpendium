{
  "name": "Reorder_items_on_a_list",
  "description": "#### &#128274; OAuth Required\n\nReorder all items on a list by sending the updated `rank` of list item ids. Use the [**/users/:id/lists/:list_id/items**](#reference/users/list-items) method to get all list item ids.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "User slug"
      },
      "list_id": {
        "type": "string",
        "description": "Trakt ID or Trakt slug"
      },
      "trakt-api-version": {
        "type": "string",
        "description": "e.g. 2"
      },
      "trakt-api-key": {
        "type": "string",
        "description": "e.g. [client_id]"
      },
      "body": {
        "example": {
          "rank": [
            923,
            324,
            98768,
            456456,
            345,
            12,
            990
          ]
        },
        "properties": {
          "rank": {
            "items": {
              "type": "number"
            },
            "type": "array"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "id",
      "list_id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.trakt.tv/users/{id}/lists/{list_id}/items/reorder"
    },
    "headers": {
      "trakt-api-version": {
        "$": "trakt-api-version"
      },
      "trakt-api-key": {
        "$": "trakt-api-key"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
