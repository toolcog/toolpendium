{
  "name": "testnet_getTokenId",
  "description": "Translates a token symbol to a tokenId if a token exists with that symbol on the network\n",
  "parameters": {
    "type": "object",
    "properties": {
      "tokensymbol": {
        "type": "string",
        "description": "Token symbol"
      }
    },
    "required": [
      "tokensymbol"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ntp1node.nebl.io//testnet/ntp1/tokenid/{tokensymbol}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**tokenId**: Unique ID of the token",
            "**tokenName**: String representing the token Symbol"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
