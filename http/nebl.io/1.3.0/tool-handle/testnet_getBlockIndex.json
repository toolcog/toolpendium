{
  "name": "testnet_getBlockIndex",
  "description": "Returns the block hash of a block at a given block index",
  "parameters": {
    "type": "object",
    "properties": {
      "blockindex": {
        "type": "number",
        "description": "Block Index"
      }
    },
    "required": [
      "blockindex"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ntp1node.nebl.io//testnet/ins/block-index/{blockindex}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**blockHash**: Hash of the requested block"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
