{
  "name": "testnet_getTxs",
  "description": "Returns all transactions by block or address",
  "parameters": {
    "type": "object",
    "properties": {
      "address": {
        "type": "string",
        "description": "Address"
      },
      "block": {
        "type": "string",
        "description": "Block Hash"
      },
      "pageNum": {
        "type": "number",
        "description": "Page number to display"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://ntp1node.nebl.io//testnet/ins/txs{?address,block,pageNum}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Object containing an array of transaction objects",
        "**Key properties:**",
        {
          "$ul": [
            "**pagesTotal**: Number of pages of transactions",
            "**txs**: Array of transaction objects"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
