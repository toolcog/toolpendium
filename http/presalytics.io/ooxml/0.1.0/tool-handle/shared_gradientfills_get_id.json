{
  "name": "shared_gradientfills_get_id",
  "description": "Get by Id: Use this method to retrieve a GradientFills object by its primary key (id)",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.presalytics.io/ooxml-automation/Shared/GradientFills/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**angle**",
            "**fillMapId**",
            "**id**",
            "**isPath**",
            "**pathType**",
            "**rotateWithShape**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
