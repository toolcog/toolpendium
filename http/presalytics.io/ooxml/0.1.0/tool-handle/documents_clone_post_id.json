{
  "name": "documents_clone_post_id",
  "description": "Clone A Document that has already been uploaded to a new Story",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "description": "the Source document Id",
        "format": "uuid",
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/DocumentCloneDTO",
        "description": "A DocumentCloneDto object with containing information required for cloning the document"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.presalytics.io/ooxml-automation/Documents/Clone/{id}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**baseElementBlobUrl**",
            "**blobLocation**",
            "**changedBaseElementBlobUrl**",
            "**documentTypeId**",
            "**filename**",
            "**id**",
            "**name**",
            "**ownerGuid**",
            "**packageUri**",
            "**storyId**",
            "**tableStylesXmlBlobUrl**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
