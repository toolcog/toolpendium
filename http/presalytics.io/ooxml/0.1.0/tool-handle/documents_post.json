{
  "name": "documents_post",
  "description": "Upload an OpenOfficeXml document (e.g., .xlsx, .pptx) for processing by the API.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "file": {
            "description": "The file to upload.  Must be of type .pptx, ppt",
            "format": "binary",
            "title": "Story File",
            "type": "string"
          },
          "storyId": {
            "description": "The story_id of the document being uploaded.",
            "format": "uuid",
            "title": "Story Id",
            "type": "string"
          }
        },
        "required": [
          "file",
          "storyId"
        ],
        "title": "Story file form data",
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.presalytics.io/ooxml-automation/Documents"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**baseElementBlobUrl**",
            "**blobLocation**",
            "**changedBaseElementBlobUrl**",
            "**documentTypeId**",
            "**filename**",
            "**id**",
            "**name**",
            "**ownerGuid**",
            "**packageUri**",
            "**storyId**",
            "**tableStylesXmlBlobUrl**",
            "**title**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.title"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
