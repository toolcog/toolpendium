{
  "name": "svgconvert",
  "description": "converts pptx file to svg image",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "file": {
            "format": "binary",
            "type": "string"
          }
        },
        "title": "File to Convert",
        "type": "object",
        "description": "Fizepath to pptx file"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.presalytics.io/doc-converter//svgconvert"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "File Url"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**blob_name**",
            "**blob_url**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
