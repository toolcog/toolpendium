{
  "name": "story_post",
  "description": "Upload new story to presalytics api",
  "parameters": {
    "type": "object",
    "properties": {
      "include_outline": {
        "type": "boolean",
        "description": "Determines whether a repsonse including story objects should include the story outline.  Defaults to true. Useful for speeding up processing times."
      },
      "body": {
        "$ref": "#/components/schemas/outline",
        "description": "A story outline json object"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/story/{?include_outline}"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "BaseModel"
        },
        "Base model for all types",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**created_by**",
            "**id**",
            "**updated_at**",
            "**updated_by**",
            "**collaborators**",
            "**is_public**",
            "**ooxml_documents**",
            "**outline**",
            "**outline_history**",
            "**revision**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "415": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
