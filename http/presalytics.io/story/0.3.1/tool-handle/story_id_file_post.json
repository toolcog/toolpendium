{
  "name": "story_id_file_post",
  "description": "Upload a file to an existing story",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "the id from the story object"
      },
      "replace_existing": {
        "type": "boolean",
        "description": "Indicates whether a put or post method would replace the existing contents"
      },
      "obsolete_id": {
        "format": "uuid",
        "type": "string",
        "description": "A primary key pointing to an obsolete item in the story. Item type is context-dependent"
      },
      "include_outline": {
        "type": "boolean",
        "description": "Determines whether a repsonse including story objects should include the story outline.  Defaults to true. Useful for speeding up processing times."
      },
      "body": {
        "description": "A pptx or xlsx file upload or story outline json object",
        "properties": {
          "file": {
            "items": {
              "format": "binary",
              "type": "string"
            },
            "type": "array"
          }
        },
        "title": "Open Office Xml document (e.g., xlsx, pptx file)"
      }
    },
    "required": [
      "id",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "/story/{id}/file{?replace_existing,obsolete_id,include_outline}"
    },
    "body": {
      "$": "body",
      "encode": "multipart"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "BaseModel"
        },
        "Base model for all types",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**created_by**",
            "**id**",
            "**updated_at**",
            "**updated_by**",
            "**collaborators**",
            "**is_public**",
            "**ooxml_documents**",
            "**outline**",
            "**outline_history**",
            "**revision**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "415": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "422": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
