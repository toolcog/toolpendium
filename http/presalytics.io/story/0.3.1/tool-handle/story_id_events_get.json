{
  "name": "story_id_events_get",
  "description": "Get a list of Events available to users of this story",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "the id from the story object"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/story/{id}/events"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "BaseModel list"
        },
        "Base model for all types",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**created_by**",
            "**id**",
            "**updated_at**",
            "**updated_by**",
            "**attributes**",
            "**chat_prompt**",
            "**conversation_id**",
            "**dom_selectors**",
            "**is_chat_hidden**",
            "**is_notify_enabled**",
            "**name**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
