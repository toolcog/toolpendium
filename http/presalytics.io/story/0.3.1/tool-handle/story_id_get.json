{
  "name": "story_id_get",
  "description": "Returns story metadata, inlcuding json object with story outline",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "uuid",
        "type": "string",
        "description": "the id from the story object"
      },
      "include_relationships": {
        "type": "boolean",
        "description": "Indicate whether the returned object should include child relationships"
      },
      "include_outline": {
        "type": "boolean",
        "description": "Determines whether a repsonse including story objects should include the story outline.  Defaults to true. Useful for speeding up processing times."
      },
      "full": {
        "type": "boolean",
        "description": "Pull a story object with associated collaborator user, permission, and session data(faster if cached from prior api call)"
      },
      "refresh_cache": {
        "type": "boolean",
        "description": "Force the api reload the `Story full` object"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/story/{id}{?include_relationships,include_outline,full,refresh_cache}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "BaseModel"
        },
        "Base model for all types",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**created_by**",
            "**id**",
            "**updated_at**",
            "**updated_by**",
            "**collaborators**",
            "**is_public**",
            "**ooxml_documents**",
            "**outline**",
            "**outline_history**",
            "**revision**",
            "**title**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
