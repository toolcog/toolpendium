{
  "name": "story_get",
  "description": "Returns a list of stories for this user identifie via the access token presentated to the api",
  "parameters": {
    "type": "object",
    "properties": {
      "include_relationships": {
        "type": "boolean",
        "description": "Indicate whether the returned object should include child relationships"
      },
      "include_outline": {
        "type": "boolean",
        "description": "Determines whether a repsonse including story objects should include the story outline.  Defaults to true. Useful for speeding up processing times."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/story/{?include_relationships,include_outline}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "BaseModel list"
        },
        "Base model for all types",
        "**Key properties:**",
        {
          "$ul": [
            "**created_at**",
            "**created_by**",
            "**id**",
            "**updated_at**",
            "**updated_by**",
            "**collaborators**",
            "**is_public**",
            "**ooxml_documents**",
            "**outline**",
            "**outline_history**",
            "**revision**",
            "**title**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.title"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Problem Detail"
        },
        "Error reponse model",
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
