{
  "name": "PicoLoadmanagementSetDynamicCurrent_Post",
  "description": "Sets the dynamic current of a load management group or a single station.",
  "parameters": {
    "type": "object",
    "properties": {
      "serial": {
        "format": "int64",
        "type": "integer",
        "description": "The serial number can be any pico serial in the group (e.g. 700001)"
      },
      "current": {
        "format": "int32",
        "type": "integer",
        "description": "The dynamic current of the group (in mA)"
      }
    },
    "required": [
      "serial",
      "current"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://smart-me.com:443/api/pico/loadmanagementgroup/current/{serial}{?current}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
