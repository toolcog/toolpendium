{
  "name": "Folder_Get",
  "description": "Gets the Values for a folder or a meter",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://smart-me.com:443/api/Folder/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Container class for the folder API",
        "**Key properties:**",
        {
          "$ul": [
            "**ElectricityCounterValue**: The Counter values for electricity (kWh)",
            "**ElectricityPower**: The Power for electricity (kW)",
            "**GasCounterValue**: The Counter values for Gas (m3)",
            "**GasFlowRate**: The Flow Rate for Gas (m3/h)",
            "**HeatCounterValue**: The Counter values for Heat (kWh)",
            "**HeatPower**: The Power for Heat (kW)",
            "**WaterCounterValue**: The Counter values for Water (m3)",
            "**WaterFlowRate**: The Flow Rate for Water (m3/h)"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
