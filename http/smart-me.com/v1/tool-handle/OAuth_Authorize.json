{
  "name": "OAuth_Authorize",
  "parameters": {
    "type": "object",
    "properties": {
      "client_id": {
        "type": "string"
      },
      "redirect_uri": {
        "type": "string"
      },
      "state": {
        "type": "string"
      },
      "scope": {
        "type": "string"
      },
      "client_secret": {
        "type": "string"
      }
    },
    "required": [
      "client_id",
      "redirect_uri",
      "state"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://smart-me.com:443/api/oauth/authorize{?client_id,redirect_uri,state,scope,client_secret}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
