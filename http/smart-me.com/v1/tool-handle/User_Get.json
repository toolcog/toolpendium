{
  "name": "User_Get",
  "description": "Gets the informations for the user.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://smart-me.com:443/api/User"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Container Class for the Web API",
        "**Key properties:**",
        {
          "$ul": [
            "**ChildUsers**: The Users created by this users.",
            "**Email**: The EMail Address of the User",
            "**Id**: The ID of the User",
            "**IdAsString**: The ID of the user as string",
            "**IsAdmin**: Flag if this User is an Admin User",
            "**Permissions**: Additional Permissions",
            "**Username**: The Username of the User"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
