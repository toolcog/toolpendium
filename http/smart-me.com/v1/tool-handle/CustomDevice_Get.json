{
  "name": "CustomDevice_Get",
  "description": "Gets all Devices",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://smart-me.com:443/api/CustomDevice"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Container Class for the Web API",
        "**Key properties:**",
        {
          "$ul": [
            "**Id**: The ID of the device",
            "**Name**: The Name of the Device",
            "**Serial**: The Serial number",
            "**ValueDate**: The Date of the Value (in UTC). If this is null the Server Time is used.",
            "**Values**: The Values of the custom Device"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
