{
  "name": "VirtualTariffConsumption_Get",
  "description": "Gets the consumption of a folder with a virtuall tariffs.",
  "parameters": {
    "type": "object",
    "properties": {
      "folderId": {
        "type": "string",
        "description": "The ID of the Folder"
      },
      "startDate": {
        "format": "date-time",
        "type": "string",
        "description": "The start date (UTC)"
      },
      "endDate": {
        "format": "date-time",
        "type": "string",
        "description": "The end date (UTC)"
      }
    },
    "required": [
      "folderId",
      "startDate",
      "endDate"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://smart-me.com:443/api/VirtualTariffConsumption{?folderId,startDate,endDate}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Container class for the virtual tariff consumption",
        "**Key properties:**",
        {
          "$ul": [
            "**Consumption**: The consumption (e.g. kWh) of this tariff",
            "**Currency**: The currency of the price",
            "**Name**: The Name of this virtual tariff",
            "**Price**: The price of the energy in this timerange",
            "**TariffType**: The type of the virtual tariff (e.g. solar)"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
