{
  "name": "ValuesInPastMultiple_Get",
  "description": "Gets multiple values of a device. This call needs a smart-me professional licence.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "The ID of the device"
      },
      "startDate": {
        "format": "date-time",
        "type": "string",
        "description": "The date when the first value should start"
      },
      "endDate": {
        "format": "date-time",
        "type": "string",
        "description": "The date when the last value should start"
      },
      "interval": {
        "format": "int32",
        "type": "integer",
        "description": "The interval in minutes betwenn the values. 0 means as fast as possible. Only 1000 values can be get in one call."
      }
    },
    "required": [
      "id",
      "startDate",
      "endDate",
      "interval"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://smart-me.com:443/api/ValuesInPastMultiple/{id}{?startDate,endDate,interval}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "API Container for a Meter Value",
        "**Key properties:**",
        {
          "$ul": [
            "**Date**: The Date of the Value",
            "**DeviceId**: The ID of the device",
            "**Values**: All values"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
