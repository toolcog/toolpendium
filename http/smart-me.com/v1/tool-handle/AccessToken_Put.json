{
  "name": "AccessToken_Put",
  "description": "Creates a Access Token to write on a Card (e.g. NFC)",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/AccessTokenToPut"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "AccessTokenToPut": {
        "description": "Container Class for the Put action of an Access Token",
        "properties": {
          "CardId": {
            "description": "The ID of the Card",
            "format": "int64",
            "type": "integer"
          },
          "UserId": {
            "description": "The ID of the User. The credentials provided must have permission to edit the user.\r\n            If no ID is provided, the user in the credentials is taken.",
            "format": "int64",
            "type": "integer"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "https://smart-me.com:443/api/AccessToken"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
