{
  "name": "PicoChargingHistory_Get",
  "description": "Gets the last charging history for a pico station",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://smart-me.com:443/api/pico/history/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "Api container for the charging station history",
        "**Key properties:**",
        {
          "$ul": [
            "**Duration**: The duration in seconds",
            "**EnergyUsed**: The energy used (in kWh)",
            "**StartTime**: The starttime of the charging (in UTC)",
            "**TransactionStopReason**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
