{
  "name": "get_collection-changeset",
  "parameters": {
    "type": "object",
    "properties": {
      "_since": {
        "pattern": "^\"([0-9]+?)\"$",
        "type": "string"
      },
      "_expected": {
        "type": "string"
      },
      "_limit": {
        "type": "integer"
      },
      "bucket": {
        "type": "string"
      },
      "collection": {
        "type": "string"
      },
      "bid": {
        "type": "string"
      },
      "cid": {
        "type": "string"
      }
    },
    "required": [
      "_expected",
      "bid",
      "cid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://firefox.settings.services.mozilla.com/v1/buckets/{bid}/collections/{cid}/changeset{?_since,_expected,_limit,bucket,collection}"
    }
  },
  "responses": {}
}
