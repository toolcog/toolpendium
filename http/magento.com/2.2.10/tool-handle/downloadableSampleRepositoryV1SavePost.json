{
  "name": "downloadableSampleRepositoryV1SavePost",
  "description": "Update downloadable sample of the given product",
  "parameters": {
    "type": "object",
    "properties": {
      "sku": {
        "type": "string"
      },
      "body": {
        "properties": {
          "isGlobalScopeContent": {
            "type": "boolean"
          },
          "sample": {
            "$ref": "#/$defs/downloadable-data-sample-interface"
          }
        },
        "required": [
          "sample"
        ],
        "type": "object",
        "xml": {
          "name": "request"
        }
      }
    },
    "required": [
      "sku"
    ],
    "$defs": {
      "downloadable-data-sample-interface": {
        "description": "",
        "properties": {
          "extension_attributes": {
            "$ref": "#/$defs/downloadable-data-sample-extension-interface"
          },
          "id": {
            "description": "Sample(or link) id",
            "type": "integer"
          },
          "sample_file": {
            "description": "relative file path",
            "type": "string"
          },
          "sample_file_content": {
            "$ref": "#/$defs/downloadable-data-file-content-interface"
          },
          "sample_type": {
            "type": "string"
          },
          "sample_url": {
            "description": "file URL",
            "type": "string"
          },
          "sort_order": {
            "description": "Order index for sample",
            "type": "integer"
          },
          "title": {
            "description": "Title",
            "type": "string"
          }
        },
        "required": [
          "title",
          "sort_order",
          "sample_type"
        ],
        "type": "object"
      },
      "downloadable-data-sample-extension-interface": {
        "description": "ExtensionInterface class for @see \\Magento\\Downloadable\\Api\\Data\\SampleInterface",
        "type": "object"
      },
      "downloadable-data-file-content-interface": {
        "description": "",
        "properties": {
          "extension_attributes": {
            "$ref": "#/$defs/downloadable-data-file-content-extension-interface"
          },
          "file_data": {
            "description": "Data (base64 encoded content)",
            "type": "string"
          },
          "name": {
            "description": "File name",
            "type": "string"
          }
        },
        "required": [
          "file_data",
          "name"
        ],
        "type": "object"
      },
      "downloadable-data-file-content-extension-interface": {
        "description": "ExtensionInterface class for @see \\Magento\\Downloadable\\Api\\Data\\File\\ContentInterface",
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://example.com/rest/default/V1/products/{sku}/downloadable-links/samples"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**errors**: Errors list",
            "**message**: Error message",
            "**parameters**: Error parameters list",
            "**trace**: Stack trace"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**errors**: Errors list",
            "**message**: Error message",
            "**parameters**: Error parameters list",
            "**trace**: Stack trace"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
