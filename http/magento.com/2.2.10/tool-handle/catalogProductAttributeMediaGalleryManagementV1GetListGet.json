{
  "name": "catalogProductAttributeMediaGalleryManagementV1GetListGet",
  "description": "Retrieve the list of gallery entries associated with given product",
  "parameters": {
    "type": "object",
    "properties": {
      "sku": {
        "type": "string"
      }
    },
    "required": [
      "sku"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://example.com/rest/default/V1/products/{sku}/media"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**content**: Image Content data interface",
              {
                "$ul": [
                  "**base64_encoded_data**: Media data (base64 encoded content)",
                  "**name**: Image name",
                  "**type**: MIME type"
                ]
              }
            ],
            "**disabled**: If gallery entry is hidden from product page",
            [
              "**extension_attributes**: ExtensionInterface class for @see \\Magento\\Catalog\\Api\\Data\\ProductAttributeMediaGalleryEntryInterface",
              {
                "$ul": [
                  [
                    "**video_content**: Video Content data interface",
                    {
                      "$ul": [
                        "**media_type**: MIME type",
                        "**video_description**: Video Description",
                        "**video_metadata**: Metadata",
                        "**video_provider**: Provider",
                        "**video_title**: Title",
                        "**video_url**: Video URL"
                      ]
                    }
                  ]
                ]
              }
            ],
            "**file**: File path",
            "**id**: Gallery entry ID",
            "**label**: Gallery entry alternative text",
            "**media_type**: Media type",
            "**position**: Gallery entry position (sort order)",
            "**types**: Gallery entry image types (thumbnail, image, small_image etc)"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**errors**: Errors list",
            "**message**: Error message",
            "**parameters**: Error parameters list",
            "**trace**: Stack trace"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**errors**: Errors list",
            "**message**: Error message",
            "**parameters**: Error parameters list",
            "**trace**: Stack trace"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
