{
  "name": "downloadableSampleRepositoryV1GetListGet",
  "description": "List of samples for downloadable product",
  "parameters": {
    "type": "object",
    "properties": {
      "sku": {
        "type": "string"
      }
    },
    "required": [
      "sku"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://example.com/rest/default/V1/products/{sku}/downloadable-links/samples"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "",
        "**Key properties:**",
        {
          "$ul": [
            "**extension_attributes**: ExtensionInterface class for @see \\Magento\\Downloadable\\Api\\Data\\SampleInterface",
            "**id**: Sample(or link) id",
            "**sample_file**: relative file path",
            [
              "**sample_file_content**: ",
              {
                "$ul": [
                  "**extension_attributes**: ExtensionInterface class for @see \\Magento\\Downloadable\\Api\\Data\\File\\ContentInterface",
                  "**file_data**: Data (base64 encoded content)",
                  "**name**: File name"
                ]
              }
            ],
            "**sample_type**",
            "**sample_url**: file URL",
            "**sort_order**: Order index for sample",
            "**title**: Title"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.title"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**errors**: Errors list",
            "**message**: Error message",
            "**parameters**: Error parameters list",
            "**trace**: Stack trace"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**code**: Error code",
            "**errors**: Errors list",
            "**message**: Error message",
            "**parameters**: Error parameters list",
            "**trace**: Stack trace"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
