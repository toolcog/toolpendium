{
  "name": "giftRegistryGuestCartShippingMethodManagementV1EstimateByRegistryIdPost",
  "description": "Estimate shipping",
  "parameters": {
    "type": "object",
    "properties": {
      "cartId": {
        "type": "string",
        "description": "The shopping cart ID."
      },
      "body": {
        "properties": {
          "registryId": {
            "description": "The estimate registry id",
            "type": "integer"
          }
        },
        "required": [
          "registryId"
        ],
        "type": "object",
        "xml": {
          "name": "request"
        }
      }
    },
    "required": [
      "cartId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://example.com/rest/default/V1/guest-giftregistry/{cartId}/estimate-shipping-methods"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
