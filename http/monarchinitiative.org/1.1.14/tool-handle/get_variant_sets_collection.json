{
  "name": "get_variant_sets_collection",
  "description": "Returns list of variant sets",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "default": 1,
        "type": "integer",
        "description": "Page number"
      },
      "per_page": {
        "default": 10,
        "enum": [
          2,
          10,
          20,
          30,
          40,
          50
        ],
        "type": "integer",
        "description": "Results per page {error_msg}"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/variation/set/{?page,per_page}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**page**: Number of this page of results",
            "**pages**: Total number of pages of results",
            "**per_page**: Number of items per page of results",
            "**total**: Total number of results",
            "**items**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
