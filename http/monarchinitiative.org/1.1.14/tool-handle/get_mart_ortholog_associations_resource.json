{
  "name": "get_mart_ortholog_associations_resource",
  "description": "Bulk download of orthologs",
  "parameters": {
    "type": "object",
    "properties": {
      "taxon2": {
        "type": "string",
        "description": "object taxon, e.g. NCBITaxon:10090"
      },
      "taxon1": {
        "type": "string",
        "description": "subject taxon, e.g. NCBITaxon:9606"
      }
    },
    "required": [
      "taxon2",
      "taxon1"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/mart/ortholog/{taxon1}/{taxon2}"
    }
  },
  "responses": {}
}
