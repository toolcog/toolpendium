{
  "name": "get_search_hpo_entities",
  "description": "Returns list of matching concepts or entities using lexical search",
  "parameters": {
    "type": "object",
    "properties": {
      "rows": {
        "default": 10,
        "type": "integer",
        "description": "number of rows"
      },
      "start": {
        "default": "0",
        "type": "string",
        "description": "row number to start from"
      },
      "phenotype_group": {
        "type": "string",
        "description": "phenotype group id"
      },
      "phenotype_group_label": {
        "type": "string",
        "description": "phenotype group label"
      },
      "anatomical_system": {
        "type": "string",
        "description": "anatomical system id"
      },
      "anatomical_system_label": {
        "type": "string",
        "description": "anatomical system label"
      },
      "highlight_class": {
        "type": "string",
        "description": "highlight class"
      },
      "term": {
        "type": "string",
        "description": "search string, e.g. muscle atrophy, frequent infections"
      }
    },
    "required": [
      "term"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/search/entity/hpo-pl/{term}{?rows,start,phenotype_group,phenotype_group_label,anatomical_system,anatomical_system_label,highlight_class}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**results**: list of AutocompleteResult docs"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
