{
  "name": "get_annotation_score",
  "description": "Get annotation score",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "Phenotype identifier (eg HP:0004935)"
      },
      "absent_id": {
        "default": [],
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "absent phenotype (eg HP:0002828)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/sim/score{?id,absent_id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**categorical_score**: categorical score",
            "**scaled_score**: scaled score",
            "**simple_score**: simple score"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
