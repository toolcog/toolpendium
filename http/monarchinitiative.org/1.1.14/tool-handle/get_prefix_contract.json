{
  "name": "get_prefix_contract",
  "description": "Returns contracted URI",
  "parameters": {
    "type": "object",
    "properties": {
      "uri": {
        "type": "string",
        "description": "URI of entity to be contracted to identifier/CURIE, e.g \"http://www.informatics.jax.org/accession/MGI:1\""
      }
    },
    "required": [
      "uri"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "/api/identifier/prefixes/contract/{uri}"
    }
  },
  "responses": {}
}
