{
  "name": "getTagsUsingGET",
  "description": "Get list of Tags against HealthID.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      },
      "X-Token": {
        "type": "string",
        "description": "Auth Token"
      }
    },
    "required": [
      "X-Token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://healthidsbx.ndhm.gov.in/api/v1/ha/tags"
    },
    "headers": {
      "Accept-Language": {
        "$": "Accept-Language"
      },
      "X-Token": {
        "$": "X-Token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
