    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/HidUpdateMobiletRequest",
        "description": "request"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
