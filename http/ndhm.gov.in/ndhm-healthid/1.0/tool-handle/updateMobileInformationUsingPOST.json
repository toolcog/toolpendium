            "**name**",
            "**new**",
            "**pincode**",
            "**profilePhoto**",
            "**stateCode**",
            "**stateName**",
            "**subDistrictCode**",
            "**subdistrictName**",
            "**tags**",
            "**townCode**",
            "**townName**",
            "**villageCode**",
            "**villageName**",
            "**wardCode**",
            "**wardName**",
            "**yearOfBirth**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
