{
  "name": "findByAadharUsingPOST",
  "description": "Search health id number using aadhar or aadhar token.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/AadharNumberRequestPayload",
        "description": "aadharNumberRequestPayload"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://healthidsbx.ndhm.gov.in/api/v1/hid/benefit/search/aadhaar"
    },
    "headers": {
      "Accept-Language": {
        "$": "Accept-Language"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
