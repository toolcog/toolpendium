{
  "name": "generatereKycAadharOTPUsingPOST",
  "description": "Generate Aadhaar OTP on registrered for link account with aadhar number",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      },
      "X-Token": {
        "type": "string",
        "description": "Auth Token"
      }
    },
    "required": [
      "X-Token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://healthidsbx.ndhm.gov.in/api/v1/account/aadhaar/generateOTP"
    },
    "headers": {
      "Accept-Language": {
        "$": "Accept-Language"
      },
      "X-Token": {
        "$": "X-Token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "TxnResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**txnId**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
