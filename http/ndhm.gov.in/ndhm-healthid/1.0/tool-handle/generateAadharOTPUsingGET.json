{
  "name": "generateAadharOTPUsingGET",
  "description": "Generate Aadhaar OTP on registrered mobile number.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      },
      "X-Token": {
        "type": "string",
        "description": "Auth Token"
      }
    },
    "required": [
      "X-Token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://healthidsbx.ndhm.gov.in/api/v1/account/change/passwd/generateAadhaarOTP"
    },
    "headers": {
      "Accept-Language": {
        "$": "Accept-Language"
      },
      "X-Token": {
        "$": "X-Token"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
