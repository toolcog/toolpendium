{
  "name": "verifyMobileOTPUsingPOST",
  "description": "Verify Mobile OTP in current registration transaction.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/VerifyMobileRequest",
        "description": "verifyOtpRequest"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://healthidsbx.ndhm.gov.in/api/v1/registration/mobile/verifyOtp"
    },
    "headers": {
      "Accept-Language": {
        "$": "Accept-Language"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "MobileVerificationToken"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**token**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
