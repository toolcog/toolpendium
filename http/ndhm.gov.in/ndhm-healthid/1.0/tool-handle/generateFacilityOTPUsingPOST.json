{
  "name": "generateFacilityOTPUsingPOST",
  "description": "Generate health facility OTP on registrered mobile number",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      },
      "X-Token": {
        "type": "string",
        "description": "Auth Token"
      },
      "body": {
        "$ref": "#/components/schemas/AadharOtpGenerateRequestPayLoad",
        "description": "generateOtpRequest"
      }
    },
    "required": [
      "X-Token",
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://healthidsbx.ndhm.gov.in/api/v1/health/facility/generateOtp"
    },
    "headers": {
      "Accept-Language": {
        "$": "Accept-Language"
      },
      "X-Token": {
        "$": "X-Token"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "TxnResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**txnId**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
