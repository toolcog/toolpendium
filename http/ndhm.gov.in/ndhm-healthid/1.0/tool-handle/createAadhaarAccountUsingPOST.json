{
  "name": "createAadhaarAccountUsingPOST",
  "description": "Create Health ID using pre-verified Aadhaar & Mobile.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      },
      "body": {
        "$ref": "#/components/schemas/CreateAccountWithPreVerifiedAadhaar",
        "description": "accountRequest"
      }
    },
    "required": [
      "body"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://healthidsbx.ndhm.gov.in/api/v1/registration/aadhaar/createHealthIdWithPreVerified"
    },
    "headers": {
      "Accept-Language": {
        "$": "Accept-Language"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "CreateAccountJwtResponse"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**authMethods**",
            "**dayOfBirth**",
            "**districtCode**",
            "**districtName**",
            "**email**",
            "**firstName**",
            "**gender**",
            "**healthId**",
            "**healthIdNumber**",
            "**kycPhoto**",
            "**lastName**",
            "**middleName**",
            "**mobile**",
            "**monthOfBirth**",
            "**name**",
            "**new**",
            "**stateCode**",
            "**stateName**",
            "**tags**",
            "**token**",
            "**yearOfBirth**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
