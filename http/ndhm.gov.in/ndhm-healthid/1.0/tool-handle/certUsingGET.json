{
  "name": "certUsingGET",
  "description": "Auth token public key.",
  "parameters": {
    "type": "object",
    "properties": {
      "Accept-Language": {
        "default": "en-US",
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://healthidsbx.ndhm.gov.in/api/v1/auth/cert"
    },
    "headers": {
      "Accept-Language": {
        "$": "Accept-Language"
      }
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
