{
  "name": "accountGetLogs",
  "description": "Get currently logged in user list of latest security activity logs. Each log returns user IP address, location and date and time of log.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://appwrite.io/v1/account/logs"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Logs List",
        "**Key properties:**",
        {
          "$ul": [
            "**logs**: List of logs."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
