{
  "name": "accountUpdatePrefs",
  "description": "Update currently logged in user account preferences. You can pass only the specific settings you wish to update.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "prefs": {
            "description": "Prefs key-value JSON object.",
            "type": "object",
            "x-example": null
          }
        },
        "required": [
          "prefs"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://appwrite.io/v1/account/prefs"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "User",
        "**Key properties:**",
        {
          "$ul": [
            "**$id**: User ID.",
            "**email**: User email address.",
            "**emailVerification**: Email verification status.",
            "**name**: User name.",
            "**passwordUpdate**: Unix timestamp of the most recent password update",
            "**prefs**: User preferences as a key-value object",
            "**registration**: User registration date in Unix timestamp.",
            "**status**: User status. 0 for Unactivated, 1 for active and 2 is blocked."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
