{
  "name": "teamsGet",
  "description": "Get a team by its unique ID. All team members have read access for this resource.",
  "parameters": {
    "type": "object",
    "properties": {
      "teamId": {
        "type": "string",
        "x-example": "[TEAM_ID]",
        "description": "Team unique ID."
      }
    },
    "required": [
      "teamId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://appwrite.io/v1/teams/{teamId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Team",
        "**Key properties:**",
        {
          "$ul": [
            "**$id**: Team ID.",
            "**dateCreated**: Team creation date in Unix timestamp.",
            "**name**: Team name.",
            "**sum**: Total sum of team members."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
