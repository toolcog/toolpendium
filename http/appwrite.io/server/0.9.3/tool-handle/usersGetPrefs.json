{
  "name": "usersGetPrefs",
  "description": "Get the user preferences by its unique ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "userId": {
        "type": "string",
        "x-example": "[USER_ID]",
        "description": "User unique ID."
      }
    },
    "required": [
      "userId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://appwrite.io/v1/users/{userId}/prefs"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Preferences",
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
