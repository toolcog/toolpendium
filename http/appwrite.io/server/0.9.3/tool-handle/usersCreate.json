{
  "name": "usersCreate",
  "description": "Create a new user.",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "properties": {
          "email": {
            "description": "User email.",
            "type": "string",
            "x-example": null
          },
          "name": {
            "description": "User name. Max length: 128 chars.",
            "type": "string",
            "x-example": null
          },
          "password": {
            "description": "User password. Must be between 6 to 32 chars.",
            "type": "string",
            "x-example": null
          }
        },
        "required": [
          "email",
          "password"
        ],
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://appwrite.io/v1/users"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "201": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "User",
        "**Key properties:**",
        {
          "$ul": [
            "**$id**: User ID.",
            "**email**: User email address.",
            "**emailVerification**: Email verification status.",
            "**name**: User name.",
            "**passwordUpdate**: Unix timestamp of the most recent password update",
            "**prefs**: User preferences as a key-value object",
            "**registration**: User registration date in Unix timestamp.",
            "**status**: User status. 0 for Unactivated, 1 for active and 2 is blocked."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
