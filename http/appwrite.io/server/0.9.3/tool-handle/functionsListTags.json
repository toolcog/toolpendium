{
  "name": "functionsListTags",
  "description": "Get a list of all the project's code tags. You can use the query params to filter your results.",
  "parameters": {
    "type": "object",
    "properties": {
      "functionId": {
        "type": "string",
        "x-example": "[FUNCTION_ID]",
        "description": "Function unique ID."
      },
      "search": {
        "default": "",
        "type": "string",
        "x-example": "[SEARCH]",
        "description": "Search term to filter your list results. Max length: 256 chars."
      },
      "limit": {
        "default": 25,
        "format": "int32",
        "type": "integer",
        "x-example": 0,
        "description": "Results limit value. By default will return maximum 25 results. Maximum of 100 results allowed per request."
      },
      "offset": {
        "default": 0,
        "format": "int32",
        "type": "integer",
        "x-example": 0,
        "description": "Results offset. The default value is 0. Use this param to manage pagination."
      },
      "orderType": {
        "default": "ASC",
        "type": "string",
        "x-example": "ASC",
        "description": "Order result by ASC or DESC order."
      }
    },
    "required": [
      "functionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://appwrite.io/v1/functions/{functionId}/tags{?search,limit,offset,orderType}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Tags List",
        "**Key properties:**",
        {
          "$ul": [
            "**sum**: Total sum of items in the list.",
            "**tags**: List of tags."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
