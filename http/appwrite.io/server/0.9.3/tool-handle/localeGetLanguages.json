{
  "name": "localeGetLanguages",
  "description": "List of all languages classified by ISO 639-1 including 2-letter code, name in English, and name in the respective language.",
  "parameters": {
    "type": "object"
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://appwrite.io/v1/locale/languages"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Languages List",
        "**Key properties:**",
        {
          "$ul": [
            "**languages**: List of languages.",
            "**sum**: Total sum of items in the list."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
