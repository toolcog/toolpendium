{
  "name": "functionsGetTag",
  "description": "Get a code tag by its unique ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "functionId": {
        "type": "string",
        "x-example": "[FUNCTION_ID]",
        "description": "Function unique ID."
      },
      "tagId": {
        "type": "string",
        "x-example": "[TAG_ID]",
        "description": "Tag unique ID."
      }
    },
    "required": [
      "functionId",
      "tagId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://appwrite.io/v1/functions/{functionId}/tags/{tagId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Tag",
        "**Key properties:**",
        {
          "$ul": [
            "**$id**: Tag ID.",
            "**command**: The entrypoint command in use to execute the tag code.",
            "**dateCreated**: The tag creation date in Unix timestamp.",
            "**functionId**: Function ID.",
            "**size**: The code size in bytes."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
