{
  "name": "functionsUpdateTag",
  "description": "Update the function code tag ID using the unique function ID. Use this endpoint to switch the code tag that should be executed by the execution endpoint.",
  "parameters": {
    "type": "object",
    "properties": {
      "functionId": {
        "type": "string",
        "x-example": "[FUNCTION_ID]",
        "description": "Function unique ID."
      },
      "body": {
        "properties": {
          "tag": {
            "description": "Tag unique ID.",
            "type": "string",
            "x-example": null
          }
        },
        "required": [
          "tag"
        ],
        "type": "object"
      }
    },
    "required": [
      "functionId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PATCH",
    "url": {
      "$uri": "https://appwrite.io/v1/functions/{functionId}/tag"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Function",
        "**Key properties:**",
        {
          "$ul": [
            "**$id**: Function ID.",
            "**$permissions**: Function permissions.",
            "**dateCreated**: Function creation date in Unix timestamp.",
            "**dateUpdated**: Function update date in Unix timestamp.",
            "**events**: Function trigger events.",
            "**name**: Function name.",
            "**runtime**: Function execution runtime.",
            "**schedule**: Function execution schedult in CRON format.",
            "**scheduleNext**: Function next scheduled execution date in Unix timestamp.",
            "**schedulePrevious**: Function next scheduled execution date in Unix timestamp.",
            "**status**: Function status. Possible values: disabled, enabled",
            "**tag**: Function active tag ID.",
            "**timeout**: Function execution timeout in seconds.",
            "**vars**: Function environment variables."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
