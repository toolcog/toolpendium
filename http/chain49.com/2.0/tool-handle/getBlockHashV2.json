{
  "name": "getBlockHashV2",
  "description": "Get block hash by its height\n\nNote: Blockbook always follows the main chain of the backend it is attached to.",
  "parameters": {
    "type": "object",
    "properties": {
      "blockchain": {
        "example": "bitcoin",
        "type": "string",
        "description": "Blockchain name"
      },
      "blockHeight": {
        "example": 15,
        "type": "integer",
        "description": "Block height/index"
      }
    },
    "required": [
      "blockchain",
      "blockHeight"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://api.chain49.com/{blockchain}/v2/block-index/{blockHeight}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**blockHash**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
