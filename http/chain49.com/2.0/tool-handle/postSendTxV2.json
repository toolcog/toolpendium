{
  "name": "postSendTxV2",
  "description": "Sends new transaction to backend for broadcasting\n\nThe trailing slash '/' at the end is mandatory\n",
  "parameters": {
    "type": "object",
    "properties": {
      "blockchain": {
        "example": "bitcoin",
        "type": "string",
        "description": "Blockchain name"
      }
    },
    "required": [
      "blockchain"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://api.chain49.com/{blockchain}/v2/sendtx/"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**result**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
