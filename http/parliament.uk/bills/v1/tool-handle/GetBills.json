{
  "name": "GetBills",
  "description": "Returns a list of Bills.",
  "parameters": {
    "type": "object",
    "properties": {
      "SearchTerm": {
        "type": "string"
      },
      "Session": {
        "format": "int32",
        "type": "integer"
      },
      "CurrentHouse": {
        "$ref": "#/$defs/House"
      },
      "OriginatingHouse": {
        "$ref": "#/$defs/OriginatingHouse"
      },
      "MemberId": {
        "format": "int32",
        "type": "integer"
      },
      "DepartmentId": {
        "format": "int32",
        "type": "integer"
      },
      "BillStage": {
        "items": {
          "format": "int32",
          "type": "integer"
        },
        "type": "array"
      },
      "BillStagesExcluded": {
        "items": {
          "format": "int32",
          "type": "integer"
        },
        "type": "array"
      },
      "IsDefeated": {
        "type": "boolean"
      },
      "IsWithdrawn": {
        "type": "boolean"
      },
      "BillType": {
        "items": {
          "format": "int32",
          "type": "integer"
        },
        "type": "array"
      },
      "SortOrder": {
        "$ref": "#/$defs/BillSortOrder"
      },
      "BillIds": {
        "items": {
          "format": "int32",
          "type": "integer"
        },
        "type": "array"
      },
      "Skip": {
        "format": "int32",
        "type": "integer"
      },
      "Take": {
        "format": "int32",
        "type": "integer"
      }
    },
    "$defs": {
      "House": {
        "enum": [
          "All",
          "Commons",
          "Lords",
          "Unassigned"
        ],
        "type": "string"
      },
      "OriginatingHouse": {
        "enum": [
          "All",
          "Commons",
          "Lords"
        ],
        "type": "string"
      },
      "BillSortOrder": {
        "enum": [
          "TitleAscending",
          "TitleDescending",
          "DateUpdatedAscending",
          "DateUpdatedDescending"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://bills-api.parliament.uk/api/v1/Bills{?SearchTerm,Session,CurrentHouse,OriginatingHouse,MemberId,DepartmentId,BillStage,BillStagesExcluded,IsDefeated,IsWithdrawn,BillType,SortOrder,BillIds,Skip,Take}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**itemsPerPage**",
            "**totalResults**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
