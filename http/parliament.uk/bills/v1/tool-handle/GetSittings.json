{
  "name": "GetSittings",
  "description": "Returns a list of Sittings.",
  "parameters": {
    "type": "object",
    "properties": {
      "House": {
        "$ref": "#/$defs/House"
      },
      "DateFrom": {
        "format": "date-time",
        "type": "string"
      },
      "DateTo": {
        "format": "date-time",
        "type": "string"
      },
      "Skip": {
        "format": "int32",
        "type": "integer"
      },
      "Take": {
        "format": "int32",
        "type": "integer"
      }
    },
    "$defs": {
      "House": {
        "enum": [
          "All",
          "Commons",
          "Lords",
          "Unassigned"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://bills-api.parliament.uk/api/v1/Sittings{?House,DateFrom,DateTo,Skip,Take}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**itemsPerPage**",
            "**totalResults**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
