{
  "name": "GetAmendments",
  "description": "Returns a list of amendments.",
  "parameters": {
    "type": "object",
    "properties": {
      "billId": {
        "format": "int32",
        "type": "integer",
        "description": "Amendments relating to a Bill with Bill ID specified"
      },
      "billStageId": {
        "format": "int32",
        "type": "integer",
        "description": "Amendments relating to a Bill stage with Bill stage ID specified"
      },
      "SearchTerm": {
        "type": "string"
      },
      "Decision": {
        "$ref": "#/$defs/Decision"
      },
      "MemberId": {
        "format": "int32",
        "type": "integer"
      },
      "Skip": {
        "format": "int32",
        "type": "integer"
      },
      "Take": {
        "format": "int32",
        "type": "integer"
      }
    },
    "required": [
      "billId",
      "billStageId"
    ],
    "$defs": {
      "Decision": {
        "enum": [
          "All",
          "NoDecision",
          "Withdrawn",
          "Disagreed",
          "NotMoved",
          "Agreed"
        ],
        "type": "string"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://bills-api.parliament.uk/api/v1/Bills/{billId}/Stages/{billStageId}/Amendments{?SearchTerm,Decision,MemberId,Skip,Take}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**items**",
            "**itemsPerPage**",
            "**totalResults**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "400": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**detail**",
            "**instance**",
            "**status**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
