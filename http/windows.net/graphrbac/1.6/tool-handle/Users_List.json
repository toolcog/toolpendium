{
  "name": "Users_List",
  "description": "Gets list of users for the current tenant.",
  "parameters": {
    "type": "object",
    "properties": {
      "$filter": {
        "type": "string",
        "description": "The filter to apply to the operation."
      },
      "$expand": {
        "type": "string",
        "description": "The expand value for the operation result."
      },
      "$top": {
        "default": 100,
        "maximum": 999,
        "minimum": 1,
        "type": "integer",
        "description": "(Optional) Set the maximum number of results per response."
      },
      "api-version": {
        "type": "string",
        "description": "Client API version."
      },
      "tenantID": {
        "type": "string",
        "description": "The tenant ID."
      }
    },
    "required": [
      "api-version",
      "tenantID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://graph.windows.net/{tenantID}/users{?$filter,$expand,$top,api-version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Server response for Get tenant users API call.",
        "**Key properties:**",
        {
          "$ul": [
            "**odata.nextLink**: The URL to get the next set of results.",
            "**value**: the list of users."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Active Directory error information.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**odata.error**: Active Directory OData error information.",
              {
                "$ul": [
                  "**code**: Error code.",
                  [
                    "**message**: Active Directory error message.",
                    {
                      "$ul": [
                        "**value**: Error message value."
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
