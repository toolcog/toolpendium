{
  "name": "OAuth2PermissionGrant_List",
  "description": "Queries OAuth2 permissions grants for the relevant SP ObjectId of an app.",
  "parameters": {
    "type": "object",
    "properties": {
      "$filter": {
        "type": "string",
        "description": "This is the Service Principal ObjectId associated with the app"
      },
      "api-version": {
        "type": "string",
        "description": "Client API version."
      },
      "tenantID": {
        "type": "string",
        "description": "The tenant ID."
      }
    },
    "required": [
      "api-version",
      "tenantID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://graph.windows.net/{tenantID}/oauth2PermissionGrants{?$filter,api-version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Server response for get oauth2 permissions grants",
        "**Key properties:**",
        {
          "$ul": [
            "**odata.nextLink**: the URL to get the next set of results.",
            "**value**: the list of oauth2 permissions grants"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
