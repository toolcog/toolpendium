{
  "name": "Domains_Get",
  "description": "Gets a specific domain in the current tenant.",
  "parameters": {
    "type": "object",
    "properties": {
      "domainName": {
        "type": "string",
        "description": "name of the domain."
      },
      "api-version": {
        "type": "string",
        "description": "Client API version."
      },
      "tenantID": {
        "type": "string",
        "description": "The tenant ID."
      }
    },
    "required": [
      "domainName",
      "api-version",
      "tenantID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://graph.windows.net/{tenantID}/domains/{domainName}{?api-version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Active Directory Domain information.",
        "**Key properties:**",
        {
          "$ul": [
            "**authenticationType**: the type of the authentication into the domain.",
            "**isDefault**: if this is the default domain in the tenant.",
            "**isVerified**: if this domain's ownership is verified.",
            "**name**: the domain name."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
