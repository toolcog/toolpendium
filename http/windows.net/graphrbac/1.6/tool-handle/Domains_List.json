{
  "name": "Domains_List",
  "description": "Gets a list of domains for the current tenant.",
  "parameters": {
    "type": "object",
    "properties": {
      "$filter": {
        "type": "string",
        "description": "The filter to apply to the operation."
      },
      "api-version": {
        "type": "string",
        "description": "Client API version."
      },
      "tenantID": {
        "type": "string",
        "description": "The tenant ID."
      }
    },
    "required": [
      "api-version",
      "tenantID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://graph.windows.net/{tenantID}/domains{?$filter,api-version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Server response for Get tenant domains API call.",
        "**Key properties:**",
        {
          "$ul": [
            "**value**: the list of domains."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
