{
  "name": "Users_Get",
  "description": "Gets user information from the directory.",
  "parameters": {
    "type": "object",
    "properties": {
      "upnOrObjectId": {
        "type": "string",
        "description": "The object ID or principal name of the user for which to get information."
      },
      "api-version": {
        "type": "string",
        "description": "Client API version."
      },
      "tenantID": {
        "type": "string",
        "description": "The tenant ID."
      }
    },
    "required": [
      "upnOrObjectId",
      "api-version",
      "tenantID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://graph.windows.net/{tenantID}/users/{upnOrObjectId}{?api-version}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Represents an Azure Active Directory object.",
        "**Key properties:**",
        {
          "$ul": [
            "**deletionTimestamp**: The time at which the directory object was deleted.",
            "**objectId**: The object ID.",
            "**objectType**: The object type."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Active Directory error information.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**odata.error**: Active Directory OData error information.",
              {
                "$ul": [
                  "**code**: Error code.",
                  [
                    "**message**: Active Directory error message.",
                    {
                      "$ul": [
                        "**value**: Error message value."
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
