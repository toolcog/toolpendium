{
  "name": "ServicePrincipals_List",
  "description": "Gets a list of service principals from the current tenant.",
  "parameters": {
    "type": "object",
    "properties": {
      "$filter": {
        "type": "string",
        "description": "The filter to apply to the operation."
      },
      "api-version": {
        "type": "string",
        "description": "Client API version."
      },
      "tenantID": {
        "type": "string",
        "description": "The tenant ID."
      }
    },
    "required": [
      "api-version",
      "tenantID"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://graph.windows.net/{tenantID}/servicePrincipals{?$filter,api-version}"
    }
  },
  "responses": {
