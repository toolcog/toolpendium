      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Server response for get tenant service principals API call.",
        "**Key properties:**",
        {
          "$ul": [
            "**odata.nextLink**: the URL to get the next set of results.",
            "**value**: the list of service principals."
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "default": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "Active Directory error information.",
        "**Key properties:**",
        {
          "$ul": [
            [
              "**odata.error**: Active Directory OData error information.",
              {
                "$ul": [
                  "**code**: Error code.",
                  [
                    "**message**: Active Directory error message.",
                    {
                      "$ul": [
                        "**value**: Error message value."
                      ]
                    }
                  ]
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
