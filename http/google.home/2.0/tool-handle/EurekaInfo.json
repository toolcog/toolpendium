              }
            ],
            [
              "**proxy**",
              {
                "$ul": [
                  "**mode**"
                ]
              }
            ],
            [
              "**settings**",
              {
                "$ul": [
                  "**closed_caption**",
                  "**control_notifications**",
                  "**country_code**",
                  "**locale**",
                  "**network_standby**",
                  "**system_sound_effects**",
                  "**time_format**",
                  "**timezone**",
                  "**wake_on_cast**"
                ]
              }
            ],
            [
              "**setup**",
              {
                "$ul": [
                  "**qr_ssid_suffix**",
                  "**setup_state**",
                  "**ssid_suffix**",
                  [
                    "**stats**",
                    {
                      "$ul": [
                        "**num_check_connectivity**",
