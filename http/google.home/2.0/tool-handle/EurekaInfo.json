                ]
              }
            ],
            [
              "**sign**",
              {
                "$ul": [
                  "**certificate**",
                  "**intermediate_certs**",
                  "**nonce**",
                  "**signed_data**"
                ]
              }
            ],
            [
              "**user_eq**",
              {
                "$ul": [
                  [
                    "**high_shelf**",
                    {
