                  "**noise_level**",
                  "**signal_level**",
                  "**ssid**",
                  "**wpa_configured**",
                  "**wpa_id**",
                  "**wpa_state**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
