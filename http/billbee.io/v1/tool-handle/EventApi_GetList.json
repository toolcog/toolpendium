{
  "name": "EventApi_GetList",
  "description": "Get a list of all events optionally filtered by date. This request is extra throttled to 2 calls per page per hour.",
  "parameters": {
    "type": "object",
    "properties": {
      "minDate": {
        "format": "date-time",
        "type": "string",
        "description": "Specifies the oldest date to include in the response"
      },
      "maxDate": {
        "format": "date-time",
        "type": "string",
        "description": "Specifies the newest date to include in the response"
      },
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "Specifies the page to request"
      },
      "pageSize": {
        "format": "int32",
        "type": "integer",
        "description": "Specifies the pagesize. Defaults to 50, max value is 250"
      },
      "typeId": {
        "items": {
          "format": "int32",
          "type": "integer"
        },
        "type": "array",
        "description": "Filter for specific event types"
      },
      "orderId": {
        "format": "int64",
        "type": "integer",
        "description": "Filter for specific order id"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/events{?minDate,maxDate,page,pageSize,typeId,orderId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
