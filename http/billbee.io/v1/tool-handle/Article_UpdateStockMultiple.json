{
  "name": "Article_UpdateStockMultiple",
  "description": "Update the stock qty for multiple articles at once",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "items": {
          "$ref": "#/$defs/Billbee.Interfaces.BillbeeAPI.Model.UpdateStockApiModel"
        },
        "type": "array"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "Billbee.Interfaces.BillbeeAPI.Model.UpdateStockApiModel": {
        "properties": {
          "AutosubtractReservedAmount": {
            "description": "Automatically reduce the NewQuantity by the currently not fulfilled amount",
            "type": "boolean"
          },
          "BillbeeId": {
            "description": "Optional the ID of the Billbee product to update",
            "format": "int64",
            "type": "integer"
          },
          "DeltaQuantity": {
            "description": "This parameter is currently ignored",
            "format": "double",
            "type": "number"
          },
          "ForceSendStockToShops": {
            "description": "If true, every sent stockchange is stored and transmitted to the connected shop, even if the value has not changed",
            "type": "boolean"
          },
          "NewQuantity": {
            "description": "The new absolute stock quantity for the product you want to set",
            "format": "double",
            "type": "number"
          },
          "OldQuantity": {
            "description": "This parameter is currently ignored",
            "format": "double",
            "type": "number"
          },
          "Reason": {
            "description": "Optional a reason text for the stock update",
            "type": "string"
          },
          "Sku": {
            "description": "The SKU of the product to update",
            "type": "string"
          },
          "StockId": {
            "description": "Optional the stock id if the feature multi stock is activated",
            "format": "int64",
            "type": "integer"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/products/updatestockmultiple"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**Data**",
              {
                "$ul": [
                  "**CurrentStock**: The new value for current stock after the update",
                  "**Message**: A human readable message that explains the result of the operation",
                  "**OldStock**: The old value for current stock before the update",
                  "**SKU**: The SKU of the article to update the current stock",
                  "**UnfulfilledAmount**: The value of the unfulfilled amount (reserved) of the article"
                ]
              }
            ],
            "**ErrorCode**",
            "**ErrorDescription**",
            "**ErrorMessage**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": "Item"
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
