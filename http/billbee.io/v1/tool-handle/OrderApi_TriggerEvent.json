{
  "name": "OrderApi_TriggerEvent",
  "description": "Triggers a rule event",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "The id of the order"
      },
      "body": {
        "$ref": "#/$defs/Rechnungsdruck.WebApp.Controllers.Api.OrderApiController.TriggerEventContainer"
      }
    },
    "required": [
      "id",
      "body"
    ],
    "$defs": {
      "Rechnungsdruck.WebApp.Controllers.Api.OrderApiController.TriggerEventContainer": {
        "properties": {
          "DelayInMinutes": {
            "description": "The delay in minutes until the rule is executed",
            "format": "int32",
            "type": "integer"
          },
          "Name": {
            "description": "Name of the event",
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/orders/{id}/trigger-event"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
