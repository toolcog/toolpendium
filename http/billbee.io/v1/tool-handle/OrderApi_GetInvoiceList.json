{
  "name": "OrderApi_GetInvoiceList",
  "description": "Get a list of all invoices optionally filtered by date. This request ist throttled to 1 per 1 minute for same page and minInvoiceDate",
  "parameters": {
    "type": "object",
    "properties": {
      "minInvoiceDate": {
        "format": "date-time",
        "type": "string",
        "description": "Specifies the oldest invoice date to include"
      },
      "maxInvoiceDate": {
        "format": "date-time",
        "type": "string",
        "description": "Specifies the newest invoice date to include"
      },
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "Specifies the page to request"
      },
      "pageSize": {
        "format": "int32",
        "type": "integer",
        "description": "Specifies the pagesize. Defaults to 50, max value is 250"
      },
      "shopId": {
        "items": {
          "format": "int64",
          "type": "integer"
        },
        "type": "array",
        "description": "Specifies a list of shop ids for which invoices should be included"
      },
      "orderStateId": {
        "items": {
          "format": "int32",
          "type": "integer"
        },
        "type": "array",
        "description": "Specifies a list of state ids to include in the response"
      },
      "tag": {
        "items": {
          "type": "string"
        },
        "type": "array"
      },
      "minPayDate": {
        "format": "date-time",
        "type": "string"
      },
      "maxPayDate": {
        "format": "date-time",
        "type": "string"
      },
      "includePositions": {
        "type": "boolean"
      },
      "excludeTags": {
        "type": "boolean",
        "description": "If true the list of tags passed to the call are used to filter orders to not include these tags"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/orders/invoices{?minInvoiceDate,maxInvoiceDate,page,pageSize,shopId,orderStateId,tag,minPayDate,maxPayDate,includePositions,excludeTags}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Data**",
            "**ErrorCode**",
            "**ErrorDescription**",
            "**ErrorMessage**",
            [
              "**Paging**",
              {
                "$ul": [
                  "**Page**",
                  "**PageSize**",
                  "**TotalPages**",
                  "**TotalRows**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
