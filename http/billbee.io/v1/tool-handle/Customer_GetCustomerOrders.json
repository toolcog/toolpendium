{
  "name": "Customer_GetCustomerOrders",
  "description": "Queries a list of orders from a customer",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "format": "int64",
        "type": "integer",
        "description": "The id of the customer"
      },
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "The current page to request starting with 1"
      },
      "pageSize": {
        "format": "int32",
        "type": "integer",
        "description": "The pagesize for the result list. Values between 1 and 250 are allowed"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/customers/{id}/orders{?page,pageSize}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Data**",
            "**ErrorCode**",
            "**ErrorDescription**",
            "**ErrorMessage**",
            [
              "**Paging**",
              {
                "$ul": [
                  "**Page**",
                  "**PageSize**",
                  "**TotalPages**",
                  "**TotalRows**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
