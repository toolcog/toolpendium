{
  "name": "CustomerAddresses_GetAll",
  "description": "Get a list of all customer addresses",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "The current page to request starting with 1 (default is 1)"
      },
      "pageSize": {
        "format": "int32",
        "type": "integer",
        "description": "The page size for the result list. Values between 1 and 250 are allowed. (default is 50)"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/customer-addresses{?page,pageSize}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Data**",
            "**ErrorCode**",
            "**ErrorDescription**",
            "**ErrorMessage**",
            [
              "**Paging**",
              {
                "$ul": [
                  "**Page**",
                  "**PageSize**",
                  "**TotalPages**",
                  "**TotalRows**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
