{
  "name": "Article_GetReservedAmount",
  "description": "Queries the reserved amount for a single article by id or by sku",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "The id or the sku of the article to query"
      },
      "lookupBy": {
        "type": "string",
        "description": "Either the value id or the value sku to specify the meaning of the id parameter"
      },
      "stockId": {
        "format": "int64",
        "type": "integer",
        "description": "Optional the stock id if the multi stock feature is enabled"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/products/reservedamount{?id,lookupBy,stockId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**Data**",
              {
                "$ul": [
                  "**ReservedAmount**: The reserve (not fulfilled) qty of the article"
                ]
              }
            ],
            "**ErrorCode**",
            "**ErrorDescription**",
            "**ErrorMessage**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
