{
  "name": "OrderApi_Find",
  "description": "Find a single order by its external id (order number)",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string",
        "description": "The order id from the external system"
      },
      "partner": {
        "type": "string",
        "description": "Optional the name of the shop/marketplace the order was imported from"
      }
    },
    "required": [
      "id",
      "partner"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/orders/find/{id}/{partner}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
