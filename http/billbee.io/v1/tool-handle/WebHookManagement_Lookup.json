{
  "name": "WebHookManagement_Lookup",
  "description": "Looks up a registered WebHook with the given {id} for a given user.",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "string"
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/webhooks/{id}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Description**",
            "**Filters**",
            "**Headers**",
            "**Id**",
            "**IsPaused**",
            "**Properties**",
            "**Secret**",
            "**WebHookUri**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
