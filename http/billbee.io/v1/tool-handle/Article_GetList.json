{
  "name": "Article_GetList",
  "description": "Get a list of all products",
  "parameters": {
    "type": "object",
    "properties": {
      "page": {
        "format": "int32",
        "type": "integer",
        "description": "The current page to request starting with 1"
      },
      "pageSize": {
        "format": "int32",
        "type": "integer",
        "description": "The pagesize for the result list. Values between 1 and 250 are allowed"
      },
      "minCreatedAt": {
        "format": "date-time",
        "type": "string",
        "description": "Optional the oldest create date of the articles to be returned"
      },
      "minimumBillBeeArticleId": {
        "format": "int64",
        "type": "integer"
      },
      "maximumBillBeeArticleId": {
        "format": "int64",
        "type": "integer"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://app.billbee.io/api/v1/products{?page,pageSize,minCreatedAt,minimumBillBeeArticleId,maximumBillBeeArticleId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**Data**",
            "**ErrorCode**",
            "**ErrorDescription**",
            "**ErrorMessage**",
            [
              "**Paging**",
              {
                "$ul": [
                  "**Page**",
                  "**PageSize**",
                  "**TotalPages**",
                  "**TotalRows**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
