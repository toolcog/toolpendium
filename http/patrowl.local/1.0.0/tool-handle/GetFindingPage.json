{
  "name": "GetFindingPage",
  "description": "Get findings on finished scans.",
  "parameters": {
    "type": "object",
    "properties": {
      "scanId": {
        "type": "integer",
        "description": "Numeric ID of the scan to get findings"
      }
    },
    "required": [
      "scanId"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://patrowl.local/getfindings/{scanId}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**confidence**",
            "**description**",
            "**issue_id**",
            "**meta_links**",
            [
              "**meta_risk**",
              {
                "$ul": [
                  "**cvss_base_score**",
                  "**cvss_vector**",
                  "**exploit_available**",
                  "**exploitability_ease**",
                  "**patch_publication_date**"
                ]
              }
            ],
            "**meta_tags**",
            [
              "**meta_vuln_refs**",
              {
                "$ul": [
                  "**bid**",
                  "**cpe**",
                  "**cve**",
                  "**cwe**"
                ]
              }
            ],
            "**raw**",
            "**severity**",
            "**solution**",
            "**target_addrs**",
            "**target_proto**",
            "**timestamp**",
            "**title**",
            "**type**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.title"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    }
  }
}
