{
  "name": "exportSnapshot",
  "description": "Export a repostiory snapshot with requested services",
  "parameters": {
    "type": "object",
    "properties": {
      "serviceIds": {
        "items": {
          "type": "string"
        },
        "type": "array",
        "description": "List of service identifiers to export"
      }
    },
    "required": [
      "serviceIds"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://microcks.local/export{?serviceIds}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    }
  }
}
