{
  "name": "deleteUser",
  "description": "Delete a user",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "block_email": {
            "type": "boolean"
          },
          "block_ip": {
            "type": "boolean"
          },
          "block_urls": {
            "type": "boolean"
          },
          "delete_posts": {
            "type": "boolean"
          }
        }
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "DELETE",
    "url": {
      "$uri": "http://discourse.local/admin/users/{id}.json"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**deleted**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
