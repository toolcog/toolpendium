{
  "name": "addGroupMembers",
  "description": "Add group members",
  "parameters": {
    "type": "object",
    "properties": {
      "id": {
        "type": "integer"
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "usernames": {
            "description": "comma separated list",
            "examples": [
              "username1,username2"
            ],
            "type": "string"
          }
        }
      }
    },
    "required": [
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://discourse.local/groups/{id}/members.json"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**emails**",
            "**success**",
            "**usernames**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
