{
  "name": "downloadBackup",
  "description": "Download backup",
  "parameters": {
    "type": "object",
    "properties": {
      "filename": {
        "type": "string"
      },
      "token": {
        "type": "string"
      }
    },
    "required": [
      "filename",
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://discourse.local/admin/backups/{filename}{?token}"
    }
  },
  "responses": {}
}
