{
  "name": "inviteToTopic",
  "description": "Invite to topic",
  "parameters": {
    "type": "object",
    "properties": {
      "Api-Key": {
        "type": "string"
      },
      "Api-Username": {
        "type": "string"
      },
      "id": {
        "type": "string"
      },
      "body": {
        "properties": {
          "email": {
            "type": "string"
          },
          "user": {
            "type": "string"
          }
        },
        "type": "object"
      }
    },
    "required": [
      "Api-Key",
      "Api-Username",
      "id"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://discourse.local/t/{id}/invite.json"
    },
    "headers": {
      "Api-Key": {
        "$": "Api-Key"
      },
      "Api-Username": {
        "$": "Api-Username"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            [
              "**user**",
              {
                "$ul": [
                  "**avatar_template**",
                  "**id**",
                  "**name**",
                  "**username**"
                ]
              }
            ]
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
