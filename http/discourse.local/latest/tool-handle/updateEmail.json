{
  "name": "updateEmail",
  "description": "Update email",
  "parameters": {
    "type": "object",
    "properties": {
      "username": {
        "type": "string"
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "email": {
            "format": "email",
            "type": "string"
          }
        },
        "required": [
          "email"
        ]
      }
    },
    "required": [
      "username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://discourse.local/u/{username}/preferences/email.json"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
