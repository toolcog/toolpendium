{
  "name": "listUserActions",
  "description": "Get a list of user actions",
  "parameters": {
    "type": "object",
    "properties": {
      "offset": {
        "type": "integer"
      },
      "username": {
        "type": "string"
      },
      "filter": {
        "type": "string"
      }
    },
    "required": [
      "offset",
      "username",
      "filter"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://discourse.local/user_actions.json{?offset,username,filter}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**user_actions**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
