{
  "name": "createUser",
  "description": "Creates a user",
  "parameters": {
    "type": "object",
    "properties": {
      "Api-Key": {
        "type": "string"
      },
      "Api-Username": {
        "type": "string"
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "active": {
            "description": "This param requires an api key in the request header\nor it will be ignored",
            "type": "boolean"
          },
          "approved": {
            "type": "boolean"
          },
          "email": {
            "type": "string"
          },
          "external_ids": {
            "type": "object"
          },
          "name": {
            "type": "string"
          },
          "password": {
            "type": "string"
          },
          "user_fields[1]": {
            "type": "boolean"
          },
          "username": {
            "type": "string"
          }
        },
        "required": [
          "name",
          "email",
          "password",
          "username"
        ]
      }
    },
    "required": [
      "Api-Key",
      "Api-Username"
    ]
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "http://discourse.local/users.json"
    },
    "headers": {
      "Api-Key": {
        "$": "Api-Key"
      },
      "Api-Username": {
        "$": "Api-Username"
      }
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**active**",
            "**message**",
            "**success**",
            "**user_id**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
