{
  "name": "changePassword",
  "description": "Change password",
  "parameters": {
    "type": "object",
    "properties": {
      "token": {
        "type": "string"
      },
      "body": {
        "additionalProperties": false,
        "properties": {
          "password": {
            "type": "string"
          },
          "username": {
            "type": "string"
          }
        },
        "required": [
          "username",
          "password"
        ]
      }
    },
    "required": [
      "token"
    ]
  },
  "handler": "http",
  "request": {
    "method": "PUT",
    "url": {
      "$uri": "http://discourse.local/users/password-reset/{token}.json"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
