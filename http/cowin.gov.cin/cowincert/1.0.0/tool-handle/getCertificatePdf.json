{
  "name": "getCertificatePdf",
  "description": "Download the certificate in pdf format",
  "parameters": {
    "type": "object",
    "properties": {
      "body": {
        "$ref": "#/$defs/CertificateRequest"
      }
    },
    "required": [
      "body"
    ],
    "$defs": {
      "CertificateRequest": {
        "properties": {
          "beneficiaryId": {
            "type": "string"
          },
          "mobile": {
            "type": "string"
          }
        },
        "type": "object"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "POST",
    "url": {
      "$uri": "https://cowin.gov.cin/cert/external/pdf/certificate"
    },
    "body": {
      "$": "body",
      "encode": "json"
    }
  },
  "responses": {}
}
