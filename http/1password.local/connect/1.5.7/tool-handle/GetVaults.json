{
  "name": "GetVaults",
  "description": "Get all Vaults",
  "parameters": {
    "type": "object",
    "properties": {
      "filter": {
        "example": "name eq \"Some Vault Name\"",
        "type": "string",
        "description": "Filter the Vault collection based on Vault name using SCIM eq filter"
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://1password.local/vaults{?filter}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "List"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**attributeVersion**: The vault version",
            "**contentVersion**: The version of the vault contents",
            "**createdAt**",
            "**description**",
            "**id**",
            "**items**: Number of active items in the vault",
            "**name**",
            "**type**",
            "**updatedAt**"
          ]
        },
        {
          "$each": "$.body.*",
          "$as": "item",
          "$block": [
            {
              "$h2": {
                "$": "$.item.name"
              }
            },
            {
              "$lang": "json",
              "$code": {
                "$encode": "json",
                "$indent": true,
                "$content": {
                  "$": "$.item"
                }
              }
            }
          ]
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**: A message detailing the error",
            "**status**: HTTP Status Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
