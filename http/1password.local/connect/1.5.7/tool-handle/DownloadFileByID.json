{
  "name": "DownloadFileByID",
  "description": "Get the content of a File",
  "parameters": {
    "type": "object",
    "properties": {
      "vaultUuid": {
        "format": "uuid",
        "type": "string",
        "description": "The UUID of the Vault the item is in"
      },
      "itemUuid": {
        "format": "uuid",
        "type": "string",
        "description": "The UUID of the Item the File is in"
      },
      "fileUuid": {
        "type": "string",
        "description": "UUID of the file to get content from"
      }
    },
    "required": [
      "vaultUuid",
      "itemUuid",
      "fileUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://1password.local/vaults/{vaultUuid}/items/{itemUuid}/files/{fileUuid}/content"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$lang": "json",
      "$code": {
        "$encode": "json",
        "$indent": true,
        "$content": {
          "$": "$.body"
        }
      }
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**: A message detailing the error",
            "**status**: HTTP Status Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**: A message detailing the error",
            "**status**: HTTP Status Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
