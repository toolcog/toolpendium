{
  "name": "GetVaultById",
  "description": "Get Vault details and metadata",
  "parameters": {
    "type": "object",
    "properties": {
      "vaultUuid": {
        "pattern": "^[\\da-z]{26}$",
        "type": "string",
        "description": "The UUID of the Vault to fetch Items from"
      }
    },
    "required": [
      "vaultUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://1password.local/vaults/{vaultUuid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**attributeVersion**: The vault version",
            "**contentVersion**: The version of the vault contents",
            "**createdAt**",
            "**description**",
            "**id**",
            "**items**: Number of active items in the vault",
            "**name**",
            "**type**",
            "**updatedAt**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**: A message detailing the error",
            "**status**: HTTP Status Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
