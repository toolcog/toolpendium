{
  "name": "GetVaultItemById",
  "description": "Get the details of an Item",
  "parameters": {
    "type": "object",
    "properties": {
      "vaultUuid": {
        "pattern": "^[\\da-z]{26}$",
        "type": "string",
        "description": "The UUID of the Vault to fetch Item from"
      },
      "itemUuid": {
        "pattern": "^[\\da-z]{26}$",
        "type": "string",
        "description": "The UUID of the Item to fetch"
      }
    },
    "required": [
      "vaultUuid",
      "itemUuid"
    ]
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "http://1password.local/vaults/{vaultUuid}/items/{itemUuid}"
    }
  },
  "responses": {
    "200": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**category**",
            "**createdAt**",
            "**favorite** (default: false)",
            "**id**",
            "**lastEditedBy**",
            "**state**",
            "**tags**",
            "**title**",
            "**updatedAt**",
            "**urls**",
            [
              "**vault**",
              {
                "$ul": [
                  "**id**"
                ]
              }
            ],
            "**version**",
            "**fields**",
            "**files**",
            "**sections**"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "401": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**: A message detailing the error",
            "**status**: HTTP Status Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "403": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**: A message detailing the error",
            "**status**: HTTP Status Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    },
    "404": {
      "$encode": "markdown",
      "$block": [
        {
          "$h1": "Object"
        },
        "**Key properties:**",
        {
          "$ul": [
            "**message**: A message detailing the error",
            "**status**: HTTP Status Code"
          ]
        },
        {
          "$lang": "json",
          "$code": {
            "$encode": "json",
            "$indent": true,
            "$content": {
              "$": "$.body"
            }
          }
        }
      ]
    }
  }
}
