{
  "name": "fetchFieldById",
  "description": "Retrieve a given **Field** by ID.",
  "parameters": {
    "type": "object",
    "properties": {
      "fieldId": {
        "format": "uuid",
        "type": "string",
        "description": "Unique identifier of the Field."
      }
