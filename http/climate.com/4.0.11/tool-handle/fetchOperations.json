  "parameters": {
    "type": "object",
    "properties": {
      "resourceOwnerId": {
        "type": "string",
        "description": "Optional comma-separated list of resource owner unique identifiers by which to filter results."
      }
    }
  },
  "handler": "http",
  "request": {
    "method": "GET",
    "url": {
      "$uri": "https://platform.climate.com//v4/operations/all{?resourceOwnerId}"
    }
